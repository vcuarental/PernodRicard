    <meta charset="UTF-8" />
    <head>
        <script>
        var isEditMode;
        var isOwner;
        var oTable;
        var deletedRowIds=[];
        var cloneSavedRow=[];
        var savedRows=[];
        var poid;
        var poType;
        var itemGroupNameList;
        var itemGroupMap;
        var warehouseNameList;
        var warehouseMap;
        var outletNameList;
