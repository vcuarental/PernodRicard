                                        if(data == vasList[i].value) {
                                            htmlPicklist += '<option value="'+vasList[i].value+'" selected>'+vasList[i].name+'</option>';
                                        } else {
                                            htmlPicklist += '<option value="'+vasList[i].value+'">'+vasList[i].name+'</option>';
                                        }
                                    }
                                    htmlPicklist +='</select>';
                                    return htmlPicklist;
