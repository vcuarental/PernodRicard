<apex:page standardController="ASI_MFM_Payment__c" extensions="ASI_MFM_CN_StructureCost_Main_Ctrl" action="{!getPrefixes}">
    <apex:sectionHeader title="Select Payment Prefix"/>
    <apex:pageMessages />
    <apex:form >
        <apex:pageBlock >
            <apex:pageBlockButtons location="bottom">
                <apex:commandButton value="Proceed" action="{!proceed}"/>
                <apex:commandButton value="Cancel" action="{!cancel}"/>
            </apex:pageBlockButtons>
            <apex:pageBlockSection >
                <apex:pageBlockSectionItem >
                    <apex:outputLabel >Prefix</apex:outputLabel>
                    <apex:selectList value="{!selected_prefix}" size="1">
                        <apex:selectOptions value="{!available_prefixes}"/>
                    </apex:selectList>
                </apex:pageBlockSectionItem> 
            </apex:pageBlockSection>
        </apex:pageBlock>
    </apex:form>
</apex:page>