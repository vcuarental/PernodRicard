navigator.language : navigator.userLanguage;	if( userLang === null || userLang ==='' || userLang === undefined ) {		userLang = 'en-US';	}		var assignedUserLng = "{!JSENCODE(userLang)}";	if( assignedUserLng === null || assignedUserLng ==='' || assignedUserLng === undefined ) {		var sURL = "{!JSENCODE(SiteLoginURL)}" + '?lang='+userLang;		if(incID != null && incID!='')			sURL = sURL + '&inc=' + incID ;        if(iscalledFromEmail!=null && iscalledFromEmail!='')            sURL = sURL + '&iscalledFromEmail=true';            if(recordId!= null && recordId!='')			sURL = sURL + '&id=' + recordId ;        if(typeStr!= null && typeStr!='')			sURL = sURL + '&type=' + typeStr ;        if(keywordStr!= null && keywordStr!='')			sURL = sURL + '&keyword=' + keywordStr ;		if(componentStr!= null && componentStr!='')			sURL = sURL + '&component=' + componentStr ;		window.location =  sURL;	}		window.onload=function() {     	 window.setTimeout('setFocus()', 800);     	}		function setFocus() {		 var loginName = document.getElementById('{!$Component.siteLoginForm.unId}');     	 loginName.focus();		}		function createCookie(name,value,days) {    		if (days) {        			var date = new Date();        			date.setTime(date.getTime()+(days*24*60*60*1000));        			var expires = "; expires="+date.toGMTString();    		} else var expires = "";    		  document.cookie = name+"="+value+expires+"; path=/;SameSite=None;Secure"; 	}	function readCookie(name) {    		var nameEQ = name + "=";    		var ca = document.cookie.split(';');    		for(var i=0;i < ca.length;i++) {        			var c = ca[i];        			while (c.charAt(0)==' ') c = c.substring(1,c.length);        			if (c.indexOf(nameEQ) == 0) 				return c.substring(nameEQ.length,c.length);    			}    			return ''; 		} 	function eraseCookie(name) {		createCookie(name,"",-1); 	}	</script>	<apex:outputPanel id="siteLoginPanel">    <body style="background-color: #ffffff; margin:0;">    	    <apex:outputPanel id="postloginpanel" rendered="{!platformUser}" >    <script  type="text/javascript" language="javascript">			document.postlogin.logoutURL.value = window.location.href;            document.postlogin.ssoStartPage.value = window.location.href;            if( incID != '')            	{	            document.postlogin.startURL.value =  document.postlogin.startURL.value + '&inc=' + incID;	            }            if(recordId!=''){	            document.postlogin.startURL.value =  document.postlogin.startURL.value + '&id=' + recordId;	        }	             if(typeStr!=''){	            document.postlogin.startURL.value =  document.postlogin.startURL.value + '&type=' + typeStr;	        }	             if(keywordStr!=''){	            document.postlogin.startURL.value =  document.postlogin.startURL.value + '&keyword=' + keywordStr;	        }	             if(componentStr!=''){	            document.postlogin.startURL.value =  document.postlogin.startURL.value + '&component=' + componentStr;	        }	             if(iscalledFromEmail!=''){                document.postlogin.startURL.value =  document.postlogin.startURL.value + '&iscalledFromEmail=' + iscalledFromEmail;            } 			document.postlogin.submit();	</script>	</apex:outputPanel>	    	<table style="width:100%;height:100%;align:center;margin:0;" cellSpacing="0" cellPadding="0" border="0" ID="Table1">	<apex:form id="siteLoginForm" forceSSL="true">	   	<tr style="height:80px;">      <td style="width:2%;">&nbsp;</td>      <td style="width:28%;">&nbsp;</td>      <td style="width:70%;">&nbsp;</td>   	</tr>   	<tr style="height:15px;">        <td style="width:2%;">&nbsp;</td>        <td style="valign:bottom;">            <c:SSLoginHeaderImageComponent />        </td>        <td style="align:right;">             <table style="width: 100%;"><tr><td width="85%">&nbsp;</td><td style="align:right;">             <c:SSLogoComponent />             </td><td width="5%">&nbsp;</td></tr></table>        </td>   	   	</tr>  	<tr style="height:15px;" valign="top">    <td style="width:2%;">&nbsp;</td>    <td style="align:left;valign:top;font-size: 10pt; font-family:Arial Narrow, MS Sans Serif; color:#fe5000">        <b tabIndex="7">Version {!JSENCODE(RFVersion)}</b>    </td>    <td style="align:right;">        &nbsp;    </td>  	</tr>  	<tr style="height:20px;"><td colspan="3"></td></tr>      <tr class="ssLoginPageBg">      <td colspan="2" style="height:192px;">      	<c:SSBackGroundImageComponent />      </td>      <td style="height:192px;">        <table border="0" cellpadding="0" cellspacing="0" ID="TableLogin" style="height:100%;width:100%;color:#c1e1f5;">                <tr style="height:8px;">                    <td colspan="1" style="width:50px;"></td>                    <td colspan="1" style="width:100px;"></td>                    <td colspan="1"></td>                </tr>                <tr style="height:25px;valign:top;">                    <td colspan="1" style="width:50px;"></td>                    <td colspan="1" style="width:100px;"></td>                    <td align="left" style="height:25px;position:fixed;margin-top:10px;margin-top/*\**/: 15px\9">                                         <span class="defaultFont">                    <strong>{!$Label.SSPleaseLogIn}</strong></span>                     </td>                </tr>                <tr style="height:25px;">                     <td colspan="1" style="width:50px;"></td>                    <td class="defaultFont"><strong>{!$Label.SSUserName}</strong></td>                    <td class="log-content" style="valign:middle;vertical-align: middle;">					<apex:outputText value="CRUDFLSCheck# No association with any sObject fields." rendered="false"/>                    <apex:inputText id="unId" size="30" title="{!$Label.bmcservicedesk__ssusernametooltip}" tabindex="1" value="{!loginUserId}" style="width:275px;height:17px;valign:middle;vertical-align: middle;padding-left:2px;padding-top:3px" />    		            <script type="text/javascript">                            document.getElementById('{!$Component.unId}').value=readCookie('un');                        </script>                    </td>                </tr>                <tr style="height:25px;">                     <td colspan="1" style="width:50px;"></td>                    <td class="defaultFont"><strong>{!$Label.SSPassword}</strong></td>                    <td class="log-content" style="valign:middle">					<apex:inputSecret id="pw" size="30" tabindex="2" title="{!$Label.bmcservicedesk__sspasswordtooltip}" value="{!loginPassword}" style="width:275px;height:17px;valign:middle;padding-left:2px;padding-top:3px" />                    </td>                </tr>                <tr style="height:3px;">                    <td colspan="3"></td>                </tr>                <tr style="height:30px;" valign="top">                     <td colspan="2"></td>                    <td valign="top">                    <table style="align:left;margin:0;">                    <tr>                    <td style="align:left;" valign="center">                        <apex:commandButton tabindex="3" value="{!$Label.bmcservicedesk__sslogin}" action="{!login}" onclick="createCookie('un',document.getElementById('{!$Component.unId}').value)" styleClass="bgBtnGrey" style="height:21px;border:0;padding:0 5px 0 5px;"/>                    </td>                    <td style="height:27px;" valign="top">                        &#160;&#160;&#160;&#160;&#160;						<apex:outputLink tabindex="4" value="{!JSENCODE(ForgotPassordPageUrl)}" id="siteForgotLink" style="color:#ffffff; valign:top" styleClass="clsCmdLink" >							<strong><apex:outputText value="{!$Label.bmcservicedesk__ssforgotyourpassword}" /></strong>						</apex:outputLink>                    </td>                    </tr>                    </table>                    </td>                </tr>                <tr style="height:10px;">                    <td colspan="3"></td>                </tr>        </table>      </td>   </tr>   <tr style="height:10px;">        <td style="width:2%;">&nbsp;</td>           <td style="valign:bottom;">        </td>        <td style="align:right;">             <table border="0" cellpadding="0" cellspacing="0" ><tr>                    <td colspan="1" style="width:50px;"></td>                    <td colspan="1" style="width:100px;"></td>                    <td align="left" style="height:25px;valign:top;">                              <!--   <apex:outputLink value="{!$Page.BMCServiceDesk__SSSiteRegister}" rendered="{!IF(hideSiteRegistration,false,true)}" id="siteRegisterLink" style="font-weight:bold;" styleClass="clsCmdLink" >             	<apex:outputText value="{!$Label.SSNewUserRegisterNow}" />             </apex:outputLink> -->                                       </td>                          </tr>             <tr style="height:20px; vertical-align:text-bottom; vertical-align:bottom;">             		<td colspan="1" style="width:50px;"></td>                    <td colspan="1" style="width:100px;"></td>	             <td style="font-size:11px; font-weight:bold; color:#FF0000; vertical-align:bottom;">	             	<div>	             	<apex:Messages layout="list">	             	</apex:Messages>	             	</div>	             </td>             </tr>                          </table>        </td>   </tr>      <tr style="height:175px;">      <td style="align:left;" colspan="3"></td>   </tr>   <tr style="height:10px;  background-color:#ebebeb;">      <td style="align:left;">&nbsp;</td>      <td style="align:left; valign:bottom; font-size: 8pt; font-family:Tahoma;" colspan="2">        <span tabindex="5">             <apex:outputText value="{!SUBSTITUTE($Label.bmcservicedesk__ssbmccopyrightline1, '{0}', TEXT(YEAR(TODAY())))}"></apex:outputText><BR/>            <LABEL>{!$Label.SSBMCCopyrightLine2}</LABEL>        </span>      </td>      </tr>		</apex:form>	</table>	</body>		</apex:outputPanel></apex:page>