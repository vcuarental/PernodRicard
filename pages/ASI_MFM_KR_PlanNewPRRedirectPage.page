<apex:page standardController="ASI_MFM_Plan__c" extensions="ASI_MFM_KR_PlanNewPRRedirectCtrl" action="{!redirect}" >
    
    
    <apex:outputField value="{!ASI_MFM_Plan__c.Name}" rendered="false" />
    <apex:outputField value="{!ASI_MFM_Plan__c.ASI_MFM_Status__c}" rendered="false" />
    <apex:outputField value="{!ASI_MFM_Plan__c.ASI_MFM_Currency__c}" rendered="false" /> 
    <apex:sectionHeader title="Make Purchase Request"/>
    
    <apex:form >
        <apex:pageMessages />
        <apex:pageBlock >
            <apex:pageBlockButtons location="bottom">
                <apex:commandButton value="Cancel" action="{!cancel}"/>
            </apex:pageBlockButtons>
        </apex:pageBlock>
    </apex:form> 
    
</apex:page>