<script type="text/javascript">
    var table;
    var myApp = angular.module('myapp',[]);
    myApp.controller('myCtrl',function($scope,$filter,$location){
        $scope.initPage = function() {
            $scope.reloading = true;
            $scope.quotationInfoS=[];
            $scope.seSupplierInfoS = [];

            var quotationList = {!quotationList};
            if (quotationList == null || typeof(quotationList) == 'undefined') {
                quotationList = [];
            }
            $scope.quotationInfoS = quotationList;
            $scope.IsWinQuotation = false;

            var selectSupplierIds = [];
            console.info('---first---');
            console.info($scope.quotationInfoS);
            $scope.quotationInfoS.forEach(function(item) {
                item.check = false; 
                var itemQ = item.Quotations_quotation__r.records[0];     
                itemQ.ASI_CTY_CN_Vendor_Unitl_Price__c = number_format(parseFloat(itemQ.ASI_CTY_CN_Vendor_Unitl_Price__c), 2, '.', ',');

                itemQ.ASI_CTY_CN_Vendor_Model__c = number_format(parseFloat(itemQ.ASI_CTY_CN_Vendor_Model__c), 2, '.', ',');

                itemQ.ASI_CTY_CN_Vendor_Sample__c = number_format(parseFloat(itemQ.ASI_CTY_CN_Vendor_Sample__c), 2, '.', ',');

                itemQ.ASI_CTY_CN_Vendor_Total_Price__c = number_format(parseFloat(itemQ.ASI_CTY_CN_Vendor_Total_Price__c), 2, '.', ',');

                itemQ.ASI_CTY_CN_Vendor_Total_Freight__c = number_format(parseFloat(itemQ.ASI_CTY_CN_Vendor_Total_Freight__c), 2, '.', ',');

                itemQ.ASI_CTY_CN_Vendor_Confirmed_Price__c = number_format(parseFloat(itemQ.ASI_CTY_CN_Vendor_Confirmed_Price__c), 2, '.', ',');
                selectSupplierIds.push(item.ASI_CTY_CN_Vendor_Supplier__r.Id);
                if (item.ASI_CTY_CN_Vendor_Result_Status__c == 'Win-Bid') {
                    $scope.IsWinQuotation = true;
                }
            });
            $scope.selectSupplierIds = selectSupplierIds;

            $scope.prlId = '{!prlIdBase}';
            $scope.initQuotationInfo();

            var mergePRLineList = {!purLineList};
            if (mergePRLineList == null || typeof(mergePRLineList) == 'undefined') {
                mergePRLineList = [];
            }

            $scope.isMerge = false;
            if (mergePRLineList.length > 0) {
                $scope.isMerge = true;
            }
            $scope.mergePRLineList = mergePRLineList;

            $scope.openSupplierList = false;

            $scope.isReadPriv = {!isReadPriv};
            console.info('---$scope.isReadPriv---')
            console.info($scope.isReadPriv);
            $scope.reloading = false;
        };

        $scope.initQuotationInfo = function() {
            var quotationList = $scope.quotationInfoS;
            if (quotationList != null && quotationList.length > 0) {
                quotationList.forEach(function(item , index) {
                    var status = item.ASI_CTY_CN_Vendor_Status__c;
                    var resultStatus = item.ASI_CTY_CN_Vendor_Result_Status__c;

                    if (!isEmpty(status) && status === 'Rejected') {
                        item.Status = '{! $Label.ASI_CTY_CN_Vendor_Bidding_Status_Rejected}';
                    }

                    if (!isEmpty(resultStatus) && resultStatus === 'Abandon-Bid') {
                        item.Status = '{! $Label.ASI_CTY_CN_Vendor_Bidding_Result_Status_Abandon_Bid}';
                    }

                    if (!isEmpty(resultStatus) && resultStatus === 'Win-Bid' && status === 'Completed') {
                        item.Status = '{! $Label.ASI_CTY_CN_Vendor_Bidding_Result_Status_Win_Bid}';
                    }

                    if (!isEmpty(resultStatus) && resultStatus === 'Fail-Bid') {
