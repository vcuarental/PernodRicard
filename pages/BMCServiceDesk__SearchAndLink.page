        function ValidateReserveCharactersForSOSL(value)
		{
			var chars = new Array('\\', '?', '&', '|', '!', '{', '}', '[', ']', '(', ')', '^', '~', '*', ':', '"', '+', '-');
			for(var j = 0 ; j < chars.length; j++)
			{
				value = value.split(chars[j]).join('\\'+chars[j]);
			}	 
			return value;
		}
