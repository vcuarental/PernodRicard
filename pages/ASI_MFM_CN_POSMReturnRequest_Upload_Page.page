        <script id="upload.html" type="text/ng-template">
            <div id="wrap">
            <div class="content">
                <!-- <section class="breadcrumbs" data-section-name="">
                  <div class="inner">                      
                      <span>CSV上载</span>
                  </div>
                </section>  --> 
                <div class="styleWrap">
                    <section class="" data-section-name="">
                        <div class="inner">
                            <article id="">
                                <div class="s-box sub-sec-ttl-style03">
                                    <span style="font-size:20px;font-weight:900;">POSM Return Request CSV上载</span>
                                    <!-- <p style="width:35%;float:left;"> POSM Return Request CSV上载</p> -->
                                    <form action="#">
                                        <div class="input-file-container" style="float:right;">
                                            <a href="" ng-click="downloadTemplate()" class="fas fa-paperclip fa-2x" style="margin-right:10px;"></a>
                                            <input class="input-file" id="csv-file" type="file" accept=".csv">
                                            <label tabindex="0" for="csv-file" class="btn-style01 csv-upload-label"  style="background-color: #7ea5d0;"><i id='csv-select' class="fa fa-file" aria-hidden="true"> 选择文件...</i></label>
                                            <a href="" ng-click="uploadCSV()" class="btn btn-style01 transit-02" style="width:190px">检视</a>          
                                        </div>
                                    </form>
                                </div>
                            </article>

                            <div class="my-order-conts" ng-show="upload.show">
    

                                <div class="s-box sub-sec-ttl-style02">
                                    <p>整体上载</p>
                                    <div class="sub-lead s-box">
                                        <p>总数: {{batchCount}} 个订单</p>
                                    </div>
                                    <div style="float:left" class="validation" ng-show="flag.reportBtnShow">
                                        
                                        <a href="" ng-click="batchSubmitCheck()" style="color:blue;" ng-show="showBatch==false"><u>批量提交审批</u></a>
                                        <a href="" ng-click="batchSubmitCancel()" style="color:blue;" ng-show="showBatch==true"><u>取消批量提交审批</u></a> 
                                        <i class="fa fa-check" style="color: green;" ng-show="showBatch==true"></i>
                                        <i class="fas fa-times" style="color: red;" ng-show="showBatch==false"></i>
                                    </div>
                                    <div style="float:right">
                                        
                                        
                                        <a href="" ng-click="downloadFailReport()" style="color:blue; cursor: pointer;" ng-show="flag.reportBtnShow"><u>汇出失败项目</u></a>

                                        <a href="" ng-click="downloadAllResultReport()" style="color:blue; cursor: pointer;margin-right:10px;" ng-show="flag.reportBtnShow"><u>汇出所有项目</u></a>
                                    </div>
                                </div><br/>

                                <div class="" >
                                    <table class="fixed_header">
                                        <thead><tr>
                                                <th style="width:15px;" >#</th>
                                                <th style="width:50px;" >退货申请日期</th>
                                                <th style="width:100px;" >退货时间段</th>
                                               <!--  <th style="width:120px;" >提货地址</th>
                                                <th style="width:120px;" >发货地址</th> -->
                                                <th style="width:110px;" >提货联系人</th>
                                                <!-- <th style="width:110px;" >退货仓库</th> -->
                                                <th style="width:110px;" >提货联系电话</th>
                                                <th style="width:110px;" >退货申请单号</th>
                                                <th style="width:110px;" class="validation" >
                                                    <a href="" ng-click="submitAll('all')" ng-show="flag.batchSubmitReady" style="color:blue;"><u>全部提交</u></a>  
                                                    <a href="" class="fas fa-chevron-up" style='color:red;' ng-click="fixedTable_ScrollUp()" ></a> 
                                                    <a href="" class="fas fa-chevron-down" style='color:green;' ng-click="fixedTable_ScrollDown()" ></a>
                                                </th>
                                        </tr></thead>
                                                
                                      <tbody id="fixed_header_tbody" ><tr ng-repeat="order in uploadBatchs track by $index" ng-click="selectHeader($index)" class="{{order.selected}}">
                                        <td>{{order.index}}</td>
                                        <td>{{order.returnRequestDate}}</td>
                                        <td>{{order.returnRequestTime}}</td>
                                       <!--  <td>{{order.formAddress}}</td>
                                        <td>{{order.toAddress}}</td> -->
                                        <td>{{order.recipient}}</td>
                                        <!-- <td>{{order.towarehouse}}</td> -->
                                        <td>{{order.recipientContact}}</td>
                                        <td>{{order.docnum}}</td>
                                        <td class="validation" >
                                            <i class="{{order.status}} fas fa-circle-notch fa-spin"></i>
                                            <i class="fas fa-exclamation-triangle" style="color: orange;" ng-show="order.status=='caution'"></i>
                                            <i class="fa fa-check" style="color: green;" ng-show="order.status=='success'"></i>
                                            <i class="fas fa-times" style="color: red;" ng-show="order.status=='fail'" data-placement="bottom" data-html="true" data-toggle="tooltip" title="{{order.error}}"></i>
                                            <a href="" ng-click="submitOrder($index)" style="color:blue;" ng-show="order.status=='ready' && flag.batchSubmitReady"><u>提交</u></a>

                                        </td>
                                        </tr></tbody>
                                    </table>
                                </div>
                                <br/><br/><br/>
                                <div class="s-box sub-sec-ttl-style02">
                                    <p>收货信息</p>
                                    <div class="sub-lead s-box">
                                        <p>&#35;{{upload.index}}</p> 
                                        <p style="color: red;">{{upload.errorMap.SubmitError}}</p> 
                                    </div>                             
                                </div>
                                
                                <div class="item-cont">
                                    <table class="upload-summary">
                                        <tr>
                                            <td class="my-order-part layoutStyle02 s-box ttl td-label" >要求提货日期</td>
                                            <td class="my-order-part layoutStyle02 s-box cont td-value" >{{upload.returnRequestDate}} <i class="fas fa-exclamation-triangle" style="color: red;" ng-show="upload.errorMap.returnRequestDate != null"  data-placement="bottom" data-html="true" data-toggle="tooltip" title="{{upload.errorMap.returnRequestDate}}"></i> </td>

                                            <td class="my-order-part layoutStyle02 s-box ttl td-label" >要求提货时间段</td>
                                            <td class="my-order-part layoutStyle02 s-box cont td-value" >{{upload.returnRequestTime}} <i class="fas fa-exclamation-triangle" style="color: red;" ng-show="upload.errorMap.returnRequestTime != null"  data-placement="bottom" data-html="true" data-toggle="tooltip" title="{{upload.errorMap.returnRequestTime}}"></i> </td>
                                        </tr>
                                        <tr>
                                            <td class="my-order-part layoutStyle02 s-box ttl td-label" >提货地址</td>
                                            <td class="my-order-part layoutStyle02 s-box cont td-value" >{{upload.formAddress}} <i class="fas fa-exclamation-triangle" style="color: red;" ng-show="upload.errorMap.formAddress != null"  data-placement="bottom" data-html="true" data-toggle="tooltip" title="{{upload.errorMap.formAddress}}"></i> </td>   

                                            <td class="my-order-part layoutStyle02 s-box ttl td-label" >送达地址</td>
                                            <td class="my-order-part layoutStyle02 s-box cont td-value" >{{upload.toAddress}} <i class="fas fa-exclamation-triangle" style="color: red;" ng-show="upload.errorMap.toAddress != null"  data-placement="bottom" data-html="true" data-toggle="tooltip" title="{{upload.errorMap.toAddress}}"></i>
                                            </td>                                    	
                                        </tr>
                                        <tr>
                                        	<td class="my-order-part layoutStyle02 s-box ttl td-label" >提货联系人</td>
                                            <td class="my-order-part layoutStyle02 s-box cont td-value" >{{upload.recipient}}
                                              <i class="fas fa-exclamation-triangle" style="color: red;" ng-show="upload.errorMap.recipient != null"  data-placement="bottom" data-html="true" data-toggle="tooltip" title="{{upload.errorMap.recipient}}"></i>
                                            </td>

                                            <td class="my-order-part layoutStyle02 s-box ttl td-label" >收货联系人</td>
                                            <td class="my-order-part layoutStyle02 s-box cont td-value" >{{upload.consignee}}
                                              <i class="fas fa-exclamation-triangle" style="color: red;" ng-show="upload.errorMap.consignee != null"  data-placement="bottom" data-html="true" data-toggle="tooltip" title="{{upload.errorMap.consignee}}"></i>
                                            </td>                          	
										</tr>

										<tr>
                                        	<td class="my-order-part layoutStyle02 s-box ttl td-label" >提货联系电话</td>
                                            <td class="my-order-part layoutStyle02 s-box cont td-value" >{{upload.recipientContact}} 
                                              <i class="fas fa-exclamation-triangle" style="color: red;" ng-show="upload.errorMap.recipientContact != null"  data-placement="bottom" data-html="true" data-toggle="tooltip" title="{{upload.errorMap.recipientContact}}"></i>
                                            </td>

                                            <td class="my-order-part layoutStyle02 s-box ttl td-label" >收货联系电话</td>
                                            <td class="my-order-part layoutStyle02 s-box cont td-value" >{{upload.consigneeContact}} 
                                              <i class="fas fa-exclamation-triangle" style="color: red;" ng-show="upload.errorMap.consigneeContact != null"  data-placement="bottom" data-html="true" data-toggle="tooltip" title="{{upload.errorMap.consigneeContact}}"></i>
                                            </td>                                                                                       
                                        </tr>
                                        <tr>
                                        	<td class="my-order-part layoutStyle02 s-box ttl td-label" >其它要求</td>
                                            <td class="my-order-part layoutStyle02 s-box cont td-value" >{{upload.otherRequest}} <i class="fas fa-exclamation-triangle" style="color: red;" ng-show="upload.errorMap.otherRequest != null"  data-placement="bottom" data-html="true" data-toggle="tooltip" title="{{upload.errorMap.otherRequest}}"></i> </td>

                                            <td class="my-order-part layoutStyle02 s-box ttl td-label" >要求送达日期</td>
                                            <td class="my-order-part layoutStyle02 s-box cont td-value" >{{upload.requestDeliveryDate}} <i class="fas fa-exclamation-triangle" style="color: red;" ng-show="upload.errorMap.requestDeliveryDate != null"  data-placement="bottom" data-html="true" data-toggle="tooltip" title="{{upload.errorMap.requestDeliveryDate}}"></i> </td>
										</tr>

                                        <tr>
                                        	
                                            <td> </td><td> </td>
                                            <td class="my-order-part layoutStyle02 s-box ttl td-label" >退货仓库</td>
                                            <td class="my-order-part layoutStyle02 s-box cont td-value" >{{upload.towarehouse}} <i class="fas fa-exclamation-triangle" style="color: red;" ng-show="upload.errorMap.towarehouse != null"  data-placement="bottom" data-html="true" data-toggle="tooltip" title="{{upload.errorMap.towarehouse}}"></i> </td>
                                        </tr>
                                       
                                    </table>
                                </div>
              
                                <!--<p class="my-order-part layoutStyle02 bdrStyle s-box" style="margin:20px 0px;"><span style="font-size:20px;">訂單摘要</span></p>-->

                                <br/><br/>
                                <div class="s-box sub-sec-ttl-style02">
                                    <p>订单摘要</p>
                                    <div class="sub-lead s-box">
                                        <p>&#35;{{upload.index}}</p>
                                    </div>
                                </div>
                                <br/>
                                
                                <div class="" >
                                    <table class="upload-preview">
                                        <tr>
                                            <th width="30%" >SKU</th>
                                            <th width="20%" >退货数量</th>
                                            <th width="20%" >库存所有者</th>
                                            <th width="20%" >增值服务</th>
                                            <th width="10%" >检测</th>
                                        </tr>
                                        <tr ng-repeat="item in upload.items track by $index" class="{{item}}">
                                            <td>{{item.sku}}</td>
                                            <td>{{item.qty}}</td> 
                                            <td>{{item.owner}}</td>
                                            <td>{{item.vas}}</td>
                                            <td class="validation">
                                                <i class="fas fa-exclamation-triangle" style="color: red;" ng-show="item.error != null"  data-placement="bottom" data-html="true" data-toggle="tooltip" title="{{item.error}}"></i>
                                                <span class="fa fa-check" ng-show="item.success != false" ></span>

                                            </td>
                                        </tr>
                                    </table>
                                </div>

                            </div>
                        </div>
                    </section>
                    </br>
                </div>
                
                                
