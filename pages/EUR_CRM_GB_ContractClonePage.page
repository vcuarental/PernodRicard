    </head>

    <apex:form >
        <apex:pageMessages id="pMessage"/>

        <apex:actionStatus id="loading">
            <apex:facet name="start">
                <c:EUR_CRM_LoadingStatus BackColor="#ffffff"
                                         borderColor="#6B6B6B"
                                         borderSize="1"
                                         height="50px"
                                         width="120px"
                                         margintop="-25px"
                                         marginleft="-60px"
                                         ImageUrl="{!$Resource.loadingStatus}"
                                         Message="Loading..."/>
            </apex:facet>
        </apex:actionStatus>

        <apex:actionFunction name="initClonedContract"
                             action="{!cloneContractHeader}"
                             status="loading" rerender="clonedContractObj, mainPanel"
                             oncomplete="console.log('Cloned Contract Obj: {!clonedContractObj}'); setTables();">
        </apex:actionFunction>

        <apex:pageBlock id="mainPanel">
            <apex:pageBlockButtons >
                <apex:commandButton value="{!$Label.EUR_CRM_Save}"
                                    action="{!saveClonedContractNew}"
                                    rerender="pMessage"
                                    status="loading"/>
                <apex:commandButton value="{!$Label.EUR_CRM_Exit}"
                                    action="{!cancel}"
                                    rerender="none"
                                    status="loading"/>
            </apex:pageBlockButtons>

            <apex:pageBlockSection title="Contract Information" columns="2" collapsible="false">
                <apex:inputField value="{!clonedContractObj.EUR_CRM_Account__c}" required="true"/>
                <apex:inputField value="{!clonedContractObj.EUR_CRM_Payment_Method__c}"/>
                <apex:inputField value="{!clonedContractObj.EUR_CRM_Contact__c}"/>
                <apex:inputField value="{!clonedContractObj.EUR_CRM_Start_Date__c}" required="true"/>
                <apex:inputField value="{!clonedContractObj.EUR_CRM_Supplier_Account__c}"/>
                <apex:inputField value="{!clonedContractObj.EUR_CRM_End_Date__c}" required="true"/>
            </apex:pageBlockSection>

            <apex:pageBlockSection title="Contract Activity Items" columns="2" >
                <apex:repeat value="{!contractObject.contractActivityItemsWithProductItems}"
                             var="activityItem">
                    <apex:pageBlock title="{!activityItem.Name}">
                        <apex:pageBlockSection columns="1" showHeader="false">
                            <apex:outputField value="{!activityItem.EUR_CRM_Amount__c}"/>
                            <apex:outputField value="{!activityItem.EUR_CRM_Mechanic_Type__c}"/>
                            <apex:outputField value="{!activityItem.EUR_CRM_Cocktail_Percent__c}"/>
                            <apex:outputText label="Number of product items" value="{!activityItem.Contract_Product_Items__r.size}"/>
                        </apex:pageBlockSection>
                    </apex:pageBlock>
                </apex:repeat>
            </apex:pageBlockSection>


