                    <apex:pageBlockSection columns="1">
                        <apex:pageBlockSectionItem rendered="mustStockWrapper.mechanicType.EUR_CRM_Has_Lump_Sum__c">
                            <apex:outputLabel value="Lump Sum:"/>
