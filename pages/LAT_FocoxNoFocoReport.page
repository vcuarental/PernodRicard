                            </apex:panelGroup>
                            
                            <apex:pageBlockSection title="{!$Label.LAT_BR_VFP03_00016}" collapsible="false" columns="1">
                                <!-- $Label.LAT_BR_VFP03_00016: "De - Até" -->
