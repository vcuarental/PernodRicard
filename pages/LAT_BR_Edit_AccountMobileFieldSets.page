		<apex:includeScript value="{!URLFOR($Resource.LAT_CompassResources, 'static/js/cnpj-validator.js')}" />
		<apex:includeScript value="{!URLFOR($Resource.LAT_CompassResources, 'static/js/edit-account.js')}"/>

	</head>

