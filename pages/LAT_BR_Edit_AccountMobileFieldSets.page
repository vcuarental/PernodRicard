		<script src="../../soap/ajax/42.0/connection.js" type="text/javascript"></script>
		<script src="../../soap/ajax/42.0/apex.js" type="text/javascript"></script>
		<apex:includeScript value="//maxcdn.bootstrapcdn.com/bootstrap/3.3.4/js/bootstrap.min.js" />
		<apex:includeScript value="//cdn.datatables.net/1.10.12/js/jquery.dataTables.min.js" />
    <style type="text/css">
    .msg-ativar {
      color: white;
    }  

    </style>


		<script type="text/javascript">

			const variables = {
				accountId: "{!Account.Id}",
				requiredFields: {!requiredFieldsMapMessageString},
				mapFieldSetWithFields: {!mapFieldSetWithFieldsString},
				isClone: {!isClone },
				accIdURL: '{!accIdURL}',
				canCreateSegment : {!canCreateSegment }
      		}

			const remoteActions = {
				getLookupFieldInformation: '{!$RemoteAction.LAT_Edit_AccountMobileFSController.getLookupFieldInformation}',
				validateAccountDataRemoting: '{!$RemoteAction.LAT_Edit_AccountMobileFSController.validateAccountDataRemoting}',
				sentToApprovalRemoting: '{!$RemoteAction.LAT_Edit_AccountMobileFSController.sentToApprovalRemoting}',
				validateRequiredFields: '{!$RemoteAction.LAT_Edit_AccountMobileFSController.validateRequiredFields}',
				doSave: '{!$RemoteAction.LAT_Edit_AccountMobileFSController.doSave}'
			}

			function segmentationClicked(segId) {
				window.location = "/apex/LAT_BR_SegmentationSlctMobile?retURL={!JSENCODE('/apex/LAT_BR_Edit_AccountMobileFieldSets?id='+Account.Id)}&sfdc.override=1&id=" + segId;
			}

			function segmentationClickedReadOnly(segId) {
				window.location = "/apex/LAT_BR_SegmentationSlctMobile?retURL={!JSENCODE('/apex/LAT_BR_Edit_AccountMobileFieldSets?id='+Account.Id)}&sfdc.override=1&readonly=1&id=" + segId;
			}

			function novaSegmentationClicked() {
				window.location.href = "/apex/LAT_BR_SegmentationSlctNewMobile?retURL={!JSENCODE('/apex/LAT_BR_Edit_AccountMobileFieldSets?id='+Account.Id)}&sfdc.override=1&accId={!Account.Id}";
			}

			function getNewCreditAnalysisResponsePage() {
