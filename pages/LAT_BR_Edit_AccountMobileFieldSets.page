<apex:page showHeader="false" sidebar="false" docType="HTML-5.0" standardController="Account" extensions="LAT_Edit_AccountMobileFSController">
	<html>

	<head>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" />

		<!-- Bootrap 3.0 -->
		<apex:stylesheet value="//maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" />

		<!-- DataTables.net -->
		<apex:stylesheet value="https://cdn.datatables.net/1.10.12/css/jquery.dataTables.min.css" />

		<apex:stylesheet value="{!URLFOR($Resource.LAT_CompassResources, 'static/css/main.css')}" />
		<apex:stylesheet value="{!URLFOR($Resource.LAT_CompassResources, 'static/css/edit-account-fieldset.css')}" />

