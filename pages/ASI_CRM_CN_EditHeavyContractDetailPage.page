    <script type='text/javascript'>
    
    var NoOfMonths=1;
    var BRSFClickAddNew= false; // BRSF Onclick new Line
    var PSFClickAddNew= false; // PSF Onclick new Line
    var VECClickAddNew= false;// VEC Onclick new Line
    var SubBrands =[];
    var CommercialTeam='';
    var Region='';
    var CityTier='';
    
