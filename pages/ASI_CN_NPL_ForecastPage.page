	<script type="text/javascript">
		$(document).ready(
            function() {
				$('input').trigger('change');
            }
		);
		
		function updateFigures_subTotal(channel, rowOrCol){
			let total = 0;
			$('.'+channel+'_'+rowOrCol).each(function(index) {
                let amount = $(this).val().replace(/,/g,"");
				total += amount ? parseFloat(amount) : 0;
            });
			$('#'+channel+'_'+rowOrCol).text(total.toLocaleString('en-US', {minimumFractionDigits: 2}));
		}
		
		function updateFigures_total(channelOrRowOrCol){
			let total = 0;
			$('.'+channelOrRowOrCol).each(function(index) {
                let amount = $(this).val().replace(/,/g,"");
				total += amount ? parseFloat(amount) : 0;
            });
			$('#'+channelOrRowOrCol).text(total.toLocaleString('en-US', {minimumFractionDigits: 2}));
		}
		
		function updateFigures(channel, row, col){			
			updateFigures_subTotal(channel, row);
			updateFigures_subTotal(channel, col);
			updateFigures_subTotal(row, col);
			updateFigures_total(channel);
			updateFigures_total(row);
			updateFigures_total(col);
			updateFigures_total('totalDynamic');
		}

		function showOrHide() {
            var showOrHide = document.getElementById('defaultHide');
            if ("none" == showOrHide.style.display) {
                showOrHide.style.display = "block";
            } else {
                showOrHide.style.display = "none";
            }
        }
	</script>
	
