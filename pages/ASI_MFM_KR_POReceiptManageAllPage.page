        function CalAmount(Id){
            var UnitCost=0;
            var Quantity=0;
            var bothvar=false;
            
            if(parseFloat($j('#UnitCost_manageAll_' + Id + '_input input').val().replace(/[$,]+/g,""))>0){
                UnitCost=parseFloat($j('#UnitCost_manageAll_' + Id + '_input input').val().replace(/[$,]+/g,""));
                bothvar=true;
            }else{
                bothvar=false;
            }
           
            
