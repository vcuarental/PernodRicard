                Quantity=parseFloat($j('#Quantity_manageAll_' + Id + '_input input').val().replace(/[$,]+/g,""));
                bothvar=true;
            }else{
                bothvar=false;
            }
            if(bothvar){
                $j('#totalAmount_manageAll_' + Id + '_totalAmount input').val(parseFloat(Quantity*UnitCost).toFixed(2));
            }
            calculateSum();
        }
        
        
        
