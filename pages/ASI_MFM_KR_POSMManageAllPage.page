                    Brands.push(br);
                }
            }
        } else {
            // TODO: Handle error
            //displayErrorMsg(event.message);
        }   
        // initalizeAutoCompleteComponent();
    } 
    
    function callbackBrandGroup(result, event){
        if (event.status) {
            if (result) {
                
                BrandGroup = [];
