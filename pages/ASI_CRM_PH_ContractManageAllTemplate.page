<apex:page >
    <script>
        const TemplateHelper = function () {
            let purchaseIncentiveSchemeComponentTemplate = `
                <apex:insert name="purchaseIncentiveSchemeComponentTemplate" />
                `;

            let contractFundSchemeComponentTemplate = `
                <apex:insert name="contractFundSchemeComponentTemplate" />
                 `;

            return {
                getPurchaseIncentiveSchemeComponentTemplate: function () {
                    return purchaseIncentiveSchemeComponentTemplate;
                },
                getContractFundSchemeComponentTemplate: function () {
                    return contractFundSchemeComponentTemplate;
                }
            };
        }();
    </script>
</apex:page>