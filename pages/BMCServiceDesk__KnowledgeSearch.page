            function doSearchObject(searchtxt, sortedListOfObjects, orderBY, sortedFldType) {
                var isToDoSort = false;
                if(orderBY)
                    isToDoSort = true;
                else {
                    orderBY = '';
					sortedFldType = '';
				}
            	var obj = sortedListOfObjects.shift();  
