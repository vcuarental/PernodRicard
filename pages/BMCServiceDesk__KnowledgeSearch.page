                    var objectDisplayName = rec.get('@objectDisplayName');
                    var resultCount = rec.get('@resultCount');
                    var pageRef = rec.get('@pageRef');
                   
                    fireCreateTabModel(objectAPIName, objectDisplayName+' '+'('+ resultCount +')', pageRef, objectDisplayName);
                }
