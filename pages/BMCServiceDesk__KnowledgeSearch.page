	function pasteResolution(resolution,KMID,type,isVisibleInSS){
		var stdLayout = getStandardLayout();
        if(stdLayout ){
           // window.opener.pasteResStd(resolution,KMID);

           	var str = resolution;
			if(str != ''){
                str=str.replace(/<\s*br\/*>/gi, "\n");
				str=str.replace(/<\s*a.*href="(.*?)".*>(.*?)<\/a>/gi, " $2 (Link->$1) ");
				str=str.replace(/<\s*\/*.+?>/ig, " ");
				str=str.replace(/ {2,}/gi, " ");
                str=str.replace(/\n+\s*/gi, "\n");
				if(type == 'text'){
					str = htmlDecode(str);
				}
			}
            if(type == 'link'){            
				
				if(stdLayoutObjName == 'Broadcasts'){ 	           	      
	           		if(getUrlParameter('isCalledFromConsole')=='true'){
                        window.opener.pasteResolution(str,isVisibleInSS);
					}else{ 
                        window.opener.parent.location.href='{!$Page.StdBroadcastExtension}' + "&id=" + recordId + "&action=pasteResolution"+ "&res=" + encodeURIComponent(str) +"&isVisibleInSS=" + isVisibleInSS ;   
                        }
	           	}else if(stdLayoutObjName == 'Tasks'){ 	           	      
	           		
	           		if(getUrlParameter('isServiceCloudConsole')=='true'){
	           	    	window.opener.pasteResolution(str);
	           	   	}else if(getUrlParameter('isCalledFromConsole')=='true'){
	           	    	window.opener.pasteResolution(str);
	           	   	}else{	           	      
	           		window.opener.parent.location.href='{!$Page.StdTaskExtension}' + "&id=" + recordId + "&action=pasteResolution"+ "&res=" + encodeURIComponent(str) +"&isVisibleInSS=" + isVisibleInSS ;   
                       
	           		}
	           		
	           	}else if(stdLayoutObjName == 'Problems'){   
                if(getUrlParameter('isServiceCloudConsole')=='true' || getUrlParameter('isCalledFromConsole')=='true'){
                        window.opener.pasteResolution(str);
                 }else{                                       
               window.opener.parent.location.href='{!$Page.StdProblemExtension}' + "&id=" + recordId + "&action=pasteResolution"+ "&res=" + encodeURIComponent(str) +"&isVisibleInSS=" + isVisibleInSS ;   
                        
                 }
                }else{
	           	   if(getUrlParameter('isServiceCloudConsole')=='true'){
