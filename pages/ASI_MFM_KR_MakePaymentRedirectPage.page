<apex:page standardController="ASI_MFM_PO__c" extensions="ASI_MFM_KR_MakePaymentRedirectController" action="{!redirect}" >
    <apex:outputField value="{!ASI_MFM_PO__c.name}" rendered="false" />
        <apex:outputField value="{!ASI_MFM_PO__c.ASI_MFM_Status__c}" rendered="false" />
    <apex:outputField value="{!ASI_MFM_PO__c.ASI_MFM_Supplier_Name__c}" rendered="false" /> 
    <apex:outputField value="{!ASI_MFM_PO__c.ASI_MFM_Supplier_Name__r.Name}" rendered="false" /> 
    <apex:outputField value="{!ASI_MFM_PO__c.ASI_MFM_Currency__c}" rendered="false" />
    <apex:outputField value="{!ASI_MFM_PO__c.ASI_MFM_Project_Code__c}" rendered="false" />
    <apex:outputField value="{!ASI_MFM_PO__c.ASI_MFM_Project_Code__r.name}" rendered="false" />
    <apex:sectionHeader title="Create New Payment Error"/>
    <apex:pageMessages />
    <apex:form >
        <apex:pageBlock >
            <apex:pageBlockButtons location="bottom">
                <apex:commandButton value="Cancel" action="{!cancel}"/>
            </apex:pageBlockButtons>
        </apex:pageBlock>
    </apex:form> 
    
    
</apex:page>