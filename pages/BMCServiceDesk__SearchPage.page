function isUserPermitted(operation){
	
		var isCreateable = {!isCreateable}; 
		var isDeletable = {!isDeletable};
		var isUpdateable = {!isUpdateable};
		if(operation == 'new' || operation == 'copy'){
			if(isCreateable){
				return true;
			}else{
				return false;
			}
		}else if(operation == 'delete'){
			if(isDeletable){
				return true;
			}else{
				return false;
			}
		} 
		else if(operation == 'update'){
			if(isUpdateable){
				return true;
			}else{
				return false;
			}
		} 
		return true;
	
}
