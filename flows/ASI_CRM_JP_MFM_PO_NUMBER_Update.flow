    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
[ASI_CRM_JP_Sales_Order__c].RecordType.DeveloperName == &apos;ASI_CRM_JP_Marketing_FOC&apos;,

ISCHANGED([ASI_CRM_JP_Sales_Order__c].ASI_CRM_AP_Code__c ) 
