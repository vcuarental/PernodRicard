<?xml version="1.0" encoding="UTF-8"?>
<LightningComponentBundle
	xmlns="urn:metadata.tooling.soap.sforce.com"
	fqn="asi_mfm_manage_all_searchpanel">
	<apiVersion>48.0</apiVersion>
	<isExposed>true</isExposed>
	<targets>
		<target>lightning__RecordPage</target>
	</targets>
	<targetConfigs>
		<targetConfig targets="lightning__RecordPage">
			<property
				name="applicationDeveloperName"
				label="Application Developer Name"
				description="Define applicationDeveloperName"
				required="false"
				placeholder="Please input applicationDeveloperName"
				type="String"/>
			<property
				name="configurationstr"
				label="Configuration String"
				description="Please input Configuration String"
				required="false"
				placeholder="Json string"
				type="String"/>
			<property
				name="displaySettingString"
				label="Display Setting String"
				description="Define Display Setting String"
				required="false"
				placeholder="Please input Display Setting String"
				type="String"/>
			<property
				name="searchListstr"
				label="Search List String"
				description="Please input search list string"
				required="false"
				placeholder="Json string"
				type="String"/>
		</targetConfig>
	</targetConfigs>
</LightningComponentBundle>