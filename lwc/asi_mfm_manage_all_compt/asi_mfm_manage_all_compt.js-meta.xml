<?xml version="1.0" encoding="UTF-8"?>
<LightningComponentBundle
	xmlns="urn:metadata.tooling.soap.sforce.com"
	fqn="asi_mfm_manage_all_compt">
	<apiVersion>48.0</apiVersion>
	<isExposed>true</isExposed>
	<masterLabel>ASI Manage All Builder</masterLabel>
	<targets>
		<target>lightning__RecordPage</target>
	</targets>
	<targetConfigs>
		<targetConfig targets="lightning__RecordPage">
			<property
				name="applicationDeveloperName"
				label="Application Developer Name"
				description="Define applicationDeveloperName"
				required="false"
				placeholder="Please input applicationDeveloperName"
				type="String"/>
			<property
				name="objectRelationString"
				label="Object Relation String"
				description="Please input Object Relation String in Json format"
				required="false"
				placeholder="Json string"
				type="String"/>
			<property
				name="functionSettingString"
				label="Function Setting String"
				description="Define Function Setting String"
				required="false"
				placeholder="Please input Function Setting String"
				type="String"/>
			<property
				name="displaySettingString"
				label="Display Setting String"
				description="Define Display Setting String"
				required="false"
				placeholder="Please input Display Setting String"
				type="String"/>
			<property
				name="fieldConfigString"
				label="Field Config String"
				description="Please input field Config String"
				required="false"
				placeholder="Json string"
				type="String"/>
			<property
				name="selectTB2MainTBMapStr"
				label="Select Table to Main Table Mapping"
				description="select table to main table mapping field List"
				required="false"
				placeholder="Json string"
				type="String"/>
		</targetConfig>
	</targetConfigs>
</LightningComponentBundle>