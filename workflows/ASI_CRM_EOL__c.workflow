    <rules>
        <fullName>ASI_CRM_CN_EOL_Approved_Email_Alert</fullName>
        <actions>
            <name>ASI_CRM_CN_EOL_Approved_Email_Alert</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <formula>ISPICKVAL(ASI_CRM_Request_Type__c, &quot;EOL&quot;) &amp;&amp; ISPICKVAL(ASI_CRM_Status__c, &quot;Approved&quot;) &amp;&amp; ISPICKVAL(PRIORVALUE( ASI_CRM_Status__c), &quot;Submitted&quot;) &amp;&amp; ( CONTAINS(RecordType.DeveloperName, &quot;ASI_CRM_CN_Product_EOL_ApprovedView&quot;))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>ASI_CRM_CN_EOL_Rejected_Email_Alert</fullName>
        <actions>
            <name>ASI_CRM_CN_EOL_Rejected_Email_Alert</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <formula>ISPICKVAL(ASI_CRM_Status__c, &quot;Rejected&quot;) &amp;&amp; ISPICKVAL(PRIORVALUE( ASI_CRM_Status__c), &quot;Submitted&quot;) &amp;&amp; (  CONTAINS(RecordType.DeveloperName, &quot;ASI_CRM_CN_EOL&quot;))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>ASI_CRM_CN_EOL_SFG_Approved_Email_Alert</fullName>
