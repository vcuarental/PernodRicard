            <type>Alert</type>
        </actions>
        <active>true</active>
        <formula>ISPICKVAL(ASI_CRM_Request_Type__c, &quot;SFG&quot;) &amp;&amp; ISPICKVAL(ASI_CRM_Status__c, &quot;Approved&quot;) &amp;&amp; ISPICKVAL(PRIORVALUE( ASI_CRM_Status__c), &quot;Open&quot;) &amp;&amp; ( CONTAINS(RecordType.DeveloperName, &quot;ASI_CRM_CN_Product_EOL_ApprovedView&quot;))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
