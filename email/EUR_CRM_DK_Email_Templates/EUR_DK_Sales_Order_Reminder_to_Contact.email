age style="display: block;vertical-align:middle;padding-top:30px;" value="https://pernod-ricard--eursb2conf--c.cs8.content.force.com/servlet/servlet.ImageServer?id=015L0000000L5oY&oid=00DL00000028NIN&lastMod=1458618995000" width="400" height="80"/></center></td>                </tr>                <tr style="height: 3px;"></tr>            </table>            <table style="background:#7ba4cc;width:600px;height:110px;border-collapse:collapse;color:#ffffff;;text-align: center;">                <tr style="height:110px;">                    <td >                        <span style="font-size: 30px;"><font face="'Open Sans','Helvetica Neue','Helvetica','Arial',sans-serif">Hello  {!relatedTo.EUR_CRM_Contact__r.FirstName} {!relatedTo.EUR_CRM_Contact__r.LastName},</font></span>                        <br/>                        <span style="font-size: 18px;"><font face="'Open Sans','Helvetica Neue','Helvetica','Arial',sans-serif">Following our discussion, please find below, as a reminder, the list of products to be ordered.</font></span>                    </td>                </tr>            </table>                        <!--ITERATE ON ORDERS -->            <c:EUR_CRM_SalesOrderItemTable soId="{!RelatedTo.Id}" totalWidth="600" />            <div style="background:#FFFFFF;width:600px; height:2px;"></div>            <br />            <br />            <span style="font-size: 14px;color:#7ba4cc;"><font face="'Open Sans','Helvetica Neue','Helvetica','Arial',sans-serif"><strong>Notes: </strong></font></span>            <span style="font-size: 14px;color:#333333;"><font face="'Open Sans','Helvetica Neue','Helvetica','Arial',sans-serif">{!relatedTo.EUR_CRM_Notes__c}</font></span>            <br />            <br />            <table style="background:#CCCCCC;width:600px; height:10px;">                <tr style="height:2px;">                        <td></td>                    </tr>            </table>            <table style="background:#FFFFFF;width:600px;height:100px;">                <tr>                    <td style="width:220px;text-align: left;" rowspan="3">                        <span style="font-size: 30px;color:#7ba4cc;font-weight:100;"><font face="'Open Sans','Helvetica Neue','Helvetica','Arial',sans-serif">Contact Name</font></span>                    </td>                    <td style="width:1px;" rowspan="3">                        <span style="font-size:65px;color:#7ba4cc;"><strong>|</strong></span>                    </td>                    <td style="width:339px;">                        <span style="font-size: 14px;color:#333333;font-weight:100; padding-left: 20px;"><font face="'Open Sans','Helvetica Neue','Helvetica','Arial',sans-serif"><strong>{!relatedTo.CreatedBy.FirstName} {!relatedTo.CreatedBy.LastName}</strong></font></span>                    </td>                </tr>                <tr>                    <td style="width:339px;">                        <span style="font-size: 14px;color:#333333;font-weight:100; padding-left: 20px;"><font face="'Open Sans','Helvetica Neue','Helvetica','Arial',sans-serif">{!relatedTo.CreatedBy.Email}</font></span>                    </td>                </tr>                <tr>                    <td style="width:339px;">                        <span style="font-size: 14px;color:#333333;font-weight:100; padding-left: 20px;"><font face="'Open Sans','Helvetica Neue','Helvetica','Arial',sans-serif">{!relatedTo.CreatedBy.Phone}</font></span>                    </td>                </tr>            </table>        </body>        </html>    </messaging:htmlEmailBody></messaging:emailTemplate>