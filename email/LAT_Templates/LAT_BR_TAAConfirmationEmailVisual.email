<messaging:emailTemplate subject="TAA - {!relatedTo.Visit__r.Conta__r.Corporate_Name__c} - {!relatedTo.Visit__r.Data_da_Visita__c} - {!relatedTo.Visit__r.Hora_da_Visita__c}" recipientType="Contact" relatedToType="LAT_BR_TAA__c">        <messaging:htmlEmailBody >        <div style="font-family: calibri, helvetica, sans-serif; font-size: 14px;">            Segue documento TAA em anexo. <br />            Obrigado <br />            {!$User.FirstName} {!$User.LastName}        </div>    </messaging:htmlEmailBody>    <messaging:plainTextEmailBody >        Segue documento TAA em anexo.        Obrigado        {!$User.FirstName} {!$User.LastName}    </messaging:plainTextEmailBody>        <messaging:attachment renderAs="PDF" filename="TAA - {!relatedTo.Visit__r.Conta__r.Corporate_Name__c} - {!relatedTo.Visit__r.Data_da_Visita__c} - {!relatedTo.Visit__r.Hora_da_Visita__c}.pdf">        <style type="text/css">            @page {                size: 1200px 2000px;            }            .wrapper {                width: 1068px;            }            p.pheader {                margin-top: 0;            }            hr {                margin-bottom: 0;                background-color: #023466;                border: 0;                height: 2px;            }            div.sheader {                background-color: #023466;                color: white;                padding: 4px 65px 5px 4px;                width: 130px;                margin-top: -15px;            }            div.fieldDetail {                border-bottom: 2px solid #023466;                border-left: 2px solid #023466;                border-right: 2px solid #023466;                padding: 5px;            }        </style>        <div class="wrapper">            <div>                <apex:image value="{!$Resource.LAT_TAAResource}/Header.png" />            </div>            <hr />            <div style="font-family: calibri, helvetica, sans-serif; font-size: 14px;">                <p class="pheader">                    <div class="sheader"><strong>Cliente:</strong></div>                </p>                <p>                    <span style="width: 800px; float:left;">Razão Social:</span>                    <span style="width: 240px; float:right;">Código AN8:</span>                     <div class="fieldDetail" style="width: 800px; float:left;">                        {!relatedTo.Visit__r.Conta__r.Corporate_Name__c}                    </div>                    <div class="fieldDetail" style="width: 235px; float:right;">                        {!relatedTo.ClientAN8__c}                    </div>                    <div style="margin-top:10px; float: left; width: 100%; margin-top: 15px;">                        <span style="width: 410px; float:left;">Contato:</span>                        <span style="width: 400px; float:left;">Executivo/Consultor:</span>                        <span style="width: 245px; float:right;">Data:</span>                        <div class="fieldDetail" style="width: 390px; float:left;">                            {!relatedTo.Contact__r.Name}                        </div>                        <div class="fieldDetail" style="width: 390px; float:left;margin-left:5px;">                            {!relatedTo.Visit__r.Conta__r.Owner.Name}                        </div>                        <br/>                        <div class="fieldDetail" style="width: 235px; float:right;">                            <apex:outputText value="{0,date,dd'/'MM'/'yyyy}">                                <apex:param value="{!relatedTo.Date__c}" />                            </apex:outputText>                        </div>                    </div>                    <div style="clear:both;"></div>                                    </p>            </div>                        <hr />            <div style="font-family: calibri, helvetica, sans-serif; font-size: 14px;">                <p class="pheader">                    <div class="sheader"><strong>Objetivos da visita:</strong></div>                </p>                <p>                    <span>PRB:</span>                    <div class="fieldDetail">                        <apex:outputText value="{!relatedTo.PRBObjectives__c}" style="white-space: pre-wrap" escape="false"/>                    </div> <br />                    <span>Cliente:</span> <br />                    <div class="fieldDetail">                        <apex:outputText value="{!relatedTo.ClientObjectives__c}" style="white-space: pre-wrap" escape="false"/>                    </div>                                    </p>            </div>                        <hr />            <div style="font-family: calibri, helvetica, sans-serif; font-size: 14px;">                <p class="pheader">                    <div class="sheader"><strong>Acompanhamento:</strong></div>                </p>                <p>                    <span>Ultima Visita:</span>                    <div class="fieldDetail">                        <apex:outputText value="{!relatedTo.LastVisit__c}" style="white-space: pre-wrap" escape="false"/>                    </div>                                    </p>            </div>                        <hr />            <div style="font-family: calibri, helvetica, sans-serif; font-size: 14px;">                <p class="pheader">                    <div class="sheader"><strong>Acordos:</strong></div>                </p>                <p>                    <span>Visita Atual:</span>                    <apex:repeat var="agreement" value="{!relatedTo.TAA_Agreements__r}">                        <div class="fieldDetail">                            <apex:outputText value="{!agreement.Items__c}" style="white-space: pre-wrap" escape="false"/>                        </div>                    </apex:repeat>                </p>            </div>            <div style="margin-top: 10px;">                <apex:image value="{!$Resource.LAT_TAAResource}/Footer.png" />            </div>        </div>    </messaging:attachment></messaging:emailTemplate>