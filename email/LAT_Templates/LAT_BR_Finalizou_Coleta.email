<messaging:emailTemplate subject="{!relatedTo.Name}: Coleta Finalizada para AN8: {!relatedTo.ReferenceAccount__r.AN8__c} - {!relatedTo.ReferenceAccount__r.AccountName__c}"                 recipientType="User"                 relatedToType="LAT_SellOutCollect__c">    <messaging:HTMLemailbody >        <b>Usuário {!relatedTo.LastModifiedBy.Name} gravou:</b> <br/><br/>        <b>AN8:</b> {!relatedTo.ReferenceAccount__r.AN8__c} - {!relatedTo.ReferenceAccount__r.AccountName__c} <br/>        <b>Protocolo</b>: {!relatedTo.Name} <br/>        <b>Mês Ref: </b> <apex:outputText value="{0,date,MM'/'yyyy}"> <apex:param value="{!relatedTo.ReferencedMonth__c}" /> </apex:outputText>  -        <b>Data Coleta: </b> <apex:outputText value="{0,date,dd'/'MM'/'yyyy}"> <apex:param value="{!relatedTo.CollectDate__c}" /> </apex:outputText>  -        <b>Data Digitação: </b><apex:outputText value="{0,date,dd'/'MM'/'yyyy HH:MM}"> <apex:param value="{!relatedTo.LastModifiedDate}" /> </apex:outputText> <br/>         <b>Unidade: </b> {!RelatedTo.Unit__c} <br/>        <br/><br/>        <table cellspacing="0" cellpadding="1" border="1">            <thead>                <tr>                    <th>MARCA</th>                    <th>SELL OUT</th>                    <th>ESTOQUE</th>                </tr>            </thead>            <tbody>                <apex:variable var="sellOut" value="{!0}" />                 <apex:variable var="stock" value="{!0}" />                 <apex:repeat var="sellOutLine" value="{!relatedTo.LAT_SellOutCollectLines__r}">                <tr>                    <td>{!sellOutLine.ReferenceProductGrouping__r.Name}</td>                     <td>{!FLOOR(sellOutLine.SellOut__c)}</td>                     <td>{!FLOOR(sellOutLine.Stock__c)}</td>                </tr>                <apex:variable var="sellOut" value="{!sellOut + sellOutLine.SellOut__c}"/>                <apex:variable var="stock" value="{!stock + sellOutLine.Stock__c}"/>                </apex:repeat>            </tbody>            <tfoot>                <tr>                    <th>TOTAL</th>                    <th><apex:OutputText value="{!FLOOR(sellOut)}"/></th>                    <th><apex:OutputText value="{!FLOOR(stock)}"/></th>                </tr>            </tfoot>        </table>    </messaging:HTMLemailbody></messaging:emailTemplate>