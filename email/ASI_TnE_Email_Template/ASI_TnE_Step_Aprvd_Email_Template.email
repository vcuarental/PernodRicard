<messaging:emailTemplate subject="Notification of Approver Approved Travel & Expense Document #{!RelatedTo.Name}" recipientType="User" relatedToType="ASI_TnE_ClaimHeader__c"><messaging:htmlEmailBody ><html>        <body>                <p>                    Please note the following Travel &amp; Expense Document has been approved by manager.            It is pending for next approver (if any) to approve.                </p>                <table>                                <tr>                        <td>Owner Name</td>                        <td>: &nbsp; {!RelatedTo.Owner.Name}</td>                    </tr>                    <tr>                        <td>Payee Name</td>                        <td>: &nbsp; {!RelatedTo.ASI_TnE_Payee_Employee__r.Name}</td>                    </tr>                    <tr>                        <td>Claim Number</td>                        <td>: &nbsp; {!RelatedTo.Name}</td>                    </tr>                    <tr>                        <td>Claim Amount</td>                        <td>: &nbsp; <apex:outputText value="{0, number, ###,###,###,###.##}">                                <apex:param value="{!RelatedTo.ASI_TnE_Total_Amount_LCY__c}" />                            </apex:outputText> ({!RelatedTo.ASI_TnE_Currency__c})                        </td>                    </tr>                    <tr>                        <td>Claim Date</td>                        <td>: &nbsp;&nbsp; <apex:outputText value="{0,date,MM/dd/yyyy}">                                <apex:param value="{!RelatedTo.ASI_TnE_Start_Date__c}" />                            </apex:outputText>~<apex:outputText value="{0,date,MM/dd/yyyy}">                                <apex:param value="{!RelatedTo.ASI_TnE_End_Date__c}" />                            </apex:outputText>                        </td>                    </tr>                    <tr>                        <td>Department</td>                        <td>: &nbsp; {!RelatedTo.ASI_TnE_Department_Division__c}                        </td>                    </tr>                    <tr>                        <td>Business Unit</td>                        <td>: &nbsp; {!RelatedTo.ASI_TnE_Business_Unit__c}                        </td>                    </tr>                    <tr>                        <td>Type of Expense Claim</td>                        <td>: &nbsp; {!RelatedTo.ASI_TnE_Type__c}                        </td>                    </tr>        </table>                </body></html></messaging:htmlEmailBody><messaging:attachment renderAs="PDF"  filename="{!RelatedTo.name}"><c:ASI_TnE_ExpenseRptComponent headerId="{!RelatedTo.Id}" /></messaging:attachment></messaging:emailTemplate>