<messaging:emailTemplate subject="Notification of Travel & Expense #{!RelatedTo.ASI_TnE_Document_No__c} Approved " recipientType="User" relatedToType="ASI_TnE_ClaimHeader__c"><messaging:htmlEmailBody ><html>    <body>        <p>        Please note the following Travel &amp; Expense document is approved. You can print out the PDF attached for accouting review.        </p>        <table>            <tr>                <td>                Applicant                </td>                <td>                : &nbsp; {!RelatedTo.Owner.Name}                </td>            </tr>            <tr>                <td>                Claim Number                </td>                 <td>                : &nbsp; {!RelatedTo.ASI_TnE_Document_No__c}                </td>            </tr>            <tr>                <td>                Claim Amount                </td>                 <td>                : &nbsp; <apex:outputText value="{0, number, ###,###,###,##0}">                       <apex:param value="{!RelatedTo.ASI_TnE_Total_Amount__c}" />                    </apex:outputText> ({!RelatedTo.ASI_TnE_Currency__c})                 </td>            </tr>            <tr>                <td>                Claim Date                </td>                 <td>                : &nbsp; <apex:outputText value="{0,date,MM/dd/yyyy}">                           <apex:param value="{!RelatedTo.ASI_TnE_Cover_Period_From__c}" />                   </apex:outputText>~<apex:outputText value="{0,date,MM/dd/yyyy}">                           <apex:param value="{!RelatedTo.ASI_TnE_Cover_Period_To__c}" />                   </apex:outputText>                </td>            </tr>            <tr>                <td>                Claim Payment Date                  </td>                 <td>                : &nbsp; <apex:outputText value="{0,date,MM/dd/yyyy}">                           <apex:param value="{!RelatedTo.ASI_TnE_Payment_Date__c}" />                   </apex:outputText>                </td>            </tr>            <tr>                <td>                Originator                  </td>                 <td>                : &nbsp; {!RelatedTo.CreatedBy.name}                </td>            </tr>        </table>            </body></html></messaging:htmlEmailBody><messaging:attachment renderAs="PDF"  filename="{!RelatedTo.name}">    <c:ASI_JP_TnE_Claim_Header_PDF headerId="{!RelatedTo.Id}" actionParam="eApprovedPrint" /></messaging:attachment></messaging:emailTemplate>