    <webLinks>
        <fullName>DCM_Analiticos_de_C_C_Arg</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Analiticos de C/C</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/27.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/27.0/apex.js&quot;)}

var idReg = &apos;{!RCP_Receipt_ARG__c.Id}&apos;;

var receiptRecord = sforce.connection.query(&quot;Select id, Status_AR__c From RCP_Receipt_ARG__c where id= &apos;&quot;+ idReg +&quot;&apos;&quot;); 
var receipt = receiptRecord.getArray(&quot;records&quot;)[0];

if(receipt.Status_AR__c == &apos;Receipt Verified&apos; &amp;&amp; &apos;{!User.Profile}&apos; != &apos;LAT_AR2_CredYCob&apos; &amp;&amp; &apos;{!User.Profile}&apos; != &apos;LAT_SystemAdministrator&apos;){
alert(&apos;Solo los usuarios con perfil ARG_Cred y Cob pueden modificar un recibo verificado. &apos;);
}else{

window.location = &apos;/apex/LAT_AR_VFP07_Relate_Documents?idRecibo=&apos; + idReg
}</url>
    </webLinks>
    <webLinks>
        <fullName>LAT_Retention</fullName>
        <availability>online</availability>
        <description>This button is used to create a retetion to the selected document</description>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>New Retention</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/27.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/27.0/apex.js&quot;)}

var selectedTitulosIds = {!GETRECORDIDS($ObjectType.DCM_Documents_ARG__c)};
var tmpMsg = sforce.apex.execute(&quot;AP01_Document_AR&quot;,&quot;createDocumentRetentions&quot;,{docIds:selectedTitulosIds });

if (tmpMsg != &apos;&apos;){
  window.location = tmpMsg;
} else {
  alert(&apos;Porfavor Seleccionar un comprobante&apos;);
}</url>
    </webLinks>
