    <validationRules>
        <fullName>DCM_VR01_ValidationAmountToApply_AR</fullName>
        <active>true</active>
        <description>The &apos;Amount to apply&apos; cannot be greater than the &apos;Net Open Balance&apos;.</description>
        <errorConditionFormula>ISCHANGED(Amounttoapply_AR__c)
&amp;&amp;
NOT(ISPICKVAL(ReceiptNo_AR__r.Status_AR__c,&apos;Receipt cancelled&apos;))
&amp;&amp;
(
Amounttoapply_AR__c = 0
||
NetOpenBalance_AR__c  = 0
||
(Amounttoapply_AR__c  &lt; 0 &amp;&amp; NetOpenBalance_AR__c  &gt; 0)
||
(Amounttoapply_AR__c &gt; 0 &amp;&amp; NetOpenBalance_AR__c  &lt; 0)
||
(Amounttoapply_AR__c &lt; 0 &amp;&amp; NetOpenBalance_AR__c  &lt; 0 &amp;&amp; (Amounttoapply_AR__c &lt; NetOpenBalance_AR__c))
||
(Amounttoapply_AR__c &gt; 0 &amp;&amp; NetOpenBalance_AR__c  &gt; 0 &amp;&amp; (Amounttoapply_AR__c &gt; NetOpenBalance_AR__c))
)</errorConditionFormula>
        <errorDisplayField>Amounttoapply_AR__c</errorDisplayField>
        <errorMessage>The value cannot be applied. Check the value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DCM_VR02_ValidAmountToApplyTypeRU_AR</fullName>
        <active>true</active>
        <description>The &apos;Amount to apply&apos; of a record with Document Type &apos;RU&apos; cannot be modified.</description>
        <errorConditionFormula>ISCHANGED (Amounttoapply_AR__c)
&amp;&amp;
tipoDocumento_AR__c = &apos;RU&apos;
&amp;&amp;
NOT(ISPICKVAL(ReceiptNo_AR__r.Status_AR__c,&apos;Receipt cancelled&apos;))</errorConditionFormula>
        <errorDisplayField>Amounttoapply_AR__c</errorDisplayField>
        <errorMessage>The value cannot be modified in a Document Type &apos;RU&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DCM_VR03_CantEditName_AR</fullName>
        <active>true</active>
        <description>Users can&apos;t modify registry name, except the administrator</description>
        <errorConditionFormula>IF( NOT(OR($Profile.Name=&apos;System Administrator&apos;, $Profile.Name=&apos;AR_System Administrator&apos;,$Profile.Name=&apos;LAT_SystemAdministrator&apos;)) , IF( ISCHANGED(Name) 
, true, false) 
, false)</errorConditionFormula>
        <errorMessage>Users can&apos;t modify the registry name.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LAT_DCM_VR04_Type_RU_AR</fullName>
        <active>true</active>
        <description>Records with Document Type &apos;RU&apos; cannot be modified by sales.</description>
        <errorConditionFormula>OR($Profile.Name=&apos;AR_Vendedor&apos;,$Profile.Name=&apos;UY_Vendedor&apos;,$Profile.Name=&apos;LAT_UY2_Vendedor&apos;,$Profile.Name=&apos;LAT_AR2_Vendedor&apos;)
&amp;&amp;
NOT(ISPICKVAL(ReceiptNo_AR__r.Status_AR__c,&apos;Receipt cancelled&apos;))
&amp;&amp;
tipoDocumento_AR__c = &apos;RU&apos;
&amp;&amp;
NOT(ISNEW())</errorConditionFormula>
        <errorMessage>RU Documents can not be modified</errorMessage>
    </validationRules>
