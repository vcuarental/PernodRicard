    <webLinks>
        <fullName>ASI_eForm_LR_Approval</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Submit for Approval</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!requireScript(&quot;/soap/ajax/29.0/connection.js&quot;)}
sforce.connection.sessionId = &quot;{!$Api.Session_ID}&quot;;

var currentHeader = sforce.connection.query(&quot;Select ASI_eForm_Total_Days__c, ASI_eForm_Status__c, Id from ASI_eForm_Leave_Request__c where Id = &apos;{!ASI_eForm_Leave_Request__c.Id}&apos; &quot;);

var currentDetails = sforce.connection.query(&quot;Select Id from ASI_eForm_Leave_Request_Line_Item__c where ASI_eForm_Leave_Reuest__c = &apos;{!ASI_eForm_Leave_Request__c.Id}&apos; &quot;);

var records = currentHeader.getArray(&quot;records&quot;);
var detailRecords = currentDetails.getArray(&quot;records&quot;);

if (detailRecords == null || detailRecords.length &lt;= 0)
{
alert(&quot;Leave Request Details Not Found.&quot;);
}
else if (records[0].ASI_eForm_Status__c == &apos;Draft&apos;)
{
if(confirm(&quot;Once you submit this record for approval, you might not be able to edit it or recall it from the approval process depending on your settings. Continue?&quot;) == true)
{
try
{
navigateToUrl(&apos;/apex/ASI_SubmitApprovalPage?id={!ASI_eForm_Leave_Request__c.Id}&apos;); 
}//end try
catch(error)
{
sforce.debug.log(error.faultcode);
sforce.debug.log(error.faultstring);
}//end catch
}//end else if
else
{}
}</url>
