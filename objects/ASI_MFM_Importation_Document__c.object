        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Submit for Approval</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/40.0/connection.js&quot;)} 

var imporDoc = new sforce.SObject(&quot;ASI_MFM_Importation_Document__c&quot;); 

if({!ASI_MFM_Importation_Document__c.ASI_MFM_Total_Replaceable_Line__c} == 0 || 
{!ASI_MFM_Importation_Document__c.ASI_MFM_Total_Replaceable_Line__c} == null 
){ 

	if(confirm(&apos;提供的报关单/税单为经涂抹敏感信息（价格覆盖，报关单/税单编号覆盖）的文件，请确认是否接受&apos;)){ 

	imporDoc.id = &apos;{!ASI_MFM_Importation_Document__c.Id}&apos;; 
	imporDoc.ASI_MFM_Allow_Submit__c = true; 

	var result = sforce.connection.update([imporDoc]); 

		if (result[0].getBoolean(&quot;success&quot;)) { 
			window.location.href = &apos;/apex/ASI_SubmitApprovalPage?id={!ASI_MFM_Importation_Document__c.Id}&apos;; 
		}else{ 
			alert(&apos;无法提交审批.&apos;); 
		} 
	}
}else{ 

	alert(&apos;申请项目能以其他文件代替，请通过以下平台获取，https://sc.pernod-ricard-china.com&apos;); 
}</url>
