        <unique>false</unique>
    </fields>
    <fields>
        <fullName>ASI_CRM_Consider_Prompt_Payment__c</fullName>
        <externalId>false</externalId>
        <formula>IF(ISBLANK(ASI_CRM_Account_Number__c),
  False,
  IF(
    OR(
      RIGHT(ASI_CRM_Account_Number__c,4)=&apos;3010&apos;,
      RIGHT(ASI_CRM_Account_Number__c,4)=&apos;3020&apos;,
      RIGHT(ASI_CRM_Account_Number__c,4)=&apos;3030&apos;
    ),
    TRUE,
    FALSE
  )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Acct# Consider Prompt Payment</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
