    </fields>
    <fields>
        <fullName>ASI_CRM_PP_With_Key_SB__c</fullName>
        <externalId>false</externalId>
        <formula>AND (
 ASI_CRM_Consider_Prompt_Payment__c,
 ASI_CRM_Sub_brand__r.ASI_CRM_Allow_Prompt_Payment__c 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Prompt Pymt With Key SB</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>ASI_CRM_Prompt_Payment_Ratio__c</fullName>
        <externalId>false</externalId>
        <formula>IF( 
  AND(
    ASI_CRM_Consider_Prompt_Payment__c ,
    ASI_CRM_Sub_brand__r.ASI_CRM_Allow_Prompt_Payment__c
  ),
  ASI_CRM_Amount__c / ASI_CRM_Invoice__r.ASI_CRM_Base_Amount_For_Prompt_Payment__c,
  0
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Prompt Payment Ratio</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>ASI_CRM_Sub_brand_Code__c</fullName>
        <externalId>false</externalId>
        <formula>IF(ISBLANK(ASI_CRM_Account_Number__c),
  &apos;&apos;,
  IF(
    OR(
      RIGHT(ASI_CRM_Account_Number__c,4)=&apos;3010&apos;,
      RIGHT(ASI_CRM_Account_Number__c,4)=&apos;3020&apos;,
      RIGHT(ASI_CRM_Account_Number__c,4)=&apos;3030&apos;
    ),
    MID(ASI_CRM_Account_Number__c , 6, 3),
    &apos;&apos;
  )
)</formula>
        <label>Sub-brand Code</label>
        <required>false</required>
