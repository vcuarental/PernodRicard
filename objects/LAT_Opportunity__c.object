    </validationRules>
    <validationRules>
        <fullName>Bloqueia_alteracao_do_pedido</fullName>
        <active>true</active>
        <errorConditionFormula>(RecordType.DeveloperName=&apos;Nova_oportunidade&apos;
||RecordType.DeveloperName=&apos;Bloqueia_alteracao_do_cabecalho&apos;
||RecordType.DeveloperName=&apos;Bloqueia_alteracao&apos;) 
&amp;&amp; 
NOT( ISPICKVAL(LAT_StageName__c, &quot;Mobile Order&quot;))
&amp;&amp;
(
OR(
         AND(  NOT(LAT_Integration__c) ,   
                     ISCHANGED(Name), 
                     ISCHANGED(LAT_StageName__c),
                     NOT(ISNEW()),
                     NOT( ISPICKVAL(LAT_StageName__c, &quot;Novo pedido&quot;)) 
                   ),
         AND(  
                     NOT(LAT_Integration__c) ,
                     ISNEW(),
                     NOT( ISPICKVAL(LAT_StageName__c, &quot;Novo pedido&quot;)) 
                   )
       )
)&amp;&amp; NOT(LAT_IsAdmin__c)</errorConditionFormula>
