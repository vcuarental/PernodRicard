        <fullName>ASI_DFPM_AttachmentRequired</fullName>
        <active>true</active>
        <errorConditionFormula>ASI_DFPM_Attachment_Required__c  &amp;&amp;  ASI_DFPM_Complete__c   &amp;&amp; !ASI_DFPM_Has_Attachment__c</errorConditionFormula>
        <errorMessage>You must upload attachment for this task before it can be completed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_DFPM_CantAddTasksToCompleteMilestone</fullName>
        <active>true</active>
        <errorConditionFormula>ASI_DFPM_Project_Milestone__r.ASI_DFPM_Complete__c</errorConditionFormula>
        <errorMessage>You can not add tasks to a completed milestone</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_DFPM_EndDate_within_milestone</fullName>
        <active>true</active>
        <errorConditionFormula>!ISBLANK(ASI_DFPM_End_Date__c ) &amp;&amp; !ISBLANK(ASI_DFPM_Project_Milestone__r.ASI_DFPM_End_Date__c) &amp;&amp; (ASI_DFPM_End_Date__c &gt; ASI_DFPM_Project_Milestone__r.ASI_DFPM_End_Date__c)</errorConditionFormula>
        <errorMessage>Project task end date must be less than or equal to the milestone end date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_DFPM_Not_Modify_Template_Task</fullName>
        <active>false</active>
        <errorConditionFormula>ASI_DFPM_Project_Milestone__r.ASI_DFPM_Promotional_Project__r.RecordType.DeveloperName  = &quot;ASI_DFPM_Template&quot;</errorConditionFormula>
        <errorMessage>You can NOT add or modify the template task. Please contact the system administrator for help</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_DFPM_StartDate_after_Pre_EndDate</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISNULL( ASI_DFPM_Start_Date__c )) &amp;&amp; NOT(ISNULL(  ASI_DFPM_Predeccessor_Task__r.ASI_DFPM_End_Date__c )) &amp;&amp; ASI_DFPM_Start_Date__c &lt; ASI_DFPM_Predeccessor_Task__r.ASI_DFPM_End_Date__c</errorConditionFormula>
        <errorMessage>Project task start date must not be earlier than its Predecessor&apos;s end date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_DFPM_StartDate_before_EndDate</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISNULL( ASI_DFPM_Start_Date__c )) &amp;&amp; NOT(ISNULL( ASI_DFPM_End_Date__c )) &amp;&amp; ASI_DFPM_End_Date__c &lt; ASI_DFPM_Start_Date__c</errorConditionFormula>
        <errorMessage>Start date must be less or equal than the end date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_DFPM_StartDate_within_milestone</fullName>
        <active>true</active>
        <errorConditionFormula>!ISBLANK(ASI_DFPM_Start_Date__c)  &amp;&amp;  !ISBLANK(ASI_DFPM_Project_Milestone__r.ASI_DFPM_Start_Date__c ) &amp;&amp; (ASI_DFPM_Start_Date__c &lt; ASI_DFPM_Project_Milestone__r.ASI_DFPM_Start_Date__c)</errorConditionFormula>
        <errorMessage>Project task start date must be greater than or equal to the milestone start date</errorMessage>
    </validationRules>
