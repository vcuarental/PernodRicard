        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/40.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/40.0/apex.js&quot;)}

var normalStatus = &quot;Normal&quot;;
var toDeleteStatus = &quot;To Delete&quot;;

var agStatus = &apos;{!EUR_CRM_AccountGroup__c.EUR_CRM_Status__c}&apos;;

if (agStatus === toDeleteStatus) {
    alert(&quot;Status already marked &apos;To Delete&apos;.&quot;);
} else {

    var query = &quot;&quot; +
        &quot;SELECT Id, EUR_CRM_Status__c &quot; +
        &quot;FROM EUR_CRM_AccountGroupUser__c &quot; +
        &quot;WHERE EUR_CRM_AccountGroup__c = &apos;{!EUR_CRM_AccountGroup__c.Id}&apos; &quot; +
        &quot;AND EUR_CRM_Status__c = &apos;&quot; + normalStatus +&quot;&apos;&quot;;

    var agusSOQL = sforce.connection.query(query);
    var agusRecords = agusSOQL.getArray(&quot;records&quot;);
    
    var agree = confirm(&quot;&quot; +
        &quot;Are you sure you want to mark current record to delete? &quot; +
        (
            agusRecords.length &gt; 0 ? &quot;All related records under the section &apos;Account Group Users (EU)&apos; with status &apos;Normal&apos; (count: &quot; 
            + agusRecords.length 
            + &quot;) will be updated as well.&quot; : &quot;&quot;
        )
    );

    if (agree) {

        var ag = new sforce.SObject(&quot;EUR_CRM_AccountGroup__c&quot;);
        ag.Id = &apos;{!EUR_CRM_AccountGroup__c.Id}&apos;;
        ag.EUR_CRM_Status__c = toDeleteStatus;

        var result = sforce.connection.update([ag]);

        if (result[0].success === &apos;false&apos;) {
            alert(&quot;Error during an updating of Account Group (EU) record: &quot; + result[0].errors.message);
        } else {

            if (agusRecords.length &gt; 0) {
                agusRecords.forEach(function(agu) {
                    agu.EUR_CRM_Status__c = toDeleteStatus;
                });

                result = sforce.connection.update(agusRecords);

                if (result[0].success === &apos;false&apos;) {
                    alert(&quot;Error during an updating of related Account Group Users (EU) records: &quot; + result[0].errors.message);
                } else {
                    window.location.reload();
                }
            } else {
                window.location.reload();
            }
        }
    }
}</url>
