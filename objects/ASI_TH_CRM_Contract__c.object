For TOTMOT, it is the Total Estimated PO Amount for all PO Version</description>
        <externalId>false</externalId>
        <formula>IF( ISPICKVAL(ASI_TH_CRM_Promotion_Type__c, &apos;Heavy Contract On&apos;) , 
IF(ASI_CRM_CN_PO_Version__c==&apos;0.0&apos;, IF(ASI_CRM_CN_Is_2nd_KPI_PO__c , ASI_CRM_Est_Total_Dummy__c+ ASI_CRM_CN_Contract_Variable_Cost_Total__c, ASI_CRM_Est_Total_Dummy__c+ ASI_CRM_CN_Contract_Variable_Cost_Total__c + ASI_CRM_CN_Contract_Fixed_Cost_Total__c ) , IF(ASI_CRM_CN_Is_2nd_KPI_PO__c , ASI_CRM_CN_Expected_PSF_Est_Total__c+ASI_CRM_CN_Expected_BRSF_Est_Total__c+ASI_CRM_EVC_Cost_Estimate_Total__c+ ASI_CRM_CN_Contract_Variable_Cost_Total__c + BLANKVALUE(ASI_CRM_CN_Fixed_Module_Est_Amount1__c, 0) +  BLANKVALUE(ASI_CRM_CN_Fixed_Module_Est_Amount2__c, 0), ASI_CRM_CN_Expected_PSF_Est_Total__c+ASI_CRM_CN_Expected_BRSF_Est_Total__c+ASI_CRM_EVC_Cost_Estimate_Total__c+ ASI_CRM_CN_Contract_Variable_Cost_Total__c + ASI_CRM_CN_Contract_Fixed_Cost_Total__c )) ,

if(ISPICKVAL(ASI_TH_CRM_Promotion_Type__c, &apos;TOT/MOT Contract&apos;), 
IF(ASI_CRM_CN_PO_Version__c==&apos;0.0&apos;, BLANKVALUE(ASI_CRM_Expected_V0_PSF_Estimated_Total__c, 0)+ BLANKVALUE(ASI_CRM_CN_Expected_V0_Display_Est_Total__c, 0)+ BLANKVALUE(ASI_CRM_CN_Contract_Fixed_Cost_Total__c, 0) , 
BLANKVALUE(ASI_CRM_CN_Expected_PSF_Est_Total__c, 0)+BLANKVALUE(ASI_CRM_CN_Expected_Display_Est_Total__c, 0)+ BLANKVALUE(ASI_CRM_CN_Contract_Fixed_Cost_Total__c,0) ) ,
if(ISPICKVAL(ASI_TH_CRM_Promotion_Type__c, &apos;Outlet Promotion&apos;)||ISPICKVAL(ASI_TH_CRM_Promotion_Type__c, &apos;Consumer Promotion&apos;)||ISPICKVAL(ASI_TH_CRM_Promotion_Type__c, &apos;Wholesaler Promotion&apos;), 
BLANKVALUE(ASI_CRM_CN_Expected_PSF_Est_Total__c, 0)+BLANKVALUE(ASI_CRM_CN_Expected_Display_Est_Total__c, 0)+ BLANKVALUE(ASI_CRM_CN_Expected_Visibility_Est_Total__c,0)  ,
( ASI_CRM_CN_BRSF_Contract_Total__c  *  ASI_CRM_CN_No_of_Months__c )  + ASI_CRM_CN_Contract_Variable_Cost_Total__c + ASI_CRM_CN_Contract_Fixed_Cost_Total__c)))</formula>
