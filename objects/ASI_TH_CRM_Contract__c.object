var recordtype = &apos;{!ASI_TH_CRM_Contract__c.RecordType}&apos; == &apos;ASI CRM SG Proposal (Read-Only)&apos; ? &apos;ASI_CRM_SG_Proposal&apos; : &apos;ASI_CRM_SG_Contract&apos;;
