        <externalId>false</externalId>
        <formula>IF (
   CONTAINS(RecordType.DeveloperName, &apos;ASI_CRM_MY&apos;),
   IF(
      AND(
         (LEFT(RIGHT(Name, 2), 1) = &apos;.&apos;),
         (LEFT(RIGHT(Name, 4), 1) = &apos;.&apos;)
      ),
      SUBSTITUTE(LEFT(Name, LEN(Name) - 2),  &apos;.&apos; + TRIM(ASI_CRM_Version__c), &apos;&apos;),
      SUBSTITUTE(Name,  &apos;.&apos; + TRIM(ASI_CRM_Version__c), &apos;&apos;)
   ),
   IF(
