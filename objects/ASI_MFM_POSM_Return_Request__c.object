    <webLinks>
        <fullName>ASI_MFM_CN_Button_Cancel</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Cancel</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/32.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/32.0/apex.js&quot;)}

try{
var status = &apos;{!ASI_MFM_POSM_Return_Request__c.ASI_MFM_Request_Status__c}&apos;;
if(status ==&apos;Open&apos; || status == &apos;Submitted&apos;){
alert(&apos;this status can not be cancelled!&apos;);
} else {
var proName = &apos;{!$Profile.Name}&apos;;
var recordname = &apos;{!ASI_MFM_POSM_Return_Request__c.RecordType}&apos;;

if(recordname.indexOf(&apos;CN&apos;) == -1 ||proName.indexOf(&apos;CN_Logistic&apos;) == -1){
alert(&apos;You do not have permission to cancel&apos;);
}else{
if(confirm(&quot;Are you sure to cancel the request?&quot;)){
var foc = new sforce.SObject(&quot;ASI_MFM_POSM_Return_Request__c&quot;);
foc.Id = &quot;{!ASI_MFM_POSM_Return_Request__c.Id}&quot;;
foc.ASI_FOC_Request_Status__c=&quot;Cancelled&quot;;
var result = sforce.connection.update([foc]);
if(result[0].success === &quot;true&quot;){
alert(&quot;Cancelled successfully!&quot;);
location.reload();
}else{
alert(&quot;An Error has Occurred. Error: &quot; +result[0].errors.message);
}
}
}
}
}catch(e){
alert(&quot;An unexpected Error has Occurred. Error: &quot; + e);
}</url>
