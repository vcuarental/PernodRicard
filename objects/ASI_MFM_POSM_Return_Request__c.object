    <validationRules>
        <fullName>ASI_MFM_CN_Logistics_Approval_Required</fullName>
        <active>true</active>
        <errorConditionFormula>((ASI_MFM_Logistic_Application__c
&amp;&amp; (ISBLANK( ASI_MFM_Estimated_Packaged_Date__c ) ||   ISPICKVAL(ASI_MFM_Packaging_Warehouse__c,&apos;&apos;)))

||
(!ASI_MFM_Logistic_Application__c &amp;&amp;  !PRIORVALUE( ASI_MFM_Special_Logistics_ForValidation__c ) &amp;&amp; ASI_MFM_Special_Logistics_ForValidation__c &amp;&amp; (ISBLANK( ASI_MFM_Estimated_Packaged_Date__c ) ||  ISPICKVAL(ASI_MFM_Packaging_Warehouse__c,&apos;&apos;))))
&amp;&amp; 
(RecordType.DeveloperName = &apos;ASI_MFM_CN_Repack_Request_Form&apos; || RecordType.DeveloperName = &apos;ASI_MFM_CN_Repack_Request_Form_RO&apos;)</errorConditionFormula>
        <errorMessage>Estimated Packaged Date, Packaging Warehouse are required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_CN_Repack_Req_Form_Charge_Remark</fullName>
        <active>true</active>
        <errorConditionFormula>AND( RecordType.DeveloperName = &apos;ASI_MFM_CN_Repack_Request_Form&apos;,   ISPICKVAL(ASI_MFM_Repack_Manpower_Fee_Charge_To__c,&apos;Other&apos;),
OR( ISBLANK(ASI_MFM_Repack_Charge_Other_Remark__c),
ISNULL(ASI_MFM_Repack_Charge_Other_Remark__c) 
))</errorConditionFormula>
        <errorDisplayField>ASI_MFM_Repack_Charge_Other_Remark__c</errorDisplayField>
        <errorMessage>Please specify the Remark.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_Traceability_Puma_Required</fullName>
        <active>false</active>
        <errorConditionFormula>ISBLANK(ASI_MFM_Traceability_Puma_Requirement__c) &amp;&amp;  ASI_MFM_Special_Logistics_ForValidation2__c &amp;&amp;  ISCHANGED(ASI_MFM_Special_Logistics_ForValidation2__c)</errorConditionFormula>
        <errorDisplayField>ASI_MFM_Traceability_Puma_Requirement__c</errorDisplayField>
        <errorMessage>Traceability Puma Requirement is required.</errorMessage>
    </validationRules>
