        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/apex.js&quot;)} 

result = sforce.connection.query(&quot;Select Id, Name,ASI_MFM_Total_Cost__c,ASI_MFM_Over_Budget__c,ASI_MFM_AccountsAdditionalField__c,ASI_MFM_A_C_Code__c,ASI_MFM_Carry_Forward__c,ASI_MFM_Month_1_Cost__c,ASI_MFM_Month_2_Cost__c,ASI_MFM_Month_3_Cost__c,ASI_MFM_Month_4_Cost__c,ASI_MFM_Month_5_Cost__c,ASI_MFM_Month_6_Cost__c,ASI_MFM_Month_7_Cost__c,ASI_MFM_Month_8_Cost__c,ASI_MFM_Month_9_Cost__c,ASI_MFM_Month_10_Cost__c,ASI_MFM_Month_11_Cost__c,ASI_MFM_Month_12_Cost__c from ASI_MFM_Plan_Line_Item__c where ASI_MFM_Plan__c=&apos;{!ASI_MFM_Plan__c.Id}&apos;&quot;);

//plan=sforce.connection.query(&quot;Select Id, ASI_MFM_PH_Overspending__c from ASI_MFM_Plan__c where Id=&apos;{!ASI_MFM_Plan__c.Id}&apos;&quot;); 
var plan = new sforce.SObject(&quot;ASI_MFM_Plan__c&quot;);
plan.id = &apos;{!ASI_MFM_Plan__c.Id}&apos;;
if (result.size &gt; 0) {
    var records = result.getArray(&apos;records&apos;);
    //var Plan_record = plan.getArray(&apos;records&apos;); 
    var PlanName = &quot;&quot;;
    var PlanLineAmount = 0;
    var i;
    var issub = true;
    var isOverBudget = false;
    var isMissAC = false;
    var isMissCus = false;
    for (i = 0; i &lt; records.length; i++) {
        PlanName = records[i].Name;
        PlanLineAmount = Number(records[i].ASI_MFM_Carry_Forward__c) + Number(records[i].ASI_MFM_Month_1_Cost__c);
        PlanLineAmount += Number(records[i].ASI_MFM_Month_2_Cost__c);
        PlanLineAmount += Number(records[i].ASI_MFM_Month_3_Cost__c);
        PlanLineAmount += Number(records[i].ASI_MFM_Month_4_Cost__c);
        PlanLineAmount += Number(records[i].ASI_MFM_Month_5_Cost__c);
        PlanLineAmount += Number(records[i].ASI_MFM_Month_6_Cost__c);
        PlanLineAmount += Number(records[i].ASI_MFM_Month_7_Cost__c);
        PlanLineAmount += Number(records[i].ASI_MFM_Month_8_Cost__c);
        PlanLineAmount += Number(records[i].ASI_MFM_Month_9_Cost__c);
        PlanLineAmount += Number(records[i].ASI_MFM_Month_10_Cost__c);
        PlanLineAmount += Number(records[i].ASI_MFM_Month_11_Cost__c);
        PlanLineAmount += Number(records[i].ASI_MFM_Month_12_Cost__c);

        if (((Number(records[i].ASI_MFM_Total_Cost__c) - Number(PlanLineAmount)) * (Number(records[i].ASI_MFM_Total_Cost__c) - Number(PlanLineAmount))) &gt; 1) {
            //alert(&apos;Plan total amount&apos;+records[i].ASI_MFM_Total_Cost__c+&apos; not equal to Plan Line amount&apos;+PlanLineAmount+&apos;,Please Recalculate it.&apos;); 
            issub = false;
        }

        // Ignore the blocking until user confirm 
        //Added by Andy
        //alert(records[i].ASI_MFM_Over_Budget__c);
        if (records[i].ASI_MFM_Over_Budget__c == &apos;true&apos;) {
            isOverBudget = true;
        }

        // isOverBudget=true; 

        if (records[i].ASI_MFM_AccountsAdditionalField__c == null)
            isMissCus = true;
        if (records[i].ASI_MFM_A_C_Code__c == null)
            isMissAC = true;
    }


    if (issub) {
        if (isOverBudget) {
            alert(&apos;Warning: Plan is overspending the budget!&apos;);
        }
        
             if (!isMissCus &amp;&amp; !isMissAC) {
            if ((Modal.confirm &amp;&amp; Modal.confirm(&apos;Once you submit this record for approval, you might not be able to edit it or recall it from the approval process depending on your settings. Continue?&apos;)) || (!Modal.confirm &amp;&amp; window.confirm(&apos;Once you submit this record for approval, you might not be able to edit it or recall it from the approval process depending on your settings. Continue?&apos;)))
                navigateToUrl(&apos;/apex/ASI_SubmitApprovalPage?id={!ASI_MFM_Plan__c.Id}&apos;);

        } else if (isMissAC) {
            alert(&quot;Warning: Plan line is missing &apos;AP Code&apos;&quot;);
        } else if (isMissCus) {
            alert(&quot;Warning: Plan line is missing &apos;Customer Name&apos;&quot;);
        } 
    
    } else {
        alert(&apos;Plan total amount not equal to Plan Line amount,Please Recalculate it.&apos;);
    }
}</url>
