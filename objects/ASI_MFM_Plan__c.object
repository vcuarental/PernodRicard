    <validationRules>
        <fullName>ASI_CRM_CN_TP_Amount_Exceeded</fullName>
        <active>true</active>
        <errorConditionFormula>RecordType.DeveloperName = &apos;ASI_CRM_CN_TP_Budget_Plan&apos; &amp;&amp; ASI_CRM_CN_TP_Remaining_Amount__c &lt; 0</errorConditionFormula>
        <errorMessage>The promotion amount of the KA account current fiscal year exceeds the limit, please contact the finance to adjust.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_Budget_Owner_Validation</fullName>
        <active>false</active>
        <description>Budget owner has to be in same team &quot;ASI_MFM_Team__c&quot; with created user</description>
        <errorConditionFormula>AND(CONTAINS( RecordType.DeveloperName, &apos;ASI_MFM_CN&apos;) ,  TEXT( Owner:User.ASI_MFM_Team__c ) !=TEXT( ASI_MFM_Budget_Owner__r.ASI_MFM_Team__c ),  TEXT(Owner:User.ASI_MFM_Team__c ) != &apos;Finance&apos;, TEXT(Owner:User.ASI_MFM_Team__c ) != &apos;BA&apos; ) &amp;&amp; $UserRole.DeveloperName != &apos;ASI_FOC_Financial_Controller&apos; &amp;&amp; $UserRole.DeveloperName != &apos;ASI_FOC_IT_Applier&apos;</errorConditionFormula>
        <errorDisplayField>ASI_MFM_Budget_Owner__c</errorDisplayField>
        <errorMessage>You are NOT allowed to created plan for budget owner not in your team</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_CAP_Future_Estimated_Depr_Date</fullName>
        <active>false</active>
        <errorConditionFormula>CONTAINS(RecordType.DeveloperName,&apos;ASI_MFM_CAP&apos;) &amp;&amp; 
ASI_MFM_Estimated_Depreciation_Start_Dat__c &lt;= TODAY() &amp;&amp; (ISCHANGED(ASI_MFM_Estimated_Depreciation_Start_Dat__c) || ISNEW() )</errorConditionFormula>
        <errorDisplayField>ASI_MFM_Estimated_Depreciation_Start_Dat__c</errorDisplayField>
        <errorMessage>Estimated Depreciation Start Date must be later than today.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_CAP_LockActualDepStartDate</fullName>
        <active>true</active>
        <description>Actual Depreciation Start Date cannot be changed once it has been saved, as Finance has already been notified to reversal entries in JDE</description>
        <errorConditionFormula>AND (
 CONTAINS( RecordType.DeveloperName , &quot;ASI_MFM_CAP_Plan&quot;),
 NOT(ISBLANK( PRIORVALUE( ASI_MFM_Actual_Depreciation_Start_Date__c ))),
 ISCHANGED( ASI_MFM_Actual_Depreciation_Start_Date__c ),
 ISPICKVAL( ASI_MFM_Plan_Type__c , &quot;CAPEX&quot;) ,
 ISPICKVAL( ASI_MFM_Status__c , &quot;Final&quot;),
 NOT(CONTAINS( $Profile.Name , &quot;Administrator&quot;))
)</errorConditionFormula>
        <errorDisplayField>ASI_MFM_Actual_Depreciation_Start_Date__c</errorDisplayField>
        <errorMessage>Actual Depreciation Start Date cannot be changed because Finance has been notified already</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_CAP_Plan_Balance_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
	CONTAINS(RecordType.DeveloperName,&apos;ASI_MFM_CAP_Plan&apos;),
	 ASI_MFM_Plan_Balance__c &lt; 0,
	 NOT(ASI_MFM_CN_Dummy__c ),
!INCLUDES($User.BypassTriggers__c,&apos;ASI_MFM_CAP_Plan_Balance_Validation&apos;)
	)</errorConditionFormula>
        <errorMessage>Plan cannot be overspent</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_Create_Status_Check</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISNEW(), ISPICKVAL(ASI_MFM_Status__c, &apos;Submitted&apos;), OR(RecordType.DeveloperName == &apos;ASI_MFM_TH_Plan&apos;, RecordType.DeveloperName == &apos;ASI_MFM_SG_Plan&apos;))</errorConditionFormula>
        <errorDisplayField>ASI_MFM_Status__c</errorDisplayField>
        <errorMessage>Please set status to &apos;Draft&apos; in Plan creation</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_Cross_Fiscal_Year_Checking</fullName>
        <active>true</active>
