    <webLinks>
        <fullName>ASI_eForm_Submit_For_Approval</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Submit For Approval</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!requireScript(&quot;/soap/ajax/30.0/connection.js&quot;)}
sforce.connection.sessionId = &quot;{!$Api.Session_ID}&quot;;

var currentHeader = sforce.connection.query(&quot;Select ASI_eForm_Status__c,ASI_eForm_Change_Category__c, Id from ASI_eForm_IT_Change_Request__c where Id = &apos;{!ASI_eForm_IT_Change_Request__c.Id}&apos;&quot;);

var currentHeaderDetails = sforce.connection.query(&quot;Select Id from ASI_eForm_IT_Change_Request_Item__c where ASI_eForm_IT_Change_Request__c= &apos;{!ASI_eForm_IT_Change_Request__c.Id}&apos; &quot;);

var records = currentHeader.getArray(&quot;records&quot;);
var detailRecords = currentHeaderDetails.getArray(&quot;records&quot;);

if (detailRecords == null || detailRecords.length &lt;= 0 )
{
  alert(&quot;IT Change Request Items Not Found&quot;);
} else if (records[0].ASI_eForm_Status__c== &apos;Draft&apos;){

  var continueRedirect = true;
  if (records[0].ASI_eForm_Change_Category__c== &apos;Medium&apos; ||  records[0].ASI_eForm_Change_Category__c== &apos;High&apos; ){

     var attachments = sforce.connection.query(&quot;SELECT Id FROM ASI_eForm_ITCRF_Attachment__c WHERE ASI_eForm_Category__c=&apos;Roll Back Plan&apos; and ASI_eForm_IT_Change_Request__c=&apos;{!ASI_eForm_IT_Change_Request__c.Id}&apos;&quot;);

     var attachmentRecords = attachments.getArray(&quot;records&quot;);

     if (attachmentRecords == null || attachmentRecords.length &lt;= 0 ){
         alert(&quot;Please upload the Roll Back Plan attachment before you submit for Approval&quot;);  
         continueRedirect = false;
     }
  }

  if (continueRedirect){
      var upObj= new sforce.SObject(&quot;ASI_eForm_IT_Change_Request__c&quot;);
      upObj.Id=&apos;{!ASI_eForm_IT_Change_Request__c.Id}&apos;;
      upObj.ASI_eForm_Submit_Flag__c=true;
      sforce.connection.update([upObj]); 
      
      try {
var rec = new sforce.SObject(&apos;ASI_eForm_IT_Change_Request__c&apos;); 
rec.Id = &quot;{!ASI_eForm_IT_Change_Request__c.Id}&quot;; 
rec.ASI_eForm_Sys_Allow_Submit_Approval__c = true; 
result = sforce.connection.update([rec]);
 
         window.open(&quot;/apex/ASI_SubmitApprovalPage?id={!ASI_eForm_IT_Change_Request__c.Id}&quot;,&quot;_self&quot;);

      } catch(error) {
        sforce.debug.log(error.faultcode);
        sforce.debug.log(error.faultstring);
      }
  }

}</url>
