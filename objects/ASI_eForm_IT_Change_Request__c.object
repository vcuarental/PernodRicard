    <validationRules>
        <fullName>ASI_eForm_Further_Authorizer_Required</fullName>
        <active>true</active>
        <description>(Change Category is High) OR (Change Category is Medium but Change Type is Emergency)</description>
        <errorConditionFormula>AND( OR(ISBLANK( ASI_eForm_High_Level_Authorizer__c ),ISBLANK(  ASI_eForm_Further_Authorizer__c )) ,OR( ISPICKVAL( ASI_eForm_Change_Category__c , &quot;High&quot;) , AND(ISPICKVAL( ASI_eForm_Change_Category__c , &quot;Medium&quot;),ISPICKVAL( ASI_eForm_Change_Type__c , &quot;Emergency&quot;))))</errorConditionFormula>
        <errorMessage>Both High-Level Authorizer and Further Authorizer are required to this form</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_eForm_High_Authorizer_Required</fullName>
        <active>true</active>
        <description>(Change Category is Medium) OR (Change Category is Low but Change Type is Emergency)</description>
        <errorConditionFormula>AND( ISBLANK( ASI_eForm_High_Level_Authorizer__c ) ,OR( AND( ISPICKVAL( ASI_eForm_Change_Category__c , &quot;Medium&quot;) ,!ISPICKVAL(  ASI_eForm_Change_Type__c  , &quot;Emergency&quot;)) , AND(ISPICKVAL( ASI_eForm_Change_Category__c , &quot;Low&quot;),ISPICKVAL(  ASI_eForm_Change_Type__c  , &quot;Emergency&quot;))))</errorConditionFormula>
        <errorMessage>High-Level Authorizer is required to this form</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_eForm_Invalid_ChangeAuthorizer</fullName>
        <active>true</active>
        <errorConditionFormula>ASI_eForm_CN_Change_Authorizer__c  =  OwnerId</errorConditionFormula>
        <errorMessage>Change Authorizer cannot be the same as the Applicant.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_eForm_Owner_Approver_Duplicate</fullName>
        <active>true</active>
        <errorConditionFormula>(ASI_eForm_Change_Authorizer__c =  OwnerId )</errorConditionFormula>
        <errorDisplayField>ASI_eForm_Change_Authorizer__c</errorDisplayField>
        <errorMessage>Approver and Owner cannot be the same.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_eForm_Ticket_No_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL( ASI_eForm_Change_Type__c , &quot;Emergency&quot;), ISBLANK( ASI_eForm_Problem_Ticket_No__c ))</errorConditionFormula>
        <errorDisplayField>ASI_eForm_Problem_Ticket_No__c</errorDisplayField>
        <errorMessage>If Emergency, please indicate the problem ticket no.</errorMessage>
    </validationRules>
