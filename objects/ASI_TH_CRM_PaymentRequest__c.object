    <fields>
        <fullName>ASI_CRM_CN_Total_Payment__c</fullName>
        <externalId>false</externalId>
        <formula>If(
	AND(ASI_CRM_CN_Promotion_Type__c==&apos;Heavy Contract On&apos;, !CONTAINS(Name, &apos;-000&apos;)) || AND(ASI_CRM_CN_Promotion_Type__c==&apos;TOT/MOT Contract&apos;, !CONTAINS(Name, &apos;-000&apos;)),
	ASI_CRM_Total_Actual_Amount__c,
	IF(ASI_CRM_CN_Promotion_Type__c==&apos;Consumer Promotion&apos;,
			ASI_CRM_Total_Variable_Amount__c + ASI_CRM_CN_Total_Free_Goods_Amount__c+ ASI_CRM_CN_Total_Other_Amount__c,
			IF(ASI_CRM_CN_Promotion_Type__c==&apos;Outlet Promotion&apos;||ASI_CRM_CN_Promotion_Type__c==&apos;Wholesaler Promotion&apos;,
				ASI_CRM_Total_Variable_Amount__c+ ASI_CRM_CN_Total_Other_Amount__c,
				(ASI_CRM_CN_Adjustment__c* ASI_CRM_CN_Total_BRSF_Amount__c)+ ASI_CRM_CN_Total_Other_Amount__c + ASI_CRM_CN_Total_PSF_Amount__c)
	)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Total Payment</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
