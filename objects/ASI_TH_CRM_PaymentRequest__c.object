{!REQUIRESCRIPT(&quot;/soap/ajax/32.0/apex.js&quot;)}

try{
    var status = &apos;{!ASI_TH_CRM_PaymentRequest__c.ASI_TH_CRM_Status__c}&apos;;
    if(status!=&apos;Submit for Cancel&apos; &amp;&amp; status!=&apos;Cancelled&apos; ){
        var isAutoRequest = &apos;{!ASI_TH_CRM_PaymentRequest__c.ASI_CRM_Auto_Generate__c}&apos;==true ? true : false;
        var msg_str = isAutoRequest==false ? &apos;Are you sure to cancel the request?&apos; : &apos;Are you sure to cancel the request? You may need to unlock offtake and re-generate.&apos;;
        if(confirm(msg_str)==true){
            var payment = new sforce.SObject(&quot;ASI_TH_CRM_PaymentRequest__c&quot;);
            payment.Id = &quot;{!ASI_TH_CRM_PaymentRequest__c.Id}&quot;;
            var msg_success=&apos;&apos;;
            
            //If status is draft, directly cancel:
            if(status == &apos;Draft&apos;){
                payment.ASI_TH_CRM_Status__c =&quot;Cancelled&quot;;
                msg_success =&apos;Cancelled successfully!&apos;;
            }else{
                payment.ASI_TH_CRM_Status__c =&quot;Submit for Cancel&quot;;
                msg_success =&apos;Cancelled successfully! You may need to inform responsible person for the rest of the process.&apos;;
            }
            
            var result = sforce.connection.update([payment]);
            if(result[0].success === &quot;true&quot;){
                alert(msg_success);
                location.reload();
                /**
                if(isAutoRequest==true){
                    if(confirm(&apos;This is an Auto request. You need to unlock offtake before re-gen for it. Do you want to proceed now?&apos;)){
                        window.top.location.href=&apos;/apex/ASI_CRM_MY_FOCRegen&apos;;
                    }else{
                        location.reload();
                    }
                }else{
                    location.reload();
                }**/
            }else{
                alert(&quot;An Error has Occurred. Error: &quot; +result[0].errors.message);
            }
        }
      
    }else if (status ==&apos;Submit for Cancel&apos;  ){
        if(confirm(&quot;Are you sure to cancel the request?&quot;)==true){
            var hasAccessRight = sforce.apex.execute(&quot;ASI_CRM_MY_WebService&quot;,&quot;isGroupMember_WS&quot;,{uid:&quot;{!User.Id}&quot;,gName:&apos;ASI_CRM_MY_Cancel_Request_User&apos;});
            if(hasAccessRight[0] === &quot;true&quot; ){
                var payment = new sforce.SObject(&quot;ASI_TH_CRM_PaymentRequest__c&quot;);
                payment.Id = &quot;{!ASI_TH_CRM_PaymentRequest__c.Id}&quot;;
                payment.ASI_TH_CRM_Status__c =&quot;Cancelled&quot;;
                var result = sforce.connection.update([payment]);
                if(result[0].success === &quot;true&quot;){
                    alert(&quot;Cancelled successfully!&quot;);
                    location.reload();
                }else{
                    alert(&quot;An Error has Occurred. Error: &quot; +result[0].errors.message);
                }
            }else {
                alert (&apos;The request has been submitted for cancel. Please inform responsible person for the rest of the process. &apos;);
            }
            location.reload();
        }
        
    }else{
         //alert(&apos;Only &quot;Approved&quot; order can be cancelled&apos;);
         alert(&apos;The request has been cancelled already.&apos;);
    }
    
}
catch(e){
    alert(&quot;An unexpected Error has Occurred. Error: &quot; + e);
}</url>
