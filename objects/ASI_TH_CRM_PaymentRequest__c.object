	) {
	        alert(&quot;Please input all required fields (Outlet, Vendor and Invoice Date) before submit.&quot;);
	} else
	if ((&apos;{! ASI_TH_CRM_PaymentRequest__c.ASI_CRM_System_Remark__c }&apos; != &apos;SG Promotion&apos; &amp;&amp; &apos;{! ASI_TH_CRM_PaymentRequest__c.ASI_CRM_System_Remark__c }&apos; != &apos;Wholesaler Back Rebate Payment&apos;) &amp;&amp; 
	(&apos;{! JSENCODE(ASI_TH_CRM_PaymentRequest__c.ASI_CRM_CN_OutletWS__c) }&apos; == &apos;&apos; ||
	&apos;{!ASI_TH_CRM_PaymentRequest__c.ASI_TH_CRM_Contract__c}&apos; == &apos;&apos; ||
	&apos;{!JSENCODE(ASI_TH_CRM_PaymentRequest__c.ASI_CRM_Vendor__c)}&apos; == &apos;&apos; ||
	&apos;{!ASI_TH_CRM_PaymentRequest__c.ASI_CRM_SG_Invoice_Date__c}&apos;== &apos;&apos;)
	) {
	        alert(&quot;Please input all required fields (Outlet, Contract, Vendor and Invoice Date) before submit.&quot;);
	} else
	if (!validateAchievement()) {
	        alert(&quot;You are not allowed to submit this payment because the sales have not been achieved. Please delete this payment request.&quot;);
	} else 
	if (!validateSalesSupport()) {
	        alert(&quot;Please upload document for sales support before submit.&quot;);
	} else 
	if (!validateManualPaymentAttachment()) {
	        alert(&quot;Please upload document for manual payment requeset before submit.&quot;);
	} else {
		var confirmToSubmit = true;
                /*
		if (&apos;{!ASI_TH_CRM_PaymentRequest__c.ASI_CRM_SG_Invoice_Date__c == TODAY()}&apos;==&apos;true&apos;){
			confirmToSubmit = confirm(&apos;Please check your Invoice Date input, you have input as &quot;TODAY&quot;.&apos;);
		}
                */
		if (confirmToSubmit) {
			var ob = new sforce.SObject(&quot;ASI_TH_CRM_PaymentRequest__c&quot;); 
			ob.Id = &apos;{!ASI_TH_CRM_PaymentRequest__c.Id}&apos;; 
			ob.ASI_CRM_Sys_Allow_Submit_Approval__c = true; 
			result = sforce.connection.update([ob]); 

			if (!result[0].getBoolean(&apos;success&apos;)) {
