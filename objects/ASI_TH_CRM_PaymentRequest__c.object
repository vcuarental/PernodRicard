    <validationRules>
        <fullName>ASI_CRM_CN_BRSF_Period_Checking</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ASI_CRM_CN_BRSF_Start_Date__c &gt;  ASI_CRM_CN_BRSF_End_Date__c ,  RecordType.DeveloperName =&apos;ASI_CRM_CN_Payment_Request&apos;)</errorConditionFormula>
        <errorMessage>Please select correct BRSF period</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_MY_Check_Customer_CRM_Code</fullName>
        <active>true</active>
        <errorConditionFormula>And(
CONTAINS(RecordType.DeveloperName,&apos;ASI_CRM_MY&apos;) ,
ISBLANK(ASI_CRM_CN_OutletWS__r.ASI_CRM_CN_JDE_Code__c),
!INCLUDES($User.BypassTriggers__c,&apos;ASI_CRM_MY_Admin&apos;)
)</errorConditionFormula>
        <errorMessage>There is not JDE code under this outlet, please select Outlet (MY) for this payment</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_MY_CreateChecking</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
$User.BypassVR__c != TRUE, 
CONTAINS(RecordType.DeveloperName,&apos;ASI_CRM_MY&apos;), 
OR( 
TEXT(ASI_TH_CRM_Contract__r.ASI_TH_CRM_Contract_Status__c) != &apos;Final&apos;, 
ASI_TH_CRM_Contract__r.ASI_CRM_Terminate__c == TRUE, 
ASI_TH_CRM_Contract__r.ASI_CRM_Obsolete_Version__c == TRUE 
), 
ISNEW() 
)</errorConditionFormula>
        <errorMessage>Only FINAL contract is allowed to create Payment Request.</errorMessage>
    </validationRules>
