        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/32.0/connection.js&quot;)}
try{
    var status = &apos;{!ASI_TH_CRM_PaymentRequest__c.ASI_TH_CRM_Status__c}&apos;;
    var entitledAmount = parseInt(&apos;{!ASI_TH_CRM_PaymentRequest__c.ASI_CRM_MY_Total_Entitled_Payment__c}&apos;);
    if(status==&apos;Draft&apos; ){
        if(entitledAmount!=null &amp;&amp; entitledAmount&gt;0){
            if(confirm(&apos;Are you sure to approve the payment?&apos;)==true){
                var payment = new sforce.SObject(&quot;ASI_TH_CRM_PaymentRequest__c&quot;);
                payment.Id = &quot;{!ASI_TH_CRM_PaymentRequest__c.Id}&quot;;
                payment.ASI_TH_CRM_Status__c =&quot;Approved&quot;;
                var result = sforce.connection.update([payment]);
                if(result[0].success === &quot;true&quot;){
                    alert(&quot;The payment is approved!&quot;);
                    location.reload();
                }else{
                    alert(&quot;An Error has Occurred. Error: &quot; +result[0].errors.message);
                }
            }
        }else{
            alert(&apos;The Entitled Amount is 0. Please update the line items first.&apos;);
        }
        
      
    }else{
         alert(&apos;Only &quot;Draft&quot; payment can be approved.&apos;);
    }
}
catch(e){
    alert(&quot;An unexpected Error has Occurred. Error: &quot; + e);
}</url>
