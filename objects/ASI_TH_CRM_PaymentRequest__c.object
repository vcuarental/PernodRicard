function validateSalesSupport() {
	if ({!ASI_TH_CRM_PaymentRequest__c.ASI_CRM_Total_No_of_Sales_Support__c}==0) return true;
	var queryString = &quot;SELECT Id, ParentId FROM Attachment WHERE ParentId = &apos;{!ASI_TH_CRM_PaymentRequest__c.Id}&apos;&quot;;
	var relatedCharts = sforce.connection.query(queryString); 
	records = relatedCharts.getArray(&apos;records&apos;); 
	if (records.length &gt; 0){
	    return true;
	}
	return false;
}

function validateManualPaymentAttachment() {
	if ({!ASI_TH_CRM_PaymentRequest__c.ASI_CRM_System_Generated__c}) return true;
	var queryString = &quot;SELECT Id, ParentId FROM Attachment WHERE ParentId = &apos;{!ASI_TH_CRM_PaymentRequest__c.Id}&apos;&quot;;
	var relatedCharts = sforce.connection.query(queryString); 
	records = relatedCharts.getArray(&apos;records&apos;); 
	if (records.length &gt; 0){
	    return true;
	}
	return false;
}

function validateAchievement(){
	if ({!!ASI_TH_CRM_PaymentRequest__c.ASI_CRM_System_Generated__c}) return true;
	var queryString = &quot;SELECT Id, ASI_CRM_Till_Date_Sales_Percent_Achieved__c FROM ASI_TH_CRM_Contract__c WHERE Id = &apos;{!ASI_TH_CRM_PaymentRequest__c.ASI_TH_CRM_ContractId__c}&apos; limit 1&quot;;
	var relatedCharts = sforce.connection.query(queryString); 
	records = relatedCharts.getArray(&apos;records&apos;); 
	if (records.length &gt; 0){ 
	    console.log(records[0].ASI_CRM_Till_Date_Sales_Percent_Achieved__c);
	    console.log({!ASI_TH_CRM_PaymentRequest__c.ASI_CRM_Achievement__c}); 
	    //return false;
	    if(records[0].ASI_CRM_Till_Date_Sales_Percent_Achieved__c &lt; {!IF(ISBLANK(ASI_TH_CRM_PaymentRequest__c.ASI_CRM_Achievement__c),0,ASI_TH_CRM_PaymentRequest__c.ASI_CRM_Achievement__c)}){
	    	return false;
	    }
	}
	return true;
}

try {
	if ((&apos;{! ASI_TH_CRM_PaymentRequest__c.ASI_CRM_System_Remark__c }&apos; == &apos;SG Promotion&apos; || &apos;{! ASI_TH_CRM_PaymentRequest__c.ASI_CRM_System_Remark__c }&apos; == &apos;Wholesaler Back Rebate Payment&apos;) &amp;&amp; 
	(&apos;{! JSENCODE(ASI_TH_CRM_PaymentRequest__c.ASI_CRM_CN_OutletWS__c) }&apos; == &apos;&apos; ||
