		ISPICKVAL(MMPJ_Ext_Vign_DBP__c,&quot;Conforme&quot;),
		ISPICKVAL(MMPJ_Ext_Vign_DEHP__c,&quot;Conforme&quot;),
		ISPICKVAL(MMPJ_Ext_Vign_DINP__c,&quot;Conforme&quot;)
	),
	&quot;Conforme&quot;,
	IF(
		OR(
			ISPICKVAL(MMPJ_Ext_Vign_DBP__c,&quot;&quot;),
			ISPICKVAL(MMPJ_Ext_Vign_DEHP__c,&quot;&quot;),
			ISPICKVAL(MMPJ_Ext_Vign_DINP__c,&quot;&quot;)
