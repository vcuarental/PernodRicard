    <webLinks>
        <fullName>PRH_Edit_All_Acc</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Edit All</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>{!requireScript(&quot;/soap/ajax/26.0/connection.js&quot;)}

sforce.connection.sessionId = &apos;{!$Api.Session_ID}&apos;;
try
{

	var AccountID = &apos;{!Account.Id}&apos;;
	var isIconic = &apos;{!Account.PRH_Iconic_Account__c}&apos;;

	if(isIconic!=0){
		var qr = sforce.connection.query(&quot;SELECT ID FROM PRH_Luxury_Accounts__c where PRH_Existing_Account__c =&apos;&quot;+ AccountID +&quot;&apos;&quot;);

		var records = qr.getArray(&quot;records&quot;);
		
		//var loc = &quot;/apex/PRH_Iconic_Platform_Account_Mass_Edit?id=&quot;.concat(records[0].Id);
		var loc = &quot;/apex/PRH_Iconic_Platform_Mass_Edit_Desktop?id=&quot;.concat(records[0].Id);
		//var returnURL = &quot;&amp;retURL=%2F&quot;.concat(AccountID);
		//window.location = loc.concat(returnURL);
		loc += &quot;&amp;AcctID=&quot;+AccountID;
		window.location = loc;
	}else{
		alert(&apos;This Account is not a Luxury Account.&apos;);
	}
}
catch (error)
{
	alert(&apos;This Account is not a Luxury Account.&apos;);
}</url>
    </webLinks>
