{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/apex.js&quot;)}

try{ 
	var idList = sforce.connection.query( &quot;SELECT PermissionSetId FROM PermissionSetAssignment WHERE AssigneeId = &quot; + &quot;&apos;&quot; + window.UserContext.userId + &quot;&apos;&quot; ); 
	var records = idList.getArray(&quot;records&quot;); 
	var permissionID = &quot;&apos;&quot;; 
	for (var i = 0; i &lt; records.length ; i++) { 
		permissionID += records[i].PermissionSetId + &quot;&apos;, &apos;&quot;; 
	} 
	permissionID = permissionID.substring(0, permissionID.length-3); 
	var permList = sforce.connection.query(&quot;Select Name From PermissionSet Where ID in (&quot; + permissionID + &quot;)&quot;); 
    /*alert(&quot;Select Name From PermissionSet Where ID in (&quot; + permissionID +&quot;)&quot;);*/
	var psa;
	var permName = permList.getArray(&quot;records&quot;); 
	var permissionSet = &quot;&quot;; 
	for (var i = 0; i &lt; permName.length ; i++) { 
		permissionSet += permName[i].Name + &quot; &quot;; 
	} 
	
	/* Check permission of user to execute delete */
	if (permissionSet.indexOf(&apos;PRH_Iconic_Platform_PR_Holding_Admin&apos;) != -1) { 
		var selectedIconicAccounts = {!GETRECORDIDS($ObjectType.PRH_Luxury_Accounts__c)}; 
		/*alert(&apos;[&apos; + selectedIconicAccounts + &apos;]&apos;);*/
		
		if (selectedIconicAccounts == &apos;&apos;) {
			/* Display a specif message in case of no Iconic Account selected */
			alert(&apos;You need to select an Iconic Account first.&apos;);		
		} else {
			var selectedItemsInStringFormat = selectedIconicAccounts.toString(); 
			/*alert(selectedItemsInStringFormat);*/
			var tabID = selectedItemsInStringFormat.split(&quot;,&quot;); 
			var singleQuote = &apos;\&apos;&apos;;
			var comma = &apos;,&apos;;
			var setOfSelectedItems = &apos;&apos;;
			for (var i = 0; i &lt; (tabID.length - 1); i++) { 
			    /*alert(tabID[i]);*/
				setOfSelectedItems = setOfSelectedItems + singleQuote + tabID[i] + singleQuote + comma; 
			}
			setOfSelectedItems = setOfSelectedItems + singleQuote + tabID[i] + singleQuote; 
			/*alert(setOfSelectedItems);*/
			/* Check status of Iconic Account */
			var query = &quot;Select Id, Name, PRH_Suppress_and_archive__c from PRH_Luxury_Accounts__c where Id IN (&quot; + setOfSelectedItems + &quot;)&quot;;
			/*alert(query);*/
			var resultQuery = sforce.connection.query(query); 
			var records = resultQuery.getArray(&apos;records&apos;); 
			var selectedItemsOk = true;
			for (var i = 0; i &lt; records.length; i++) { 
				var record = records[i]; 
				/*alert(record.Id + &quot; -- &quot; + record.PRH_Suppress_and_archive__c); */
				if (record.PRH_Suppress_and_archive__c.toString() == &quot;false&quot;) {
					selectedItemsOk = false;
				}
			}
			
			if (selectedItemsOk == true) {
				/* Assign PermissionSet Author Apex to user in order to be able to execute the batch */
				var psQuery = &quot;select id from PermissionSet where name = &apos;PRH_Iconic_Platform_Support_LeCercle&apos;&quot;;
				/*alert(&apos;psQuery=&apos; + psQuery);*/
				var listPs = sforce.connection.query(psQuery);
				var records = listPs.getArray(&apos;records&apos;);
				/*alert(&apos;records=&apos; + records);*/
				/*alert(&apos;records.length=&apos; + records.length);*/
				if (records.length &gt; 0) {
					var psId = records[0];
					/*alert(&quot;psId=&quot;+psId.Id);*/
					var queryUserPsa = &quot;SELECT Id FROM PermissionSetAssignment WHERE AssigneeId = &quot; + &quot;&apos;&quot; + window.UserContext.userId + &quot;&apos; AND PermissionSetId=&apos;&quot; + psId.Id + &quot;&apos;&quot;;
					/*alert(queryUserPsa);*/
					var userPsaList = sforce.connection.query(queryUserPsa); 
					var psarecords = userPsaList.getArray(&apos;records&apos;); 
					if (psarecords.length == 0) {		
						psa = new sforce.SObject(&quot;PermissionSetAssignment&quot;); 
						psa.assigneeId = window.UserContext.userId;
						psa.PermissionSetId = psId.Id;
						/*alert(&quot;Is going to assign Author Apex permission &quot; + psa);*/
						var result = sforce.connection.create([psa]); 
						if (result[0].getBoolean(&quot;success&quot;)) { 
							/*alert(&quot;New PermissionSetAssignment temporary assigned with id &quot; + result[0].id); */
							psa.Id = result[0].id;
						} else { 
							alert(&quot;Failed to create PermissionSetAssignment &quot; + result[0]); 
						} 
					}
				}
				/* Add a tempo for batch execution */
				var startTempo1 = new Date().getTime(); 
				while (new Date().getTime() &lt; startTempo1 + 1000);
				/* Launch batch for Iconic Accounts deletion */
				var scr = &quot;PRH_Iconic_Account_Deletion c = new PRH_Iconic_Account_Deletion(&apos;&quot; + selectedItemsInStringFormat + &quot;&apos;);&quot; + &quot; Database.executeBatch(c, 200); &quot;;
				/*alert(scr);*/
				var result = sforce.apex.executeAnonymous(scr);
				/*alert(result);*/
				alert(&apos;Iconic Account(s) deletion with collects and LeCercle/Competitor brands has started. An email notification will be sent once the process has been completed.&apos;);
				/* Add a tempo for second batch execution */
				var startTempo2 = new Date().getTime(); 
				while (new Date().getTime() &lt; startTempo2 + 5000);
				/* Remove PermissionSet Author Apex for user */
				/*alert(&quot;psa.Id = &quot; + psa.Id); */
				var scr2 = &quot;PRH_Iconic_Account_Deletion_End c = new PRH_Iconic_Account_Deletion_End();&quot; + &quot; Database.executeBatch(c, 200); &quot;;
				var result2 = sforce.apex.executeAnonymous(scr2);
			} else {
				alert(&apos;Only Iconic Accounts with Suppress value set to true can be selected for deletion.&apos;);
			}
		}
	} else { 
		alert(&apos;Insufficient Privilege to perform this action.&apos;); 
	}
} catch (err) {
	alert(err);
}</url>
