    <validationRules>
        <fullName>ASI_CRM_TH_GWP_Promotion</fullName>
        <active>true</active>
        <description>For GWP Promotion, GWP Allocation must be input</description>
        <errorConditionFormula>AND( 
CONTAINS(TEXT(ASI_CRM_Promotion__r.ASI_CRM_Type__c),&quot;GWP&quot;)
, ISNULL(ASI_CRM_GWP_Allocation__c)
)</errorConditionFormula>
        <errorMessage>Please fill in GWP Allocation.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_TH_TastingOREvent_Promotion</fullName>
        <active>true</active>
        <description>For Tasting/Event Promotion, Volume Target	and No. Activities must be input</description>
        <errorConditionFormula>AND(
OR(
CONTAINS(TEXT(ASI_CRM_Promotion__r.ASI_CRM_Type__c),&quot;Tasting&quot;),
CONTAINS(TEXT(ASI_CRM_Promotion__r.ASI_CRM_Type__c),&quot;Event&quot;)
),
OR(
ISNULL(ASI_CRM_Volume_Target__c), 
ISNULL(ASI_CRM_No_of_Activities__c)
)
)</errorConditionFormula>
        <errorMessage>Please fill in Volume Target and No. Activities.</errorMessage>
    </validationRules>
