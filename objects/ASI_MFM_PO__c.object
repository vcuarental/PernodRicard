    <fields>
        <fullName>ASI_MFM_PO_Balance__c</fullName>
        <description>**Note to Developer** 
The field is shared between B5 and B6, please compare the difference before deployment</description>
        <externalId>false</externalId>
        <formula>IF (
	OR(CONTAINS(RecordType.DeveloperName, &quot;ASI_MFM_RM&quot;),CONTAINS(RecordType.DeveloperName, &quot;ASI_MFM_HK&quot;)), 
	ASI_MFM_Base_Currency_Amount__c - ASI_MFM_Invoice_Amount_from_JDE__c, 
	IF (
		CONTAINS(RecordType.DeveloperName, &quot;ASI_MFM_TR&quot;), 
		ASI_MFM_Base_Currency_Amount__c - ASI_MFM_Invoice_Amount_from_JDE__c + ASI_MFM_TR_PO_Balance_Variance__c, 
		IF (
			CONTAINS(RecordType.DeveloperName, &quot;ASI_MFM_MY&quot;), 
			ASI_MFM_Base_Currency_Amount__c - ASI_MFM_Payment_Request_Amount__c - ASI_MFM_T_E_Amount__c, 
			IF (
				CONTAINS(RecordType.DeveloperName, &quot;ASI_MFM_TH&quot;), 
				ASI_MFM_Base_Currency_Amount__c - ASI_MFM_Payment_Request_Amount__c - ASI_MFM_T_E_Amount__c - ASI_MFM_FOC_Actual__c - ASI_MFM_Payroll_Actual__c, 
				IF (
					CONTAINS(RecordType.DeveloperName, &quot;ASI_MFM_PH&quot;), 
					ASI_MFM_Base_Currency_Amount__c-ASI_MFM_FOC_Actual__c - ASI_MFM_Payment_Request_Amount__c, 
					IF (
						CONTAINS(RecordType.DeveloperName, &quot;ASI_MFM_SG&quot;), 
						ASI_MFM_Base_Currency_Amount__c - ASI_MFM_Payment_Request_Amount__c - ASI_MFM_T_E_Amount__c - ASI_MFM_Opening_Balance__c , 
						IF (
							OR(CONTAINS(RecordType.DeveloperName, &quot;ASI_MFM_CAP_Accrual_PO&quot;),CONTAINS(RecordType.DeveloperName, &quot;ASI_MFM_CAP_PO&quot;)), 
							ASI_MFM_Base_Currency_Amount__c - (FLOOR(ASI_MFM_Payment_Request_Amount__c*100)/100) , 
							IF (
								CONTAINS(RecordType.DeveloperName, &quot;ASI_MFM_JP_PO&quot;), 
								ASI_MFM_Base_Currency_Amount__c - ASI_MFM_PaidAmount_in_BaseCurrency__c - ASI_MFM_Opening_Balance__c, 
								ASI_MFM_Base_Currency_Amount__c - ASI_MFM_Payment_Request_Amount__c
							) 
						) 
					) 
				) 
			) 
		) 
	) 
)</formula>
