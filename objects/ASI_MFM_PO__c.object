    <webLinks>
        <fullName>ASI_MFM_HK_Submit_for_Approval</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Submit for Approval</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/apex.js&quot;)}
var isValid = true;
var oriPO = &apos;{!ASI_MFM_PO__c.ASI_MFM_Original_PO_for_AdjustmentId__c}&apos;;
var recType = &apos;{!ASI_MFM_PO__c.RecordType}&apos;;

if(oriPO != null){
 var query = &quot;SELECT Id, ASI_MFM_Status__c, Name FROM ASI_MFM_PO__c WHERE Id = \&apos;&quot; + oriPO + &quot;\&apos; LIMIT 1&quot;;
 var records = sforce.connection.query(query); 
 var records1 = records.getArray(&apos;records&apos;); 
 //alert(records);  
 for(var i=0;i&lt;records1.length;i++){ 
  if(records1[i].ASI_MFM_Status__c == &apos;Voided&apos;){
   isValid = false;
   alert(&apos;Cannot submit a Voided PO.&apos;);
  } 
 } 
}

// [SH] 2018-11-07
if(recType.indexOf(&apos;HK&apos;)&gt; -1){
 var APAD = &apos;{!ASI_MFM_PO__c.ASI_MFM_AP_AD__c}&apos;;
 var filter = &apos;&apos;;
 if(APAD == &apos;A&amp;P&apos;){
  filter = &quot; and ASI_MFM_A_C_Code__r.ASI_MFM_Reporting_Category__c != \&apos;Sales Reduction\&apos; &quot;;
 }else{
  filter = &quot; and ASI_MFM_A_C_Code__r.ASI_MFM_Reporting_Category__c = \&apos;Sales Reduction\&apos; &quot;;
 }
 var Strquery = &quot;Select id from ASI_MFM_PO_Line_Item__c where ASI_MFM_PO__c = \&apos;{!ASI_MFM_PO__c.Id}\&apos;&quot; + filter;
 var POL = sforce.connection.query(Strquery);
 var POLrecords = POL.getArray(&apos;records&apos;); //alert(POLrecords.length);
 if(POLrecords.length == 0){ isValid = false; alert(&apos;The account(s) being used in the detail lines not aligning with the A&amp;D/A&amp;P selection in the PO header&apos;);}
}
var resultAtt=sforce.connection.query(&quot;Select Id from Attachment where parentId=&apos;{!ASI_MFM_PO__c.Id}&apos;&quot;); 
var recordsAtt = resultAtt.getArray(&apos;records&apos;); 
var pafId = &apos;{!ASI_MFM_PO__c.ASI_MFM_PAF_Number__c}&apos;; 
if(recordsAtt.length &lt; 1 &amp;&amp; pafId == null){
  isValid = false;
  alert(&apos;At leasr 1 attachment attached before submit for approval&apos;);
}
// END - [SH]

if(isValid == true){
	var amount =  &quot;{!ASI_MFM_PO__c.ASI_MFM_PO_Amount__c}&quot;;

	if(amount != null &amp;&amp; amount != &apos;&apos;){
		if ((Modal.confirm &amp;&amp; Modal.confirm(&apos;Once you submit this record for approval, you might not be able to edit it or recall it from the approval process depending on your settings. Continue?&apos;)) || (!Modal.confirm &amp;&amp; window.confirm(&apos;Once you submit this record for approval, you might not be able to edit it or recall it from the approval process depending on your settings. Continue?&apos;))) navigateToUrl(&apos;/apex/ASI_SubmitApprovalPage?id={!ASI_MFM_PO__c.Id}&apos;);
	}
	/*else{
		alert(&apos;PO Amount has to be larger than zero&apos;);
	}*/
}</url>
