if (resultPO.size &gt; 0) {

    var recordsPO = resultPO.getArray(&apos;records&apos;);
    var po = recordsPO[0];
    var issub = true;
    var Msg = &apos;&apos;;
    var landingbalance =0;
	 
    if(po.ASI_MFM_Plan__r.ASI_MFM_TW_Landing_Amount__c!=null){
    	landingbalance = po.ASI_MFM_Plan__r.ASI_MFM_TW_Landing_Amount__c- po.ASI_MFM_Base_Currency_Amount__c;

    	landingbalance = po.ASI_MFM_Plan__r.ASI_MFM_TnE_Amount__c!=null? (landingbalance-po.ASI_MFM_Plan__r.ASI_MFM_TnE_Amount__c):landingbalance;
		landingbalance = po.ASI_MFM_Plan__r.ASI_CRM_Total_FOC_Amount__c!=null? (landingbalance-po.ASI_MFM_Plan__r.ASI_CRM_Total_FOC_Amount__c):landingbalance;
		landingbalance = po.ASI_MFM_Plan__r.ASI_MFM_Total_PO_Amount__c!=null? (landingbalance-po.ASI_MFM_Plan__r.ASI_MFM_Total_PO_Amount__c):landingbalance;
		landingbalance = po.ASI_MFM_Plan__r.ASI_MFM_Total_without_PO_Actual_Amount__c!=null? (landingbalance-po.ASI_MFM_Plan__r.ASI_MFM_Total_without_PO_Actual_Amount__c):landingbalance;
		landingbalance = po.ASI_MFM_Plan__r.ASI_MFM_Direct_Payment_PO_Amount__c!=null? (landingbalance-po.ASI_MFM_Plan__r.ASI_MFM_Direct_Payment_PO_Amount__c):landingbalance;
		
		if(landingbalance&lt;0){
			Msg += &apos;Landing Amount Checking:  Balance is (&apos; + landingbalance + &apos;) &lt; 0 \n&apos;;
		}
    }
	var totalPlanbalance = po.ASI_MFM_Plan__r.ASI_MFM_Plan_Balance__c - po.ASI_MFM_Base_Currency_Amount__c;
	console.log(&apos;totalPlanbalance : &apos;+totalPlanbalance); 
	if(totalPlanbalance &lt; 0){
		Msg += &apos;Total Plan Balance Checking:  Balance is (&apos; + totalPlanbalance + &apos;) &lt; 0 \n&apos;;
	}
    var APamount = po.ASI_MFM_Plan__r.ASI_MFM_TW_Plan_Balance_AnP__c-po.ASI_MFM_TW_PO_Amount_AP__c;
	if(APamount &lt; 0){
		Msg += &apos;A&amp;P Checking: A&amp;P Balance is (&apos; + APamount + &apos;) &lt; 0 \n&apos;;
	}
		

    var ADamount = po.ASI_MFM_Plan__r.ASI_MFM_TW_Plan_Balance_AnD__c-po.ASI_MFM_TW_PO_Amount_AD__c;
    if(ADamount &lt; 0){
		Msg += &apos;\n Checking: A&amp;D Balance is (&apos; + ADamount + &apos;) &lt; 0 \n&apos;;
	}
		
	
	if(APamount &lt; 0 || ADamount &lt; 0 || totalPlanbalance &lt; 0 || landingbalance&lt;0){
		alert(Msg);
	
	}else {
		if ((Modal.confirm &amp;&amp; Modal.confirm(&apos;Once you submit this record for approval, you might not be able to edit it or recall it from the approval process depending on your settings. Continue?&apos;)) || (!Modal.confirm &amp;&amp; window.confirm(&apos;Once you submit this record for approval, you might not be able to edit it or recall it from the approval process depending on your settings. Continue?&apos;))){
			navigateToUrl(&apos;/apex/ASI_SubmitApprovalPage?id={!ASI_MFM_PO__c.Id}&apos;);
		}
			
	}
}</url>
