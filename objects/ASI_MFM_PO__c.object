var RecordType=&quot;&quot;;

result=sforce.connection.query(&quot;Select Id from Attachment where parentid=&apos;{!ASI_MFM_PO__c.Id}&apos;&quot;);

result1=sforce.connection.query(&quot;SELECT Id From ASI_MFM_PO__Feed where Type = &apos;ContentPost&apos; and ParentId =&apos;{!ASI_MFM_PO__c.Id}&apos; and id != null&quot;);

result2=sforce.connection.query(&quot;SELECT Id, ASI_MFM_PO_End_Date__c, ASI_MFM_PO_Start_Date__c, ASI_MFM_Amount_Total__c From ASI_MFM_PO_Receipt_Phasing__c where ASI_MFM_PO__c = &apos;{!ASI_MFM_PO__c.Id}&apos; AND ASI_MFM_Modify_BY__c != &apos;SYSTEM&apos; ORDER BY CreatedDate DESC LIMIT 1&quot;);

result3 = sforce.connection.query(&quot;SELECT Id, ASI_MFM_PO_Type__c, ASI_MFM_Total_POSM_Line__c, ASI_MFM_BU_Code__r.ASI_MFM_Auto_Receipt__c FROM ASI_MFM_PO__c where id = &apos;{!ASI_MFM_PO__c.Id}&apos; &quot;);

var amount = &quot;{!ASI_MFM_PO__c.ASI_MFM_PO_Amount__c}&quot;;
var POType = &apos;{!ASI_MFM_PO__c.ASI_MFM_PO_Type__c}&apos;;

if(result2.size == 0 &amp;&amp;
&apos;{!LOWER(TEXT(ASI_MFM_PO__c.ASI_MFM_PO_Type__c))}&apos; != &apos;emarket&apos; &amp;&amp; &apos;{!LOWER(TEXT(ASI_MFM_PO__c.ASI_MFM_PO_Type__c))}&apos; != &apos;it opex&apos; &amp;&amp; &apos;{!LOWER(TEXT(ASI_MFM_PO__c.ASI_MFM_PO_Type__c))}&apos; != &apos;it capex hardware region&apos; &amp;&amp; &apos;{!LOWER(TEXT(ASI_MFM_PO__c.ASI_MFM_PO_Type__c))}&apos; != &apos;it capex hardware hq&apos; &amp;&amp; &apos;{!LOWER(TEXT(ASI_MFM_PO__c.ASI_MFM_PO_Type__c))}&apos; != &apos;it capex software&apos; &amp;&amp;
&apos;{!LOWER(TEXT(ASI_MFM_PO__c.ASI_MFM_PO_Type__c))}&apos; != &apos;iT capex renovation&apos; &amp;&amp; &apos;{!ASI_MFM_PO__c.ASI_MFM_Supplier_Number__c}&apos; != &apos;61202305&apos; &amp;&amp; result3.size &gt; 0 &amp;&amp; result3.records.ASI_MFM_BU_Code__r.ASI_MFM_Auto_Receipt__c == &apos;true&apos;){

alert(&apos;Please create receipt phasing before submitting for approval&apos;);

}else if(result2.size &gt; 0 &amp;&amp;( result2.records.ASI_MFM_Amount_Total__c * 1 &gt; {!VALUE(TEXT(ASI_MFM_PO__c.ASI_MFM_PO_Amount__c))} ||
new Date(result2.records.ASI_MFM_PO_Start_Date__c).getDate() != {!DAY(ASI_MFM_PO__c.ASI_MFM_PO_Start_Date__c)} ||
new Date(result2.records.ASI_MFM_PO_Start_Date__c).getMonth() + 1 != {!MONTH(ASI_MFM_PO__c.ASI_MFM_PO_Start_Date__c)} ||
new Date(result2.records.ASI_MFM_PO_Start_Date__c).getFullYear() != {!YEAR(ASI_MFM_PO__c.ASI_MFM_PO_Start_Date__c)} ||
new Date(result2.records.ASI_MFM_PO_End_Date__c).getDate() != {!DAY(ASI_MFM_PO__c.ASI_MFM_PO_End_Date__c)} ||
new Date(result2.records.ASI_MFM_PO_End_Date__c).getMonth() + 1 != {!MONTH(ASI_MFM_PO__c.ASI_MFM_PO_End_Date__c)} ||
new Date(result2.records.ASI_MFM_PO_End_Date__c).getFullYear() != {!YEAR(ASI_MFM_PO__c.ASI_MFM_PO_End_Date__c)})){

alert(&apos;Please modify PO Receipt Phasing before submitting for approval&apos;);

}else if(result3.size &gt; 0 &amp;&amp; result3.records.ASI_MFM_PO_Type__c == &apos;RTMK POSM&apos; &amp;&amp; result3.records.ASI_MFM_Total_POSM_Line__c == 0){

alert(&apos;Please create at least one POSM Line before submitting for approval&apos;);

}else{

if(result.size &gt;0 || result1.size&gt;0 || POType.indexOf(&quot;Gift material&quot;) !== -1){
if(POType.indexOf(&quot;Gift material&quot;) !== -1){
if ((Modal.confirm &amp;&amp; Modal.confirm(&apos;Once you submit this record for approval, you might not be able to edit it or recall it from the approval process depending on your settings. Continue?&apos;)) || (!Modal.confirm &amp;&amp; window.confirm(&apos;Once you submit this record for approval, you might not be able to edit it or recall it from the approval process depending on your settings. Continue?&apos;)))
navigateToUrl(&apos;/apex/ASI_MFM_CN_PO_GiftMaterial_SubmitPage?id={!ASI_MFM_PO__c.Id}&apos;);
}
else{
if(amount != &quot;0&quot;){
if((Modal.confirm &amp;&amp; Modal.confirm(&apos;Once you submit this record for approval, you might not be able to edit it or recall it from the approval process depending on your settings. Continue?&apos;)) || (!Modal.confirm &amp;&amp; window.confirm(&apos;Once you submit this record for approval, you might not be able to edit it or recall it from the approval process depending on your settings. Continue?&apos;))) navigateToUrl(&apos;/apex/ASI_SubmitApprovalPage?id={!ASI_MFM_PO__c.Id}&apos;);

}
else{
alert(&apos;PO Amount has to be larger than zero&apos;);
}
}
}
else{
alert(&apos;Attachment (e.g. Draft Quotation) is needed before approval.&apos;);
}
}</url>
