    <validationRules>
        <fullName>ASI_MFM_Back_Date_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND ( 
NOT(INCLUDES( $User.BypassTriggers__c , &quot;ASI_MFM_ValidateSubBrandCode&quot;)), 
OR (CONTAINS(RecordType.DeveloperName, &quot;ASI_MFM_GF&quot;),CONTAINS(RecordType.DeveloperName, &quot;ASI_MFM_HK&quot;)), 
OR ( 
NOT(ISNEW()) &amp;&amp; ((ASI_MFM_PO_End_Date__c &lt; Today() &amp;&amp; ISCHANGED(ASI_MFM_PO_End_Date__c ) )||(ASI_MFM_PO_Raised_Date__c &lt; Today() &amp;&amp; ISCHANGED(ASI_MFM_PO_Raised_Date__c)) 
||(ASI_MFM_PO_Start_Date__c &lt; Today() &amp;&amp; ISCHANGED(ASI_MFM_PO_Start_Date__c))), 
ISNEW() &amp;&amp; (ASI_MFM_PO_End_Date__c &lt; Today() || ASI_MFM_PO_Raised_Date__c &lt; Today() ||ASI_MFM_PO_Start_Date__c &lt; Today()) 
) 
)</errorConditionFormula>
        <errorMessage>You are NOT allowed to input any back date for PO Start Date, PO End Date and PO Raised Date</errorMessage>
    </validationRules>
