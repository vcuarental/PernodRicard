        <masterLabel>Submit for Approval</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/apex.js&quot;)}

result=sforce.connection.query(&quot;Select Id from Attachment where parentid=&apos;{!ASI_MFM_PO__c.Id}&apos;&quot;); 
result1=sforce.connection.query(&quot;SELECT Id From ASI_MFM_PO__Feed where Type = &apos;ContentPost&apos; and ParentId =&apos;{!ASI_MFM_PO__c.Id}&apos; and id != null&quot;); 
resultpoline=sforce.connection.query(&quot;Select Id,Name,ASI_MFM_Pay_Acc__c from ASI_MFM_PO_Line_Item__c where ASI_MFM_PO__c=&apos;{!ASI_MFM_PO__c.Id}&apos;&quot;); 
var payacc=true;
var message=&apos;&apos;;
if(resultpoline.size &gt; 0){ 
	var poline = resultpoline.getArray(&apos;records&apos;); 
	for(i=0;i&lt;poline.length;i++){
		if(!poline[i].ASI_MFM_Pay_Acc__c){
			payacc=false;
		}
	}
}

var amount = &quot;{!ASI_MFM_PO__c.ASI_MFM_PO_Amount__c}&quot;; 
// Added by 2017-07-24 Linus@Introv 
var IsOverBalance = &quot;{!ASI_MFM_PO__c.ASI_MFM_TR_Over_Plan_Balance__c}&quot;; 
var IsBracket = &quot;{!ASI_MFM_PO__c.ASI_MFM_TR_Is_Bracket__c}&quot;; 
var IsOverBracket = &quot;{!ASI_MFM_PO__c.ASI_MFM_TR_Over_Bracket_Limit__c}&quot;; 

// [SH] 2018-11-07
var APAD = &apos;{!ASI_MFM_PO__c.ASI_MFM_AP_AD__c}&apos;;
var filter = &apos;&apos;;
if(APAD == &apos;A&amp;P&apos;){
 filter = &quot; and ASI_MFM_A_C_Code__r.ASI_MFM_Reporting_Category__c != \&apos;Sales Reduction\&apos; &quot;;
}else{
 filter = &quot; and ASI_MFM_A_C_Code__r.ASI_MFM_Reporting_Category__c = \&apos;Sales Reduction\&apos; &quot;;
}
var Strquery = &quot;Select id from ASI_MFM_PO_Line_Item__c where ASI_MFM_PO__c = \&apos;{!ASI_MFM_PO__c.Id}\&apos;&quot; + filter;
var POL = sforce.connection.query(Strquery);
var POLrecords = POL.getArray(&apos;records&apos;); //alert(Strquery);//alert(POL.size);


if(result.size &gt;0 || result1.size&gt;0){ 
	if(amount != &quot;0&quot;){  
	     if(payacc){ 
                if(POLrecords.length &gt; 0){    
                  if(IsBracket == true){ 
                        if(IsOverBracket== false &amp;&amp; IsOverBalance == false){
                             if ((Modal.confirm &amp;&amp; Modal.confirm(&apos;Once you submit this record for approval, you might not be able to edit it or recall it from the approval process depending on your settings. Continue?&apos;)) || (!Modal.confirm &amp;&amp; window.confirm(&apos;Once you submit this record for approval, you might not be able to edit it or recall it from the approval process depending on your settings. Continue?&apos;))) navigateToUrl(&apos;/apex/ASI_SubmitApprovalPage?id={!ASI_MFM_PO__c.Id}&apos;); 
                        }else if(IsOverBracket == true){
                             alert(&apos;Total PO Amount must be within HKD amount equivalent to 5,000 EUR @ budget FX&apos;); 
                        }else if(IsOverBalance == true &amp;&amp; IsBracket == true){  
                             alert(&apos;Bracket PO balance is exceeded&apos;);
                        }
                  }else{   
			     if ((Modal.confirm &amp;&amp; Modal.confirm(&apos;Once you submit this record for approval, you might not be able to edit it or recall it from the approval process depending on your settings. Continue?&apos;)) || (!Modal.confirm &amp;&amp; window.confirm(&apos;Once you submit this record for approval, you might not be able to edit it or recall it from the approval process depending on your settings. Continue?&apos;))) navigateToUrl(&apos;/apex/ASI_SubmitApprovalPage?id={!ASI_MFM_PO__c.Id}&apos;); 
                  }
                }else{
                  alert(&apos;The account(s) being used in the detail lines not aligning with the A&amp;D/A&amp;P selection in the PO header&apos;);
                }
             }else{   
			alert(&apos;Pay/Acc is needed.&apos;);
	     }
		
	} 
	else{ 
		alert(&apos;PO Amount has to be larger than zero&apos;); 
	} 
} 
else{ 
	alert(&apos;Attachment (e.g. Draft Quotation) is needed before approval.&apos;); 
}</url>
