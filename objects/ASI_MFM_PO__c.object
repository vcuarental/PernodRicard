$j = jQuery.noConflict(); 

function downloadAll(files){ 
if(files.length == 0) return; 
file = files.pop(); 
var theAnchor = $j(&apos;&lt;a /&gt;&apos;) 
.attr(&apos;href&apos;, file[1]) 
.attr(&apos;download&apos;,file[0]) 
// Firefox does not fires click if the link is outside 
// the DOM 
.appendTo(&apos;body&apos;); 

theAnchor[0].click(); 
theAnchor.remove(); 
downloadAll(files); 
} 


//the vars 
var relatedAttachments, 
dialogText, 
records, 

//the query 
relatedAttachments = sforce.connection.query(&quot;SELECT Id, Name, ParentId, LastModifiedDate, CreatedBy.Name FROM Attachment where ParentId = &apos;{!ASI_MFM_PO__c.Id}&apos;&quot;); 

//the results 
