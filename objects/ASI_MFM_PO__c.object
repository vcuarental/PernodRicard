        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/apex.js&quot;)}

result=sforce.connection.query(&quot;Select Id, ASI_MFM_Status__c from ASI_MFM_PO__c where id=&apos;{!ASI_MFM_PO__c.Id}&apos;&quot;);
var allowSetStatus=&apos;false&apos;;

