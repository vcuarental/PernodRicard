    <validationRules>
        <fullName>ASI_eForm_HK_Not_Allow_Approver_Creation</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISNEW(),!ASI_eForm_Preview_Approver__c,  ASI_eForm_Route_Type__r.RecordType.DeveloperName = &quot;ASI_eForm_HK_Route_Type&quot;,
 
OR(
AND(ISPICKVAL( ASI_eForm_Route_Type__r.ASI_eForm_Form_Type__c , &quot;IT Change Request&quot;), ASI_eForm_Route_Type__r.ASI_eForm_Sys_Approver_Count__c &gt;= 3),

AND(ISPICKVAL( ASI_eForm_Route_Type__r.ASI_eForm_Form_Type__c , &quot;Pre-Trip Approval&quot;), ASI_eForm_Route_Type__r.ASI_eForm_Sys_Approver_Count__c &gt;= 0),

AND(ISPICKVAL(  ASI_eForm_Route_Type__r.ASI_eForm_Form_Record_Type__c , &quot;Hardware &amp; Software Request (HK)&quot;), ASI_eForm_Route_Type__r.ASI_eForm_Sys_Approver_Count__c &gt;= 3),

AND(ISPICKVAL(  ASI_eForm_Route_Type__r.ASI_eForm_Form_Record_Type__c , &quot;IT Service Request (HK)&quot;), ASI_eForm_Route_Type__r.ASI_eForm_Sys_Approver_Count__c &gt;= 1),

AND(ISPICKVAL(  ASI_eForm_Route_Type__r.ASI_eForm_Form_Record_Type__c , &quot;New Employee Temporary (HK)&quot;) || ISPICKVAL(  ASI_eForm_Route_Type__r.ASI_eForm_Form_Record_Type__c , &quot;New Employee Permanent (HK)&quot;), ASI_eForm_Route_Type__r.ASI_eForm_Sys_Approver_Count__c &gt;= 1),

AND(ISPICKVAL(  ASI_eForm_Route_Type__r.ASI_eForm_Form_Record_Type__c , &quot;Remove ID (HK)&quot;), ASI_eForm_Route_Type__r.ASI_eForm_Sys_Approver_Count__c &gt;= 1),

AND(ISPICKVAL(  ASI_eForm_Route_Type__r.ASI_eForm_Form_Record_Type__c , &quot;Change Profile (HK)&quot;), ASI_eForm_Route_Type__r.ASI_eForm_Sys_Approver_Count__c &gt;= 0)

)
)</errorConditionFormula>
