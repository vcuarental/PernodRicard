        <url>{!requireScript(&quot;/soap/ajax/30.0/connection.js&quot;)} 
sforce.connection.sessionId = &quot;{!$Api.Session_ID}&quot;; 

var hasEnrollment = false; 
var queryString = &quot;SELECT Id FROM ASI_HK_CRM_Promotion__c WHERE ASI_CRM_Promotion_Plan__c = &apos;{!ASI_CRM_Promotion_Plan__c.Id}&apos;&quot;; 
var relatedCharts = sforce.connection.query(queryString); 
records = relatedCharts.getArray(&apos;records&apos;); 
if (records.length &gt; 0){ 
hasEnrollment = true; 
}

if(!hasEnrollment){
alert(&apos;Please enroll at least one sales rep.&apos;);
} else {
var ob = new sforce.SObject(&quot;ASI_CRM_Promotion_Plan__c&quot;); 
ob.Id = &apos;{!ASI_CRM_Promotion_Plan__c.Id}&apos;; 
ob.ASI_CRM_SYS_Send_Email__c = true; 
result = sforce.connection.update([ob]); 

if (!result[0].getBoolean(&apos;success&apos;)) { 
alert(&apos;An error occurred. Details : &apos; + result[0].errors.message); 
} else { 
navigateToUrl(&apos;/{!ASI_CRM_Promotion_Plan__c.Id}&apos;); 
}
}</url>
