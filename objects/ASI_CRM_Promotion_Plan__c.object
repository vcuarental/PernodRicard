        <externalId>false</externalId>
        <formula>((YEAR(ASI_CRM_End_Date__c ) - YEAR(ASI_CRM_Start_Date__c ) ) *12) + (MONTH(ASI_CRM_End_Date__c )) - (MONTH(ASI_CRM_Start_Date__c )) 


