    <validationRules>
        <fullName>ASI_CRM_SG_CheckEndDate</fullName>
        <active>true</active>
        <description>End date should be equal to last day of a month</description>
        <errorConditionFormula>AND(
    CONTAINS(RecordType.DeveloperName, &apos;ASI_CRM_SG&apos;),
    ASI_CRM_End_Date__c != ( 
      IF( 
        MONTH( ASI_CRM_End_Date__c ) = 12,
        DATE( YEAR( ASI_CRM_End_Date__c ) + 1, 1, 1 ),
        DATE( YEAR( ASI_CRM_End_Date__c ), MONTH( ASI_CRM_End_Date__c ) + 1, 1 )
      ) - 1 
    )
)</errorConditionFormula>
        <errorDisplayField>ASI_CRM_End_Date__c</errorDisplayField>
        <errorMessage>Please select last day of a month as End Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_SG_CheckEnrollmentDate</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
CONTAINS(RecordType.DeveloperName, &apos;ASI_CRM_SG&apos;), 
 ASI_CRM_Enrollment_Cut_off_Date__c &lt; TODAY(),
OR(ISNEW(), ISCHANGED(ASI_CRM_Enrollment_Cut_off_Date__c ) )
)</errorConditionFormula>
        <errorDisplayField>ASI_CRM_Enrollment_Cut_off_Date__c</errorDisplayField>
        <errorMessage>Enrollment Cuf-off Date should be later or equal to today.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_SG_CheckStartDate</fullName>
        <active>true</active>
        <description>Start date should be equal to first day of a month</description>
        <errorConditionFormula>AND(
    CONTAINS(RecordType.DeveloperName, &apos;ASI_CRM_SG&apos;),
    DAY(ASI_CRM_Start_Date__c) != 1
)</errorConditionFormula>
        <errorDisplayField>ASI_CRM_Start_Date__c</errorDisplayField>
        <errorMessage>Please select first day of a month as Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_SG_CheckStartEndDate</fullName>
        <active>true</active>
        <description>Start Date should be earlier than End Date.</description>
        <errorConditionFormula>AND(
    CONTAINS(RecordType.DeveloperName, &apos;ASI_CRM_SG&apos;),
    ASI_CRM_Start_Date__c &gt; ASI_CRM_End_Date__c
)</errorConditionFormula>
        <errorDisplayField>ASI_CRM_End_Date__c</errorDisplayField>
        <errorMessage>Please enter a End Date which is later than Start Date.</errorMessage>
    </validationRules>
