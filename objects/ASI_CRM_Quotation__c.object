    <fields>
        <fullName>ASI_CTY_CN_Vendor_Confirmed_Price__c</fullName>
        <externalId>false</externalId>
        <formula>IF((ASI_CTY_CN_Vendor_IsConfirmed__c = true &amp;&amp; Not(ISPICKVAL(ASI_CTY_CN_Vendor_Result_Status__c, &apos;Win-Bid&apos;))), (ASI_CTY_CN_Vendor_Sample__c + ASI_CTY_CN_Vendor_Model__c),  IF( ISPICKVAL(ASI_CTY_CN_Vendor_Result_Status__c, &apos;Win-Bid&apos;) &amp;&amp; (ASI_CTY_CN_Vendor_IsConfirmed__c = true) , ((ASI_CTY_CN_Vendor_Unitl_Price__c * ASI_CTY_CN_Vendor_Quantity__c) + (ASI_CTY_CN_Vendor_Unitl_Price__c * ASI_CTY_CN_Vendor_Total_Quantity__c) 
+ ASI_CTY_CN_Vendor_Total_Freight__c + ASI_CTY_CN_Vendor_Sample__c + ASI_CTY_CN_Vendor_Merge_Total_Freight__c + ASI_CTY_CN_Vendor_Model__c), 0))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Confirmed Price</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
