    <validationRules>
        <fullName>ASI_CRM_CN_Effective_Date_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>IF( CONTAINS(RecordType.DeveloperName, &quot;ASI_CRM_CN&quot;) &amp;&amp; RecordType.DeveloperName  &lt;&gt; &quot;ASI_CRM_CN_NPL_T1_Price_Request_RO&quot; &amp;&amp; RecordType.DeveloperName  &lt;&gt; &quot;ASI_CRM_CN_NPL_T1_Price_Request&quot; &amp;&amp; ASI_CRM_Effective_From__c &gt; ASI_CRM_Effective_To__c , TRUE, FALSE )</errorConditionFormula>
        <errorDisplayField>ASI_CRM_Effective_From__c</errorDisplayField>
        <errorMessage>Efftective From can&apos;t be greater than Effective To</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_CN_Effective_Date_Validation_2</fullName>
        <active>false</active>
        <errorConditionFormula>IF( CONTAINS(RecordType.DeveloperName, &quot;ASI_CRM_CN&quot;)  &amp;&amp; RecordType.DeveloperName  &lt;&gt; &quot;ASI_CRM_CN_NPL_T1_Price_Request_RO&quot; &amp;&amp; RecordType.DeveloperName  &lt;&gt; &quot;ASI_CRM_CN_NPL_T1_Price_Request&quot; &amp;&amp; ASI_CRM_Effective_From__c &lt;= TODAY() &amp;&amp; ISPICKVAL( ASI_CRM_Status__c, &quot;Open&quot;), TRUE, FALSE )</errorConditionFormula>
        <errorDisplayField>ASI_CRM_Effective_From__c</errorDisplayField>
        <errorMessage>Effective From must be greater than today</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_CN_Effective_Date_Validation_3</fullName>
        <active>true</active>
        <errorConditionFormula>IF( RecordType.DeveloperName  == &quot;ASI_CRM_CN_NPL_T1_Price_Request_RO&quot; &amp;&amp; RecordType.DeveloperName == &quot;ASI_CRM_CN_NPL_T1_Price_Request&quot; &amp;&amp; ASI_CRM_Effective_From__c &lt; TODAY(), TRUE, FALSE )</errorConditionFormula>
        <errorDisplayField>ASI_CRM_Effective_From__c</errorDisplayField>
        <errorMessage>Effective From must be equal or greater than today</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_CN_checkReason</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(CONTAINS($Profile.Name,&quot;System Admin&quot;))
&amp;&amp;CONTAINS(RecordType.DeveloperName,&apos;ASI_CRM_CN_Customer_Price_and_Discount_Request&apos;)
&amp;&amp;CONTAINS(ASI_CRM_Customer__r.RecordType.DeveloperName,&apos;ASI_CRM_Outlet_CN&apos;)
&amp;&amp;ASI_CRM_CN_Total_Gross_Sales_Impact__c &gt; 0
&amp;&amp;(ISNULL(ASI_CRM_Reason_for_Investment__c)||ISBLANK(ASI_CRM_Reason_for_Investment__c)||ISNULL(ASI_CRM_ReasonForThePriceIsLowerThanT1__c)||ISBLANK(ASI_CRM_ReasonForThePriceIsLowerThanT1__c))</errorConditionFormula>
        <errorMessage>Please input “Reason for Investment” and “Reason For The Price Is Lower Than T1:”</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_SG_Base_Price_Approval_Check</fullName>
        <active>false</active>
        <description>(Obsolete) Changed to display error message in Lightning record page</description>
        <errorConditionFormula>(ASI_CRM_SG_No_of_Price_Group__c = 0 ||
ASI_CRM_No_Of_Detail_Cannot_be_Approved__c &gt; 0 ) &amp;&amp; ( ispickval( ASI_CRM_SG_PRICEGROUP_Status__c , &quot;Submit for Approval&quot;)|| ispickval( ASI_CRM_SG_PRICEGROUP_Status__c , &quot;Approval In Process&quot;))&amp;&amp;
 ASI_CRM_SG_Record_Type_Dev_Name_Custom__c = &quot;ASI_CRM_SG_Versioning_Price_Group&quot;</errorConditionFormula>
        <errorMessage>This record cannot be submitted for approval. Please make sure you have Price and Discount Details added or comments are added to all Items which are flagged as Amber and Red.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_SG_EffectiveDate_Needs_to_be_17</fullName>
        <active>true</active>
        <errorConditionFormula>RecordType.DeveloperName = &quot;ASI_CRM_SG_Rebate_Period&quot; &amp;&amp;
not(DAY(  ASI_CRM_SG_Effective_Date__c ) = 1 &amp;&amp;
Month(  ASI_CRM_SG_Effective_Date__c  ) = 7)</errorConditionFormula>
        <errorDisplayField>ASI_CRM_SG_Effective_Date__c</errorDisplayField>
        <errorMessage>Please make sure the Effective From is set as the first day of the Fiscal Year (1st July)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_SG_Effective_Date_Cant_be_change</fullName>
        <active>true</active>
        <errorConditionFormula>RecordType.DeveloperName = &quot;ASI_CRM_SG_Rebate_Period&quot; &amp;&amp;
not (isnew()) &amp;&amp; ischanged( ASI_CRM_SG_Effective_Date__c )</errorConditionFormula>
        <errorDisplayField>ASI_CRM_SG_Effective_Date__c</errorDisplayField>
        <errorMessage>You are not allowed to change the Effective Date after it is created.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_SG_New_Version_Validation</fullName>
        <active>true</active>
        <description>Ensure user create price revision by clicking Master Price Group [New Version] button</description>
        <errorConditionFormula>AND ( CONTAINS( RecordType.DeveloperName , &quot;ASI_CRM_SG_Versioning_Price_Group&quot;),
 ISBLANK( ASI_CRM_SG_Master_Price_Group__c ) 
)</errorConditionFormula>
        <errorMessage>Please create new price by selecting Master Price Group and then click [New Version] button</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_of_JDE_Code</fullName>
        <active>true</active>
        <errorConditionFormula>CONTAINS(RecordType.DeveloperName, &apos;ASI_CRM_CN_Customer_Price_and_Discount_Request&apos;) &amp;&amp; ( ( NOT( ASI_CRM_RTD__c ) &amp;&amp; ISBLANK( ASI_CRM_Customer__r.ASI_CRM_CN_JDE_Code__c ) ) || ( ASI_CRM_RTD__c &amp;&amp; ISBLANK( ASI_CRM_Customer__r.ASI_CRM_CN_JDE_Code_RTD__c ) ) )</errorConditionFormula>
        <errorMessage>Customer JDE Code can not be NULL</errorMessage>
    </validationRules>
