    <validationRules>
        <fullName>Controla_perc_e_percent_bonus</fullName>
        <active>true</active>
        <errorConditionFormula>if(isblank( cd_line_status__c )&amp;&amp; 
Percentual_de_bonus__c &gt; LAT_PercentualBonus__c, 
true,false)</errorConditionFormula>
        <errorDisplayField>Percentual_de_bonus__c</errorDisplayField>
        <errorMessage>Bonificação excedida.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Controla_perc_e_quant_de_bonificacao</fullName>
        <active>true</active>
        <errorConditionFormula>if(isblank( cd_line_status__c )&amp;&amp; 
QT_Bonus__c &gt; FLOOR( (LAT_PercentualBonus__c *  Quantity)/100 ), true,false)</errorConditionFormula>
        <errorDisplayField>QT_Bonus__c</errorDisplayField>
        <errorMessage>Bonificação excedida.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Impede_alteracao_e_delecao_do_item</fullName>
        <active>true</active>
        <errorConditionFormula>AND(  OR(ISCHANGED(Quantity),  ISCHANGED(QT_Bonus__c), ISCHANGED( Percentual_de_bonus__c )), NOT(ISNEW()),  
NOT(ISNULL(Codigo_status_numerico__c)),  NOT(Remover__c), NOT(Integracao__c)
)</errorConditionFormula>
        <errorDisplayField>Quantity</errorDisplayField>
        <errorMessage>Este item não pode ser alterado devido ao seu status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Impede_cancelamento_item_do_pedido</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Codigo_status_numerico__c &gt; 540,Codigo_status_numerico__c &lt;&gt; 999,NOT(Integracao__c) , Remover__c)</errorConditionFormula>
        <errorDisplayField>Remover__c</errorDisplayField>
        <errorMessage>Este item não pode ser cancelado devido a seu status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Valida_cupom_diponivel</fullName>
        <active>true</active>
        <errorConditionFormula>if(isblank( cd_line_status__c )&amp;&amp; 
Quantity &gt; Cupom_qtde__c, 
true,false)</errorConditionFormula>
        <errorDisplayField>Quantity</errorDisplayField>
        <errorMessage>A quantidade pedida para este item excede a quota de produto disponível.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Valida_multiplos</fullName>
        <active>true</active>
        <errorConditionFormula>if(isblank( cd_line_status__c )&amp;&amp; 
NOT(ISNULL(  QT_multiple__c )) &amp;&amp;  QT_multiple__c  &lt;&gt; 0 &amp;&amp; MOD( Quantity ,QT_multiple__c ) &lt;&gt; 0 ,
true,false)</errorConditionFormula>
        <errorDisplayField>Quantity</errorDisplayField>
        <errorMessage>Quantidade vendida diferente de seu múltiplo.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Valida_quantidade_como_inteiro</fullName>
        <active>true</active>
        <errorConditionFormula>if(isblank( cd_line_status__c )&amp;&amp; 
Quantity &lt;&gt; 0 &amp;&amp; Quantity &lt;&gt; FLOOR( Quantity ), 
true,false)</errorConditionFormula>
        <errorDisplayField>Quantity</errorDisplayField>
        <errorMessage>A quantidade deve ser um número inteiro.</errorMessage>
    </validationRules>
