<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Used to configure Serenity home page list views. The required &quot;Label&quot; and &quot;Homepage List View Name&quot; fields are arbitrary and not visible to users. Only those with (API) in their label will be displayed, and the visible name of the listview will match the browser text string (e.g. these label fields are not displayed).</description>
    <fields>
        <fullName>gvp__API_Object_Name__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <fieldManageability>SubscriberControlled</fieldManageability>
        <label>Object Name (API)</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>gvp__List_View_Name__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <fieldManageability>SubscriberControlled</fieldManageability>
        <label>Object List Name or List API Name</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>gvp__Number_Of_Items_To_Show__c</fullName>
        <defaultValue>5</defaultValue>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <fieldManageability>SubscriberControlled</fieldManageability>
        <label>Number of Items to Show</label>
        <precision>2</precision>
        <required>true</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>gvp__Order_Number__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <fieldManageability>SubscriberControlled</fieldManageability>
        <label>Order Number</label>
        <precision>2</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <label>Home List View</label>
    <pluralLabel>Home List Views</pluralLabel>
    <visibility>Public</visibility>
</CustomObject>
