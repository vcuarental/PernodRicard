    <validationRules>
        <fullName>ASI_CRM_MY_AOLine_BlankChk</fullName>
        <active>true</active>
        <description>when Adjustment = FALSE</description>
        <errorConditionFormula>AND(
$User.BypassVR__c != TRUE,
$RecordType.Name = &quot;Actual Offtake (MY)&quot;,
!ASI_CRM_Adjustment__c,
OR( 
    ISBLANK(ASI_CRM_SKU__c),
    ISBLANK(ASI_CRM_FromWS__c),
    ISBLANK(ASI_CRM_InvoiceNumber__c),
    ISBLANK(ASI_CRM_InvoiceDate__c),
    ISBLANK(ASI_TH_CRM_Quantity__c),
    ISBLANK(ASI_CRM_Price__c)
) 
)</errorConditionFormula>
        <errorMessage>The following fields must be filled:
 - SKU, From WS, Invoice Number, Invoice Date, Bottle, Price</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_MY_AOLine_BlankChkAdj</fullName>
        <active>true</active>
        <description>when Adjustment ==TRUE</description>
        <errorConditionFormula>AND( 
$User.BypassVR__c != TRUE,
$RecordType.Name = &quot;Actual Offtake (MY)&quot;, 
ASI_CRM_Adjustment__c, 
ISBLANK(ASI_CRM_Adjustment_Remarks__c),
ISBLANK(ASI_CRM_SKU__c), 
ISBLANK(ASI_CRM_FromWS__c), 
ISBLANK(ASI_CRM_InvoiceNumber__c), 
ISBLANK(ASI_CRM_InvoiceDate__c), 
ISBLANK(ASI_TH_CRM_Quantity__c), 
ISBLANK(ASI_CRM_Price__c)
)</errorConditionFormula>
        <errorMessage>The following fields must be filled: 
- SKU, From WS, Invoice Number, Invoice Date, Bottle, Price, Adjustment Remarks</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_MY_AO_Status_Chk</fullName>
        <active>true</active>
        <description>Block to edit if the actual offtake is final</description>
        <errorConditionFormula>AND( 
 $User.BypassVR__c != TRUE,
 RecordType.DeveloperName = &apos;ASI_CRM_MY_Actual_Offtake&apos;,
  TEXT(ASI_CRM_ActualOfftakeHeader__r.ASI_CRM_Status__c) = &apos;Final&apos; 
)</errorConditionFormula>
        <errorDisplayField>ASI_TH_CRM_Actual_Offtake_External_ID__c</errorDisplayField>
        <errorMessage>Cannot modify Actual Offtake record that is &apos;Final&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_MY_InvoiceDate</fullName>
        <active>true</active>
        <description>ActualOfftakeHeader&apos;s Offtake Date  must equal or larger than Actual Offtake&apos;s InvoiceDate</description>
        <errorConditionFormula>AND(((YEAR(ASI_CRM_ActualOfftakeHeader__r.ASI_CRM_OfftakeDate__c)*100)+MONTH(ASI_CRM_ActualOfftakeHeader__r.ASI_CRM_OfftakeDate__c)) &lt; ((YEAR(ASI_CRM_InvoiceDate__c)*100)+MONTH(ASI_CRM_InvoiceDate__c)), RecordType.DeveloperName = &apos;ASI_CRM_MY_Actual_Offtake&apos;)</errorConditionFormula>
        <errorMessage>Invoice date should not larger than offtake date</errorMessage>
    </validationRules>
