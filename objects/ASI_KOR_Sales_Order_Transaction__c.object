    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>ASI_CRM_CN_Qty_Checking</fullName>
        <active>false</active>
        <errorConditionFormula>IF ( RecordType.DeveloperName == &apos;ASI_CRM_CN_SalesOrder_Item&apos; &amp;&amp; ASI_KOR_Order_Qty__c &gt;  ASI_CRM_CN_Promotion_Code__r.ASI_CRM_Buy_X__c  &amp;&amp;  MOD(ASI_KOR_Order_Qty__c, ASI_CRM_CN_Promotion_Code__r.ASI_CRM_Buy_X__c) != 0, True, False)</errorConditionFormula>
        <errorDisplayField>ASI_KOR_Order_Qty__c</errorDisplayField>
        <errorMessage>For Buy X Get Y Promotion type, order quantity must be less than X or multiple of X</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_KH_Order_Qty_UOM</fullName>
        <active>true</active>
        <description>Either Request Quantity in Bottle or in Carton can be selected</description>
        <errorConditionFormula>AND (
	CONTAINS( RecordType.DeveloperName , &quot;ASI_CRM_KH&quot;) ,
	AND (
		ASI_CRM_SG_Request_Quantity_Bottle__c &gt; 0,
		ASI_CRM_SG_Request_Quantity_Carton__c &gt; 0 
	)
)</errorConditionFormula>
        <errorMessage>Please enter either Request Quantity (Bottle) or Request Quantity (Carton)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_KH_Unavailable_Quantity</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
				CONTAINS( RecordType.DeveloperName , &quot;ASI_CRM_KH&quot;),
				ASI_CRM_Total_Request_Quantity_Bottle__c &gt;  ASI_CRM_SG_SKU__r.ASI_HK_CRM_Available_Quantity__c 
)</errorConditionFormula>
        <errorMessage>Requested Quantity can&apos;t be more than available Quantity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_SG_QtyValidation</fullName>
        <active>true</active>
        <errorConditionFormula>(ASI_CRM_SG_Available_Quantity__c &lt;  ASI_KOR_Order_Qty__c &amp;&amp;  CONTAINS(RecordType.DeveloperName, &apos;ASI_CRM_SG&apos;))</errorConditionFormula>
        <errorDisplayField>ASI_KOR_Order_Qty__c</errorDisplayField>
        <errorMessage>Order Quantity cannot be greater than Available Quantity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_SG_TBCN_PreventNegativeQty</fullName>
        <active>true</active>
        <errorConditionFormula>AND ( 
  RecordType.DeveloperName = &apos;ASI_CRM_SG_TBCN&apos;, 
  ASI_CRM_SG_Request_Quantity_Bottle__c &lt; 0 
)</errorConditionFormula>
        <errorDisplayField>ASI_CRM_SG_Request_Quantity_Bottle__c</errorDisplayField>
        <errorMessage>Negative quantity is not allowed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_SG_WS_PreventNegativeQty</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
  RecordType.DeveloperName = &apos;ASI_CRM_SG_Wholesaler&apos;,
  OR (
     (ASI_CRM_SG_FOC__c + ASI_CRM_FOC_Additional__c) &lt; 0,
     ASI_CRM_SG_Request_Quantity_Carton__c &lt; 0,
     ASI_CRM_SG_Request_Quantity_Bottle__c &lt; 0
  )
)</errorConditionFormula>
        <errorMessage>Negative quantity is not allowed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Disallow_SOR_Item_Edit</fullName>
        <active>false</active>
        <description>Disallow user to edit SOR Item if the parent SOR status is not New</description>
        <errorConditionFormula>TEXT(ASI_KOR_Sales_Order_Request__r.ASI_KOR_Order_Status__c ) &lt;&gt; &quot;New&quot; &amp;&amp; $Profile.Name &lt;&gt; &quot;ASI KOR System Administrator&quot; &amp;&amp; $Profile.Name &lt;&gt; &quot;System Administrator&quot; &amp;&amp; $Profile.Name &lt;&gt; &quot;System Administrator GDT&quot;</errorConditionFormula>
        <errorMessage>The Sales Order Request Item cannot be edited because the Sales Order Request has already been submitted.</errorMessage>
    </validationRules>
