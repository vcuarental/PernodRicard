    <validationRules>
        <fullName>Controla_perc_e_percent_bonus</fullName>
        <active>true</active>
        <errorConditionFormula>if(isblank( LAT_CdLineStatus__c )&amp;&amp; 
LAT_PercentualOfBonus__c &gt; LAT_PercentualBonus__c, 
true,false)
&amp;&amp;NOT(LAT_IsAdmin__c)</errorConditionFormula>
        <errorDisplayField>LAT_PercentualOfBonus__c</errorDisplayField>
        <errorMessage>Bonificação excedida.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Controla_perc_e_quant_de_bonificacao</fullName>
        <active>true</active>
        <errorConditionFormula>if(isblank( LAT_CdLineStatus__c )&amp;&amp; 
LAT_QTBonus__c &gt; FLOOR( (LAT_PercentualBonus__c *  LAT_Quantity__c)/100 ), true,false)
&amp;&amp;NOT(LAT_IsAdmin__c)</errorConditionFormula>
        <errorDisplayField>LAT_QTBonus__c</errorDisplayField>
        <errorMessage>Bonificação excedida.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Impede_alteracao_e_delecao_do_item</fullName>
        <active>true</active>
        <errorConditionFormula>AND(  OR(ISCHANGED(LAT_Quantity__c),  ISCHANGED(LAT_QTBonus__c), ISCHANGED( LAT_PercentualOfBonus__c )), NOT(ISNEW()),  
NOT(ISNULL(LAT_NumericStatusCode__c)),  NOT(LAT_Remover__c), NOT(LAT_Integration__c),NOT(LAT_IsAdmin__c)
)</errorConditionFormula>
        <errorDisplayField>LAT_Quantity__c</errorDisplayField>
        <errorMessage>Este item não pode ser alterado devido ao seu status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Impede_cancelamento_item_do_pedido</fullName>
        <active>true</active>
        <errorConditionFormula>AND(LAT_NumericStatusCode__c &gt; 540,LAT_NumericStatusCode__c &lt;&gt; 999,NOT(LAT_Integration__c) , LAT_Remover__c,
            NOT(LAT_IsAdmin__c))</errorConditionFormula>
        <errorDisplayField>LAT_Remover__c</errorDisplayField>
        <errorMessage>Este item não pode ser cancelado devido a seu status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Valida_cupom_diponivel</fullName>
        <active>true</active>
        <errorConditionFormula>if(isblank( LAT_CdLineStatus__c )&amp;&amp; 
LAT_Quantity__c &gt; LAT_CuponQuantity__c, 
true,false)&amp;&amp;NOT(LAT_IsAdmin__c)</errorConditionFormula>
        <errorDisplayField>LAT_Quantity__c</errorDisplayField>
        <errorMessage>A quantidade pedida para este item excede a quota de produto disponível.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Valida_multiplos</fullName>
        <active>true</active>
        <errorConditionFormula>if(isblank( LAT_CdLineStatus__c )&amp;&amp; 
NOT(ISNULL(  LAT_QTMultiple__c )) &amp;&amp;  LAT_QTMultiple__c  &lt;&gt; 0 &amp;&amp; MOD( LAT_Quantity__c ,LAT_QTMultiple__c ) &lt;&gt; 0 ,
true,false)&amp;&amp;NOT(LAT_IsAdmin__c)</errorConditionFormula>
        <errorDisplayField>LAT_Quantity__c</errorDisplayField>
        <errorMessage>Quantidade vendida diferente de seu múltiplo.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Valida_quantidade_como_inteiro</fullName>
        <active>true</active>
        <errorConditionFormula>if(isblank( LAT_CdLineStatus__c )&amp;&amp; 
LAT_Quantity__c &lt;&gt; 0 &amp;&amp; LAT_Quantity__c &lt;&gt; FLOOR( LAT_Quantity__c ), 
true,false)&amp;&amp;NOT(LAT_IsAdmin__c)</errorConditionFormula>
        <errorDisplayField>LAT_Quantity__c</errorDisplayField>
        <errorMessage>A quantidade deve ser um número inteiro.</errorMessage>
    </validationRules>
