    <webLinks>
        <fullName>ASI_eForm_KR_Submit_for_Approval</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Submit for Approval</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>&apos;{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/connection.js&quot;)}&apos; 
&apos;{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/apex.js&quot;)} &apos; 
try { 
var hasAttachment = false;
var queryString = &quot;SELECT Id, ParentId FROM Attachment WHERE ParentId = &apos;{!ASI_eForm_Corp_Card_Request__c.Id}&apos;&quot;;
var relatedCharts = sforce.connection.query(queryString); 
records = relatedCharts.getArray(&apos;records&apos;); 
if (records.length &gt; 0){
hasAttachment = true;
}

var confirmReportToCardCompany = true;
if({!AND(ASI_eForm_Corp_Card_Request__c.ASI_eForm_Type__c = &apos;Reissue&apos;, !ASI_eForm_Corp_Card_Request__c.ASI_eForm_Report_to_the_card_company__c)}){
   /*
   if(!confirm(&quot;카드사에 반드시 분실신고 하시기 바랍니다.&quot;)){
      confirmReportToCardCompany = false;
   }
   */
   alert(&quot;카드사에 반드시 분실신고 하시기 바랍니다.&quot;);
}
var confirmRequestForThisMonthTransactions = true;
if({!AND(OR(ASI_eForm_Corp_Card_Request__c.ASI_eForm_Type__c = &apos;Cancellation&apos;, ASI_eForm_Corp_Card_Request__c.ASI_eForm_Type__c = &apos;Suspension&apos;), !ASI_eForm_Corp_Card_Request__c.ASI_eForm_this_month_transactions__c)}){
   /*
   if(!confirm(&quot;당월 거래내역 요청여부를 다시한번 확인해 주십시오.&quot;)){
      confirmReportToCardCompany = false;
   }
   */
   alert(&quot;당월 거래내역 요청여부를 다시한번 확인해 주십시오.&quot;);
}


// Only New card request need to check attachment
if({!ASI_eForm_Corp_Card_Request__c.ASI_eForm_Type__c = &apos;New&apos;} &amp;&amp; !hasAttachment){
    alert(&apos;Please attach the application form.&apos;);
}else
if(confirmReportToCardCompany &amp;&amp; confirmRequestForThisMonthTransactions){
    var ob = new sforce.SObject(&quot;ASI_eForm_Corp_Card_Request__c&quot;); 
   ob.Id = &apos;{!ASI_eForm_Corp_Card_Request__c.Id}&apos;; 
   ob.ASI_eForm_Sys_Allow_Submit_Approval__c = true; 
   result = sforce.connection.update([ob]); 

   if (!result[0].getBoolean(&apos;success&apos;)) {
       alert(&apos;An error occurred. Details : &apos; + result[0].errors.message);
   } else {
       navigateToUrl(&apos;/apex/ASI_SubmitApprovalPage?id={!ASI_eForm_Corp_Card_Request__c.Id}&apos;);
   }
}
}catch(error) { 
sforce.debug.log(error.faultcode); 
sforce.debug.log(error.faultstring); 
alert(&apos;An error occurred. Details : &apos; + error); 
}</url>
    </webLinks>
