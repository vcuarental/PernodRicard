    <validationRules>
        <fullName>ASI_eForm_KR_CheckPeriod</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
   OR(
     CONTAINS(RecordType.DeveloperName, &apos;ASI_eForm_KR_CC&apos;),
     CONTAINS(RecordType.DeveloperName, &apos;ASI_eForm_KR_Suspension&apos;)
   ),
   ASI_eForm_Period_From__c &gt; ASI_eForm_Period_To__c
)</errorConditionFormula>
        <errorDisplayField>ASI_eForm_Period_To__c</errorDisplayField>
        <errorMessage>Please input a reasonable period.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_eForm_KR_Remarks_Mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>AND ( 
CONTAINS( RecordType.DeveloperName , &quot;ASI_eForm_KR_CC&quot;), 
OR( 
AND(TEXT(ASI_eForm_Reason__c) = &quot;기타 (본인입력)&quot;, ISBLANK(ASI_eForm_Remarks__c) ),
AND(TEXT(PRIORVALUE(ASI_eForm_Reason__c)) != TEXT(ASI_eForm_Reason__c), TEXT(ASI_eForm_Reason__c) = &quot;기타 (본인입력)&quot;, ISBLANK(ASI_eForm_Remarks__c) ), 
AND(ISBLANK(ASI_eForm_Remarks__c), NOT(ISBLANK(PRIORVALUE(ASI_eForm_Remarks__c))), TEXT(ASI_eForm_Reason__c) = &quot;기타 (본인입력)&quot; ) 
))</errorConditionFormula>
        <errorDisplayField>ASI_eForm_Remarks__c</errorDisplayField>
        <errorMessage>Please enter remarks in case you choose &quot;기타(본인입력)&quot; as a reason.</errorMessage>
    </validationRules>
