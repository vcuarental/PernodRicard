    <validationRules>
        <fullName>LAT_AmountInvalid</fullName>
        <active>true</active>
        <errorConditionFormula>(Amount__c &lt; 1) ||  (ISBLANK(Amount__c))</errorConditionFormula>
        <errorDisplayField>Amount__c</errorDisplayField>
        <errorMessage>O valor deve ser maior do que 0.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LAT_ValidateAmount</fullName>
        <active>true</active>
        <errorConditionFormula>(ISNEW() &amp;&amp; Amount__c &gt;  RegionalPOP__r.AmountLeft__c ) ||
(!ISNEW() &amp;&amp; (Amount__c - PRIORVALUE(Amount__c)) &gt;  RegionalPOP__r.AmountLeft__c)</errorConditionFormula>
        <errorDisplayField>Amount__c</errorDisplayField>
        <errorMessage>Quantidade maior que o Saldo disponível.</errorMessage>
    </validationRules>
