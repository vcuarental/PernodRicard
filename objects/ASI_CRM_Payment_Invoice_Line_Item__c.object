    <validationRules>
        <fullName>ASI_CRM_JP_Cant_Change_Invoice_Line</fullName>
        <active>true</active>
        <errorConditionFormula>CONTAINS( ASI_CRM_Payment_Invoice__r.RecordType.DeveloperName , &apos;ASI_CRM_JP&apos;) &amp;&amp; (ISCHANGED( ASI_CRM_Payment_Invoice__c ) || ISCHANGED(  ASI_CRM_Purchase_Contract_Line_Item__c ))</errorConditionFormula>
        <errorMessage>You are not allowed to update Invoice Line data</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_JP_Cant_Change_SKU_Selection</fullName>
        <active>true</active>
        <description>SKU Selection may not change if it was not set to &apos;Representative SKU&apos; or &apos;Alternative SKU&apos;</description>
        <errorConditionFormula>ISCHANGED(ASI_CRM_SKU_Selection__c) 
&amp;&amp; (ASI_CRM_Payment_Invoice__r.RecordType.Name = &quot;JP SPTD FOC Invoice Read-Only&quot;) 
&amp;&amp; (TEXT(ASI_CRM_SKU_Selection__c) = &quot;Representative SKU&quot; 
|| TEXT(ASI_CRM_SKU_Selection__c) = &quot;Alternative SKU&quot;)</errorConditionFormula>
        <errorDisplayField>ASI_CRM_SKU_Selection__c</errorDisplayField>
        <errorMessage>SKU Selection may not change if it was not set to &apos;Representative SKU&apos; or &apos;Alternative SKU&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_JP_Cant_Enter_Negative_Quantity</fullName>
        <active>false</active>
        <errorConditionFormula>/* Prevent user to enter negative figure into actual quantity field.

Criteria: 
* Record Type match JP SPTD FOC Invoice Line Item 
* Actual quantity field has negative amount
* Bypass ETL account(PR Japan SOA)
*/ 

RecordType.DeveloperName = &apos;ASI_CRM_JP_SPTD_FOC_Invoice_Line_Item&apos; &amp;&amp; 

ASI_CRM_Actual_Quantity__c &lt; 0 &amp;&amp; 

!AND(CONTAINS($Setup.ASI_JP_Validation_Bypass__c.ASI_JP_Object_API__c, &apos;ASI_CRM_Payment_Invoice_Line_Item__c&apos; ), ISBLANK( $Setup.ASI_JP_Validation_Bypass__c.ASI_JP_Record_Type__c ))</errorConditionFormula>
        <errorDisplayField>ASI_CRM_Actual_Quantity__c</errorDisplayField>
        <errorMessage>Actual quantity cannot be negative.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_JP_EmptySKUError</fullName>
        <active>true</active>
        <errorConditionFormula>( ISNEW() ||  ISCHANGED(ASI_CRM_SKU__c) ||  ISCHANGED( ASI_CRM_SKU_Selection__c ) )&amp;&amp; ISBLANK(ASI_CRM_SKU__c)&amp;&amp; RecordType.DeveloperName = &quot;ASI_CRM_JP_SPTD_FOC_Invoice_Line_Item&quot;</errorConditionFormula>
        <errorDisplayField>ASI_CRM_SKU__c</errorDisplayField>
        <errorMessage>SKU cannot be empty</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_JP_Promotion_Result_BuybackwCond</fullName>
        <active>true</active>
        <description>For JP CRM: If Payment Method in Contract Line is Buyback w. condition, Promotion Result in Invoice Line cannot be blank.</description>
        <errorConditionFormula>CONTAINS(RecordType.DeveloperName , &quot;ASI_CRM_JP&quot;) &amp;&amp;
ISPICKVAL(ASI_CRM_Purchase_Contract_Line_Item__r.ASI_CRM_Payment_Method__c, &quot;Buyback w. condition&quot;) &amp;&amp;
ISBLANK(ASI_CRM_Sales_Result__c)</errorConditionFormula>
        <errorDisplayField>ASI_CRM_Sales_Result__c</errorDisplayField>
        <errorMessage>For Contract Line of Payment Method &apos;Buyback w. condition&apos;, Promotion Result cannot be blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_JP_Promotion_Result_FOCwCond</fullName>
        <active>true</active>
        <description>For JP CRM: If Payment Method in Contract Line is FOC w. condition, Promotion Result in Invoice Line cannot be blank.</description>
        <errorConditionFormula>CONTAINS(RecordType.DeveloperName , &quot;ASI_CRM_JP&quot;) &amp;&amp;
ISPICKVAL(ASI_CRM_Purchase_Contract_Line_Item__r.ASI_CRM_Payment_Method__c, &quot;FOC w. condition&quot;) &amp;&amp;
ISBLANK(ASI_CRM_Sales_Result__c)</errorConditionFormula>
        <errorDisplayField>ASI_CRM_Sales_Result__c</errorDisplayField>
        <errorMessage>For Contract Line of Payment Method &apos;FOC w. condition&apos;, Promotion Result cannot be blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_JP_Promotion_Result_FeewTarget</fullName>
        <active>true</active>
        <description>For JP CRM: If Payment Method in Contract Line is Promotion fee with target, Promotion Result in Invoice Line cannot be blank.</description>
        <errorConditionFormula>CONTAINS(RecordType.DeveloperName , &quot;ASI_CRM_JP&quot;) &amp;&amp;
ISPICKVAL(ASI_CRM_Purchase_Contract_Line_Item__r.ASI_CRM_Payment_Method__c, &quot;Promotion fee with target&quot;) &amp;&amp;
ISBLANK(ASI_CRM_Sales_Result__c)</errorConditionFormula>
        <errorDisplayField>ASI_CRM_Sales_Result__c</errorDisplayField>
        <errorMessage>For Contract Line of Payment Method &apos;Promotion fee with target&apos;, Promotion Result cannot be blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_JP_Promotion_Result_GiftCoupon</fullName>
