    <validationRules>
        <fullName>data_da_distribuicao_de_valores_dentro</fullName>
        <active>true</active>
        <description>data de distribuição de valores está dentro do intervalo de contratos.</description>
        <errorConditionFormula>OR(

AND( ISBLANK( Clausulas_do_Contrato__r.Contrato__r.ProvisionarAte__c ) &amp;&amp; (Data_de_Referencia_para_Apuracao__c &gt; Data_termino_do_contrato__c ||
Data_de_Referencia_para_Apuracao__c &lt; Data_inicio_do_contrato__c )),

AND( NOT(ISBLANK(Clausulas_do_Contrato__r.Contrato__r.ProvisionarAte__c )) &amp;&amp; (Data_de_Referencia_para_Apuracao__c &gt; Clausulas_do_Contrato__r.Contrato__r.ProvisionarAte__c ||
Data_de_Referencia_para_Apuracao__c &lt; Data_inicio_do_contrato__c ))

)</errorConditionFormula>
        <errorDisplayField>Data_de_Referencia_para_Apuracao__c</errorDisplayField>
        <errorMessage>A data de referencia para apuração deve ser maior que a data de ínicio, menor que a data término e que a data provisionar até.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>nao_ter_registro_se_for_percentual</fullName>
        <active>true</active>
        <description>O objeto de distribuição não pode ter nenhum registro se for percentual</description>
        <errorConditionFormula>(NOT ISBLANK(Clausulas_do_Contrato__r.Percentual__c))   &amp;&amp; 
 (NOT ISBLANK(Data_de_Referencia_para_Apuracao__c ))</errorConditionFormula>
        <errorMessage>Quando a Cláusula é Percentual não pode ter distribuição de valores</errorMessage>
    </validationRules>
