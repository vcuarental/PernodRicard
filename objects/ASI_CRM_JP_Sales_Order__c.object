    <validationRules>
        <fullName>ASI_CRM_JP_AKABOU_DELIVERY</fullName>
        <active>true</active>
        <description>This is the validation rule to restrict AKABOU availability for selection of shipping</description>
        <errorConditionFormula>AND(
OR(
 RecordType.DeveloperName = &apos;ASI_CRM_JP_Marketing_FOC&apos;,
 RecordType.DeveloperName = &apos;ASI_CRM_JP_Direct_Sales&apos;
),
ISPICKVAL( ASI_CRM_Delivery_Time__c , &apos;AKABOU&apos;),
OR(
AND( 
OR(
ISPICKVAL( ASI_CRM_Branch_Plant__c ,&apos;711&apos;),
ISPICKVAL( ASI_CRM_Branch_Plant__c ,&apos;111&apos;),
ISPICKVAL( ASI_CRM_Branch_Plant__c ,&apos;116&apos;),
ISPICKVAL( ASI_CRM_Branch_Plant__c ,&apos;211&apos;),
ISPICKVAL( ASI_CRM_Branch_Plant__c ,&apos;214&apos;),
ISPICKVAL( ASI_CRM_Branch_Plant__c ,&apos;721&apos;)
),

NOT(
OR(
ASI_CRM_Ship_To_Prefecture__c = &apos;Saitama&apos;,
ASI_CRM_Ship_To_Prefecture__c = &apos;Chiba&apos;,
ASI_CRM_Ship_To_Prefecture__c = &apos;Tokyo&apos;,
ASI_CRM_Ship_To_Prefecture__c = &apos;Kanagawa&apos;)
)
),
AND( 
OR(
ISPICKVAL( ASI_CRM_Branch_Plant__c ,&apos;131&apos;),
ISPICKVAL( ASI_CRM_Branch_Plant__c ,&apos;136&apos;)
),
NOT(
OR(
ASI_CRM_Ship_To_Prefecture__c = &apos;Kyoto&apos;,
ASI_CRM_Ship_To_Prefecture__c = &apos;Osaka&apos;,
ASI_CRM_Ship_To_Prefecture__c = &apos;Hyogo&apos;,
ASI_CRM_Ship_To_Prefecture__c = &apos;Nara&apos;)
)
)
)
)</errorConditionFormula>
        <errorMessage>For AKABOU Delivery, only applicable for selected prefectures. Please check before proceeding</errorMessage>
