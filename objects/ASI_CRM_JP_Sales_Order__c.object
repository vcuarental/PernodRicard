    <validationRules>
        <fullName>ASI_CRM_JP_Check_Delivery_Time</fullName>
        <active>false</active>
        <errorConditionFormula>/* Prevent user input afternoon for On-Trade Outlet; Prevent users input concrete time for non-On-Trade Outlet 

Criteria: 
* Customer Record is On-Trade AND Delivery Time is After Noon (&quot;1200-0000&quot;), OR 
* Customer Record is non-On-Trade AND Delivery Time is NOT After Noon (&quot;1200-0000&quot;) AND Delivery Time is NOT Before Noon (&quot;0000-1200&quot;) 

Remark: this validation rule should not bypass any user to ensure data integrity. 
Remark: this validation rule should not restrict Duty Free SO 
*/ 

IF ( RecordType.DeveloperName = &apos;ASI_CRM_JP_Inactive_Sales_Order&apos; || RecordType.DeveloperName = &apos;ASI_CRM_JP_Duty_Free_SO&apos; || RecordType.DeveloperName = &apos;ASI_CRM_JP_Duty_Free_SO_Locked&apos; || RecordType.DeveloperName = &apos;ASI_CRM_JP_Manual_JDE_SO&apos;,
    false, 
OR (
    ASI_CRM_Ship_To__r.RecordType.DeveloperName = &apos;ASI_CRM_JP_On_Trade_Outlet&apos; &amp;&amp; 
    OR(
        ISPICKVAL( ASI_CRM_Delivery_Time__c, &quot;1200-0000&quot;),
        ISPICKVAL( ASI_CRM_Delivery_Time__c, &quot;0000-0000&quot;) 
    ),
    (ASI_CRM_Ship_To__r.RecordType.DeveloperName = &apos;ASI_CRM_JP_Off_Trade_Outlet&apos; ||
     ASI_CRM_Ship_To__r.RecordType.DeveloperName = &apos;ASI_CRM_JP_Wholesaler&apos; ||
     ASI_CRM_Ship_To__r.RecordType.DeveloperName = &apos;ASI_CRM_JP_Bar_Supplier&apos; )&amp;&amp;
    !ISPICKVAL( ASI_CRM_Delivery_Time__c, &quot;1200-0000&quot;) &amp;&amp; 
    !ISPICKVAL( ASI_CRM_Delivery_Time__c, &quot;0000-1200&quot;) &amp;&amp; 
    !ISPICKVAL( ASI_CRM_Delivery_Time__c, &quot;0000-0000&quot;) &amp;&amp;
    !ISPICKVAL( ASI_CRM_Delivery_Time__c, &quot;AKABOU&quot;)
) &amp;&amp; 
OR (ISNEW(), ISCHANGED(ASI_CRM_Delivery_Time__c) )
)</errorConditionFormula>
        <errorDisplayField>ASI_CRM_Delivery_Time__c</errorDisplayField>
        <errorMessage>The Delivery Time input is not valid. Please correct</errorMessage>
    </validationRules>
