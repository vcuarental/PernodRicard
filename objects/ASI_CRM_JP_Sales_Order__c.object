        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/36.0/connection.js&quot;)} 

var recordTypeAllowRecallWhenApproved = [];
recordTypeAllowRecallWhenApproved.push(&quot;ASI_CRM_JP_Direct_Sales_Locked&quot;);
recordTypeAllowRecallWhenApproved.push(&quot;ASI_CRM_JP_Entertainment_Sales_Locked&quot;);
recordTypeAllowRecallWhenApproved.push(&quot;ASI_CRM_JP_Marketing_FOC_Locked&quot;);
recordTypeAllowRecallWhenApproved.push(&quot;ASI_CRM_JP_Sample_Sales_Locked&quot;);

var recordTypeAllowRecallWhenRequested = [];
recordTypeAllowRecallWhenRequested.push(&quot;ASI_CRM_JP_Duty_Free_SO_Locked&quot;);
recordTypeAllowRecallWhenRequested.push(&quot;ASI_CRM_JP_Employee_Sales_Locked&quot;);
recordTypeAllowRecallWhenRequested.push(&quot;ASI_CRM_JP_Normal_SO_Locked&quot;);

var recordTypeNameMapping = {}; 
recordTypeNameMapping[&quot;ASI_CRM_JP_Normal_SO_Locked&quot;] = &quot;ASI_CRM_JP_Normal_SO&quot;; 
recordTypeNameMapping[&quot;ASI_CRM_JP_Direct_Sales_Locked&quot;] = &quot;ASI_CRM_JP_Direct_Sales&quot;; 
recordTypeNameMapping[&quot;ASI_CRM_JP_Marketing_FOC_Locked&quot;] = &quot;ASI_CRM_JP_Marketing_FOC&quot;; 
recordTypeNameMapping[&quot;ASI_CRM_JP_Sample_Sales_Locked&quot;] = &quot;ASI_CRM_JP_Sample_Sales&quot;; 
recordTypeNameMapping[&quot;ASI_CRM_JP_Employee_Sales_Locked&quot;] = &quot;ASI_CRM_JP_Employee_Sales&quot;; 
recordTypeNameMapping[&quot;ASI_CRM_JP_Entertainment_Sales_Locked&quot;] = &quot;ASI_CRM_JP_Entertainment_Sales&quot;; 
recordTypeNameMapping[&quot;ASI_CRM_JP_Duty_Free_SO_Locked&quot;] = &quot;ASI_CRM_JP_Duty_Free_SO&quot;; 

var recordTypeMap = {}; 

var query = &quot;SELECT Id, Name, DeveloperName &quot; + 
&quot;FROM RecordType &quot; + 
&quot;Where sObjectType = &apos;ASI_CRM_JP_Sales_Order__c&apos;&quot;; 

var recordTypeResult = sforce.connection.query(query); 
var recordTypeList = recordTypeResult.getArray(&quot;records&quot;); 

var currentRecordTypeDeveloperName = &apos;&apos;; 
for(var index = 0 ; index &lt; recordTypeList.length ; index++) { 
	var recordType = recordTypeList[index]; 
	if(recordType.Id.includes(&apos;{!ASI_CRM_JP_Sales_Order__c.RecordTypeId}&apos;)) { 
		currentRecordTypeDeveloperName = recordType.DeveloperName; 
	} 
	recordTypeMap[recordType.DeveloperName] = recordType; 
} 

if(recordTypeAllowRecallWhenApproved.indexOf(currentRecordTypeDeveloperName) != -1) { 
	if (&quot;{!ASI_CRM_JP_Sales_Order__c.ASI_CRM_Status__c}&quot; != &quot;Approved&quot;) { 
		alert(&apos;{!$Label.ASI_CRM_JP_Only_Recall_When_Status_Approved}&apos;); 
	} else if(confirm(&apos;{!$Label.ASI_CRM_JP_Recall_Message}&apos;)) { 
		recallRecord(currentRecordTypeDeveloperName); 
	} 
} else { 
	if (&quot;{!ASI_CRM_JP_Sales_Order__c.ASI_CRM_Status__c}&quot; != &quot;Requested&quot;) { 
		alert(&apos;{!$Label.ASI_CRM_JP_Only_Recall_When_Status_Requested}&apos;); 
	} else if (confirm(&apos;{!$Label.ASI_CRM_JP_Recall_Message}&apos;)) { 
		recallRecord(currentRecordTypeDeveloperName); 
	} 
} 

function recallRecord(currentRecordTypeDeveloperName) { 
	var relatedRecordTypeDeveloperName = recordTypeNameMapping[currentRecordTypeDeveloperName]; 
	var relatedRecordType = recordTypeMap[relatedRecordTypeDeveloperName]; 

	var salesOrder = new sforce.SObject(&quot;ASI_CRM_JP_Sales_Order__c&quot;); 
	salesOrder.Id = &quot;{!ASI_CRM_JP_Sales_Order__c.Id}&quot;; 
	salesOrder.ASI_CRM_Status__c = &quot;Open&quot;; 

	salesOrder.RecordTypeId = relatedRecordType.Id; 
	result = sforce.connection.update([salesOrder]); 
	location.reload(true); 
}</url>
