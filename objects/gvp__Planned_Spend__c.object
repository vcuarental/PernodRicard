        <fullName>gvp__PR_Vouchers_are_6_characters_long</fullName>
        <active>false</active>
        <errorConditionFormula>AND( Len(Name)  &lt;&gt;  6, RecordType.Name = &apos;Planned Spend&apos;)</errorConditionFormula>
        <errorMessage>*** Promotion Voucher names MUST be 6 Characters ***</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>gvp__Prevent_Duplicate_Names</fullName>
        <active>false</active>
        <errorConditionFormula>AND  ( ISNEW() , Name = VLOOKUP
       (  $ObjectType.gvp__Planned_Spend__c.Fields.Name  , $ObjectType.gvp__Planned_Spend__c.Fields.Name , Name )
)</errorConditionFormula>
        <errorMessage>** The Promotion Voucher Name has already been used!  Please try again. **</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>gvp__Value_Exceeds_Plan_Amount</fullName>
        <active>false</active>
        <errorConditionFormula>And ( RecordType.Name = &apos;Planned Spend&apos;,
(  gvp__Program__r.gvp__Total_Program_Cost__c -  gvp__Total_Amount__c   ) &lt; 0 )</errorConditionFormula>
        <errorMessage>!! WARNING !!!  Your amount is greater than the budgeted amount. Please change status to &apos;Needs Revision&apos;.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
