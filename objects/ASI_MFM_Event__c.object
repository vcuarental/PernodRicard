        <fullName>ASI_MFM_Budget_Check</fullName>
        <active>true</active>
        <errorConditionFormula>AND(CONTAINS(RecordType.DeveloperName,&apos;ASI_MFM_CN&apos;), (ASI_MFM_Key_Event_Type_Budget_Share__c + ASI_MFM_Sub_Event_Type_Budget_Share__c) != 100)</errorConditionFormula>
        <errorDisplayField>ASI_MFM_Key_Event_Type_Budget_Share__c</errorDisplayField>
        <errorMessage>Total budget share for Key and Sub Event Type must be equal to 100 %</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_CN_Date_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>ASI_MFM_End_Date__c &lt;  ASI_MFM_Start_Date__c</errorConditionFormula>
        <errorMessage>End Date should not smaller than Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_Event_Type_Checking</fullName>
        <active>true</active>
        <errorConditionFormula>AND(CONTAINS(RecordType.DeveloperName,&apos;ASI_MFM_CN&apos;),TEXT(ASI_MFM_Event_Type__c) =  Text(ASI_MFM_Sub_Event_Type__c))</errorConditionFormula>
        <errorDisplayField>ASI_MFM_Sub_Event_Type__c</errorDisplayField>
        <errorMessage>Sub Event Type cannot be same as Key Event Type</errorMessage>
    </validationRules>
