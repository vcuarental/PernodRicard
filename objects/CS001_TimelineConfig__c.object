<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <description>Timeline : This custom setting allows to customize the project timeline</description>
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>DisplayLegend__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Check this checkbox to display the legend.</description>
        <externalId>false</externalId>
        <inlineHelpText>Check this checkbox to display the legend.</inlineHelpText>
        <label>Display Legend</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>EventSupport__c</fullName>
        <defaultValue>false</defaultValue>
        <description>You have to check this checkbox in order to see the events associated to the project</description>
        <externalId>false</externalId>
        <inlineHelpText>You have to check this checkbox in order to see the events associated to the project</inlineHelpText>
        <label>Event Support</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>PriorityTaskFilter__c</fullName>
        <defaultValue>&quot;All&quot;</defaultValue>
        <description>Value of the filter used to retrieve Tasks (values supported: Low, Normal, High, All)</description>
        <externalId>false</externalId>
        <inlineHelpText>Value of the filter used to retrieve Tasks (values supported: Low, Normal, High, All). The default value is All</inlineHelpText>
        <label>Priority Task Filter</label>
        <length>20</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>TaskDescriptionTeaserLength__c</fullName>
        <description>Represents the number of characters of the Comments field displayed in the timeline through the popup bubbles.</description>
        <externalId>false</externalId>
        <inlineHelpText>Represents the number of characters of the Comments field displayed in the timeline through the popup bubbles.</inlineHelpText>
        <label>Task Description Teaser Length</label>
        <precision>3</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>TaskSupport__c</fullName>
        <defaultValue>false</defaultValue>
        <description>You have to check this checkbox in order to see the tasks associated to the project</description>
        <externalId>false</externalId>
        <inlineHelpText>You have to check this checkbox in order to see the tasks associated to the project</inlineHelpText>
        <label>Task Support</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>TypeTaskFilter__c</fullName>
        <description>Represents the different types of Task displayed in the timeline (the values are comma-separated). If the field is empty, all the type of Tasks will be retrieved.</description>
        <externalId>false</externalId>
        <inlineHelpText>Represents the different types of Task displayed in the timeline (the values are comma-separated). If the field is empty, all the type of Tasks will be retrieved.</inlineHelpText>
        <label>Type Task  Filter</label>
        <length>100</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
