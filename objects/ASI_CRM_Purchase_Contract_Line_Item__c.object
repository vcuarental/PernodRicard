        <active>true</active>
        <errorConditionFormula>AND(OR(RecordType.DeveloperName = &quot;ASI_CRM_JP_Direct_Rebate_Contract_Line_Item&quot;,RecordType.DeveloperName = &quot;ASI_CRM_JP_Indirect_Rebate_Contract_Line_Item&quot;), ISPICKVAL(  ASI_CRM_Rebate_Type__c, &quot;&quot;))</errorConditionFormula>
        <errorDisplayField>ASI_CRM_Rebate_Type__c</errorDisplayField>
        <errorMessage>The Rebate Type cannot be blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_JP_Remarks_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND( CONTAINS(RecordType.DeveloperName, &quot;ASI_CRM_JP_SPTD_Contract_Line_Item&quot;),  ISPICKVAL( ASI_CRM_Payment_Method__c ,&quot;Other Cash&quot;), ISBLANK( ASI_CRM_Remarks__c )    )</errorConditionFormula>
        <errorDisplayField>ASI_CRM_Remarks__c</errorDisplayField>
        <errorMessage>Remarks cannot be empty on Other Cash payment method.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_JP_Target_Volume_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>CONTAINS(RecordType.DeveloperName, &quot;ASI_CRM_JP&quot;) &amp;&amp; (ISPICKVAL(ASI_CRM_Payment_Method__c, &quot;Buyback w. condition&quot;) || ISPICKVAL(ASI_CRM_Payment_Method__c, &quot;FOC w. condition&quot;) || ISPICKVAL(ASI_CRM_Payment_Method__c, &quot;Promotion fee with target&quot;) || ISPICKVAL(ASI_CRM_Payment_Method__c, &quot;Gift Coupon&quot;)) &amp;&amp; ISBLANK(ASI_CRM_Target_Volume__c)</errorConditionFormula>
        <errorDisplayField>ASI_CRM_Target_Volume__c</errorDisplayField>
        <errorMessage>Target Volume must not be empty.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_JP_Unit_Price_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>CONTAINS(RecordType.DeveloperName, &quot;ASI_CRM_JP&quot;) &amp;&amp; (ISPICKVAL(ASI_CRM_Payment_Method__c, &quot;Gift Coupon&quot;) || ISPICKVAL(ASI_CRM_Payment_Method__c, &quot;Promotion fee with target&quot;) || ISPICKVAL(ASI_CRM_Payment_Method__c, &quot;Other Cash&quot;)) &amp;&amp; ISBLANK(ASI_CRM_Unit_Price__c)</errorConditionFormula>
        <errorDisplayField>ASI_CRM_Unit_Price__c</errorDisplayField>
        <errorMessage>Unit Price/Cost must not be empty.</errorMessage>
    </validationRules>
