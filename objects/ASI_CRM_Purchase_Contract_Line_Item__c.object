        <fullName>ASI_CRM_JP_AP_Code_Cannot_Null</fullName>
        <active>true</active>
        <errorConditionFormula>AND((RecordType.DeveloperName = &quot;ASI_CRM_JP_SPTD_Contract_Line_Item&quot;), ISBLANK(ASI_CRM_AP_Code__c))</errorConditionFormula>
