    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_JP_AP_Code_for_FOC</fullName>
        <active>true</active>
        <description>If the Payment Method is FOC, then the A&amp;P Code selected should be for FOC</description>
        <errorConditionFormula>AND(
OR( ISNEW() , ISCHANGED( ASI_CRM_AP_Code__c), ISCHANGED( ASI_CRM_Payment_Method__c ) ), 
    CONTAINS(RecordType.DeveloperName, &quot;ASI_CRM_JP_SPTD_Contract&quot;),
    (ISPICKVAL(ASI_CRM_Payment_Method__c, &quot;FOC w/o condition&quot;)||
    ISPICKVAL(ASI_CRM_Payment_Method__c, &quot;FOC w. condition&quot;)),
    !ASI_CRM_AP_Code__r.ASI_CRM_FOC_Invoice__c
)</errorConditionFormula>
        <errorDisplayField>ASI_CRM_AP_Code__c</errorDisplayField>
        <errorMessage>You selected an AP Code that is not for FOC payment method</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_JP_Condition_Of_To_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>CONTAINS(RecordType.DeveloperName, &quot;ASI_CRM_JP_SPTD_Contract&quot;) &amp;&amp; (ISPICKVAL(ASI_CRM_Payment_Method__c, &quot;Buyback w. condition&quot;) || ISPICKVAL(ASI_CRM_Payment_Method__c, &quot;FOC w. condition&quot;) || ISPICKVAL(ASI_CRM_Payment_Method__c, &quot;Promotion fee with target&quot;) || ISPICKVAL(ASI_CRM_Payment_Method__c, &quot;Gift Coupon&quot;)) &amp;&amp; (ISBLANK(ASI_CRM_Condition_Of__c) || ISBLANK(ASI_CRM_Condition_To__c))</errorConditionFormula>
