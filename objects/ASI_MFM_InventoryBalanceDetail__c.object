        <fullName>ASI_MFM_StockinDate__c</fullName>
        <externalId>false</externalId>
        <formula>IF((LEN(Name)==8),
IF(
ISNUMBER(Name),
DATE(VALUE(LEFT(Name, 4)), MIN(12,VALUE(MID(Name,5,2))), VALUE(RIGHT(Name, 2))),
Null
),
Null
)</formula>
