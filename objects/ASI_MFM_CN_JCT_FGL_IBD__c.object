    <fields>
        <fullName>ASI_MFM_Check_Holded__c</fullName>
        <externalId>false</externalId>
        <formula>AND(
(ASI_MFM_Holdied_Quantity__c == FALSE),
(ASI_MFM_FG_ETL__c == FALSE),

OR
(ASI_MFM_FreeGoodsRequestStatus__c == &apos;Submitted&apos;,
ASI_MFM_FreeGoodsRequestStatus__c == &apos;In Progress&apos;,
ASI_MFM_FreeGoodsRequestStatus__c == &apos;Approved by Logistic&apos;
)

)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Check Holded</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>ASI_MFM_Check_Recalculate__c</fullName>
        <externalId>false</externalId>
        <formula>NOT(
ASI_MFM_FG_Quantity__c == ASI_MFM_RequestQty__c
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Check Recalculate</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
