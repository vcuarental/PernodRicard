    <validationRules>
        <fullName>ASI_CN_NPL_AtLeastOnePricingStrategyInpu</fullName>
        <active>true</active>
        <description>Ensure at least one pricing strategy has been entered</description>
        <errorConditionFormula>AND
(
	OR
	(
		RecordType.Name = &apos;Step 3.3 Class A Proposed Price&apos;,
		RecordType.Name = &apos;Step 4 Launch Plan&apos;,
		RecordType.Name = &apos;Step 5 Class A Vol Est&apos;,
		RecordType.Name = &apos;Step 6 Class A S&amp;OP&apos;,
		RecordType.Name = &apos;Step 7 Class A Final Approval&apos;,
		RecordType.Name = &apos;Step 8 Class A Final Approved&apos;
	),
	AND
	(
		ISBLANK(ASI_CN_NPL_Pricing_Strategy_Trade__c),
		ISBLANK(ASI_CN_NPL_Pricing_Strategy_Bar__c),
		ISBLANK(ASI_CN_NPL_Pricing_Strategy_FKTV__c),
		ISBLANK(ASI_CN_NPL_Pricing_Strategy_KTV__c),
		ISBLANK(ASI_CN_NPL_Pricing_Strategy_CR__c),
		ISBLANK(ASI_CN_NPL_Pricing_Strategy_Off_Trade__c)
	),
	NOT(ISBLANK(ASI_CN_NPL_Competitor__c)),
 ISCHANGED(ASI_CN_NPL_Final_Proposed_Selling_Price__c) 
)</errorConditionFormula>
        <errorMessage>Please enter at least one Pricing Strategy</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CN_NPL_Dom_Launch_Date_Mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>AND ( INCLUDES(ASI_CN_NPL_Justification_Market__c , &quot;Domestic&quot;),
 ISBLANK( ASI_CN_NPL_Domestic_Launch_Date__c ) 
)</errorConditionFormula>
        <errorDisplayField>ASI_CN_NPL_Domestic_Launch_Date__c</errorDisplayField>
        <errorMessage>Please enter Domestic Launch Date</errorMessage>
    </validationRules>
