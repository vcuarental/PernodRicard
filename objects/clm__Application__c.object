        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/connection.js&quot;)}

appId = &quot;{!clm__Application__c.Id}&quot;;
currentVer = &quot;{!clm__Application__c.clm__Version__c}&quot;;
function insertScript(script){
	var targetNode = document.createElement(&apos;div&apos;); // construct div for script injection
	document.body.appendChild(targetNode);
		try {
			var el = document.createElement(&apos;script&apos;);
				el.type=&quot;text/javascript&quot;;
				el.text = script;
			targetNode.appendChild(el);
		} catch (e){
			var el = document.createElement(&apos;span&apos;);
			targetNode.appendChild(el);
			el.innerHTML = &quot;&lt;br /&gt;&lt;scr&quot;+&quot;ipt type=&apos;text/javascript&apos; defer=&apos;defer&apos;&gt;&quot;+script+&quot;&lt;/script&quot; + &quot;&gt;&quot;;
		}

	var box = new SimpleDialog(&quot;hersh&quot;+Math.random(), true);
		//parent.box = box;
		box.setTitle(&quot;{!$Label.clm__edetpublishapptitle}&quot;);
		box.createDialog();
		box.setWidth(350);
		box.setContentInnerHTML(&quot;&lt;p align=&apos;center&apos;&gt;&lt;img src=&apos;/img/icon/custom51_100/chalkboard32.png&apos; style=&apos;margin:0 5px;&apos;/&gt;&lt;img src=&apos;/changemgmt/img/cm_inlinearrowright.png&apos; style=&apos;margin:5px;&apos;/&gt;&lt;img src=&apos;/img/icon/custom51_100/globe32.png&apos; style=&apos;margin:0 5px;&apos;/&gt;&lt;/p&gt;&lt;p align=&apos;center&apos;&gt;&lt;small&gt;{!$Label.clm__edetpublishappdescription}&lt;/small&gt;&lt;/p&gt;&lt;p align=&apos;center&apos;&gt;&lt;/p&gt;&lt;p align=&apos;center&apos;&gt;{!$Label.clm__forcedload}: &lt;select id=&apos;forcedload&apos;&gt;&lt;option value=&apos;0&apos;&gt;{!$Label.clm__immediately}&lt;/option&gt;&lt;option value=&apos;3&apos;&gt;{!$Label.clm__in3days}&lt;/option&gt;&lt;option value=&apos;7&apos;&gt;{!$Label.clm__in7days}&lt;/option&gt;&lt;option value=&apos;15&apos;&gt;{!$Label.clm__in15days}&lt;/option&gt;&lt;/select&gt;&lt;/p&gt;&lt;p align=&apos;center&apos; id=&apos;message&apos; style=&apos;color:green&apos;&gt;&lt;/p&gt;&lt;p align=&apos;center&apos;&gt;&lt;br /&gt;&lt;button class=&apos;btn&apos; onclick=&apos;publishApp(); return false;&apos;&gt;{!$Label.clm__edetpublishbutton}&lt;/button&gt; &lt;button class=&apos;btn&apos; onclick=&apos;/*window.parent.box.hide()*/window.location.reload(); return false;&apos;&gt;{!$Label.clm__edetcancelbutton}&lt;/button&gt;&lt;/p&gt;&quot;);
		box.setupDefaultButtons();
	box.show();
}

script = &quot;function publishApp(){var newVersion; var forcedDate=new Date(); if (typeof document.getElementById(&apos;forcedload&apos;) != &apos;undefined&apos; &amp;&amp; document.getElementById(&apos;forcedload&apos;) != null){forcedDate.setDate(forcedDate.getDate()+parseInt(document.getElementById(&apos;forcedload&apos;).value));}if (currentVer == &apos;&apos; || currentVer == null){currentVer = 1;}currentVer = currentVer.replace(&apos;,&apos;,&apos;.&apos;);newVersion = parseFloat(parseFloat(currentVer)+parseFloat(0.1));var app = new sforce.SObject(&apos;clm__Application__c&apos;);app.Id = appId;app.clm__Version__c = newVersion;app.clm__IsActive__c = true;app.clm__ForcedLoadDate__c = forcedDate; var result = sforce.connection.update([app]);if (result[0].getBoolean(&apos;success&apos;)){document.getElementById(&apos;message&apos;).innerHTML=&apos;{!$Label.clm__edetsuccessmessage}&apos;;window.setTimeout(function(){document.location.reload(true)},1000);} else {alert(&apos;{!$Label.clm__edeterrormessage}&apos;);box.hide();}}&quot;;

insertScript(script);</url>
