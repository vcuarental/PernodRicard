        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/25.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/25.0/apex.js&quot;)} 

app= &quot;{! clm__Application__c.Id }&quot;; 

function insertScript(script){ 
var targetNode = document.createElement(&apos;div&apos;);	//	construct div for script injection 
document.body.appendChild(targetNode); 
try { 
var el = document.createElement(&apos;script&apos;); 
el.type=&quot;text/javascript&quot;; 
el.innerHTML = script; 
targetNode.appendChild(el); 
} catch (e){ 
var el = document.createElement(&apos;span&apos;); 
targetNode.appendChild(el); 
el.innerHTML = &quot;&lt;br /&gt;&lt;scr&quot;+&quot;ipt type=&apos;text/javascript&apos; defer=&apos;defer&apos;&gt;&quot;+script+&quot;&lt;/script&quot; + &quot;&gt;&quot;; 
} 

var box = new SimpleDialog(&quot;hersh&quot;+Math.random(), true); 
parent.box = box; 
box.setTitle(&quot;Clone&quot;); 
box.createDialog(); 
box.setWidth(300); 
box.setContentInnerHTML(&quot;&lt;table align=&apos;center&apos; cellpadding=&apos;5&apos;&gt;&lt;tr&gt;&lt;td&gt;&lt;img src=&apos;/img/msg_icons/info24.png&apos;/&gt;&lt;/td&gt;&lt;td align=&apos;center&apos;&gt;Do you really want to clone application?&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt;&lt;p align=&apos;center&apos;&gt;&lt;br /&gt;&lt;button class=&apos;btn&apos; onclick=&apos;cloneApp(); return false;&apos;&gt;Clone&lt;/button&gt;&lt;button class=&apos;btn&apos; onclick=&apos;window.parent.box.hide(); return false;&apos;&gt;Cancel&lt;/button&gt;&lt;/p&gt;&quot;); 
box.setupDefaultButtons(); 
box.show(); 
} 

script = &quot;function cloneApp(){var result = sforce.apex.execute(\&quot;clm.HelperClass\&quot;, \&quot;presentationClone\&quot;,{app_id:app}); window.parent.box.hide(); location.replace(\&quot;/\&quot; + result);}&quot;; 

insertScript(script);</url>
