    <validationRules>
        <fullName>ASI_CN_NPL_Check_Description</fullName>
        <active>true</active>
        <errorConditionFormula>CONTAINS(RecordType.DeveloperName, &apos;ASI_CN_NPL&apos;) &amp;&amp;
ISPICKVAL(ASI_CN_NPL_New_Brand_Sub_Brand_Picklist__c,&apos;Yes&apos;) &amp;&amp;
OR(
ISBLANK(ASI_CN_NPL_Brand_Description__c),
ISBLANK(ASI_CN_NPL_Sub_brand_Description__c)
) &amp;&amp;
ISPICKVAL(ASI_CN_NPL_Status__c, &apos;Waiting For BA Input Category Code&apos;)</errorConditionFormula>
        <errorMessage>Brand description and sub brand description are required if new brand/sub brand is checked</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CN_NPL_Check_T1Price</fullName>
        <active>true</active>
        <errorConditionFormula>CONTAINS(RecordType.DeveloperName, &apos;ASI_CN_NPL&apos;) &amp;&amp; 
ISCHANGED(ASI_CN_NPL_Item_Goup_Code__c) &amp;&amp;
ISPICKVAL(ASI_CN_NPL_Status__c,&apos;Completed&apos;) &amp;&amp;
ASI_CN_NPL_NPLT1PriceFlag__c &amp;&amp;
NOT(ASI_CN_NPL_Ignore_Checking__c)</errorConditionFormula>
        <errorMessage>Price is created. Please check &quot;Ignore Checking&quot; to proceed.</errorMessage>
    </validationRules>
