        <errorConditionFormula>AND( CONTAINS(RecordType.DeveloperName, &apos;ASI_MFM_KR&apos;), !CONTAINS($Profile.Name,&apos;Administrator&apos;),ISCHANGED(  Name ))</errorConditionFormula>
