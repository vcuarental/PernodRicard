    <validationRules>
        <fullName>ASI_MFM_KR_Set_to_Close_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND ( 
CONTAINS( RecordType.DeveloperName , &quot;ASI_MFM_KR&quot;), 
OR( 
AND(TEXT(PRIORVALUE(ASI_MFM_Status__c)) != TEXT(ASI_MFM_Status__c), TEXT(ASI_MFM_Status__c) = &quot;Closed&quot;, ISBLANK(ASI_MFM_Closing_Comments__c) ), 
AND(ISBLANK(ASI_MFM_Closing_Comments__c), NOT(ISBLANK(PRIORVALUE(ASI_MFM_Closing_Comments__c))), TEXT(ASI_MFM_Status__c) = &quot;Closed&quot; ) 
))</errorConditionFormula>
        <errorDisplayField>ASI_MFM_Closing_Comments__c</errorDisplayField>
        <errorMessage>Closing Comment is Required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_KR_ShipToAddress_Checking</fullName>
        <active>true</active>
        <errorConditionFormula>AND( CONTAINS(RecordType.DeveloperName, &apos;ASI_MFM_KR&apos;), !CONTAINS($Profile.Name,&apos;Administrator&apos;),CONTAINS(  ASI_MFM_Ship_To__r.Name,&apos;Agency&apos;), ISBLANK(ASI_MFM_KR_Ship_to_Address__c) )</errorConditionFormula>
        <errorMessage>Please input Company Address</errorMessage>
    </validationRules>
