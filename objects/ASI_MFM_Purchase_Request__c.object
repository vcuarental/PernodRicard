        <url>//bring in our dialog box code 
{!REQUIRESCRIPT(&quot;/resource/ASI_JS_JQuery_V1_9_1/js/jquery.min.js&quot;)} 
{!REQUIRESCRIPT(&quot;/resource/ASI_MFM_CustomButtonUtils&quot;)} 
//Salesforce ajax toolkit libraries 
{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/24.0/apex.js&quot;)} 


$j = jQuery.noConflict(); 

function downloadAll(files){ 
if(files.length == 0) return; 
file = files.pop(); 
var theAnchor = $j(&apos;&lt;a /&gt;&apos;) 
.attr(&apos;href&apos;, file[1]) 
.attr(&apos;download&apos;,file[0]) 
// Firefox does not fires click if the link is outside 
// the DOM 
.appendTo(&apos;body&apos;); 

theAnchor[0].click(); 
theAnchor.remove(); 
downloadAll(files); 
} 


//the vars 
var relatedAttachments, 
dialogText, 
records, 

//the query 
relatedAttachments = sforce.connection.query(&quot;SELECT Id, Name, ParentId, LastModifiedDate, CreatedBy.Name FROM Attachment where ParentId = &apos;{!ASI_MFM_Purchase_Request__c.Id}&apos;&quot;); 

//the results 
records = relatedAttachments.getArray(&quot;records&quot;); 

//if we have results, do something with them, else, show a message 
if (records.length &gt; 0) { 

//add some css and html to our message and construct a table 
dialogText = &apos;&lt;style&gt;.modal-table{width:100%;border-collapse:collapse;}.modal-table tr th{border-bottom:2px solid #bbb;padding:10px 0;}.modal-table tr td{border-bottom:1px solid #ccc;padding:10px 0;}&lt;/style&gt;&apos; 
dialogText += &apos;&lt;table class=\&quot;modal-table\&quot;&gt;&lt;thead&gt;&apos;; 
dialogText += &apos;&lt;th&gt;File Name #&lt;/th&gt;&apos;; 
dialogText += &apos;&lt;th&gt;Last Modified&lt;/th&gt;&apos;; 
dialogText += &apos;&lt;th&gt;Created By&lt;/th&gt;&apos;; 
dialogText += &apos;&lt;/thead&gt;&lt;tbody&gt;&apos; 

//add a table row for each attachment 
var attachments = [,]; 

for (i = 0; i &lt; records.length; i++) { 


var result = []; 
result[0] = records[i].get(&apos;Name&apos;); 
result[1] = &apos;/servlet/servlet.FileDownload?file=&apos; + records[i].Id; 
attachments[i] = result; 

var datetimeString = DateUtil.getDateTimeStringFromUserLocale(new Date(records[i].LastModifiedDate)); 

dialogText += &apos;&lt;tr&gt;&apos;; 
dialogText += &apos;&lt;td&gt;&lt;a target=\&quot;_blank\&quot; href=\&quot;/servlet/servlet.FileDownload?file=&apos; + records[i].Id + &apos;\&quot;&gt;&apos; + records[i].Name + &apos;&lt;/a&gt;&lt;/td&gt;&apos;; 
dialogText += &apos;&lt;td&gt;&apos; + datetimeString+ &apos;&lt;/td&gt;&apos;; 
dialogText += &apos;&lt;td&gt;&apos; + records[i].CreatedBy.Name + &apos;&lt;/td&gt;&apos;; 
dialogText += &apos;&lt;/tr&gt;&apos;; 

} 

dialogText += &apos;&lt;/tbody&gt;&apos;; 

//tfoot for download all attachment 

dialogText += &apos;&lt;tfoot&gt;&lt;tr&gt;&lt;td colspan=&quot;3&quot;&gt;&lt;input type=&quot;button&quot; class=&quot;button download-all&quot; value=&quot;Download All Attachments&quot;&gt;&lt;/input&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tfoot&gt;&lt;/table&gt;&apos;; 
} 
else { 
dialogText = &apos;&lt;p&gt;&lt;em&gt;No Related Attachment Found&lt;/em&gt;&lt;/p&gt;&apos;; 
} 



$j(document).ready(function() { 
//give the user some feedback 
showDialog(&apos;Related Attachments - PR {!ASI_MFM_Purchase_Request__c.Name}&apos;, dialogText); 
$j(&apos;.download-all&apos;).click(function() { 
downloadAll(attachments);	
window.location.reload();	
});	

});</url>
