    <webLinks>
        <fullName>ASI_MFM_KR_Make_PO</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Make PO</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/apex.js&quot;)} 
var RecordType=&quot;&quot;; 
result=sforce.connection.query(&quot;Select Id, ASI_MFM_Status__c from ASI_MFM_Purchase_Request__c where id=&apos;{!ASI_MFM_Purchase_Request__c.Id}&apos;&quot;); 
if(result.size &gt; 0){ 
records = result.getArray(&apos;records&apos;); 
if(records[0].ASI_MFM_Status__c == &apos;Approved&apos;) 
{ 
//navigateToUrl(&apos;/apex/ASI_MFM_KR_SelectProjectCodePage?Module=PO&apos;);  window.open

navigateToUrl(&apos;/apex/ASI_MFM_KR_PRNewPORedirectPage?id={!ASI_MFM_Purchase_Request__c.Id}&apos;); 
} 
else 
{ 

alert(&apos;Cannot make PO.\nReason: Status IS NOT &quot;Approved&quot;&apos;); 
} 

}</url>
