        <active>true</active>
        <errorConditionFormula>AND( CONTAINS(RecordType.DeveloperName, &apos;ASI_MFM_KR&apos;), !CONTAINS($Profile.Name,&apos;Administrator&apos;),ISCHANGED(  ASI_MFM_Prefix__c ))</errorConditionFormula>
