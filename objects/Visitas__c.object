        <fullName>Encerrar_Visita</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Encerrar Visita</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&apos;/soap/ajax/28.0/connection.js&apos;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/15.0/apex.js&quot;)}

var varType = sforce.connection.query(&quot;SELECT Channel__c FROM Account WHERE id = &apos;{! Visitas__c.ContaId__c}&apos;&quot;);
records = varType.getArray(&quot;records&quot;);

var continueWithNoAtt = true;
var logUserId = &quot;{!User.Id}&quot;;

var mobileResult = sforce.apex.execute( &quot;Utils&quot;,&quot;isMobileUser&quot;, {userId : logUserId});

if(mobileResult != &apos;&apos;){
    alert(mobileResult);
}
else{
    if (!{!Visitas__c.LAT_BR_HasAttachment__c} &amp;&amp; !{!Visitas__c.Visita_n_o_realizada__c}) {
  continueWithNoAtt = confirm(&apos;{!$Label.LAT_AnexoVisita}&apos;);
}

if (continueWithNoAtt) {
  if( records[0].Channel__c != &apos;On Trade&apos;){
    var encerramento = &apos;Encerrada&apos;; 

    if (&quot;{!Visitas__c.Status__c}&quot;==encerramento){ 
      alert(&apos;{!$Label.VF_VTS_00042_AR}&apos;); 
    }else{ 
      var visitaRec = new sforce.SObject(&apos;Visitas__c&apos;); 
      var ID = &quot;{!Visitas__c.Id}&quot;; 

      visitaRec.ID = ID; 
      visitaRec.Status__c = encerramento; 

      var result = sforce.connection.update([visitaRec]); 

      if(result[0].getBoolean(&quot;success&quot;)){ 
        window.location.reload(); 
      } else { 
        var errors = result[0].errors; 
        var errorMessages = &quot;&quot;; 
        if(errors.length &gt; 1){ 
          for(var i = 0; i &lt; errors.length; i++){ 
            errorMessages += errors[i].message + &apos;\n&apos;; 
          } 
        }else{ 
          errorMessages += errors.message; 
        } 
        alert( errorMessages ); // display all validation errors 
      } 
    }
  } else {
    if (&quot;{!Visitas__c.Status__c}&quot;==&apos;Encerrada&apos;){ 
      alert(&apos;{!$Label.VF_VTS_00042_AR}&apos;); 
    }else{
      var idVis = &quot;{!Visitas__c.Id}&quot;; 
      var resul = sforce.apex.execute( &quot;LAT_BR_BTN01_CloseVisitValidations&quot;,&quot;serviceName&quot;, {id : idVis}); 

      if (resul != &apos;&apos; &amp;&amp; !{!Visitas__c.Visita_n_o_realizada__c}) { 
        alert(resul); 
      } else {       
         window.location.href = &quot;/apex/LAT_BR_VFP25_TrainingSurvey?visita={!Visitas__c.Id}&quot;;
      }
    }
  }

}

}</url>
    </webLinks>
