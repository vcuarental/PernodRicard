    <webLinks>
        <fullName>LAT_Clientes_Validados</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Clientes Validados</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/15.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/15.0/apex.js&quot;)} 

var Ids= {!GETRECORDIDS($ObjectType.LAT_AccountInAction__c)};
var regionalId= &quot;{! LAT_ClientByRegion__c.Id }&quot;; 
var msg = &apos;&apos;;
var continueAnyway = true;
if (Ids.length &gt; 0) {
	msg = sforce.apex.execute(&quot;LAT_ClientByRegionHandler&quot;,&quot;ValidateClients&quot;,{cfIds:Ids});

	if (msg != &apos;&apos;){
		alert(msg);
	} else {
		msg = sforce.apex.execute(&quot;LAT_ClientByRegionHandler&quot;,&quot;validateSendToApproval&quot;,{regId : regionalId});
	}

} else {
	msg = &apos;Nenhum cliente foi selecionado. Deseja continuar?&apos;;
}

continueAnyway = confirm(msg);

if (msg == &apos;&apos; || continueAnyway) {
	msg = sforce.apex.execute(&quot;LAT_ClientByRegionHandler&quot;,&quot;closeTask&quot;,{taskId : regionalId});
	if (msg != &apos;&apos;) {
		alert(msg);
	}

	location.reload();
}</url>
