    <validationRules>
        <fullName>ASI_GiftBox_VN_LockConfirmedTransfer</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
	CONTAINS(RecordType.DeveloperName, &quot;ASI_GiftBox_VN&quot;),
	ISPICKVAL(PRIORVALUE(ASI_GiftBox_Status__c),&quot;Confirmed&quot;),
	OR (
		ISCHANGED(ASI_GiftBox_Quantity__c), 
		ISCHANGED(ASI_GiftBox_Status__c),
		ISCHANGED(ASI_GiftBox_Transfer_From__c),
		ISCHANGED(ASI_GiftBox_Transfer_To__c)
	)
)</errorConditionFormula>
        <errorMessage>You cannot update a confirmed transfer</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_GiftBox_VN_TransfeQuantityValidation</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
 CONTAINS( RecordType.DeveloperName , &quot;ASI_GiftBox_VN&quot;),
 ASI_GiftBox_Quantity__c &lt;= 0
)</errorConditionFormula>
        <errorDisplayField>ASI_GiftBox_Quantity__c</errorDisplayField>
        <errorMessage>Transfer Quantity must be greater than 0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_GiftBox_VN_WarehouseValidation</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
 CONTAINS( RecordType.DeveloperName , &quot;ASI_GiftBox_VN&quot;),
   TEXT(ASI_GiftBox_Transfer_From__c ) =   TEXT(ASI_GiftBox_Transfer_To__c) 
)</errorConditionFormula>
        <errorMessage>Cannot transfer from and to the same warehouse</errorMessage>
    </validationRules>
