    <webLinks>
        <fullName>ASI_CRM_VN_ClosePromotionPlan</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Close Plan</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/31.0/connection.js&quot;)} 

var publicGroupQuery = &quot;SELECT Id FROM GroupMember &quot;
                     + &quot;WHERE Group.Name = &apos;ASI CRM VN Promotion Plan Group&apos; &quot;
                     + &quot;AND UserOrGroupId = &apos;{!$User.Id}&apos; &quot;;

var publicGroupRequests = sforce.connection.query(publicGroupQuery);
var publicGroupRecords = publicGroupRequests.getArray(&quot;records&quot;);

if(publicGroupRecords.length == 0) {
	alert(&quot;Only Trade Marketing User can close the Promotion Plan!&quot;);
} else {
	var query = &quot;SELECT Id, Name FROM ASI_GiftBox_Request_Header__c &quot;
	          + &quot;WHERE ASI_CRM_Trade_A_P_Plan__c = &apos;{!ASI_CRM_Trade_A_P_Plan__c.Id}&apos; &quot;
	          + &quot;AND ASI_GiftBox_Status__c != &apos;Complete&apos; &quot;;

	var requests = sforce.connection.query(query); 
	var records = requests.getArray(&quot;records&quot;);

	if(records.length &gt; 0) {
		var nameStr = &apos;&apos;;
		for(var i in records) {
			nameStr += records[i].Name + &apos;,&apos;;
		}
		nameStr = nameStr.slice(0, -1);

		alert(&quot;You cannot close the Promotion Plan when Promotion Request records are processing (&quot; + nameStr + &quot;)&quot;);
	} else {
		var rec = new sforce.SObject(&apos;ASI_CRM_Trade_A_P_Plan__c&apos;); 
		rec.Id = &apos;{!ASI_CRM_Trade_A_P_Plan__c.Id}&apos;;
		rec.ASI_CRM_Status__c = &apos;Closed&apos;;
		try{ 
			result = sforce.connection.update([rec]); 
			window.open(&quot;/{!ASI_CRM_Trade_A_P_Plan__c.Id}&quot;,&quot;_self&quot;); 
		} catch(error) { 
			sforce.debug.log(error.faultcode); 
			sforce.debug.log(error.faultstring); 
		} 
	}
}</url>
    </webLinks>
    <webLinks>
        <fullName>ASI_CRM_VN_Submit_for_Approval</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Submit for Approval</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!requireScript(&quot;/soap/ajax/31.0/connection.js&quot;)} 

var tradeAPLineItemQuery = sforce.connection.query(&quot;Select Id from ASI_CRM_Trade_A_P_Plan_Item__c where ASI_CRM_Trade_A_P_Plan__c = &apos;{!ASI_CRM_Trade_A_P_Plan__c.Id}&apos;&quot;); 
var tradeAPLineItemRecords = tradeAPLineItemQuery.getArray(&quot;records&quot;); 

if(tradeAPLineItemRecords.length == 0) { 
	alert(&quot;This Trade A&amp;P Plan does not contain any item!&quot;); 
} else { 
	var plan = new sforce.SObject(&apos;ASI_CRM_Trade_A_P_Plan__c&apos;);  
	plan.Id = &apos;{!ASI_CRM_Trade_A_P_Plan__c.Id}&apos;;
	plan.ASI_CRM_Sys_Pending_Approval__c = true;

	try{ 
		result = sforce.connection.update([plan]); 
		window.open(&quot;/apex/ASI_SubmitApprovalPage?id={!ASI_CRM_Trade_A_P_Plan__c.Id}&quot;,&quot;_self&quot;); 
	}catch(error){ 
		alert(error.faultstring); 
	} 
}</url>
    </webLinks>
