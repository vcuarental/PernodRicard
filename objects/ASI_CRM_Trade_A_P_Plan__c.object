    <validationRules>
        <fullName>ASI_CRM_VN_CheckFiscalYear</fullName>
        <active>true</active>
        <errorConditionFormula>OR
(
	AND(
		MONTH(ASI_CRM_Start_Date__c) &lt;= 6,
		OR(
			MONTH(ASI_CRM_End_Date__c) &gt; 6,
			YEAR(ASI_CRM_Start_Date__c) &lt; YEAR(ASI_CRM_End_Date__c)
		)
	),
	AND(
		MONTH(ASI_CRM_Start_Date__c) &gt; 6,
		MONTH(ASI_CRM_End_Date__c) &gt; 6,
		YEAR(ASI_CRM_Start_Date__c) &lt; YEAR(ASI_CRM_End_Date__c)
	)
)</errorConditionFormula>
        <errorMessage>The start date and end date do not match the fiscal year.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_VN_CheckStartDate</fullName>
        <active>true</active>
        <errorConditionFormula>ASI_CRM_End_Date__c &lt; ASI_CRM_Start_Date__c</errorConditionFormula>
        <errorDisplayField>ASI_CRM_Start_Date__c</errorDisplayField>
        <errorMessage>Start date should be smaller than end date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_VN_CheckTotalOutlet</fullName>
        <active>true</active>
        <errorConditionFormula>ASI_CRM_Total_Outlet__c &lt;= 0</errorConditionFormula>
        <errorDisplayField>ASI_CRM_Total_Outlet__c</errorDisplayField>
        <errorMessage>Total Outlet should be greater than 0</errorMessage>
    </validationRules>
