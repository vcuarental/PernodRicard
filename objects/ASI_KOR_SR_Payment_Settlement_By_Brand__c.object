    <fields>
        <fullName>ASI_KOR_Progression__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
  AND(
    (ISBLANK(ASI_KOR_Target_Volume__c)||ASI_KOR_Target_Volume__c==0),
    (ISBLANK(ASI_KOR_Actual_Volume__c)||ASI_KOR_Actual_Volume__c==0)
  ),
  0,
  IF(
    (ISBLANK(ASI_KOR_Target_Volume__c)||ASI_KOR_Target_Volume__c==0),
    1,
    ASI_KOR_Actual_Volume__c / ASI_KOR_Target_Volume__c
  )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
