.appendTo(&apos;body&apos;); 

theAnchor[0].click(); 
theAnchor.remove(); 
downloadAll(files); 
} 


//the vars 
var relatedAttachments, 
dialogText, 
records, 

//the query 
relatedAttachments = sforce.connection.query(&quot;SELECT Id, Name, ParentId, LastModifiedDate, CreatedBy.Name FROM Attachment where ParentId = &apos;{!ASI_MFM_Payment__c.Id}&apos;&quot;); 

//the results 
records = relatedAttachments.getArray(&quot;records&quot;); 

//if we have results, do something with them, else, show a message 
