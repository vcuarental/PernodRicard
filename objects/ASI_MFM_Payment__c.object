if ((records[0].ASI_MFM_Payment_Amount__c != records[0].ASI_MFM_Payment_Amt_Inv__c) &amp;&amp; records[0].ASI_MFM_CN_SYS_Count_Payment_Line_Items__c &gt; 0){
