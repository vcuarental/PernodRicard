        <url>console.log(&apos;Entered&apos;);
{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/connection.js&quot;)}
try {
    var recordTypeDevName = null;
    var records = null;
    var recordTypeId = null;
    
    if(&apos;{!ASI_MFM_Payment__c.RecordType}&apos; == &apos;CAP Payment&apos; || &apos;{!ASI_MFM_Payment__c.RecordType}&apos; == &apos;CAP Payment Read-Only&apos;) {
        recordTypeDevName = &apos;ASI_MFM_CAP_Payment&apos;
    }
    if(&apos;{!ASI_MFM_Payment__c.RecordType}&apos; == &apos;CAP T&amp;E Payment&apos;) {
        recordTypeDevName = &apos;ASI_MFM_CAP_T_E_Payment&apos;
    }
    if(&apos;{!ASI_MFM_Payment__c.RecordType}&apos; == &apos;CAP TW Payment&apos; || &apos;{!ASI_MFM_Payment__c.RecordType}&apos; == &apos;CAP TW Payment Read-Only&apos;) {
        recordTypeDevName = &apos;ASI_MFM_CAP_TW_Payment&apos;
    }
    if(&apos;{!ASI_MFM_Payment__c.RecordType}&apos; == &apos;OPEX SG Payment&apos; || &apos;{!ASI_MFM_Payment__c.RecordType}&apos; == &apos;OPEX SG Payment Read-Only&apos;) {
        recordTypeDevName = &apos;ASI_MFM_CAP_SG_Payment&apos;
    }
    if(&apos;{!ASI_MFM_Payment__c.RecordType}&apos; == &apos;CAP TW T&amp;E Payment&apos;) {
        recordTypeDevName = &apos;ASI_MFM_CAP_TW_T_E_Payment&apos;
    }
    var queryString = &apos;SELECT Id FROM RecordType WHERE DeveloperName = \&apos;&apos; + recordTypeDevName + &apos;\&apos; AND SObjectType = \&apos;ASI_MFM_Payment__c\&apos; LIMIT 1&apos;;
    
    var recordTypes = sforce.connection.query(queryString);
    
    if(recordTypes.getBoolean(&apos;done&apos;) &amp;&amp; recordTypes.getInt(&apos;size&apos;)&gt;=0) {
        records = recordTypes.getArray(&apos;records&apos;); 
        recordTypeId = records[0].Id;
    }

	//Wilken 20170607: Default cloned CAPEX Payment Status as Draft
	var statusFieldID = null;
	/* Prod Org ID */
	
	if(&apos;{!$Organization.Id}&apos; == &apos;00DD0000000qC0e&apos;)
		statusFieldID = &apos;00ND0000005pd49&apos;;
	/* B5 Org ID */
	if(&apos;{!$Organization.Id}&apos; == &apos;00DM00000011xOo&apos;)
		statusFieldID = &apos;00NM0000000r5Oz&apos;;
		
    if(recordTypeId != null) {
		if(&apos;{!ASI_MFM_Payment__c.RecordType}&apos; == &apos;CAP Payment&apos; || &apos;{!ASI_MFM_Payment__c.RecordType}&apos; == &apos;CAP Payment Read-Only&apos;){
			window.location.assign(&apos;/{!ASI_MFM_Payment__c.Id}/e?clone=1&amp;RecordType=&apos; + recordTypeId + &apos;&amp;&apos; + statusFieldID + &apos;=Draft&apos;);
		}
		else {
			window.location.assign(&apos;/{!ASI_MFM_Payment__c.Id}/e?clone=1&amp;RecordType=&apos; + recordTypeId);
		}
		
        console.log(window.location);
    } else {
        alert(&apos;There seems to be an error.&apos;);
    }

} catch(e) {
    alert(&apos;An error has occurred. Error:\n&apos; + e);
}</url>
