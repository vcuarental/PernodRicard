window.location.reload();
}
else if (detailRecords == null || detailRecords.length &lt;= 0){
alert(&quot;For Payment from PO owned by you, please add Payment Line Items before submitting for approval.&quot;);
}

else{
rec.ASI_MFM_Sys_Allow_Submit_Approval__c = true;
result = sforce.connection.update([rec]);

window.open(&quot;/apex/ASI_MFM_CAP_PaymentAssignApproverPage?id={!ASI_MFM_Payment__c.Id}&quot;,&quot;_self&quot;);
} */

if (detailRecords == null || detailRecords.length &lt;= 0){
alert(&quot;Please add Payment Line Items before submitting for approval.&quot;);
}
else{
rec.ASI_MFM_Sys_Allow_Submit_Approval__c = true;
result = sforce.connection.update([rec]);

