    <webLinks>
        <fullName>ASI_MFM_SC_Submit_for_Approval</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Submit for Approval</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!requireScript(&quot;/soap/ajax/42.0/connection.js&quot;)}
sforce.connection.sessionId = &quot;{!$Api.Session_ID}&quot;;

var currentHeader = sforce.connection.query(&quot;Select ASI_MFM_Status__c, Id, ASI_MFM_Payment_Type__c, ASI_MFM_OCR__c, ASI_MFM_Plan__c , ASI_MFM_Payment_Amount__c,ASI_MFM_Payment_Amt_Inv__c from ASI_MFM_Payment__c where Id = &apos;{!ASI_MFM_Payment__c.Id}&apos;&quot;);

var currentPO=sforce.connection.query(&quot;Select id,ASI_MFM_Remaining_Balance__c,ASI_MFM_Status__c from ASI_MFM_po__c where name = &apos;{!ASI_MFM_Payment__c.ASI_MFM_PO__c}&apos;&quot;);

var currentDetails = sforce.connection.query(&quot;Select Id ,ASI_MFM_BU__c,ASI_MFM_AP_Code__c,ASI_MFM_AP_Code__r.asi_mfm_sc_Need_Subleger__c,ASI_MFM_SC_Subledger__c,ASI_MFM_Payment_List_Item_Description__c from ASI_MFM_Payment_Line_Item__c where ASI_MFM_Payment__c = &apos;{!ASI_MFM_Payment__c.Id}&apos; &quot;);

var currentAttachments = sforce.connection.query(&quot;SELECT Id,Name,ContentType FROM Attachment WHERE ParentID = &apos;{!ASI_MFM_Payment__c.Id}&apos; &quot;);

var records = currentHeader.getArray(&quot;records&quot;);
var detailRecords = currentDetails.getArray(&quot;records&quot;);
var attachmentRecords = currentAttachments.getArray(&quot;records&quot;);
var invalidPayment = false;
var poL = currentPO.getArray(&quot;records&quot;);
var po=poL[0];

var checkDetailPass=true;
var i;

var bypassval =&apos;{! INCLUDES( $User.BypassTriggers__c, &apos;ASI_MFM_SC_AP_Subledger&apos;)}&apos;; 

for(i=0;i&lt;detailRecords.length;i++){ 
	if(detailRecords[i].ASI_MFM_BU__c==null){
		checkDetailPass=false;
		alert(&apos;Missing BU in detail&apos;);
		break;
	}else if(detailRecords[i].ASI_MFM_AP_Code__c==null){
		checkDetailPass=false;
		alert(&apos;Missing AP Code in detail&apos;);
		break;
	}else if(detailRecords[i].ASI_MFM_AP_Code__r.asi_mfm_sc_Need_Subleger__c==&apos;true&apos; &amp;&amp; detailRecords[i].ASI_MFM_SC_Subledger__c==null &amp;&amp; bypassval ==&apos;false&apos;  ){
		checkDetailPass=false;
		alert(&apos;Missing Subledger in detail&apos;);
		break;
	}else if(detailRecords[i].ASI_MFM_Payment_List_Item_Description__c==null){
		checkDetailPass=false;
		alert(&apos;Missing Payment Description in detail&apos;);
		break;
	}
}
if(checkDetailPass){
	if (detailRecords == null || detailRecords.length &lt;= 0){
		alert(&quot;Before submit for approval, add Payment Line Items.&quot;);
	}
        else if ({!ISBLANK(  ASI_MFM_Payment__c.ASI_MFM_SC_Supplier_Name__c   )} ) 
        { 
                alert(&quot;Before submit for approval, please fill the Payment Supplier&quot;); 
        }
	else if(po.ASI_MFM_Remaining_Balance__c&lt;0){
		alert(&quot;PO over spent.&quot; +po.ASI_MFM_Remaining_Balance__c);
	}
	else if(po.ASI_MFM_Status__c!=&apos;Final&apos;){
		alert(&quot;PO is not in &apos;Finial&apos; Status&quot;);
	}
	else if({!ISBLANK(ASI_MFM_Payment__c.ASI_MFM_Payment_Amount__c) || ROUND(ASI_MFM_Payment__c.ASI_MFM_Payment_Amount__c,2)!= 		ROUND(ASI_MFM_Payment__c.ASI_MFM_Payment_Amt_Inv__c,2)}){
		alert(&quot;Payment Amount is not same as invoice total&quot;);
	}
	else if (records[0].ASI_MFM_Status__c== &apos;Draft&apos;){
		var withAttchment=true;
		if((attachmentRecords == null || attachmentRecords.length &lt;=0)){
			if({!ASI_MFM_Payment__c.ASI_MFM_OCR__c}){
				withAttchment=false;
				alert(&quot;Before submit for approval, add attachments.&quot;);
			}else{
				if (confirm(&apos;No attachments.&apos;)) {
					withAttchment=true;
				} else {
					withAttchment=false;
				}
			}	
		}
		
		if(withAttchment){
			try {
				var rec = new sforce.SObject(&apos;ASI_MFM_Payment__c&apos;);
				rec.Id = &quot;{!ASI_MFM_Payment__c.Id}&quot;;
				rec.ASI_MFM_Sys_Allow_Submit_Approval__c = true;
rec.asi_mfm_submitter__c=&quot;{!$User.Id}&quot;;
				result = sforce.connection.update([rec]);

				window.open(&quot;/apex/ASI_MFM_CAP_PaymentAssignApproverPage?id={!ASI_MFM_Payment__c.Id}&quot;,&quot;_self&quot;);

			} catch(error) {
				sforce.debug.log(error.faultcode);
				sforce.debug.log(error.faultstring);
			}
		}
	}
}</url>
