if ((Modal.confirm &amp;&amp; Modal.confirm(&apos;Set to Approved. Continue?&apos;)) ) 
navigateToUrl(&apos;/apex/ASI_MFM_SetPaymentStatusPage?id={!ASI_MFM_Payment__c.Id}&amp;status=Approved&apos;);
 
}else{ 
alert(&apos;You do not have sufficient privileges to change Payment Status.&apos;); 
}</url>
