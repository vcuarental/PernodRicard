    <validationRules>
        <fullName>ASI_MFM_CAP_BlockDraftPO</fullName>
        <active>true</active>
        <description>Payment can only be made on a Final PO</description>
        <errorConditionFormula>AND (
 CONTAINS( RecordType.DeveloperName , &quot;ASI_MFM_CAP_Payment&quot;),
 NOT(ISPICKVAL( ASI_MFM_CAP_PO__r.ASI_MFM_Status__c , &quot;Final&quot;)),
!INCLUDES($User.BypassTriggers__c,&apos;ASI_MFM_CAP_BlockDraftPO&apos;) 
)</errorConditionFormula>
        <errorDisplayField>ASI_MFM_PO__c</errorDisplayField>
        <errorMessage>Payment can only be made on a Final PO</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_CAP_CAPEX_Plan_Validation</fullName>
        <active>false</active>
        <description>Plan is required for CAPEX Payment.</description>
        <errorConditionFormula>ASI_MFM_Is_Direct_Payment__c &amp;&amp; ISPICKVAL( ASI_MFM_Payment_Type__c , &apos;CAPEX&apos;) &amp;&amp; !ISPICKVAL(ASI_MFM_Plan__r.ASI_MFM_Plan_Type__c , &apos;CAPEX&apos;) &amp;&amp; RecordType.DeveloperName = &apos;ASI_MFM_CAP_Payment&apos;</errorConditionFormula>
        <errorDisplayField>ASI_MFM_Plan__c</errorDisplayField>
        <errorMessage>A CAPEX Plan is required if payment type is CAPEX.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_CAP_GoLivePlanPaymentCheck</fullName>
        <active>true</active>
        <description>CAPEX Plan that already go live can only issuing payment from accrual PO</description>
        <errorConditionFormula>AND (
	CONTAINS( RecordType.DeveloperName , &quot;ASI_MFM_CAP_Payment&quot;),
	NOT(ISBLANK(ASI_MFM_Plan__r.ASI_MFM_Actual_Depreciation_Start_Date__c)),
	ISPICKVAL( ASI_MFM_Payment_Type__c , &quot;CAPEX&quot;),
	OR ( ASI_MFM_Is_Direct_Payment__c , ASI_MFM_OCR__c )
        ,AND(
             NOT(ISPICKVAL(ASI_MFM_Status__c, &quot;Final&quot;)), 
             NOT(ISPICKVAL(ASI_MFM_Status__c, &quot;Finance Verified&quot;))
         )
)</errorConditionFormula>
        <errorMessage>CAPEX Plan is already live, please create payment from Accrual PO only.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_CAP_HK_Supplier_RecordType</fullName>
        <active>false</active>
        <description>Only HK Supplier can be selected if CAP Payment created</description>
        <errorConditionFormula>AND( 
RecordType.DeveloperName == &apos;ASI_MFM_CAP_Payment&apos;, 
ASI_MFM_Supplier_Name__r.RecordTypeId &lt;&gt; &apos;012D00000007Jvh&apos; 
)</errorConditionFormula>
        <errorDisplayField>ASI_MFM_Supplier_Name__c</errorDisplayField>
        <errorMessage>Only HK Supplier can be selected if CAP Payment created</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_CAP_JP_Finance_GLDate</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
	CONTAINS( RecordType.DeveloperName , &quot;ASI_MFM_CAP_JP&quot;),
	OR ( 
		ISPICKVAL( ASI_MFM_Status__c , &quot;Final&quot;) ,
		ISPICKVAL( ASI_MFM_Status__c , &quot;Finance Verified&quot;)
	),
	ISCHANGED( ASI_MFM_G_L_Date__c ),
	AND (
		$UserRole.DeveloperName != &quot;ASI_JP_Accounting_Team&quot;,
		NOT(CONTAINS($Profile.Name, &quot;Admin&quot;))
	)
)</errorConditionFormula>
        <errorDisplayField>ASI_MFM_G_L_Date__c</errorDisplayField>
        <errorMessage>G/L Date can only be updated by Accounting Team</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_CAP_JP_Finance_Status</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
	CONTAINS( RecordType.DeveloperName , &quot;ASI_MFM_CAP_JP&quot;),
	OR ( 
		ISPICKVAL(PRIORVALUE(ASI_MFM_Status__c),&quot;Final&quot;),
		ISPICKVAL(PRIORVALUE(ASI_MFM_Status__c),&quot;Finance Verified&quot;)
	),
	ISCHANGED(ASI_MFM_Status__c ) ,
	AND (
		$UserRole.DeveloperName != &quot;ASI_JP_Accounting_Team&quot;,
		NOT(CONTAINS($Profile.Name, &quot;Admin&quot;))
	)
)</errorConditionFormula>
        <errorDisplayField>ASI_MFM_Status__c</errorDisplayField>
        <errorMessage>Status can only be updated by Accounting Team</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_CAP_Lock_Payment_Type</fullName>
        <active>false</active>
        <errorConditionFormula>CONTAINS(RecordType.DeveloperName,&apos;ASI_MFM_CAP&apos;) &amp;&amp; !ASI_MFM_OCR__c &amp;&amp; ISCHANGED(ASI_MFM_Payment_Type__c) &amp;&amp;  ASI_MFM_CN_SYS_Count_Payment_Line_Items__c &gt; 0</errorConditionFormula>
        <errorDisplayField>ASI_MFM_Payment_Type__c</errorDisplayField>
        <errorMessage>You are NOT allowed to change Payment Type once any payment line is created</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_CAP_Lock_Status</fullName>
        <active>true</active>
        <description>Prevent normal user to change status after approval.
For Brand Owner Invoice of:
- PRHK</description>
        <errorConditionFormula>/* Prevent user to change status of Approved Payment to either Final or Draft. 

Criteria: 
* Record Type match OPEX HK Payment Read-Only 
* Status is changed to either Draft or Final 
* Bypass Finance Users 
*/

(RecordType.DeveloperName = &apos;ASI_MFM_CAP_HK_Payment_Read_Only&apos; &amp;&amp; 

(OR(ISPICKVAL(ASI_MFM_Status__c, &apos;Draft&apos;), ISPICKVAL(ASI_MFM_Status__c, &apos;Final&apos;)) &amp;&amp; ISCHANGED(ASI_MFM_Status__c)))

&amp;&amp; !AND(CONTAINS($Setup.ASI_MFM_Validation_Bypass__c.ASI_MFM_Object_API__c, &apos;ASI_MFM_Payment__c&apos;), CONTAINS($Setup.ASI_MFM_Validation_Bypass__c.ASI_MFM_Record_Type__c, &apos;ASI_MFM_CAP_HK_Payment_Read_Only&apos;), $Setup.ASI_MFM_Validation_Bypass__c.ASI_MFM_Bypass_Validation__c)</errorConditionFormula>
        <errorDisplayField>ASI_MFM_Status__c</errorDisplayField>
        <errorMessage>You are not allowed to update Status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_CAP_OPEX_Plan_Validation</fullName>
        <active>false</active>
        <errorConditionFormula>ASI_MFM_Is_Direct_Payment__c &amp;&amp; ISPICKVAL( ASI_MFM_Payment_Type__c , &apos;OPEX&apos;) &amp;&amp; !ISPICKVAL(ASI_MFM_Plan__r.ASI_MFM_Plan_Type__c , &apos;OPEX&apos;) &amp;&amp; RecordType.DeveloperName = &apos;ASI_MFM_CAP_Payment&apos;</errorConditionFormula>
