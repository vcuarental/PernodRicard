        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Submit</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/connection.js&quot;)} 
sforce.connection.sessionId = &quot;{!$Api.Session_ID}&quot;; 

var PaymentStatus = &quot;{! ASI_MFM_Payment__c.ASI_MFM_Status__c}&quot;;

if (PaymentStatus == &quot;Draft&quot; || PaymentStatus == &quot;Invalid&quot;){
    var attachments = sforce.connection.query(&quot;SELECT Id, ParentId FROM Attachment WHERE ParentId = &apos;{!ASI_MFM_Payment__c.Id}&apos;&quot;); 
    var records = attachments.getArray(&quot;records&quot;); 

    if(records.length == 0)
    {
        alert(&quot;Please attach Invoice before submission.&quot;);
    }
    else
    {
        var Payment = new sforce.SObject(&quot;ASI_MFM_Payment__c&quot;);
        Payment.Id = &apos;{! ASI_MFM_Payment__c.Id}&apos;; 

        Payment.ASI_MFM_Status__c = &quot;Pending Goods/Service Receipt&quot;;

        var result = sforce.connection.update([Payment]);

        if (result[0].success==&apos;false&apos;) { 
            alert(result[0].errors.message); 
        } 
        else { 
            window.location.reload();
        }
    }
}
else{
    alert(&quot;Only Draft/Invalid Payment can be submitted.&quot;);
}</url>
