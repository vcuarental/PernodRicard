    <validationRules>
        <fullName>ASI_MFM_CAP_PaymentPlanFiscalYearCheck</fullName>
        <active>true</active>
        <description>Direct Payment and OCR Payment can only be made on current fiscal year CAPEX Plan</description>
        <errorConditionFormula>AND (
 CONTAINS( RecordType.DeveloperName , &quot;ASI_MFM_CAP_Payment&quot;),
 NOT(ISBLANK( ASI_MFM_Plan__c )),
  TEXT(ASI_MFM_Prefix__r.ASI_MFM_Fiscal_year__c)  !=  TEXT(ASI_MFM_Plan__r.ASI_MFM_Fiscal_year__c),
OR (ASI_MFM_Is_Direct_Payment__c, ASI_MFM_OCR__c ),
 ISPICKVAL( ASI_MFM_Payment_Type__c , &quot;CAPEX&quot;) 
