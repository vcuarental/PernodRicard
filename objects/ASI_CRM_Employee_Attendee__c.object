var recTypeId = recType.getArray(&quot;records&quot;)[0].Id; 

window.open(&quot;/aIf/e?CF00ND0000005s2DE={!ASI_CRM_Call_Detail__c.Name}&amp;RecordType=&quot; + recTypeId + &quot;&amp;retURL={!ASI_CRM_Call_Detail__c.Id}&quot;,&quot;_self&quot;);</url>
