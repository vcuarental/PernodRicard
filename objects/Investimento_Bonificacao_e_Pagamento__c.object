    <webLinks>
        <fullName>Gerar_Pagamento</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Gerar Pagamento</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/15.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/15.0/apex.js&quot;)} 

var records = {!GETRECORDIDS($ObjectType.Investimento_Bonificacao_e_Pagamento__c)}; 

var idToUpdate=&quot;{!Account.Id }&quot;; 

if ( records==&apos;&apos; ) alert(&apos;Pelo menos um item deve ser selecionado&apos;);
else
{
  var sMsg = sforce.apex.execute(&quot;PagamentoGerar&quot;,&quot;gerar&quot;,{aIBPs:records, aContaID:idToUpdate}); 
  if (sMsg[0].length == 18 ) window.location.href= &quot;/&quot; + sMsg; 
  else alert( sMsg );
}</url>
    </webLinks>
