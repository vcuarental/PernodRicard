    <fields>
        <fullName>ASI_CRM_Gross_Sales_Impact__c</fullName>
        <externalId>false</externalId>
        <formula>/* modify VAT rate from 1.17 to 1.16 */
/* modify VAT rate from 1.16 to 1.13 */
ABS( IF(
(( ASI_CRM_Price_And_Discount__r.RecordType.DeveloperName = &quot;ASI_CRM_CN_Customer_Price_and_Discount_Request&quot; || ASI_CRM_Price_And_Discount__r.RecordType.DeveloperName = &quot;ASI_CRM_CN_Customer_Price_and_Discount_Request_RO&quot;))
, IF( NOT(ISBLANK(ASI_CRM_Promotion_Code__c ) ) &amp;&amp;  ASI_CRM_Buy_X__c &gt; 0 &amp;&amp;  ASI_CRM_Get_Y__c &gt; 0, IF( ASI_CRM_Total_Estimated_Vol_BT__c * ( (ASI_CRM_New_Price_BT_non_VAT__c * ( ASI_CRM_Buy_X__c/ (ASI_CRM_Buy_X__c + ASI_CRM_Get_Y__c ) ) ) - (ASI_CRM_Current_T1_Price_BT_with_VAT__c/IF( ASI_CRM_CN_Tax_Rate__c == 0 , 1.13,1+ASI_CRM_CN_Tax_Rate__c ))) &lt; 0
, ASI_CRM_Total_Estimated_Vol_BT__c * ( (ASI_CRM_New_Price_BT_non_VAT__c * ( ASI_CRM_Buy_X__c/(ASI_CRM_Buy_X__c + ASI_CRM_Get_Y__c ) ) ) - (ASI_CRM_Current_T1_Price_BT_with_VAT__c/IF( ASI_CRM_CN_Tax_Rate__c == 0 , 1.13,1+ASI_CRM_CN_Tax_Rate__c ))), 0)
, IF( ( ASI_CRM_New_Price_BT_non_VAT__c &lt; (ASI_CRM_Current_T1_Price_BT_with_VAT__c/IF( ASI_CRM_CN_Tax_Rate__c == 0 , 1.13,1+ASI_CRM_CN_Tax_Rate__c )) )
, ASI_CRM_Total_Estimated_Vol_BT__c * ( (ASI_CRM_New_Price_BT_non_VAT__c ) - (ASI_CRM_Current_T1_Price_BT_with_VAT__c/IF( ASI_CRM_CN_Tax_Rate__c == 0 , 1.13,1+ASI_CRM_CN_Tax_Rate__c ))), 0)), 0))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Gross Sales Impact</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
