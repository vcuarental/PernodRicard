    <validationRules>
        <fullName>ASI_MFM_AC_Mandatory_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>(RecordType.DeveloperName = &apos;ASI_MFM_TW_PO_Line_Item&apos;) &amp;&amp; ISBLANK( ASI_MFM_A_C_Code__c )</errorConditionFormula>
        <errorDisplayField>ASI_MFM_A_C_Code__c</errorDisplayField>
        <errorMessage>A/C Code is Mandatory for selection</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_CAP_GLDatePeriodCheck</fullName>
        <active>false</active>
        <description>GL Date must be within PO Period</description>
        <errorConditionFormula>AND (
	CONTAINS( RecordType.DeveloperName , &quot;ASI_MFM_CAP_PO_Line_Item&quot;),
	OR (
		ASI_MFM_G_L_Date__c &lt;  ASI_MFM_PO__r.ASI_MFM_PO_Start_Date__c  ,
		ASI_MFM_G_L_Date__c &gt;  ASI_MFM_PO__r.ASI_MFM_PO_End_Date__c  
	)
)</errorConditionFormula>
        <errorDisplayField>ASI_MFM_G_L_Date__c</errorDisplayField>
        <errorMessage>G/L Date must be within PO Period</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_CN_BlockChangingACnSubbrand</fullName>
        <active>true</active>
        <description>If receipt amount &gt; 0, A/C Code and Subbrand cannot be changed</description>
        <errorConditionFormula>CONTAINS( RecordType.DeveloperName, &apos;ASI_MFM_CN_PO_Line_Item&apos;) &amp;&amp; ASI_MFM_CN_Receipt_Amount__c != 0 &amp;&amp; NOT(ISBLANK(ASI_MFM_CN_Receipt_Amount__c)) &amp;&amp; ( ISCHANGED(ASI_MFM_A_C_Code__c) || ISCHANGED(ASI_MFM_Sub_brand_Code__c)) &amp;&amp; $Profile.Name != &apos;ASI CN IT System Administrator&apos; &amp;&amp; $Profile.Name != &apos;ASI CN Finance User&apos; &amp;&amp; $Profile.Name != &apos;ASI CN BA User&apos;</errorConditionFormula>
        <errorMessage>You are NOT allowed to change A/C Code and Subbrand once PO has receipt</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_CN_Receipt_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>(RecordType.DeveloperName = &apos;ASI_MFM_CN_PO_Line_Item&apos;) &amp;&amp; ABS(ASI_MFM_CN_Receipt_Amount__c) &gt; ABS(ASI_MFM_Amount__c)+0.1</errorConditionFormula>
        <errorDisplayField>ASI_MFM_Amount__c</errorDisplayField>
        <errorMessage>PO Amount Cannot Be Smallar Than Receipt Amount</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_GF_DistAmount_CannotExceed_POAmt</fullName>
        <active>true</active>
        <description>Distributor Amount cannot be greater than PO Line Amount</description>
        <errorConditionFormula>AND ( CONTAINS( RecordType.DeveloperName , &quot;ASI_MFM_GF&quot;) ,
 ASI_MFM_Distributor_Amount__c &gt;  ASI_MFM_Amount__c 
)</errorConditionFormula>
        <errorDisplayField>ASI_MFM_Distributor_Amount__c</errorDisplayField>
        <errorMessage>Distributor Amount cannot exceed PO Amount</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_GF_Distributor_PRG_Amt_Validate</fullName>
        <active>false</active>
        <errorConditionFormula>AND (
 CONTAINS( RecordType.DeveloperName , &quot;ASI_MFM_GF&quot;),
ASI_MFM_Paid_Amount_in_PO_Currency_PRG__c &gt; ASI_MFM_GF_PRG_Amount__c
)</errorConditionFormula>
        <errorMessage>Payment Amount (PR Gulf) cannot be greater than PO Amount (PR Gulf)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_GF_EventFOC_APCode_Check</fullName>
        <active>true</active>
        <errorConditionFormula>AND ( CONTAINS(RecordType.DeveloperName, &quot;ASI_MFM_GF&quot;),
   ASI_MFM_PO_Type__c == &quot;Event FOC&quot;,
 ASI_MFM_AP_Code__r.Name != &quot;Sponsorship [Activation]&quot;,
 ASI_MFM_AP_Code__r.Name != &quot;Customer and Trade Event [Trade AP]&quot;,
 ASI_MFM_AP_Code__r.Name != &quot;Distributor Reconciliation [Activation]&quot;
)</errorConditionFormula>
        <errorDisplayField>ASI_MFM_AP_Code__c</errorDisplayField>
        <errorMessage>Please select Consumer Event, Trade Event or Distributor Reconciliation for Event FOC PO</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_GF_GLDate_With_PO_Date_Range</fullName>
        <active>false</active>
        <errorConditionFormula>AND (
	CONTAINS( RecordType.DeveloperName , &quot;ASI_MFM_GF&quot;),
	OR (
		ASI_MFM_G_L_Date__c &gt;  ASI_MFM_PO__r.ASI_MFM_PO_End_Date__c,
		ASI_MFM_G_L_Date__c &lt;  ASI_MFM_PO__r.ASI_MFM_PO_Start_Date__c 
	),
NOT(ISNEW())
)</errorConditionFormula>
        <errorDisplayField>ASI_MFM_G_L_Date__c</errorDisplayField>
        <errorMessage>G/L Date has to be within PO Start Date and End Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_GF_OverReceiptValidation</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
 CONTAINS( RecordType.DeveloperName , &quot;ASI_MFM_GF&quot;),
  ABS(ASI_MFM_CN_Receipt_Amount__c) &gt; ABS(ASI_MFM_Amount__c) 
)</errorConditionFormula>
        <errorMessage>The PO Line has already been receipt.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_GF_POLI_SubBrand_Mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>AND
(
 CONTAINS(RecordType.DeveloperName, &quot;ASI_MFM_GF&quot;),
OR
(
  ISBLANK(ASI_MFM_Sub_brand_Code__c )&amp;&amp; ISBLANK( ASI_MFM_Sub_brand_Allocation_Type__c ),
  NOT(ISBLANK(ASI_MFM_Sub_brand_Code__c ))&amp;&amp; NOT(ISBLANK( ASI_MFM_Sub_brand_Allocation_Type__c))
)
)</errorConditionFormula>
        <errorMessage>Please select Sub-brand or Sub-brand Allocation</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_GF_POLine_Mandatory_Check</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
	CONTAINS( RecordType.DeveloperName , &quot;ASI_MFM_GF&quot;),
	OR (
		ISBLANK( ASI_MFM_Customer_Name__c ),
		ISBLANK(  ASI_MFM_AP_Code__c )
	),
	NOT(CONTAINS($Profile.Name, &quot;Administrator&quot;))
)</errorConditionFormula>
        <errorMessage>Please select all required fields</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_GF_Recharge_Percent_Mandatory</fullName>
        <active>false</active>
        <errorConditionFormula>AND ( CONTAINS(RecordType.DeveloperName, &quot;ASI_MFM_GF&quot;),
   OR (ASI_MFM_PO_Type__c == &quot;Normal&quot;, ASI_MFM_PO_Type__c == &quot;Event FOC&quot;),
   ASI_MFM_AP_Code__r.Name = &quot;Distributor Reconciliation&quot;,
   ISBLANK( ASI_MFM_Recharge__c ) 
)</errorConditionFormula>
        <errorDisplayField>ASI_MFM_Recharge__c</errorDisplayField>
        <errorMessage>Please enter recharge % for Distributor Reconciliation activity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_GF_Recharge_Percent_Over100</fullName>
        <active>false</active>
        <errorConditionFormula>AND ( 
CONTAINS(RecordType.DeveloperName, &quot;ASI_MFM_GF&quot;), 
ASI_MFM_Recharge__c &gt; 1
)</errorConditionFormula>
        <errorDisplayField>ASI_MFM_Recharge__c</errorDisplayField>
        <errorMessage>Recharge Percentage cannot exceed 100%</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_GF_Recharge_Percent_Validation</fullName>
        <active>false</active>
        <errorConditionFormula>AND ( CONTAINS(RecordType.DeveloperName, &quot;ASI_MFM_GF&quot;),
   ASI_MFM_AP_Code__r.Name != &quot;Distributor Reconciliation&quot;,
   NOT(ISBLANK(ASI_MFM_Recharge__c))
)</errorConditionFormula>
        <errorDisplayField>ASI_MFM_Recharge__c</errorDisplayField>
        <errorMessage>Please clear recharge % for activities other than Distributor Reconciliation.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_GF_SKU_Mandatory_For_FOC_PO</fullName>
        <active>true</active>
        <description>PR Gulf: SKU is mandatory to input for PO with FOC type</description>
        <errorConditionFormula>AND (
 CONTAINS( RecordType.DeveloperName , &quot;ASI_MFM_GF&quot;),
   ASI_MFM_PO_Type__c == &quot;VAP FOC&quot;,
 ISBLANK( ASI_MFM_SKU_Code__c ) 
)</errorConditionFormula>
        <errorDisplayField>ASI_MFM_SKU_Code__c</errorDisplayField>
        <errorMessage>Please select SKU for VAP FOC PO</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_GLDate_After_PO_StartDate</fullName>
        <active>true</active>
        <errorConditionFormula>AND( OR(CONTAINS( RecordType.DeveloperName , &quot;ASI_MFM_TH&quot;),CONTAINS( RecordType.DeveloperName , &quot;ASI_MFM_GF&quot;)),ASI_MFM_G_L_Date__c &lt; ASI_MFM_PO__r.ASI_MFM_PO_Start_Date__c,!INCLUDES($User.BypassTriggers__c,&apos;GLDate_After_PO_StartDate&apos;)
 )</errorConditionFormula>
        <errorMessage>G/L Date has to be  after PO Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_GLDate_BackDateValidation</fullName>
        <active>false</active>
        <description>Do not allow G/L Date to back date</description>
        <errorConditionFormula>AND (
	CONTAINS(RecordType.DeveloperName, &quot;ASI_MFM_HK&quot;),
	OR (
		NOT(ISNEW()) &amp;&amp; (ASI_MFM_G_L_Date__c &lt; Today() &amp;&amp;  ISCHANGED(ASI_MFM_G_L_Date__c )),
		ISNEW() &amp;&amp; (ASI_MFM_G_L_Date__c &lt; Today())
	)
)</errorConditionFormula>
        <errorDisplayField>ASI_MFM_G_L_Date__c</errorDisplayField>
        <errorMessage>G/L Date cannot be earlier than today</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_GLDate_NoPastYear</fullName>
        <active>true</active>
        <description>Do not allow G/L Date to back date</description>
        <errorConditionFormula>AND ( ISNEW() ,
CONTAINS(RecordType.DeveloperName, &quot;ASI_MFM_HK&quot;),
Date(IF(month(today())&gt;=7,Year(today()),year(today())-1),7,1)&gt;ASI_MFM_G_L_Date__c,
not(CONTAINS($Profile.Name, &apos;Administrator&apos;))
)</errorConditionFormula>
        <errorDisplayField>ASI_MFM_G_L_Date__c</errorDisplayField>
        <errorMessage>G/L Date cannot be in past FY</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_GLDate_With_PO_Date_Range</fullName>
        <active>false</active>
        <errorConditionFormula>AND ( 
CONTAINS( RecordType.DeveloperName , &quot;ASI_MFM_SG&quot;), 
OR ( 
ASI_MFM_G_L_Date__c &gt; ASI_MFM_PO__r.ASI_MFM_PO_End_Date__c, 
ASI_MFM_G_L_Date__c &lt; ASI_MFM_PO__r.ASI_MFM_PO_Start_Date__c 
)
)</errorConditionFormula>
        <errorDisplayField>ASI_MFM_A_C_Code__c</errorDisplayField>
        <errorMessage>G/L Date has to be within PO Start Date and End Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_HK_POLineCustomerBUValidation</fullName>
        <active>true</active>
        <description>Ensure correct customer (HK or Macau) is selected</description>
        <errorConditionFormula>AND ( 
CONTAINS(RecordType.DeveloperName, &quot;ASI_MFM_HK&quot;), 
TEXT(ASI_MFM_Customer_Name__r.ASI_HK_CRM_Location__c) != ASI_MFM_PO__r.ASI_MFM_Prefix__r.ASI_MFM_Default_BU__r.ASI_MFM_BU_Code__c 
)</errorConditionFormula>
        <errorDisplayField>ASI_MFM_Customer_Name__c</errorDisplayField>
        <errorMessage>Please select Customer of the correct location (Hong Kong/Macau)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_JP_Allow_Update_Amount</fullName>
        <active>false</active>
        <errorConditionFormula>(TEXT(ASI_MFM_PO__r.ASI_MFM_Status__c) == &apos;Final&apos; ||
TEXT(ASI_MFM_PO__r.ASI_MFM_Status__c) == &apos;Complete&apos; )
&amp;&amp;
CONTAINS($Profile.Name, &apos;Standard&apos; )
&amp;&amp;
 CONTAINS(ASI_MFM_PO__r.RecordType.Name ,&apos;JP PO&apos;)</errorConditionFormula>
        <errorDisplayField>ASI_MFM_Amount__c</errorDisplayField>
        <errorMessage>PO Line Amount cannot be changed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_JP_Remaining_Receipt_Balance</fullName>
        <active>false</active>
        <errorConditionFormula>AND(CONTAINS( RecordType.DeveloperName , &quot;ASI_MFM_JP&quot;),  ASI_MFM_CN_Remaining_Receipt_Balance__c &lt;(0- ASI_MFM_PO_Amount__c *0.1))</errorConditionFormula>
        <errorDisplayField>ASI_MFM_A_C_Code__c</errorDisplayField>
        <errorMessage>POL: PO Receipt Amount could be no more than 10% of the PO Line Amount</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_Lock_HK_POL</fullName>
        <active>false</active>
        <errorConditionFormula>( ASI_MFM_PO__r.RecordType.DeveloperName = &apos;ASI_MFM_HK_PO_Read_Only&apos;) &amp;&amp; ( ASI_MFM_Sys_Sync_Time__c = PRIORVALUE(ASI_MFM_Sys_Sync_Time__c) )</errorConditionFormula>
        <errorDisplayField>ASI_MFM_A_C_Code__c</errorDisplayField>
        <errorMessage>You can NOT modify the PO Line under a Final or Complete PO</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_Lock_TW_POL_Obsolete</fullName>
        <active>false</active>
        <errorConditionFormula>( ASI_MFM_PO__r.RecordType.DeveloperName = &apos;ASI_MFM_TW_PO_Read_Only&apos;) &amp;&amp; !CONTAINS($Profile.Name, &apos;ASI_MFM_TW Power&apos;) &amp;&amp; ( ASI_MFM_Sys_Sync_Time__c = PRIORVALUE(ASI_MFM_Sys_Sync_Time__c) )</errorConditionFormula>
        <errorDisplayField>ASI_MFM_A_C_Code__c</errorDisplayField>
        <errorMessage>You can NOT modify the PO Line under a Final or Complete PO</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_NegativeAmtCheck_CloneForAdjust</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
NOT(CONTAINS($Profile.Name, &apos;System Administrator&apos;)),
CONTAINS(RecordType.DeveloperName, &quot;ASI_MFM_HK&quot;),
 ISBLANK(ASI_MFM_PO__r.ASI_MFM_Original_PO_for_Adjustment__c),
  ASI_MFM_PO__r.ASI_MFM_PO_Amount__c &lt; 0
)</errorConditionFormula>
        <errorDisplayField>ASI_MFM_Amount__c</errorDisplayField>
        <errorMessage>Please use Clone for Adjust function for negative amount PO.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_Remaining_Balance_Check</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 
	CONTAINS( RecordType.DeveloperName , &quot;ASI_MFM_SG&quot;)
	, 
	ASI_MFM_Amount__c -  ASI_MFM_Paid_Amount_in_PO_Currency__c &lt; -0.1,  
