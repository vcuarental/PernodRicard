ASI_MFM_Amount__c * 
if(TEXT( ASI_MFM_PO__r.ASI_MFM_PO_Type__c ) = &apos;In Channel Event&apos;, ASI_MFM_PO__r.ASI_MFM_Event_ID__r.ASI_MFM_Event__r.ASI_MFM_CN_Completed_Outlet_Ratio__c ,
if(
	if(
		OR(TODAY()&gt;=ASI_MFM_PO__r.ASI_MFM_PO_End_Date__c,
			AND(MONTH(TODAY())=MONTH(ASI_MFM_PO__r.ASI_MFM_PO_End_Date__c),YEAR(TODAY())=YEAR(ASI_MFM_PO__r.ASI_MFM_PO_End_Date__c))
			),1,

(if (month (Today()) =12, DATE (YEAR (Today()),12,31),Date(Year(Today()),MONTH(TODAY())+1,1)-1)-ASI_MFM_PO__r.ASI_MFM_PO_Start_Date__c+1)/
(ASI_MFM_PO__r.ASI_MFM_PO_End_Date__c - ASI_MFM_PO__r.ASI_MFM_PO_Start_Date__c+1)

)&lt;0,0,if(((if (month (Today()) =12, DATE (YEAR (Today()),12,31),Date(Year(Today()),MONTH(TODAY())+1,1)-1)-ASI_MFM_PO__r.ASI_MFM_PO_Start_Date__c+1)/
(ASI_MFM_PO__r.ASI_MFM_PO_End_Date__c - ASI_MFM_PO__r.ASI_MFM_PO_Start_Date__c+1))&gt;1,1,
((if (month (Today()) =12, DATE (YEAR (Today()),12,31),Date(Year(Today()),MONTH(TODAY())+1,1)-1)-ASI_MFM_PO__r.ASI_MFM_PO_Start_Date__c+1)/
(ASI_MFM_PO__r.ASI_MFM_PO_End_Date__c - ASI_MFM_PO__r.ASI_MFM_PO_Start_Date__c+1))
)
)
)
)</formula>
