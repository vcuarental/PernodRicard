	!INCLUDES($User.BypassTriggers__c,&apos;ASI_MFM_POLine_Remaining_Balance_Check&apos;) 
)</errorConditionFormula>
        <errorDisplayField>ASI_MFM_Amount__c</errorDisplayField>
        <errorMessage>PO Line: PO Line Amount cannot be overspent</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_SC_Need_Subledger</fullName>
        <active>true</active>
        <errorConditionFormula>CONTAINS(RecordType.Name , &apos;SC PO&apos;) &amp;&amp;   ISBLANK(ASI_MFM_SC_Subledger__c ) &amp;&amp;  
 ASI_MFM_AP_Code__r.asi_mfm_sc_Need_Subleger__c</errorConditionFormula>
        <errorMessage>Need to fill in Subledger</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_SG_Available_Quantity</fullName>
        <active>true</active>
        <errorConditionFormula>AND(CONTAINS(RecordType.DeveloperName, &quot;ASI_MFM_SG&quot;), ISPICKVAL(ASI_MFM_PO__r.ASI_MFM_Status__c , &apos;Draft&apos;), ASI_MFM_PO__r.ASI_MFM_Is_FOC_PO__c  = false,
 ASI_MFM_SKU__r.ASI_HK_CRM_Available_Quantity__c &lt;= 0 
)</errorConditionFormula>
        <errorDisplayField>ASI_MFM_SKU__c</errorDisplayField>
        <errorMessage>There is no available quantity for SKU.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_TH_Check_PO_Amount</fullName>
        <active>true</active>
        <errorConditionFormula>AND(!ISBLANK(ASI_MFM_TH_Unit__c),  
!ISBLANK(ASI_MFM_Quantity__c),  
!ISBLANK(ASI_MFM_Amount__c),
(  ASI_MFM_Amount__c &lt;&gt; ASI_MFM_Quantity__c * ASI_MFM_TH_Unit__c),
RecordType.DeveloperName = &quot;ASI_MFM_TH_PO_Line_Item&quot;)</errorConditionFormula>
        <errorMessage>Unit Cost x Quantity must be equal to PO Amount</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_TR_Receipt_Checking</fullName>
        <active>true</active>
        <errorConditionFormula>AND( RecordType.DeveloperName = &apos;ASI_MFM_TR_PO_Line_Item&apos;, ASI_MFM_Has_Receipt__c &gt;1,!CONTAINS($Profile.Name,&apos;Administrator&apos;),!ASI_MFM_Complete__c,! INCLUDES($User.BypassTriggers__c,&apos;ASI_MFM_POLineTR_Receipt_Checking&apos;) )</errorConditionFormula>
        <errorMessage>Cannot add new PO Receipt Line.</errorMessage>
    </validationRules>
