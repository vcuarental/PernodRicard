IF(ISPICKVAL(ASI_GiftBox_Request_Item__r.ASI_CRM_Category__c, &quot;Podium stand&quot;),
   Text(ASI_GiftBox_Request_Id__r.ASI_CRM_Customer__r.ASI_CRM_Service_Pack__r.ASI_CRM_No_of_Podium_stand__c),
   IF(ISPICKVAL(ASI_GiftBox_Request_Item__r.ASI_CRM_Category__c, &quot;Glorifier&quot;),
      Text(ASI_GiftBox_Request_Id__r.ASI_CRM_Customer__r.ASI_CRM_Service_Pack__r.ASI_CRM_No_of_Glorifier__c),
	  IF(ISPICKVAL(ASI_GiftBox_Request_Item__r.ASI_CRM_Category__c, &quot;Glassware&quot;),
	     Text(ASI_GiftBox_Request_Id__r.ASI_CRM_Customer__r.ASI_CRM_Service_Pack__r.ASI_CRM_No_of_Glassware__c),
		 IF(ISPICKVAL(ASI_GiftBox_Request_Item__r.ASI_CRM_Category__c, &quot;Dice &amp; Dice Cup&quot;),
		    Text(ASI_GiftBox_Request_Id__r.ASI_CRM_Customer__r.ASI_CRM_Service_Pack__r.ASI_CRM_No_of_Dice_Dice_Cup__c),
			IF(ISPICKVAL(ASI_GiftBox_Request_Item__r.ASI_CRM_Category__c, &quot;Drink menus&quot;),
			   Text(ASI_GiftBox_Request_Id__r.ASI_CRM_Customer__r.ASI_CRM_Service_Pack__r.ASI_CRM_No_of_Drink_menus__c),
			   &quot;Unrecognized&quot;
			)
