    <validationRules>
        <fullName>ASI_CRM_VN_CapsuleCS_BlockNegativeAmt</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
 CONTAINS( RecordType.DeveloperName , &quot;ASI_CRM_VN&quot;),
ASI_CRM_Amount__c &lt; 0
)</errorConditionFormula>
        <errorDisplayField>ASI_CRM_Amount__c</errorDisplayField>
        <errorMessage>Amount should not be negative, please contact system administrator</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_VN_CapsuleCS_OverOriginAmount</fullName>
        <active>true</active>
        <errorConditionFormula>ASI_CRM_Amount__c &gt; ASI_CRM_Original_Amount__c</errorConditionFormula>
        <errorDisplayField>ASI_CRM_Amount__c</errorDisplayField>
        <errorMessage>Amount cannot be increased</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_VN_Validate_Master_Item</fullName>
        <active>true</active>
        <errorConditionFormula>(RecordType.DeveloperName == &quot;ASI_CRM_VN_POSM_Request_Item&quot; &amp;&amp;   ISBLANK(TEXT(ASI_GiftBox_Request_Item__r.ASI_CRM_Category__c)) ) || (RecordType.DeveloperName == &quot;ASI_VN_GiftBox_Request_Item&quot; &amp;&amp;   !ISBLANK(TEXT(ASI_GiftBox_Request_Item__r.ASI_CRM_Category__c)) )</errorConditionFormula>
        <errorDisplayField>ASI_GiftBox_Request_Item__c</errorDisplayField>
        <errorMessage>Please select correct master item.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_GiftBox_Req_Item_Item</fullName>
        <active>true</active>
        <description>Lock sales change item after submit</description>
        <errorConditionFormula>AND (
	PRIORVALUE(ASI_GiftBox_Request_Item__c)&lt;&gt;ASI_GiftBox_Request_Item__c ,  
	NOT(ISPICKVAL(ASI_GiftBox_Request_Id__r.ASI_GiftBox_Status__c,&apos;Open&apos; )),
	NOT(INCLUDES($User.BypassTriggers__c, &quot;ASI_GiftBox_VN_HR_User&quot;))
)</errorConditionFormula>
        <errorMessage>Not allowed to change item because the Request is not in &apos;Open&apos; status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_GiftBox_Req_Item_OtherPurpose</fullName>
        <active>true</active>
        <description>Validate Other Purpose need to be entered when purpose is &quot;Other&quot;</description>
        <errorConditionFormula>AND (ISPICKVAL( ASI_GiftBox_Request_Purpose__c , &quot;Other&quot;),
 ISBLANK( ASI_GiftBox_Other_Purpose__c ))</errorConditionFormula>
        <errorMessage>Please enter Other Purpose</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_GiftBox_Req_Item_Upper_Limit_Sales</fullName>
        <active>false</active>
        <errorConditionFormula>IF(  ISPICKVAL(ASI_GiftBox_Request_Item_Warehouse__c , &apos;&apos;) ,if(ASI_GiftBox_Request_Quantity__c &gt; ASI_GiftBox_Request_Item__r.ASI_GiftBox_Item_Available_Quantity__c,true,false),false)</errorConditionFormula>
        <errorMessage>Not enough stock for the gift box selected</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
