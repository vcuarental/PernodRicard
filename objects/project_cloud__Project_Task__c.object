        <externalId>false</externalId>
        <formula>IF(project_cloud__IsComplete__c,
	$Label.project_cloud__Project_Status_Completed,
/*ELSE*/ IF(project_cloud__Worked_Hours__c &gt; 0 || project_cloud__Completed_Percent__c &gt; 0,
	IF(TODAY() &gt; project_cloud__End__c,
		$Label.project_cloud__Project_Status_In_Progress_Late,
	/*ELSE*/
		$Label.project_cloud__Project_Status_In_Progress
	),
/*ELSE*/
	IF(TODAY() &gt; project_cloud__Start__c,
		$Label.project_cloud__Project_Status_Not_Started_Late,
	/*ELSE*/
		$Label.project_cloud__Project_Status_Not_Started
	)
))</formula>
