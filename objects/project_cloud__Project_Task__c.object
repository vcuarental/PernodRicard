    <fields>
        <fullName>project_cloud__Start__c</fullName>
        <deprecated>false</deprecated>
        <description>This is the calculated start date for the task. It is the dependent start date if that exists otherwise it is the autonomous start date (The date the creator of the task wants to get started)</description>
        <externalId>false</externalId>
        <formula>/*If the task is complete and the completed date is before the calculated start date use the completed date as the start date to prevent negative length tasks*/
IF(project_cloud__Completed_Date__c &lt; project_cloud__Autonomous_Start__c,
	project_cloud__Completed_Date__c,
/*ELSE*/
	IF(ISBLANK(project_cloud__Parents_End__c) || project_cloud__Autonomous_Start__c &gt; project_cloud__Parents_End__c || project_cloud__IsAutonomous__c,
		/*If there is no Parents End date the task has not parents so use the autonomous start date*/
		project_cloud__Autonomous_Start__c,
	/*ELSE*/
		IF(project_cloud__Completed_Date__c &lt; project_cloud__Parents_End__c + IF(project_cloud__Parents_Hours_Offset__c = 0, 1, 0),
			/*If the autonomous start date is after the parents end date still use the autonomous start*/
			project_cloud__Completed_Date__c,
		/*ELSE*/
			/*Otherwise use the parents end date. Use the day after the parents end date if the parent task finishes at the end of the day*/
			project_cloud__Parents_End__c + IF(project_cloud__Parents_Hours_Offset__c = 0, 1, 0)
		)
	)
)</formula>
        <inlineHelpText>The calculated start date of the Project Task, derived from the Cannot Start Before date, or if this is a dependent task then the Project Task will start after the all the parents have ended.</inlineHelpText>
        <label>Calculated Start Date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
