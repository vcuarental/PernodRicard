        <fullName>project_cloud__Resource_Status_Image__c</fullName>
        <deprecated>false</deprecated>
        <description>Image formula field for displaying the resource status  (how much of a project task has been resourced)</description>
        <externalId>false</externalId>
        <formula>IMAGE(
	&apos;/resource/&apos; + $Setup.project_cloud__Configuration_Options__c.project_cloud__Namespace__c + IF(ISBLANK($Setup.project_cloud__Configuration_Options__c.project_cloud__Namespace__c), &apos;&apos;, &apos;__&apos;) + &apos;resourcing_resources_zip/icons/resourced-transparent/&apos; &amp;

	/*Fully allocated because the phase or project has been marked fully subscribed*/
	IF(project_cloud__Project_Phase__r.project_cloud__Subscribed__c || project_cloud__Project_Phase__r.project_cloud__Project__r.project_cloud__Subscribed__c, &apos;all&apos;,

	/*Fully allocated because the task has been marked fully subscribed*/
	IF(project_cloud__Subscribed__c, &apos;all&apos;,

	/*Fully allocated because the Calendar Allocations have been fully allocated*/
	IF(project_cloud__Allocated_Hours__c &gt;= project_cloud__Estimated_Hours__c, &apos;all&apos;,

	/*Partly allocated because some allocations have been created but not enough to cover the estimated hours*/
	IF(project_cloud__Allocated_Hours__c &gt; 0 &amp;&amp; project_cloud__Allocated_Hours__c &lt; project_cloud__Estimated_Hours__c, &apos;part&apos;,

	/*Partly allocated because some subscriptions have been created, but task is not fully subscribed*/
	IF(project_cloud__Subscriptions__c &gt; 0, &apos;part&apos;,

	/*Default value is not resourced*/
	&apos;none&apos;

	)))))

	&amp; &apos;.png&apos;,
	/*Alt text*/
	&apos;Resource Status&apos;,
	/*Image dimensions*/
	16, 16
)</formula>
        <label>Resource Status Image</label>
