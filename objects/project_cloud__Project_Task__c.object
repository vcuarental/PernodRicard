    <fields>
        <fullName>project_cloud__Completion_Status__c</fullName>
        <deprecated>false</deprecated>
        <description>Status of task at the time of completion</description>
        <externalId>false</externalId>
        <formula>IF(ISNULL(project_cloud__Completed_Date__c), &apos;Incomplete&apos;,
    CASE(MAX(project_cloud__Completed_Date__c - project_cloud__Start__c - MAX(
        /*Perform Length calculation as if Task was not completed*/
        IF(ISPICKVAL(project_cloud__Units__c, &quot;Duration&quot;),
            BLANKVALUE(project_cloud__Duration__c + IF(project_cloud__Duration_IsWorking__c, project_cloud__Non_Working__c, 0) - 1, 0),
            /*ELSE*/
            IF(project_cloud__IsAllocated__c,
                BLANKVALUE(project_cloud__Allocated_End__c - project_cloud__Start__c, 0),
                /*ELSE*/
                CEILING((project_cloud__Estimated_Hours__c + project_cloud__Parents_Hours_Offset__c) / project_cloud__Hours_In_Day__c) + project_cloud__Non_Working__c - 1
            )
        ),
        BLANKVALUE(project_cloud__Minimum_End__c - project_cloud__Start__c, 0),
        0
    ), -1),
       -1 , &apos;Early&apos;,
        0, &apos;On Time&apos;,
        &apos;Late&apos;
    )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Status of task at the time of completion</inlineHelpText>
        <label>Completion Status</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>project_cloud__Contribution_Project_Phase__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(ISPICKVAL(project_cloud__Units__c, &apos;Duration&apos;),
	project_cloud__Duration__c / project_cloud__Project_Phase__r.project_cloud__Duration__c,
/*ELSE*/
	project_cloud__Estimated_Hours__c / project_cloud__Project_Phase__r.project_cloud__Estimated_Hours__c
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Contribution To Phase (%)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
    </fields>
    <fields>
