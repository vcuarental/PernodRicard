        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(ISPICKVAL(project_cloud__Units__c, &apos;Duration&apos;),
	project_cloud__Duration__c / project_cloud__Project_Phase__r.project_cloud__Project__r.project_cloud__Duration__c,
/*ELSE*/
	project_cloud__Estimated_Hours__c / project_cloud__Project_Phase__r.project_cloud__Project__r.project_cloud__Estimated_Hours__c
