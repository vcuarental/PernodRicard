        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(ISBLANK(project_cloud__Load_Override__c), (1 - project_cloud__Allocation_Group__r.project_cloud__Allocated_Override_Load__c) /  project_cloud__Allocation_Group__r.project_cloud__Total_Subscriptions__c, project_cloud__Load_Override__c)</formula>
