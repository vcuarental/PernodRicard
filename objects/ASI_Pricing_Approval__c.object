        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/15.0/apex.js&quot;)} 
if(sforce.apex.execute(&quot;ASI_PricingApproval_Cls&quot;,&quot;checkVoidPermission&quot;,{})==&quot;true&quot;){
if(confirm(&apos;Are you sure you want to void this pricing approval?&apos;)){
var c = sforce.connection.query(&quot;SELECT Id, ASI_Status__c, ASI_Remarks__c FROM ASI_Pricing_Approval__c where Id=&apos;&quot; + &quot;{!ASI_Pricing_Approval__c.Id}&quot; + &quot;&apos;&quot;); 
var records = c.getArray(&quot;records&quot;); 
if(records.length&gt;=1){ 
for( var i=0; i&lt;records.length; i++){ 
var newC = new sforce.SObject(&quot;ASI_Pricing_Approval__c&quot;); 
newC.Id=records[i].Id; 
if (records[i].ASI_Status__c != &quot;Final Approved&quot;){
alert(&apos;Void only apply to final approved record.&apos;); 
}else{
var person = prompt(&quot;Please enter remarks&quot;, records[i].ASI_Remarks__c);
if (person != &quot;&quot; &amp;&amp; person != null){
newC.ASI_Remarks__c = person;
newC.ASI_Status__c = &quot;Void&quot;;
var result = sforce.connection.update([newC]);
window.location.reload(); 
}else if (person == &quot;&quot;){
alert(&apos;Remarks is required.&apos;);
}
}
}
}
}
}else{
alert(&apos;You are not permitted to void this record.&apos;);
}</url>
