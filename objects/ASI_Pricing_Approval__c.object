    <fields>
        <fullName>ASI_Pricing_Total_Approval_Levels__c</fullName>
        <externalId>false</externalId>
        <formula>IF (
	AND (
		NOT(ISBLANK(ASI_Pricing_First_Level_Approval__c)),
		ISBLANK(ASI_Pricing_Second_Level_Approval__c),
		ISBLANK(ASI_Pricing_Third_Level_Approval__c),
		ISBLANK(ASI_Pricing_Forth_Level_Approval__c)
	),
	1,
	IF (	
		AND (
			NOT(ISBLANK(ASI_Pricing_First_Level_Approval__c)),
			NOT(ISBLANK(ASI_Pricing_Second_Level_Approval__c)),
			ISBLANK(ASI_Pricing_Third_Level_Approval__c),
			ISBLANK(ASI_Pricing_Forth_Level_Approval__c)
		),
		2,
		IF (
			AND (
				NOT(ISBLANK(ASI_Pricing_First_Level_Approval__c)),
				NOT(ISBLANK(ASI_Pricing_Second_Level_Approval__c)),
				NOT(ISBLANK(ASI_Pricing_Third_Level_Approval__c)),
				ISBLANK(ASI_Pricing_Forth_Level_Approval__c)
			),
			3,
			IF (
				AND (
				NOT(ISBLANK(ASI_Pricing_First_Level_Approval__c)),
				NOT(ISBLANK(ASI_Pricing_Second_Level_Approval__c)),
				NOT(ISBLANK(ASI_Pricing_Third_Level_Approval__c)),
				NOT(ISBLANK(ASI_Pricing_Forth_Level_Approval__c))
				),
				4,
				-1				
			)
		)
	)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Total Approval Levels</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
