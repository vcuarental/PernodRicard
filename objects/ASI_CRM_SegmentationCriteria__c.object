    <validationRules>
        <fullName>ASI_CRM_CN_2nd_Dimension_Validation</fullName>
        <active>true</active>
        <description>If select &quot;Predefined Theoretical Value&quot; in 2nd Dimension, check if Predefined Theoretical Value has been selected.</description>
        <errorConditionFormula>AND(CONTAINS(RecordType.DeveloperName, &quot;ASI_CRM_CN&quot;),TEXT(ASI_CRM_2nd_Dimension__c ) = &quot;Predefined Theoretical Value&quot;,
ISBLANK(TEXT(AS_CRM_Predefined_Theoretical_Value__c )))</errorConditionFormula>
        <errorMessage>Please select a Predefined Theoretical Value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_CN_Alignment_Validation</fullName>
        <active>true</active>
        <description>If select &quot;Theoretical Value&quot; in 2nd Dimension, check if Alignment is inputted.</description>
        <errorConditionFormula>AND(CONTAINS(RecordType.DeveloperName, &quot;ASI_CRM_CN&quot;),
TEXT(ASI_CRM_2nd_Dimension__c ) = &quot;Theoretical Value&quot;,
TEXT(ASI_CRM_Operation__c) &lt;&gt; &quot;Within&quot;,
AND( ISNULL( ASI_CRM_Alignment_Value__c ), ISNULL( ASI_CRM_Alignment_Percentage__c)))</errorConditionFormula>
        <errorMessage>Please fill in Alignment (Value) or Alignment (Percentage).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_CN_Floating_Interval_Validtion</fullName>
        <active>true</active>
        <errorConditionFormula>AND(CONTAINS(RecordType.DeveloperName,&quot;ASI_CRM_CN&quot;),
TEXT(ASI_CRM_2nd_Dimension__c ) &lt;&gt; &quot;Predefined Theoretical Value&quot;,
TEXT(ASI_CRM_2nd_Dimension__c ) &lt;&gt; &quot;Theoretical Value&quot;, 
TEXT( ASI_CRM_Operation__c ) = &quot;Within&quot;,
AND(OR(ISNULL(ASI_CRM_Floating_Interval_Percent_Down__c),ISNULL(ASI_CRM_Floating_Interval_Percent_Up__c)), OR(ISBLANK( ASI_CRM_Floating_Interval_Down__c ),
ISBLANK(ASI_CRM_Floating_Interval_Up__c )
))
)</errorConditionFormula>
        <errorMessage>Please fill in Floating Interval (Down) and Floating Interval (Up).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_CN_Max_Min_Value_Validation</fullName>
        <active>true</active>
        <description>To check if both max and min value are filled</description>
        <errorConditionFormula>AND(CONTAINS(RecordType.DeveloperName, &quot;ASI_CRM_CN&quot;),
Text(ASI_CRM_Operation__c) = &quot;Within&quot;,
Text( ASI_CRM_2nd_Dimension__c ) = &quot;Theoretical Value&quot;,OR(ISNULL(ASI_CRM_Max_Value__c),ISNULL(ASI_CRM_Min_Value__c))
 )</errorConditionFormula>
        <errorMessage>Please fill in both Max Value and Min Value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_CN_Methodology_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR(AND(TEXT(ASI_CRM_Financial_Indicator__c) = &quot;Methodology: BRSF Methodology&quot;, TEXT(AS_CRM_Predefined_Theoretical_Value__c) &lt;&gt; &quot;PRC Default Methodology&quot;),
AND(TEXT(ASI_CRM_Financial_Indicator__c) = &quot;Methodology: PSF Methodology&quot;, TEXT(AS_CRM_Predefined_Theoretical_Value__c) &lt;&gt; &quot;PRC Default Methodology&quot;
)),CONTAINS(RecordType.DeveloperName, &quot;ASI_CRM_CN&quot;))</errorConditionFormula>
        <errorMessage>Please select PRC Default Methodology in Predefined Theoretical Value.</errorMessage>
