    <validationRules>
        <fullName>ASI_CRM_JP_APPROVED_VALIDATION</fullName>
        <active>true</active>
        <description>This is to validate which fields could be editable after approved</description>
        <errorConditionFormula>AND
(ASI_CRM_JP_Active__c = TRUE,
NOT( 
INCLUDES($User.BypassTriggers__c, &quot;ASI_CRM_JP_ADMIN&quot;) 
), 
OR
( 
RecordType.DeveloperName = &quot;ASI_CRM_JP_Wholesaler_Product_Price&quot;, 
RecordType.DeveloperName = &quot;ASI_CRM_JP_Direct_Sales_Product_Price&quot;, 
RecordType.DeveloperName = &quot;ASI_CRM_JP_D_C_Product_Price&quot; 
)
,
OR
(
ISCHANGED( ASI_MFM_Item_Group__c ),
ISCHANGED(  ASI_CRM_Customer_Group__c  ),
ISCHANGED(  ASI_Bottle_Price__c  ),
ISCHANGED(   ASI_CRM_RSP__c  ),
ISCHANGED(  ASI_CRM_MAX_1__c  ),
ISCHANGED(   ASI_CRM_MAX_2__c   ),
ISCHANGED(   ASI_CRM_JP_Ex_Price__c  ),
ISCHANGED(   ASI_Rebate_Effective_From__c   )
)
)</errorConditionFormula>
        <errorMessage>Only &quot;Effective to&quot; and &quot;Remarks&quot; field are editable after approval.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_JP_CUSTOMER_GROUP_CUSTOMER</fullName>
        <active>true</active>
        <description>Must select either Customer or Customer Group
26/11 SFDC Price Data issue: Allow WS Price not to select either one lookup</description>
        <errorConditionFormula>AND ( 
OR(
RecordType.DeveloperName =&quot;ASI_CRM_JP_Direct_Sales_Product_Price&quot;,
RecordType.DeveloperName = &quot;ASI_CRM_JP_D_C_Product_Price&quot;
), 
ISBLANK(ASI_Customer_Name__c),  ISBLANK(ASI_CRM_Customer_Group__c)
)</errorConditionFormula>
        <errorDisplayField>ASI_CRM_JP_Active__c</errorDisplayField>
        <errorMessage>You must select either &quot;Customer&quot; or &quot;Customer Group&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_JP_CUSTOMER_GROUP_CUSTOMER_2</fullName>
        <active>true</active>
        <description>Check if Both Customer and Customer Group is captured and prompt error</description>
        <errorConditionFormula>AND (
OR(RecordType.DeveloperName =&quot;ASI_CRM_JP_Direct_Sales_Product_Price&quot;,
RecordType.DeveloperName =&quot;ASI_CRM_JP_D_C_Product_Price&quot;,
RecordType.DeveloperName =&quot;ASI_CRM_JP_Wholesaler_Product_Price&quot;)
, 
NOT ISBLANK(ASI_Customer_Name__c), 
NOT ISBLANK(ASI_CRM_Customer_Group__c)
)</errorConditionFormula>
        <errorMessage>You can only enter either &quot;Customer&quot; or &quot;Customer Group&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_JP_EFFECTIVETO_FROM_VALIDATOR</fullName>
        <active>true</active>
        <description>Effective To cannot be earlier than Effective From.</description>
        <errorConditionFormula>AND(
OR( RecordType.DeveloperName = &apos;ASI_CRM_JP_D_C_Product_Price&apos;,
RecordType.DeveloperName = &apos;ASI_CRM_JP_Direct_Sales_Product_Price&apos;,
RecordType.DeveloperName = &apos;ASI_CRM_JP_Wholesaler_Product_Price&apos;
)
,
ASI_Expiry_Date__c &lt;  ASI_Effective_Date__c
)</errorConditionFormula>
        <errorMessage>Effective To Date cannot be earlier than Effective From Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_JP_Effective_From_BD_Protect</fullName>
        <active>true</active>
        <description>Backdate Protection validation for Effective From</description>
        <errorConditionFormula>AND ( 
NOT( 
INCLUDES($User.BypassTriggers__c, &quot;ASI_CRM_JP_ADMIN&quot;) 
), 
!ASI_CRM_JP_Active__c 
,
OR
(RecordType.DeveloperName  = &quot;ASI_CRM_JP_D_C_Product_Price&quot;, 
RecordType.DeveloperName  = &quot;ASI_CRM_JP_Direct_Sales_Product_Price&quot;,
RecordType.DeveloperName  = &quot;ASI_CRM_JP_Wholesaler_Product_Price&quot;
), ASI_Effective_Date__c &lt; TODAY())</errorConditionFormula>
        <errorDisplayField>ASI_Effective_Date__c</errorDisplayField>
        <errorMessage>Effective From cannot be earlier than today.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_JP_GROUP_RSP_CHECK</fullName>
        <active>true</active>
        <description>RSP must be filled when Group RSP is empty</description>
        <errorConditionFormula>AND (  RecordType.DeveloperName  =&quot;ASI_CRM_JP_D_C_Product_Price&quot;, ISBLANK(  ASI_MFM_Item_Group__r.ASI_CRM_Group_RSP__c  ), ISBLANK (ASI_CRM_RSP__c))</errorConditionFormula>
        <errorDisplayField>ASI_CRM_RSP__c</errorDisplayField>
        <errorMessage>Item Group RSP is not available. Please fill RSP before submission.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_JP_MAX_VALIDATOR</fullName>
        <active>true</active>
        <description>MAX should be lower than Bottle Price</description>
        <errorConditionFormula>AND(
OR( 
ASI_CRM_MAX_1__c &gt;   ASI_Bottle_Price__c  ,
ASI_CRM_MAX_2__c &gt; ASI_Bottle_Price__c ,
ASI_CRM_MAX_2__c &lt; ASI_CRM_MAX_1__c
),
 RecordType.DeveloperName = 	&apos;ASI_CRM_JP_Wholesaler_Product_Price&apos;
)</errorConditionFormula>
        <errorMessage>Please ensure the below conditions are fulfilled. 
- MAX1 and MAX2 should be less than bottle price. 
- MAX2 should be larger than MAX1.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_JP_RebateEff_From_BD_Protect</fullName>
        <active>true</active>
        <description>Backdate Protection for Rebate Effective From</description>
        <errorConditionFormula>AND ( RecordType.DeveloperName =&quot;ASI_CRM_JP_Wholesaler_Product_Price&quot;, ASI_Rebate_Effective_From__c &lt; TODAY())</errorConditionFormula>
        <errorDisplayField>ASI_Rebate_Effective_From__c</errorDisplayField>
        <errorMessage>Rebate Effective From cannot be earlier than today.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_PRICE_VALIDATOR</fullName>
        <active>true</active>
        <description>Non-negative number for all pricing</description>
        <errorConditionFormula>AND(
OR(
RecordType.DeveloperName = &apos;ASI_CRM_JP_D_C_Product_Price&apos;,
RecordType.DeveloperName =&apos;ASI_CRM_JP_Wholesaler_Product_Price&apos;,
RecordType.DeveloperName = &apos;ASI_CRM_JP_Direct_Sales_Product_Price&apos;
)
,
OR(
 ASI_CRM_JP_Ex_Price__c &lt; 0,
 ASI_CRM_MAX_1__c  &lt; 0,
 ASI_CRM_MAX_2__c &lt; 0,
 ASI_CRM_RSP__c &lt; 0,
 ASI_Bottle_Price__c &lt; 0
)
)</errorConditionFormula>
        <errorDisplayField>ASI_CRM_JP_Active__c</errorDisplayField>
        <errorMessage>Price could not be negative.</errorMessage>
    </validationRules>
