    <validationRules>
        <fullName>ASI_CRM_MY_DateChecking</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 
$User.BypassVR__c != TRUE, 
CONTAINS(RecordType.DeveloperName, &apos;ASI_CRM_MY_&apos;), 
OR(ISNEW(),ISCHANGED(ASI_CRM_OfftakeDate__c)),
OR(OR(
 DAY(ASI_CRM_OfftakeDate__c) = 27,
 DAY(ASI_CRM_OfftakeDate__c) = 28,
 DAY(ASI_CRM_OfftakeDate__c) = 29,
 DAY(ASI_CRM_OfftakeDate__c) = 30,
 DAY(ASI_CRM_OfftakeDate__c) = 31
),
NOT(OR(AND(YEAR(ASI_CRM_OfftakeDate__c) = YEAR(TODAY()),MONTH(ASI_CRM_OfftakeDate__c) = MONTH(TODAY())),ASI_CRM_OfftakeDate__c &gt;=TODAY()))
))</errorConditionFormula>
        <errorDisplayField>CurrencyIsoCode</errorDisplayField>
        <errorMessage>Only allow to enter current and future month offtake, (After cutoff should enter the offtake as next month)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_MY_DateChecking2</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 
$User.BypassVR__c != TRUE, 
CONTAINS(RecordType.DeveloperName, &apos;ASI_CRM_MY_&apos;), 
OR(ISNEW(),ISCHANGED(ASI_CRM_OfftakeDate__c)), 
DAY(TODAY())&gt;=26,
OR(OR( 
DAY(ASI_CRM_OfftakeDate__c) = 27, 
DAY(ASI_CRM_OfftakeDate__c) = 28, 
DAY(ASI_CRM_OfftakeDate__c) = 29, 
DAY(ASI_CRM_OfftakeDate__c) = 30, 
DAY(ASI_CRM_OfftakeDate__c) = 31 
), 
ASI_CRM_OfftakeDate__c &lt; TODAY()) 
)</errorConditionFormula>
        <errorDisplayField>CurrencyIsoCode</errorDisplayField>
        <errorMessage>Only allow to enter the future month offtake.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_TH_Offtake_Verification_Checking</fullName>
        <active>true</active>
        <description>For TH CRM2, checks actual offtake header has wholesaler, and outlet before verification.</description>
        <errorConditionFormula>AND( RecordType.DeveloperName = &apos;ASI_CRM_TH_ActualOfftakeHeader&apos;, OR(ISBLANK(ASI_CRM_Wholesaler__c ) , ISBLANK( ASI_CRM_Outlet__c ) ), ASI_CRM_Verified__c = true, ISCHANGED(ASI_CRM_Verified__c) )</errorConditionFormula>
        <errorMessage>Actual Offtake cannot be verified since &apos;Wholesaler&apos;, or &apos;Outlet&apos; is blank.</errorMessage>
    </validationRules>
