        <active>true</active>
        <errorConditionFormula>AND(
ISPICKVAL(BMCServiceDesk__IncidentType__c, &quot;Demand&quot;), CONTAINS(BMC_RF_Demand_Region__c, &quot;BCO&quot;),  OR(ISPICKVAL(CCPE_Portfolio_Type__c, &quot;Project&quot;), ISPICKVAL(CCPE_Portfolio_Type__c, &quot;Release&quot;)))</errorConditionFormula>
        <errorMessage>Demands created for the Brand Companies can only have Portfolio Type of Configuration, Emergency, or Minor Enhancement.  Please update the Portfolio Type before saving again.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMC_RF_CGI_EmailMessageRequiredFields</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
CONTAINS($User.Title,&quot;CGI&quot;),
ISPICKVAL(BMCServiceDesk__contactType__c, &quot;Direct Email&quot;),
ISBLANK(BMC_RF_Received_Date__c),
TEXT($User.BMC_RF_Validation_Override_Level__c) &lt;&gt; &quot;Administrator&quot;)</errorConditionFormula>
        <errorMessage>Email Message tickets require both a Received Date and a Responded Date.  Please provide these details before saving.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMC_RF_CGI_ReceivedDateNotInFuture</fullName>
        <active>true</active>
        <errorConditionFormula>BMC_RF_Received_Date__c &gt; BMCServiceDesk__openDateTime__c</errorConditionFormula>
        <errorMessage>The date/time value in the Received Date field cannot be a future date/time value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMC_RF_CGI_RecievedDateLocked</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
AND(NOT(ISCHANGED(BMCServiceDesk__contactType__c)), NOT(ISBLANK(BMC_RF_Received_Date__c)), NOT(ISPICKVAL(BMCServiceDesk__contactType__c,&quot;Direct Email&quot;)),
TEXT($User.BMC_RF_Validation_Override_Level__c) &lt;&gt; &quot;Administrator&quot;)
, 
AND(NOT(ISCHANGED(BMCServiceDesk__contactType__c)), ISCHANGED(BMC_RF_Received_Date__c ), 
TEXT($User.BMC_RF_Validation_Override_Level__c) &lt;&gt; &quot;Administrator&quot;)
)</errorConditionFormula>
        <errorMessage>The Received Date field cannot be edited.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMC_RF_CGI_RespondedDateLocked</fullName>
        <active>false</active>
        <errorConditionFormula>AND(NOT(ISBLANK(PRIORVALUE(BMC_RF_Responded_Date__c))),NOT(ISCHANGED(BMCServiceDesk__contactType__c)),
ISCHANGED(BMC_RF_Responded_Date__c), 
TEXT($User.BMC_RF_Validation_Override_Level__c) &lt;&gt; &quot;Administrator&quot;)</errorConditionFormula>
        <errorMessage>The Responded Date field cannot be edited.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMC_RF_CGI_RespondedDateWithinFiveMin</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
OR(ISNEW(),
ISCHANGED(BMC_RF_Responded_Date__c)
),
Now() -  BMC_RF_Responded_Date__c  &gt; 0.00347,
ISBLANK(TEXT($User.BMC_RF_Validation_Override_Level__c))
)</errorConditionFormula>
        <errorMessage>The time indicated in the Responded Date field for this ticket is more than five minutes ago.  Please update the time value in the Responded Date field to a more current date/time to continue.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMC_RF_NonAdminCannotClose</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED(BMCServiceDesk__Status_ID__c)
&amp;&amp; BMCServiceDesk__Status_ID__c = &quot;CLOSED&quot; 
&amp;&amp; ISBLANK(BMC_RF_Problem__c) 
&amp;&amp;  BMC_RF_Closed_by_Rule__c = False 
&amp;&amp; TEXT($User.BMC_RF_Validation_Override_Level__c) &lt;&gt; &quot;Administrator&quot;</errorConditionFormula>
        <errorMessage>The status for this Incident cannot be closed without client approval.  Please resolve the Incident by setting Status to Resolved, providing a Closure Category, and Resolution details.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMC_RF_NonAdminCannotReopen</fullName>
        <active>true</active>
        <errorConditionFormula>PRIORVALUE(BMCServiceDesk__Status_ID__c) = &quot;CLOSED&quot; 
&amp;&amp; ISCHANGED(BMCServiceDesk__Status_ID__c) 
&amp;&amp; ISBLANK(TEXT($User.BMC_RF_Validation_Override_Level__c))</errorConditionFormula>
        <errorMessage>This Incident has been Closed and cannot be reopened.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMC_RF_NonAdminCannotUpdateAfterClosed</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT( BMCServiceDesk__Status_ID__c = &apos;DUPLICATED&apos;),
NOT( BMCServiceDesk__Status_ID__c = &apos;CANCELLED&apos;),
NOT( ISCHANGED(BMCServiceDesk__Is_New_Record__c)), 
NOT( ISCHANGED(BMCServiceDesk__FKBusinessService__c)), 
NOT( BMCServiceDesk__state__c ) , 
NOT( PRIORVALUE( BMCServiceDesk__state__c ) ), 
NOT( ISNEW()) ,
NOT( ISCHANGED( BMCServiceDesk__Total_Duration__c ) ), 
NOT( ISCHANGED( BMCServiceDesk__Feedback__c ) ), 
NOT( ISCHANGED( BMCServiceDesk__Compliments_and_Complaints__c ) ), 
NOT( ISCHANGED( BMCServiceDesk__Feedback_Last_Submitted__c)),
NOT(ISCHANGED(BMCServiceDesk__FKBroadcast__c)),
NOT(AND(ISCHANGED(BMCServiceDesk__dueDateTime__c),NOT(BMCServiceDesk__state__c), (LastModifiedDate  -  BMCServiceDesk__closeDateTime__c) &lt; 0.0003 , (LastModifiedDate  -  BMCServiceDesk__closeDateTime__c) &gt; -0.1 )),
ISBLANK(TEXT( BMC_RF_Incident_Review_Review_Status__c)),
NOT(ISBLANK(BMC_RF_Problem__c)),
TEXT($User.BMC_RF_Validation_Override_Level__c) &lt;&gt; &quot;Administrator&quot;)</errorConditionFormula>
        <errorMessage>This incident is Closed and cannot be modified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMC_RF_ReqQueue</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISBLANK(TEXT(BMCServiceDesk__IncidentType__c))),ISBLANK(BMCServiceDesk__Queue__c))</errorConditionFormula>
        <errorMessage>Please select a Queue.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMC_RF_ResolutionReqCategory</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
   BMCServiceDesk__FKStatus__c  = &quot;a48D000000000n4&quot;,
   AND(BMCServiceDesk__FKStatus__c  = &quot;a48D000000000nG&quot;, ISBLANK(BMC_RF_Waiting_for_User_Details__c)),
   BMC_RF_External_Vendor__r.BMC_RF_Auto_Close_Tickets__c = TRUE,
   BMCServiceDesk__Queue__c = &quot;PRG - HQ Solutions&quot;)
&amp;&amp; BMCServiceDesk__FKCategory__r.BMCServiceDesk__HasChildren__c &lt;&gt; 0 
&amp;&amp; BMCServiceDesk__FKCategory__r.BMC_RF_Lowest_Level_Category__c = FALSE</errorConditionFormula>
        <errorMessage>To update this incident, the Category selected must be from the lowest level within the Category Tree.  Please update the Category field and then re-save changes.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMC_RF_ResolutionReqClosureCategory</fullName>
        <active>true</active>
        <errorConditionFormula>OR( 
ISBLANK(TEXT(BMCServiceDesk__ClosureCategory__c)), 
AND(
NOT(ISPICKVAL(BMCServiceDesk__ClosureCategory__c, &quot;Change Request&quot;)),
CONTAINS(TEXT(BMCServiceDesk__ClosureCategory__c), &quot;Request&quot;)), 
CONTAINS(TEXT(BMCServiceDesk__ClosureCategory__c), &quot;Cancelled&quot;), 
CONTAINS(TEXT(BMCServiceDesk__ClosureCategory__c), &quot;Hardware -&quot;)) 
&amp;&amp; (BMCServiceDesk__Status_ID__c = &quot;CLOSED&quot;|| BMCServiceDesk__Status_ID__c = &quot;RESOLVED&quot;) 
&amp;&amp; ISBLANK(BMC_RF_Problem__c) 
&amp;&amp; ISPICKVAL(BMCServiceDesk__IncidentType__c,&quot;Incident&quot;)</errorConditionFormula>
        <errorMessage>To resolve this incident, please confirm your Closure Category.  Closure Category for Incidents cannot be Request Fullfilled/Denied, Cancelled, or Hardware.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMC_RF_ResolutionReqClosureCategoryDEM</fullName>
        <active>true</active>
        <errorConditionFormula>OR( 
ISBLANK(TEXT(BMCServiceDesk__ClosureCategory__c)), 
AND( 
CONTAINS(TEXT(BMCServiceDesk__ClosureCategory__c), &quot;Request&quot;), 
NOT(ISPICKVAL(BMCServiceDesk__ClosureCategory__c, &quot;Change Request&quot;))), 
CONTAINS(TEXT(BMCServiceDesk__ClosureCategory__c), &quot;Cancelled&quot;), 
AND(NOT(ISPICKVAL(BMCServiceDesk__ClosureCategory__c, &quot;Hardware Failure&quot;)),CONTAINS(TEXT(BMCServiceDesk__ClosureCategory__c), &quot;Hardware&quot;))) 
&amp;&amp; (BMCServiceDesk__Status_ID__c = &quot;CLOSED&quot;|| BMCServiceDesk__Status_ID__c = &quot;RESOLVED&quot;) 
&amp;&amp; ISBLANK(BMC_RF_Problem__c) 
&amp;&amp; ISPICKVAL(BMCServiceDesk__IncidentType__c,&quot;Incident&quot;)</errorConditionFormula>
        <errorMessage>To resolve this Demand, please confirm your Closure Category. Closure Category for Demands must either be Request Fullfilled/Denied or Cancelled.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMC_RF_ResolutionReqClosureCategorySR</fullName>
        <active>true</active>
        <errorConditionFormula>OR( 
 ISBLANK(TEXT(BMCServiceDesk__ClosureCategory__c)), 
 AND(NOT(CONTAINS(TEXT(BMCServiceDesk__ClosureCategory__c), &quot;Other&quot;)), 
 NOT(CONTAINS(TEXT(BMCServiceDesk__ClosureCategory__c), &quot;Request&quot;)), 
 AND(CONTAINS(Parent_Tree__c, &quot;Hardware&quot;), 
 NOT(CONTAINS(TEXT(BMCServiceDesk__ClosureCategory__c), &quot;Hardware&quot;)) 
 ) 
 ) 
) 

&amp;&amp; (BMCServiceDesk__Status_ID__c = &quot;CLOSED&quot;|| BMCServiceDesk__Status_ID__c = &quot;RESOLVED&quot;) 
&amp;&amp; ISBLANK(BMC_RF_Problem__c) 
&amp;&amp; ISPICKVAL(BMCServiceDesk__IncidentType__c,&quot;Service Request&quot;)</errorConditionFormula>
        <errorMessage>To resolve this Service Request, please confirm your Closure Category. Closure Category for Service Requests must be either Request Fullfilled or Request Denied.  If this is a Hardware request, please select a Hardware option.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMC_RF_ResolutionReqNoOpenTasks</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(CONTAINS(Summary__c, &quot;CUARF&quot;)) &amp;&amp;
(BMCServiceDesk__Status_ID__c  = &apos;RESOLVED&apos; ||
BMCServiceDesk__Status_ID__c  = &apos;CLOSED&apos; ||
BMCServiceDesk__Status_ID__c  = &apos;CANCELLED&apos; ||
BMCServiceDesk__Status_ID__c  = &apos;DUPLICATED&apos;) &amp;&amp;
BMC_RF_Incident_Has_Open_Tasks__c = TRUE &amp;&amp;
BMCServiceDesk__AllTaskCloseController__c = FALSE &amp;&amp;
BMC_RF_All_Tasks_Closed_Bypass__c = FALSE</errorConditionFormula>
        <errorMessage>This Incident has open Tasks that are preventing it from being resolved.  To resolve this incident, please make sure all related Tasks have been closed (Status = Closed).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMC_RF_ResolutionReqResolution</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( BMCServiceDesk__incidentResolution__c)
&amp;&amp; (BMCServiceDesk__Status_ID__c = &quot;CLOSED&quot; ||  BMCServiceDesk__Status_ID__c = &quot;RESOLVED&quot;)</errorConditionFormula>
        <errorMessage>To resolve this Incident, please provide Resolution details.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMC_RF_SSC_RequiresExternalVendor</fullName>
        <active>true</active>
        <errorConditionFormula>BMCServiceDesk__FKClient__r.BMC_RF_Shared_Services_Center_User__c = TRUE &amp;&amp; NOT(CONTAINS(BMC_RF_External_Vendor__r.BMCServiceDesk__Name__c, &quot;SSC&quot;))</errorConditionFormula>
        <errorMessage>This ticket was raised by a Shared Services Center user.  To save this record, please indicate the responsible Support Team via the External Vendor Field (e.g. ASI SSC (CN)).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMC_RF_SubmitterCannotApproveRequest</fullName>
        <active>true</active>
        <errorConditionFormula>CONTAINS(BMCServiceDesk__incidentDescription__c, &apos;Approver: &apos; &amp; BMCServiceDesk__FKClient__r.BMC_RF_Full_Name__c)</errorConditionFormula>
        <errorMessage>You cannot submit a request to yourself for approval. Please select another approver.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMC_RF_WorkdayQueueSystemAlertsOnly</fullName>
        <active>true</active>
        <errorConditionFormula>BMC_RF_External_User_Email__c &lt;&gt; &quot;pernodricard@myworkday.com&quot;
&amp;&amp;
BMCServiceDesk__Queue__c = &quot;PRG - Workday (System Alerts Only)&quot;</errorConditionFormula>
        <errorMessage>The PRG - Workday queue is intended for system alerts only.  Please contact your local HR business partner to request access or log an issue with the Workday system.</errorMessage>
    </validationRules>
