    <fields>
        <fullName>BMC_RF_Security_Score__c</fullName>
        <externalId>false</externalId>
        <formula>Case(
  BMC_RF_Security_Project_Type__c,
  &quot;New Application Development&quot;,5,
  &quot;Upgrade or Modification to Existing System&quot;,3,
  &quot;Infrastructure Setup&quot;,3,
  &quot;Tool Development&quot;,3,
  0)
+

Case(
  BMC_RF_Requires_New_Technology__c,&quot;Yes&quot;,5,
  0)

+

Case(
  BMC_RF_Application_Type__c,
  &quot;OLTP: E-Commerce/Automated Billing System/Payment Processing System&quot;,5,
  &quot;Marketing Application/Brand Website&quot;,3,
  &quot;Customer Relationship Management (CRM)&quot;,5,
  &quot;Enterprise Resource Planning (ERP)&quot;,5,
  &quot;Business Intelligence (BI)&quot;,5,
  &quot;Internal Function Development&quot;,2,
  0)

+

Case(
  BMC_RF_Application_Development_Model__c,
  &quot;Commercial Off the Shelf (COTS)&quot;,1,
  &quot;Customized COTS&quot;,2,
  &quot;Outsourced Development&quot;,5,
  &quot;In-House Development&quot;,3,
  0)

+ 

Case(
  BMC_RF_Application_Hosting_Model__c,
  &quot;Pernod Ricard Managed Cloud&quot;,3,
  &quot;Vendor Managed Datacenter or Cloud&quot;,10,
  &quot;Pernod Ricard Datacenter&quot;,1,
  0)

+

Case(
  BMC_RF_Requires_Internet_Access__c,
  &quot;Yes&quot;,10,
  0)

+

Case(
  BMC_RF_Personal_Information_Storage__c,
  &quot;Yes&quot;,10,
  0)

+

Case(
  BMC_RF_Credit_Card_Information_Storage__c,
  &quot;Yes&quot;,10,
  0)

+ 

Case(
  BMC_RF_Application_Data_Type__c,
  &quot;Highly Confidential&quot;,5,
  &quot;Confidential&quot;,3,
  &quot;Internal Use&quot;,2,
  &quot;Public&quot;,1,
  5)

+
 
Case(
  BMC_RF_Application_Obligation__c,
  &quot;Yes, Please Specify&quot;,5,
  0)

+

IF(INCLUDES(BMC_RF_Application_Integrations__c,&quot;JDE&quot;),5,0) +
IF(INCLUDES(BMC_RF_Application_Integrations__c,&quot;CDB&quot;),3,0) +
IF(INCLUDES(BMC_RF_Application_Integrations__c,&quot;Salesforce&quot;),5,0) +
IF(INCLUDES(BMC_RF_Application_Integrations__c,&quot;ICS or SCADA&quot;),5,0) +
IF(INCLUDES(BMC_RF_Application_Integrations__c,&quot;Interface with External/Third Party&quot;),5,0)

+

IF(INCLUDES(BMC_RF_Application_Users__c,&quot;Customers&quot;),5,0) + 
IF(INCLUDES(BMC_RF_Application_Users__c,&quot;External Partners&quot;),3,0) + 
IF(INCLUDES(BMC_RF_Application_Users__c,&quot;Internal Employees&quot;),1,0) + 
IF(INCLUDES(BMC_RF_Application_Users__c,&quot;Public&quot;),1,0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
