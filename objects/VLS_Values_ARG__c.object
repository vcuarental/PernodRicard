    <validationRules>
        <fullName>LAT_AR_VLS_VR03_BankBranchValidation</fullName>
        <active>true</active>
        <description>Prevent user from creating a value if the receipt doesn&apos;t have bank or branch en value type is different from &quot;E&quot; or &quot;0&quot;</description>
        <errorConditionFormula>$RecordType.DeveloperName = &apos;VLS_tandard_AR&apos; &amp;&amp;
NOT(
ISPICKVAL(ValueType_AR__c,&apos;E&apos;) ||
ISPICKVAL(ValueType_AR__c,&apos;0&apos;)
) &amp;&amp;
(
ISBLANK(Bank_AR__c) ||
ISBLANK(BranBranch_AR__c)
)</errorConditionFormula>
        <errorDisplayField>AccType_AR__c</errorDisplayField>
        <errorMessage>Bank and branch are required for the selected value type</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LAT_AR_VLS_VR04_ValueTypeValidation</fullName>
        <active>true</active>
        <errorConditionFormula>$RecordType.DeveloperName = &apos;VLS_tandard_AR&apos; &amp;&amp; 
ISPICKVAL(ValueType_AR__c,&apos;0&apos;) &amp;&amp;
ISPICKVAL(ReceiptNo_AR__r.LAT_AR_DepositAccountType__c, &apos;R&apos;)</errorConditionFormula>
        <errorDisplayField>ValueType_AR__c</errorDisplayField>
        <errorMessage>Receipts type &quot;R&quot; cannot have values type &quot;0&quot;. Please modify the value to continue.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VLS_VR01_NameLengthValidation_AR</fullName>
        <active>false</active>
        <description>&apos;Cheque No.&apos; length cannot be greater than 9.</description>
        <errorConditionFormula>NOT(ISBLANK(Name)) 
&amp;&amp; 
(NOT(ISNUMBER(Name))) || (LEN(Name) &gt; 9)</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>You cannot enter a Cheque No. with more than 9 characters.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VLS_VR02_AmountValidation_AR</fullName>
        <active>true</active>
        <errorConditionFormula>Amount_AR__c &lt;= 0</errorConditionFormula>
        <errorDisplayField>Amount_AR__c</errorDisplayField>
        <errorMessage>You can not enter an amount less than or equal to 0.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VLS_VR05_TypeValidation_AR</fullName>
        <active>true</active>
        <errorConditionFormula>(ReceiptNo_AR__r.RecordType.DeveloperName = &apos;LAT_AR_RCP_ReceiptPrintedValueGuarantee&apos;
|| 
ISPICKVAL(ReceiptNo_AR__r.Status_AR__c, &apos;Receipt Value with Guarantee&apos;)
)
&amp;&amp;
 !ISPICKVAL(ValueType_AR__c, &apos;3&apos;)</errorConditionFormula>
        <errorMessage>Printed Receipts with value in guarantee  cannot have values type &quot;3&quot;. Please modify the value to continue.</errorMessage>
    </validationRules>
