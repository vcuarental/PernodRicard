    <validationRules>
        <fullName>ASI_CRM_SG_Budget_Type_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
 CONTAINS( RecordType.DeveloperName , &quot;ASI_CRM_SG&quot;),
 NOT(ISBLANK( ASI_CRM_Sales_Rep__c)),
 NOT(ISBLANK(  ASI_CRM_Wholesaler__c ))

)</errorConditionFormula>
        <errorMessage>Please enter either Salesman or Wholesaler</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_SG_FiscalYearValidation</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
 CONTAINS( RecordType.DeveloperName , &quot;ASI_CRM_SG&quot;),
OR (
  LEN ( TEXT(ASI_CRM_Fiscal_Year__c )) = 0,
  NOT(REGEX( TEXT(ASI_CRM_Fiscal_Year__c ) , &quot;FY[0-9]{4}&quot;))
)
)</errorConditionFormula>
        <errorDisplayField>ASI_CRM_Fiscal_Year__c</errorDisplayField>
        <errorMessage>Please enter correct format of Fiscal Year (e.g. FY1415)</errorMessage>
    </validationRules>
