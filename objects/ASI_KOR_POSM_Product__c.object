        <fullName>ASI_KOR_ValidateFiscalYear</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
 RecordType.DeveloperName = &apos;ASI_KOR_Bar_Styling_Item&apos;, 
 ((LEFT(ASI_KOR_Fiscal_Year__c, 2)) &lt;&gt; &apos;FY&apos;) 
 || NOT(
	VALUE(RIGHT(TEXT((VALUE(LEFT(RIGHT(ASI_KOR_Fiscal_Year__c, 4), 2)) +1)), 2)) = VALUE((RIGHT(ASI_KOR_Fiscal_Year__c, 2)))
	)
)</errorConditionFormula>
        <errorDisplayField>ASI_KOR_Fiscal_Year__c</errorDisplayField>
        <errorMessage>Invalid Fiscal Year Format</errorMessage>
    </validationRules>
