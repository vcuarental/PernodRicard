        <fullName>Obriga_Bandeira_Regional</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
    Criterio_regional_de_cota__r.Bandeira_regional__c , 
    isblank( Bandeira_regional__c ))</errorConditionFormula>
