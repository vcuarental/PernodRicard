    </validationRules>
    <validationRules>
        <fullName>Obriga_Bandeira_Regional_Vazia</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
    Not(Criterio_regional_de_cota__r.Bandeira_regional__c ), 
    not(isblank( Bandeira_regional__c )))</errorConditionFormula>
        <errorDisplayField>Bandeira_regional__c</errorDisplayField>
        <errorMessage>O critério regional pai indica que esta cota não pode ter bandeira</errorMessage>
    </validationRules>
