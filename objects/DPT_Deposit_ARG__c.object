    <validationRules>
        <fullName>DPT_VR01_NameLengthValidation_AR</fullName>
        <active>false</active>
        <description>&apos;Bank Slip No.&apos; length cannot be greater than 10.</description>
        <errorConditionFormula>NOT(ISBLANK(Name)) 
&amp;&amp; 
(NOT(ISNUMBER(Name))) || (LEN(Name) &gt; 10)</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>You cannot enter a Bank Slip No. with more than 10 characters.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DPT_VR02_AmountValidation_AR</fullName>
        <active>true</active>
        <errorConditionFormula>Amount_AR__c &lt;= 0</errorConditionFormula>
        <errorDisplayField>Amount_AR__c</errorDisplayField>
        <errorMessage>You can not enter an amount less than or equal to 0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LAT_DPT_VR03_AccTypeValidation_AR</fullName>
        <active>false</active>
        <description>verifies the profiles that can change the obtained value</description>
        <errorConditionFormula>$RecordType.DeveloperName == &apos;DPT_Standard_AR&apos; &amp;&amp;
			 (
			(ISPICKVAL( ReceiptNo_AR__r.LAT_AR_DepositAccountType__c,&apos;C&apos;) &amp;&amp;
			  NOT(ISPICKVAL(AccType_AR__c, &apos;Bank Account&apos;)))
			 ||
			  (ISPICKVAL(ReceiptNo_AR__r.LAT_AR_DepositAccountType__c,&apos;R&apos;) &amp;&amp;
			  NOT(ISPICKVAL(AccType_AR__c, &apos;Collection Bank Account&apos;)))
			)</errorConditionFormula>
        <errorDisplayField>AccType_AR__c</errorDisplayField>
        <errorMessage>The following field can&apos;t be modified</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LAT_DPT_VR04_DocDeposit</fullName>
        <active>false</active>
        <description>The profile &apos;ventas&apos; cannot update a deposit created from a document.</description>
        <errorConditionFormula>OR($Profile.Name = &apos;AR_Vendedor&apos;, $Profile.Name = &apos;LAT_AR2_Vendedor&apos;) &amp;&amp; 
!ISNEW() &amp;&amp;
 From_Document__c = True</errorConditionFormula>
        <errorMessage>The profile &apos;AR_Vendedor&apos; cannot update a deposit created from a document.</errorMessage>
    </validationRules>
