        <protected>false</protected>
        <url>// Nazim Dabouz nazim.dabouz@bluewolfgroup.com &amp; Christophe Averseng christophe.averseng@bluewolfgroup.com

// This is a button that will update a hidden field (Task Sent) on a project page. Once the field has been updated, a trigger will launch which will start a workflow rule on Tasks (Actions) that will send emails to the assigned users.



{!REQUIRESCRIPT(&quot;/soap/ajax/32.0/connection.js&quot;)}

try{
    var projectToUpdate = new sforce.SObject(&quot;Milestone1_Project__c&quot;);

    projectToUpdate.Id = &quot;{!Milestone1_Project__c.Id}&quot;;
    projectToUpdate.RIC_Task_Status__c = &quot;Sent&quot;;
    projectToUpdate.RIC_Status__c = &quot;En cours&quot;;

    var isContinue = 
        confirm(
            &quot;Vous êtes sur le point de diffuser le planning. &quot; + 
            &quot;Les destinataires des Actions vont recevoir chacun &quot; + 
            &quot;une notification email et le Projet va passer en Statut &apos;En cours&apos;.&quot;
        );

    var isStatusValid = (&quot;{!Milestone1_Project__c.RIC_Status__c}&quot; === &quot;Validé&quot;  || &quot;{!Milestone1_Project__c.RIC_Status__c}&quot; === &quot;En cours&quot;)   &amp;&amp;  (&quot;{!Milestone1_Project__c.RIC_CoordinateurId__c}&quot;  === &quot;{!User.Id}&quot;)  ;

    if(isContinue &amp;&amp; isStatusValid){
        result = sforce.connection.update([projectToUpdate]);
        console.log(&quot;in the loop&quot;);
        if(result[0].success === &quot;success&quot;){
            location.reload();
        }
        else{
             window.location.reload();
        }
    }
    else if(!isStatusValid){
        alert(&quot;Le statut doit être ègal à Validé pour pouvoir diffuser le planning. Seul le coordinateur du projet peut déclencher cette opération.&quot;);
    }
}
