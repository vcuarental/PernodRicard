    <validationRules>
        <fullName>CCPE_ProjectBudgetProjectCharterLocked</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
AND(
  ISNEW(),
   CCPE_Request_for_Project_Change__r.CCPE_Locked__c
)

,

AND(
  ISBLANK(CCPE_Request_for_Project_Change__c), 
  AME_project_cloud_Project_Charter__r.CCPE_Locked__c, 
  OR(
    ISCHANGED(AME_project_cloud_Description__c),
    ISCHANGED(AME_project_cloud_CAPEX_OPEX__c),
    ISCHANGED(AME_project_cloud_Budget__c),
    ISCHANGED(AME_project_cloud_Currency__c)
  )
)

,

AND(
  NOT(ISBLANK(CCPE_Request_for_Project_Change__c)),
  CCPE_Request_for_Project_Change__r.CCPE_Locked__c,
  OR(
    ISCHANGED(AME_project_cloud_Description__c),
    ISCHANGED(AME_project_cloud_CAPEX_OPEX__c),
    ISCHANGED(AME_project_cloud_Budget__c),
    ISCHANGED(AME_project_cloud_Currency__c)
  )
)
)</errorConditionFormula>
        <errorMessage>This Project Charter for this Project is either pending approval or has already been approved; therefore, Project Budgets cannot be created or edited.  To create or edit the Project Budget, either submit a Request for Project Change or unlock the Charter.</errorMessage>
    </validationRules>
