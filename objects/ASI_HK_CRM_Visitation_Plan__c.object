        <fullName>ASI_CRM_VN_Reject</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Reject</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/32.0/connection.js&quot;)}
if(&apos;{!ASI_HK_CRM_Visitation_Plan__c.ASI_CRM_VN_Submit_Count__c}&apos; &lt;=2 )
{
    sforce.connection.sessionid = &apos;{!GETSESSIONID()}&apos;;
    var sUserId = sforce.connection.getUserInfo().userId;
    var c = new sforce.SObject(&quot;ASI_HK_CRM_Visitation_Plan__c&quot;);
    var recordtype= sforce.connection.query(&quot;select name, Id from recordtype where DeveloperName=&apos;ASI_CRM_VN_Visitation_Plan&apos;&quot;);
    var records = recordtype.getArray(&quot;records&quot;);
    c.id = &quot;{!ASI_HK_CRM_Visitation_Plan__c.Id}&quot;;
    c.ownerid = &quot;{!ASI_HK_CRM_Visitation_Plan__c.OwnerId}&quot;;
    if (sUserId.indexOf(c.ownerid) &gt; -1) {
        alert(&apos;Only Line Managers can reject the record.&apos;);
    }else if (c.ASI_HK_CRM_Confirmed__c == true) {
        alert(&apos;Visitation Plan has been confirmed. Cannot Reject.&apos;);
    } else {
        c.ASI_CRM_Sys_Submitted__c = false;
        c.RecordTypeId = records[0].Id;
        result = sforce.connection.update([c]);

        if(result[0].getBoolean(&quot;success&quot;))
        {
              window.location.reload();
        }
        else
        {
              alert(result[0].errors.message);
        }
    }
}
else
{
    alert(&quot;It has already been rejected&quot;);
}</url>
