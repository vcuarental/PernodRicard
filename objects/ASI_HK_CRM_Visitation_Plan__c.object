    <validationRules>
        <fullName>ASI_CRM_MY_Confirmed_Chk</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
	$User.BypassVR__c != TRUE, 
	RecordType.DeveloperName = &apos;ASI_CRM_MY_VisitationPlan&apos;, 
	ISNEW() ,
	ASI_HK_CRM_Confirmed__c = TRUE
)</errorConditionFormula>
        <errorMessage>Visitation plan CANNOT be confirmed when creating a new plan.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_MY_Confirmed_Chk2</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
	$User.BypassVR__c != TRUE,
	RecordType.DeveloperName = &apos;ASI_CRM_MY_VisitationPlan&apos;,  
	ASI_HK_CRM_Confirmed__c = TRUE,
	NOT(ISCHANGED(ASI_HK_CRM_Confirmed__c)),
	NOT(ISCHANGED(ASI_CRM_TH_No_of_Actual__c)),
	OR(
		ISCHANGED(Name),
		ISCHANGED(ASI_HK_CRM_Month__c),
		ISCHANGED(ASI_HK_CRM_Year__c),
		ISCHANGED(OwnerId)
	)
)</errorConditionFormula>
        <errorMessage>The visitation plan is already CONFIRMED.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_HK_CRM_CannotEditConfirmed</fullName>
        <active>true</active>
        <description>Should not update confirmed visitations</description>
        <errorConditionFormula>AND( RecordType.DeveloperName = &apos;ASI_HK_CRM_Visitation_Plan&apos;,
ISCHANGED( ASI_HK_CRM_Confirmed__c ) &lt;&gt; (ASI_HK_CRM_Confirmed__c == TRUE))</errorConditionFormula>
        <errorDisplayField>ASI_HK_CRM_Confirmed__c</errorDisplayField>
        <errorMessage>The visitation plan is already confirmed and copied to visitation events.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_HK_CRM_ForbidUpdateDateIfHavingDetai</fullName>
        <active>true</active>
        <description>Do not allow to update Year/Month if already having Visitation Details</description>
        <errorConditionFormula>(ISCHANGED( ASI_HK_CRM_Year__c ) || ISCHANGED( ASI_HK_CRM_Month__c )) &amp;&amp; ASI_HK_CRM_Visits_Generated__c &gt; 0</errorConditionFormula>
        <errorMessage>Visitation details are already created. Update of Year / Month is not allowed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_HK_CRM_YearValidation</fullName>
        <active>true</active>
        <description>Check the format of year</description>
        <errorConditionFormula>NOT( ISNUMBER( ASI_HK_CRM_Year__c ) )
|| CEILING( VALUE( ASI_HK_CRM_Year__c ) ) != VALUE( ASI_HK_CRM_Year__c )
|| VALUE( ASI_HK_CRM_Year__c ) &lt;= 1000
|| VALUE( ASI_HK_CRM_Year__c ) &gt;= 10000</errorConditionFormula>
        <errorDisplayField>ASI_HK_CRM_Year__c</errorDisplayField>
        <errorMessage>Invalid Year</errorMessage>
    </validationRules>
