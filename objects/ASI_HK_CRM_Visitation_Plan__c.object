    c.ASI_CRM_VN_Submit_Count__c = Number(c.ASI_CRM_VN_Submit_Count__c) + 1;
    result = sforce.connection.update([c]);

    if(result[0].getBoolean(&quot;success&quot;))
    {
          window.location.reload();
    }
    else
    {
          alert(result[0].errors.message);
    }
}
else
{
    alert(&quot;It has already been modified once&quot;);
}</url>
