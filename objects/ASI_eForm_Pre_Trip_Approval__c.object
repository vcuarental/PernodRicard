        <fullName>ASI_eForm_Company_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>(  NOT(ISBLANK( ASI_eForm_Company__c )) &amp;&amp;  NOT( ISBLANK( ASI_eForm_Traveller_Company__c ) )   
&amp;&amp;  NOT( ISBLANK(  ASI_eForm_Employee_Traveller__c  ) ))</errorConditionFormula>
        <errorMessage>Only Either Company or External Traveller Company can be filled.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_eForm_External_Traveller_Company_Req</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISBLANK(ASI_eForm_Traveller__c)) &amp;&amp; ISBLANK( ASI_eForm_Traveller_Company__c)</errorConditionFormula>
        <errorDisplayField>ASI_eForm_Traveller_Company__c</errorDisplayField>
        <errorMessage>Must input a value on External Traveller Company for the External Traveller.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_eForm_Owner_Approver_Duplicate</fullName>
        <active>true</active>
        <errorConditionFormula>ASI_eForm_Approver__c  =  OwnerId</errorConditionFormula>
        <errorDisplayField>ASI_eForm_Approver__c</errorDisplayField>
        <errorMessage>Approver and Owner cannot be the same.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_eForm_Traveller_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>(ISBLANK(ASI_eForm_Traveller__c) &amp;&amp;  ISBLANK( ASI_eForm_Employee_Traveller__c)) || ( NOT( ISBLANK(ASI_eForm_Traveller__c)) &amp;&amp; NOT( ISBLANK(ASI_eForm_Employee_Traveller__c)) )</errorConditionFormula>
        <errorMessage>Only either External Traveller or Employee Traveller can be inputted.</errorMessage>
    </validationRules>
