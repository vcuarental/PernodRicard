    <webLinks>
        <fullName>ASI_CRM_VN_GMA_WithCustomer</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>New GMA(With Customer)</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&apos;/soap/ajax/29.0/connection.js&apos;)} 

var form = document.createElement(&quot;form&quot;);
form.setAttribute(&quot;method&quot;, &quot;POST&quot;);
form.setAttribute(&quot;action&quot;, &quot;/a6V/e&quot;);

var query = &quot;SELECT Id, Name FROM ASI_CRM_AccountsAdditionalField__c &quot; 
          + &quot; WHERE Name = &apos;&quot; + &quot;{!ASI_HK_CRM_Visitation_Plan_Detail__c.ASI_CRM_MY_Customer__c}&quot; + &quot;&apos;&quot;;

var customerQueryResult = sforce.connection.query(query);
var customerList = customerQueryResult.getArray(&quot;records&quot;);

var customerId = customerList[0].Id;

var visitationPlanDetailLookupName = document.createElement(&quot;input&quot;);
visitationPlanDetailLookupName.setAttribute(&quot;type&quot;, &quot;hidden&quot;);
visitationPlanDetailLookupName.setAttribute(&quot;name&quot;, &quot;CF00ND0000005r1Mc&quot;);
visitationPlanDetailLookupName.setAttribute(&quot;value&quot;, &quot;{!ASI_HK_CRM_Visitation_Plan_Detail__c.Name}&quot;);
form.appendChild(visitationPlanDetailLookupName);

var visitationPlanDetailLookupId = document.createElement(&quot;input&quot;);
visitationPlanDetailLookupId.setAttribute(&quot;type&quot;, &quot;hidden&quot;);
visitationPlanDetailLookupId.setAttribute(&quot;name&quot;, &quot;CF00ND0000005r1Mc_lkid&quot;);
visitationPlanDetailLookupId.setAttribute(&quot;value&quot;, &quot;{!ASI_HK_CRM_Visitation_Plan_Detail__c.Id}&quot;);
form.appendChild(visitationPlanDetailLookupId);

var customerLookupName = document.createElement(&quot;input&quot;);
customerLookupName.setAttribute(&quot;type&quot;, &quot;hidden&quot;);
customerLookupName.setAttribute(&quot;name&quot;, &quot;CF00NL0000003jNdz&quot;);
customerLookupName.setAttribute(&quot;value&quot;, &quot;{!ASI_HK_CRM_Visitation_Plan_Detail__c.ASI_CRM_MY_Customer__c}&quot;);
form.appendChild(customerLookupName);

var customerLookupId = document.createElement(&quot;input&quot;);
customerLookupId.setAttribute(&quot;type&quot;, &quot;hidden&quot;);
customerLookupId.setAttribute(&quot;name&quot;, &quot;CF00NL0000003jNdz_lkid&quot;);
customerLookupId.setAttribute(&quot;value&quot;, customerId);
form.appendChild(customerLookupId);

document.body.appendChild(form); 
form.submit();</url>
    </webLinks>
