        <fullName>ASI_CRM_JP_GROUPCODE_VALIDATOR</fullName>
        <active>true</active>
        <description>When Synced to JDE, cannot update group code.</description>
        <errorConditionFormula>AND(
ASI_CRM_JDE_Synced__c = TRUE, 
ISCHANGED( ASI_CRM_Customer_Group_Code__c ) 
)</errorConditionFormula>
        <errorMessage>Customer Group Code is not editable after Synced to JDE.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
    <webLinks>
        <fullName>ASI_CRM_JP_CG_Activate_Deactivate</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Activate / Deactivate</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/19.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/19.0/apex.js&quot;)} 

var Customer= new sforce.SObject(&quot;ASI_CRM_Customer_Group__c&quot;); 
Customer.id = &quot;{!ASI_CRM_Customer_Group__c.Id}&quot;;
var isAction = true;

if({!ASI_CRM_Customer_Group__c.ASI_CRM_Inactive__c}){ 

	var confirmed = false; 
	confirmed = confirm(&apos;Please click OK to continue the activation process\n有効化プロセスを続行するには、[OK]をクリックしてください&apos;); 
	if(confirmed){ Customer.ASI_CRM_Inactive__c = false 
}else{isAction = false} 

}else{ 

	var confirmed = false; 
	confirmed = confirm(&apos;Please click OK to confirm and deactivate\n無効化プロセスを続行するには、[OK]をクリックしてください&apos;); 
	if(confirmed){
		Customer.ASI_CRM_Inactive__c = true; 
	}else{isAction = false} 
} 

if(isAction){
var result = sforce.connection.update([Customer]); 

if ( result[0].getBoolean( &quot;success&quot; ) ) { 
	if(!alert(&apos;{!ASI_CRM_Customer_Group__c.Name} has been updated\n{!ASI_CRM_Customer_Group__c.Name}顧客更新されました&apos;)){ 
		window.location.reload(); 
	} 
} 
else{ alert(result[0].errors.message); }
}</url>
    </webLinks>
</CustomObject>
