    <validationRules>
        <fullName>ASI_DFPM_CannotAddMSToCompleteProject</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(ASI_DFPM_Promotional_Project__r.ASI_DFPM_Status__c , &quot;Completed&quot;)</errorConditionFormula>
        <errorMessage>You can not add milestones to a completed project</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_DFPM_CannotBeCompleteWithOpenTasks</fullName>
        <active>true</active>
        <errorConditionFormula>if( and( ASI_DFPM_Complete__c == true,  ASI_DFPM_Total_Task__c &gt;  ASI_DFPM_Total_Complete_Task__c ), true, false)</errorConditionFormula>
        <errorMessage>You cannot mark a milestone as complete if it has open tasks.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_DFPM_EndDate_in_project</fullName>
        <active>true</active>
        <errorConditionFormula>!ISBLANK(ASI_DFPM_End_Date__c) &amp;&amp; !ISBLANK(ASI_DFPM_Promotional_Project__r.ASI_DFPM_End_Date__c) &amp;&amp; (ASI_DFPM_End_Date__c &gt; ASI_DFPM_Promotional_Project__r.ASI_DFPM_End_Date__c)</errorConditionFormula>
        <errorMessage>Milestone end date must be less than or equal to the project end date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_DFPM_Not_Modify_Template_Milestone</fullName>
        <active>false</active>
        <errorConditionFormula>ASI_DFPM_Promotional_Project__r.RecordType.DeveloperName = &quot;ASI_DFPM_Template&quot;</errorConditionFormula>
        <errorMessage>You can NOT add or modify the template milestone. Please contact the system administrator for help</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_DFPM_StartDate_after_Pred_EndDate</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISNULL( ASI_DFPM_Start_Date__c )) &amp;&amp; NOT(ISNULL( ASI_DFPM_Predeccessor_Milestone__r.ASI_DFPM_End_Date__c )) &amp;&amp;  ASI_DFPM_Start_Date__c &lt; ASI_DFPM_Predeccessor_Milestone__r.ASI_DFPM_End_Date__c</errorConditionFormula>
        <errorMessage>Milestone start date must not be earlier than its Predecessor&apos;s end date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_DFPM_StartDate_before_EndDate</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISNULL( ASI_DFPM_Start_Date__c )) &amp;&amp; NOT(ISNULL( ASI_DFPM_End_Date__c )) &amp;&amp; ASI_DFPM_End_Date__c &lt; ASI_DFPM_Start_Date__c</errorConditionFormula>
        <errorMessage>Start date must be less or equal than the end date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_DFPM_StartDate_in_project</fullName>
        <active>true</active>
        <errorConditionFormula>!ISBLANK(ASI_DFPM_Start_Date__c) &amp;&amp; !ISBLANK( ASI_DFPM_Promotional_Project__r.ASI_DFPM_Start_Date__c  ) &amp;&amp; (ASI_DFPM_Start_Date__c &lt; ASI_DFPM_Promotional_Project__r.ASI_DFPM_Start_Date__c)</errorConditionFormula>
        <errorMessage>Milestone start date must be greater than or equal to the project start date.</errorMessage>
    </validationRules>
