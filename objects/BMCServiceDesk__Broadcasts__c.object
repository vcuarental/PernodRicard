    <validationRules>
        <fullName>BMCServiceDesk__BroadcastMessage_Required</fullName>
        <active>true</active>
        <errorConditionFormula>OR( ISNULL(BMCServiceDesk__broadcastMessage__c ) , ISBLANK(BMCServiceDesk__broadcastMessage__c ))</errorConditionFormula>
        <errorMessage>Please enter Broadcast Message.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMCServiceDesk__Cannot_Update_Closed_Broadcast</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISCHANGED(BMCServiceDesk__RF_HasAttachments__c)), NOT(BMCServiceDesk__state__c ), NOT( PRIORVALUE(BMCServiceDesk__state__c)),NOT( ISNEW( )) )</errorConditionFormula>
        <errorMessage>This broadcast is closed. Reopen the broadcast to modify it.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMCServiceDesk__Category_Required</fullName>
        <active>true</active>
        <errorConditionFormula>OR( ISNULL(BMCServiceDesk__Category_ID__c) , ISBLANK(BMCServiceDesk__Category_ID__c))</errorConditionFormula>
        <errorMessage>Please enter Category.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMCServiceDesk__Check_ExpirationDate</fullName>
        <active>true</active>
        <description>The expiration date and time should not be earlier than the current date and time or the post date and time.</description>
        <errorConditionFormula>AND(NOT(OR(ISNULL(BMCServiceDesk__expirationDate__c),ISBLANK(BMCServiceDesk__expirationDate__c) ) ) ,OR(BMCServiceDesk__expirationDate__c &lt;  NOW() ,BMCServiceDesk__expirationDate__c &lt; BMCServiceDesk__postDate__c))</errorConditionFormula>
        <errorMessage>The expiration date and time should not be earlier than the current date and time or the post date and time.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMCServiceDesk__UniqueBroadcastIDCheck</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR(ISNEW( ), ISCHANGED( Name )),NOT( ISBLANK(VLOOKUP( $ObjectType.BMCServiceDesk__Broadcasts__c.Fields.Name,$ObjectType.BMCServiceDesk__Broadcasts__c.Fields.Name,NULLVALUE(Name, Name) ))))</errorConditionFormula>
        <errorMessage>A Broadcast record with this Broadcast ID already exists. Enter unique Broadcast ID.</errorMessage>
    </validationRules>
