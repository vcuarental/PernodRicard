ISBLANK(ASI_CRM_Adjustment_Remark__c) 
)</errorConditionFormula>
        <errorMessage>Please input Remark if you have adjustment.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_SG_WSRebate_Amount</fullName>
        <active>true</active>
        <errorConditionFormula>AND ( 
	RecordType.DeveloperName = &apos;ASI_CRM_SG_Wholesaler_Credit_Debit_Note&apos;, 
	AND (
		OR (
			ASI_CRM_Amount__c != ASI_CRM_Actual_Amount__c,
			ISBLANK(ASI_CRM_Amount__c)
		),
		ISBLANK(ASI_CRM_Adjustment_Remark__c)
	)
	
)</errorConditionFormula>
        <errorDisplayField>ASI_CRM_Adjustment_Remark__c</errorDisplayField>
        <errorMessage>Please enter Adjustment Remark if the Actual Amount is different from system calculated amount</errorMessage>
    </validationRules>
