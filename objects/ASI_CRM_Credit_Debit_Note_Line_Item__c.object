    <validationRules>
        <fullName>ASI_CRM_SG_BackRebate_BlockGiveMore</fullName>
        <active>false</active>
        <description>Wilken 22/10/2020: Deactivate validation rule as SG Back Rebate is not used</description>
        <errorConditionFormula>AND ( 
  OR(
    RecordType.DeveloperName = &apos;ASI_CRM_SG_Back_Rebate_Credit_Note_Line_Item&apos;,
    RecordType.DeveloperName = &apos;ASI_CRM_SG_Back_Rebate_Debit_Note_Line_Item&apos;
  ), 
   ASI_CRM_Credit_Debit_Note__r.ASI_CRM_SYS_System_Generated__c , 
  ISPICKVAL( ASI_CRM_Type__c , &apos;Back Rebate&apos;), 
  ASI_CRM_Actual_Amount__c &gt; ASI_CRM_Amount__c 
)</errorConditionFormula>
        <errorMessage>Actual Amount must be less than or equal to the computed amount.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_SG_BackRebate_BlockNegative</fullName>
        <active>false</active>
        <description>Wilken 22/10/2020: Deactivate validation rule as SG Back Rebate is not used</description>
        <errorConditionFormula>AND ( 
  OR(
    RecordType.DeveloperName = &apos;ASI_CRM_SG_Back_Rebate_Credit_Note_Line_Item&apos;,
    RecordType.DeveloperName = &apos;ASI_CRM_SG_Back_Rebate_Debit_Note_Line_Item&apos;
  ), 
   ASI_CRM_Credit_Debit_Note__r.ASI_CRM_SYS_System_Generated__c , 
  ISPICKVAL( ASI_CRM_Type__c , &apos;Back Rebate&apos;), 
  OR ( 
      ISBLANK( ASI_CRM_Actual_Amount__c ) , 
      ASI_CRM_Actual_Amount__c &lt; 0 
  ) 
)</errorConditionFormula>
        <errorMessage>Actual Amount must be greater than or equal to $0.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_SG_ContractMargin_BlockNegative</fullName>
        <active>false</active>
        <description>Wilken 22/10/2020: Deactivate validation rule as SG Contract Margin is not used</description>
        <errorConditionFormula>AND ( 
  OR(
    RecordType.DeveloperName = &apos;ASI_CRM_SG_Contract_Margin_Credit_Note_Line_Item&apos;,
    RecordType.DeveloperName = &apos;ASI_CRM_SG_Contract_Margin_Debit_Note_Line_Item&apos;
  ), 
   ASI_CRM_Credit_Debit_Note__r.ASI_CRM_SYS_System_Generated__c , 
  ISPICKVAL( ASI_CRM_Type__c , &apos;Contract Margin&apos;), 
  OR ( 
      ISBLANK( ASI_CRM_Actual_Amount__c ) , 
      ASI_CRM_Actual_Amount__c &lt; 0 
  ) 
)</errorConditionFormula>
        <errorMessage>Actual Amount must be greater than or equal to $0.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_SG_ContractMargin_Remarks</fullName>
        <active>false</active>
        <description>Wilken 22/10/2020: Deactivate validation rule as SG Contract Margin is not used</description>
        <errorConditionFormula>AND ( 
  OR(
    RecordType.DeveloperName = &apos;ASI_CRM_SG_Back_Rebate_Credit_Note_Line_Item&apos;, 
    RecordType.DeveloperName = &apos;ASI_CRM_SG_Back_Rebate_Debit_Note_Line_Item&apos;
  ),
  ASI_CRM_Credit_Debit_Note__r.ASI_CRM_SYS_System_Generated__c , 
  ISPICKVAL(ASI_CRM_Type__c , &apos;Back Rebate&apos;), 
  ASI_CRM_Amount__c != ASI_CRM_Actual_Amount__c, 
  ISBLANK(ASI_CRM_Adjustment_Remark__c) 
)</errorConditionFormula>
        <errorMessage>Please input Remark if you have adjustment.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_SG_ManualCNote_DeptCodeMandatory</fullName>
        <active>true</active>
        <errorConditionFormula>AND ( 
RecordType.DeveloperName = &apos;ASI_CRM_SG_Manual_CD_Note_Line_Item&apos;, 
ASI_CRM_A_C_Code__r.ASI_CRM_A_C_Code_Type__c = &quot;DP&quot;, 
ISBLANK(  ASI_CRM_Credit_Debit_Note__r.Owner:User.ASI_TnE_Department_Code__c  ) 
)</errorConditionFormula>
        <errorMessage>Please setup TnE Department Code for owner</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_SG_ManualCNote_SubBrandMandatory</fullName>
        <active>true</active>
        <errorConditionFormula>AND ( 
RecordType.DeveloperName = &apos;ASI_CRM_SG_Manual_CD_Note_Line_Item&apos;,   
ASI_CRM_A_C_Code__r.ASI_CRM_A_C_Code_Type__c = &quot;DF&quot;, 
ISBLANK( ASI_CRM_Sub_brand__c ) 
)</errorConditionFormula>
        <errorDisplayField>ASI_CRM_Sub_brand__c</errorDisplayField>
        <errorMessage>Please select Sub Brand for Account Code start with 4 or 5</errorMessage>
    </validationRules>
    <validationRules>
