    <validationRules>
        <fullName>gvp__Single_Product_Validation</fullName>
        <active>true</active>
        <description>Validate no more than one product lookup is entered</description>
        <errorConditionFormula>IF
(
    ( 0 +
      IF (ISBLANK(gvp__Brand__c), 0, 1) +
      IF (ISBLANK(gvp__Label__c), 0, 1) +
      IF (ISBLANK(gvp__Item__c), 0, 1)
    ) &gt; 1,
    true,
    false
)</errorConditionFormula>
        <errorMessage>Only 1 of the 3 product lookups can be populated</errorMessage>
    </validationRules>
