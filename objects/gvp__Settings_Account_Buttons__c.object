<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>Used to generate custom mini buttons to appear at the top of the mobile Account page</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>gvp__External_URL__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>Checked if the button should open in an external browser, unchecked to open within the app (including Boxapp://)</description>
        <externalId>false</externalId>
        <inlineHelpText>Checked if the button link should open in an external browser, unchecked to open in the in-app browser</inlineHelpText>
        <label>External URL</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>gvp__Geography_Key_2__c</fullName>
        <deprecated>false</deprecated>
        <externalId>true</externalId>
        <label>Geography Key</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>gvp__Geography_Key__c</fullName>
        <caseSensitive>false</caseSensitive>
        <deprecated>false</deprecated>
        <externalId>true</externalId>
        <label>DO NOT USE</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>gvp__Link_URL__c</fullName>
        <deprecated>false</deprecated>
        <description>The URL to link to with an available {{merge token}} (typically Account ID {{id}})</description>
        <externalId>false</externalId>
        <inlineHelpText>The URL to link to with an available {{merge token}} (typically Account ID {{id}})</inlineHelpText>
        <label>Link URL</label>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Url</type>
    </fields>
    <fields>
        <fullName>gvp__Profile_Id__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <label>Profile Id</label>
        <length>18</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>gvp__Target_Device__c</fullName>
        <deprecated>false</deprecated>
        <description>The target device (phone, tablet, etc. - leave blank for any)</description>
        <externalId>false</externalId>
        <label>Target Device</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Mobile Account Buttons</label>
    <visibility>Public</visibility>
</CustomObject>
