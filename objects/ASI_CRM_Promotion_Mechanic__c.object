&quot;Give:&lt;br/&gt;&quot; +
if(ASI_CRM_Trade_Deal_Bottle_Qty__c &lt;&gt; 0,
  &quot;- &quot; + if(ASI_CRM_Trade_Deal_Bottle_Qty__c &lt; 1, &quot;0&quot;, &quot;&quot;) + Text(ASI_CRM_Trade_Deal_Bottle_Qty__c) + &quot; bottles of &quot; + ASI_CRM_Promotional_SKU__r.Name + &quot;&lt;br/&gt;&quot;,
  &quot;&quot;
) +
if(ASI_CRM_Additional_SKU_Bottle_Qty__c &lt;&gt; 0,
  &quot;- &quot; + if(ASI_CRM_Additional_SKU_Bottle_Qty__c &lt; 1, &quot;0&quot;, &quot;&quot;) + Text(ASI_CRM_Additional_SKU_Bottle_Qty__c) + &quot; bottles of &quot; + ASI_CRM_Additional_SKU_POSM__r.Name + &quot;&lt;br/&gt;&quot;,
  &quot;&quot;
) +
if(ASI_CRM_Ad_hoc_Cash__c &lt;&gt; 0,
  &quot;- Ad-hoc $&quot; + if(ASI_CRM_Ad_hoc_Cash__c &lt; 1, &quot;0&quot;, &quot;&quot;) + Text(ASI_CRM_Ad_hoc_Cash__c) + &quot;&lt;br/&gt;&quot;,
  &quot;&quot;
) +
if(!ISBLANK(ASI_CRM_Gift_Voucher__c),
  &quot;- Voucher &quot; + ASI_CRM_Gift_Voucher__r.Name + &quot;&lt;br/&gt;&quot;,
  &quot;&quot;
) +
if(ASI_CRM_Other_in_Kind_Cash__c &lt;&gt; 0,
  &quot;- Other in Kind $&quot; + if(ASI_CRM_Other_in_Kind_Cash__c &lt; 1, &quot;0&quot;, &quot;&quot;) + Text(ASI_CRM_Other_in_Kind_Cash__c) + &quot; (&quot; + ASI_CRM_Remark__c + &quot;)&lt;br/&gt;&quot;,
  &quot;&quot;
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
