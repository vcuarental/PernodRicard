        <formula>&quot;Buy every &quot; + Text(ASI_CRM_Buy_Qty__c) + &quot; &quot; + Text(ASI_CRM_Buy_Unit__c) + &quot;s of &quot; +
if(!ISBLANK(ASI_CRM_Promotional_Sub_brand__c),
