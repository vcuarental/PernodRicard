    <validationRules>
        <fullName>EUR_ISP_Spend_Activity_Dates</fullName>
        <active>true</active>
        <description>Spend Activity Dates must between within Spend Date range and Start Date must be set before End Date</description>
        <errorConditionFormula>AND(
OR(
ISCHANGED(EUR_ISP_Activity_Start_Date__c),
ISCHANGED(EUR_ISP_Activity_End_Date__c),
ISCHANGED(EUR_ISP_Sales_In_Start_Date__c),
ISCHANGED(EUR_ISP_Sales_In_End_Date__c),
ISCHANGED(EUR_ISP_Sales_Out_Start_Date__c),
ISCHANGED(EUR_ISP_Sales_Out_End_Date__c)
),

OR(
EUR_ISP_Activity_Start_Date__c &lt;  EUR_ISP_Spend__r.EUR_ISP_Start_Date__c,
EUR_ISP_Activity_End_Date__c &gt;  EUR_ISP_Spend__r.EUR_ISP_End_Date__c,
EUR_ISP_Activity_Start_Date__c &gt; EUR_ISP_Activity_End_Date__c,
EUR_ISP_Sales_In_Start_Date__c &lt;  EUR_ISP_Spend__r.EUR_ISP_Start_Date__c,
EUR_ISP_Sales_In_End_Date__c &gt;  EUR_ISP_Spend__r.EUR_ISP_End_Date__c,
EUR_ISP_Sales_In_Start_Date__c &gt; EUR_ISP_Sales_In_End_Date__c,
EUR_ISP_Sales_Out_Start_Date__c &lt;  EUR_ISP_Spend__r.EUR_ISP_Start_Date__c,
EUR_ISP_Sales_Out_End_Date__c &gt;  EUR_ISP_Spend__r.EUR_ISP_End_Date__c,
EUR_ISP_Sales_Out_Start_Date__c &gt; EUR_ISP_Sales_Out_End_Date__c
)

)</errorConditionFormula>
        <errorMessage>Spend Activity Dates must between within Spend Date range and Start Date must be set before End Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EUR_ISP_Spend_Activity_Matched_Amount</fullName>
        <active>true</active>
        <description>Can’t reduce to below the matched amount</description>
        <errorConditionFormula>EUR_ISP_Total_Activity_Amount__c &lt;  EUR_ISP_Matched__c</errorConditionFormula>
        <errorDisplayField>EUR_ISP_Activity_Amount__c</errorDisplayField>
        <errorMessage>Can’t be reduced below already matched amount</errorMessage>
    </validationRules>
