    <validationRules>
        <fullName>ASI_CRM_HK_ReqReleaseQtyCheck</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
	CONTAINS( RecordType.DeveloperName , &quot;ASI_HK_CRM_Sales_Order_Item&quot;),
	(ASI_CRM_Request_Release_Qty__c-ASI_HK_CRM_Quantity__c)/ASI_HK_CRM_Quantity__c &gt;  0.5,
	NOT(CONTAINS( ASI_HK_CRM_Free_Issue__c , &quot;STK&quot;))
)</errorConditionFormula>
        <errorDisplayField>ASI_CRM_Request_Release_Qty__c</errorDisplayField>
        <errorMessage>Free Goods cannot be more than half of Order Quantity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_HK_StockReleaseMandatoryCheck</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
	CONTAINS( RecordType.DeveloperName , &quot;ASI_HK_CRM_Sales_Order_Item&quot;),
	OR (
		AND (NOT(ISBLANK( ASI_CRM_Request_Release_Qty__c )),  ISBLANK(ASI_HK_CRM_Reserve_Stock_No__c ))
	)
)</errorConditionFormula>
        <errorMessage>Please enter both Reserve Stock No and Request Release Qty</errorMessage>
    </validationRules>
