    <validationRules>
        <fullName>ASI_eForm_ETA_ETD_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ASI_eForm_ETA__c &lt;  ASI_eForm_ETD__c , NOT( ISBLANK(ASI_eForm_ETA__c) ),  NOT( ISBLANK(ASI_eForm_ETD__c)) )</errorConditionFormula>
        <errorDisplayField>ASI_eForm_ETA__c</errorDisplayField>
        <errorMessage>ETA must not be before ETD.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_eForm_ETA_ReturnDate_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ASI_eForm_Return_Date__c  &lt; ASI_eForm_ETA__c , NOT( ISBLANK(ASI_eForm_Return_Date__c ) ), NOT( ISBLANK(ASI_eForm_ETA__c)) )</errorConditionFormula>
        <errorDisplayField>ASI_eForm_Return_Date__c</errorDisplayField>
        <errorMessage>Return Date ETA must be greater than the ETA.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_eForm_Return_Date_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ASI_eForm_Round_Trip__c , ISNULL( ASI_eForm_Return_Date__c ) )</errorConditionFormula>
        <errorDisplayField>ASI_eForm_Return_Date__c</errorDisplayField>
        <errorMessage>Return Date cannot be undefined when the Line is a round trip.</errorMessage>
