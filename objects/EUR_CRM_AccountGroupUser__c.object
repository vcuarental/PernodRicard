    <webLinks>
        <fullName>EUR_CRM_Copy_Status_from_Account_Group</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Copy Status from Account Group (EU)</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/40.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/40.0/apex.js&quot;)}

var newStatus = &quot;New&quot;;

var agStatus = &apos;{!EUR_CRM_AccountGroup__c.EUR_CRM_Status__c}&apos;;

var query = &quot;&quot; +
    &quot;SELECT Id, EUR_CRM_Status__c &quot; +
    &quot;FROM EUR_CRM_AccountGroupUser__c &quot; +
    &quot;WHERE EUR_CRM_AccountGroup__c = &apos;{!EUR_CRM_AccountGroup__c.Id}&apos; &quot; +
    &quot;AND EUR_CRM_Status__c NOT IN (&apos;&quot; + agStatus +&quot;&apos;, &apos;&quot; + newStatus + &quot;&apos;)&quot;
;

var agusSOQL = sforce.connection.query(query);
var agusRecords = agusSOQL.getArray(&quot;records&quot;);

if (agusRecords.length &gt; 0) {
    var agree = confirm(&quot;All related records under the section &apos;Account Group Users (EU)&apos; with status not &apos;&quot; + agStatus + &quot;&apos; and not &apos;&quot; + newStatus + &quot;&apos; (count: &quot; 
        + agusRecords.length + &quot;) will be updated. Are you sure?&quot;
    );

    if (agree) {

        agusRecords.forEach(function(agu) {
            agu.EUR_CRM_Status__c = agStatus;
        });

        result = sforce.connection.update(agusRecords);

        if (result[0].success === &apos;false&apos;) {
            alert(&quot;Error during an updating of related Account Group Users (EU) records: &quot; + result[0].errors.message);
        } else {
            window.location.reload();
        }
        
    }
} else {
    alert(&quot;There is nothing to update.&quot;);
}</url>
    </webLinks>
    <webLinks>
        <fullName>EUR_CRM_Mark_To_Delete</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Mark &apos;To Delete&apos;</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/40.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/40.0/apex.js&quot;)}

var toDeleteStatus = &quot;To Delete&quot;;

var aguStatus = &apos;{!EUR_CRM_AccountGroupUser__c.EUR_CRM_Status__c}&apos;;

if (aguStatus === toDeleteStatus) {
    alert(&quot;Status already marked &apos;To Delete&apos;.&quot;);
} else {
    var agree = confirm(&quot;Are you sure you want to mark current record to delete?&quot;);

    if (agree) {

        var agu = new sforce.SObject(&quot;EUR_CRM_AccountGroupUser__c&quot;);
        agu.Id = &apos;{!EUR_CRM_AccountGroupUser__c.Id}&apos;;
        agu.EUR_CRM_Status__c = toDeleteStatus;

        var result = sforce.connection.update([agu]);

        if (result[0].success === &apos;false&apos;) {
            alert(result[0].errors.message);
        } else {
            window.location.reload();
        }

    }
}</url>
