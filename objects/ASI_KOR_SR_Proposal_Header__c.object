        <url>if (&apos;{!ASI_KOR_SR_Proposal_Header__c.ASI_KOR_Status__c}&apos; == &apos;Approved by BM&apos; || &apos;{!ASI_KOR_SR_Proposal_Header__c.ASI_KOR_Status__c}&apos; == &apos;Submitted&apos;) {
  alert(&apos;You cannot upload the transactions because the Sales Planning is submitted or approved.&apos;);
} else {
  confirmToProceed = confirm(&quot;주의) 이미 입력된 내용은 모두 삭제됩니다&quot;);  
  if(confirmToProceed){
    navigateToUrl(&apos;/apex/ASI_KOR_SalesUploadProposal?id={!ASI_KOR_SR_Proposal_Header__c.Id}&apos;);
