    <validationRules>
        <fullName>ASI_CRM_KR_Status_Change</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISCHANGED(ASI_KOR_Status__c), ISPICKVAL(PRIORVALUE(ASI_KOR_Status__c), &quot;Approved by BM&quot;))</errorConditionFormula>
        <errorDisplayField>ASI_KOR_Approval_Comments__c</errorDisplayField>
        <errorMessage>The status is not allow to change when approved by BM</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
    <webLinks>
        <fullName>ASI_CRM_KR_Clone_From_Last_Month</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Clone From Last Month</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>if (&apos;{!ASI_KOR_SR_Proposal_Header__c.ASI_KOR_Status__c}&apos; == &apos;Approved by BM&apos; || &apos;{!ASI_KOR_SR_Proposal_Header__c.ASI_KOR_Status__c}&apos; == &apos;Submitted&apos;) {
  alert(&apos;You cannot clone the transactions because the Sales Planning is submitted or approved.&apos;);
} else {
  confirmToProceed = confirm(&quot;지난 달에 입력된 Plan과 동일하게 생성됩니다\n주의) 이미 입력된 내용은 모두 삭제됩니다&quot;);  
  if(confirmToProceed){
    navigateToUrl(&apos;/apex/ASI_KOR_SRClonePlanningDetail?id={!ASI_KOR_SR_Proposal_Header__c.Id}&apos;);
  }
}</url>
    </webLinks>
