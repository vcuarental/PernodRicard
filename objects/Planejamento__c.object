        <fullName>Preenchimento__c</fullName>
        <externalId>false</externalId>
        <formula>IF($RecordType.DeveloperName = &apos;BRA_Standard&apos;, 
IF((Data_da_Visitas_em_Branco__c =0 &amp;&amp; Hora_da_Visita_em_Branco__c = 0), 
IMAGE(&quot;servlet/servlet.ImageServer?id=015D0000001wULm&amp;oid=00DL0000005tqQL&quot;, &quot;OK&quot;), 
IMAGE(&quot;servlet/servlet.ImageServer?id=015D0000001wUM7&amp;oid=00DL0000005tqQL&quot;, &quot;Visitas não agendadas&quot;)), 

IF(($RecordType.DeveloperName = &apos;PLV_Standard_AR&apos; || $RecordType.DeveloperName = &apos;PLV_Standard_UY&apos; || 
$RecordType.DeveloperName = &apos;LAT_MX_PLV_NewPlanning&apos; || $RecordType.DeveloperName = &apos;LAT_MX_PLV_Approved&apos;), 
IF(Data_da_Visitas_em_Branco__c =0, &quot;OK para enviar a aprobación&quot;, &quot;Faltan agendar visitas&quot;) , 
&quot;&quot;))</formula>
        <label>Preenchimento</label>
        <required>false</required>
