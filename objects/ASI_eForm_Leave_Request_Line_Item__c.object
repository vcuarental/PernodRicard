    <validationRules>
        <fullName>ASI_eForm_KR_Check_LOA</fullName>
        <active>true</active>
        <errorConditionFormula>IF(ASI_eForm_Leave_Reuest__r.ASI_eForm_Requester_Record__r.ASI_eForm_Employee_Is_Salesman__c = True &amp;&amp; (ISPICKVAL( ASI_eForm_Leave_Type__c ,&apos;기타휴직 Other leave of Absence&apos;) ||ISPICKVAL( ASI_eForm_Leave_Type__c ,&apos;병가휴직 Sick Leave of Absence&apos;)||ISPICKVAL( ASI_eForm_Leave_Type__c ,&apos;육아휴직 Child-care unpaid leave of Absence&apos;))&amp;&amp; (ASI_eForm_Reason__c==null|| ASI_eForm_Reason__c==&apos;&apos;),true,false)</errorConditionFormula>
        <errorDisplayField>ASI_eForm_Reason__c</errorDisplayField>
        <errorMessage>Reason for Leave of Absence required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_eForm_checkDays90</fullName>
        <active>false</active>
        <errorConditionFormula>IF( ISPICKVAL(ASI_eForm_Leave_Type__c, &apos;출산 휴가 Maternity leave&apos;) &amp;&amp; ASI_eForm_No_of_Days__c  &lt;&gt;  90, True, False)</errorConditionFormula>
        <errorDisplayField>ASI_eForm_No_of_Days__c</errorDisplayField>
        <errorMessage>출산 휴가 Maternity leave  - Must enter 90 days</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_eForm_checkMaxDays180</fullName>
        <active>true</active>
        <errorConditionFormula>IF( (ISPICKVAL( ASI_eForm_Leave_Type__c , &apos;병가휴직 Sick Leave of Absence&apos;) || ISPICKVAL( ASI_eForm_Leave_Type__c , &apos;기타휴직 Other leave of Absence&apos;)) &amp;&amp;  ASI_eForm_No_of_Days__c  &gt; 180 ,  ASI_eForm_No_of_Days__c  &gt; 180, null)</errorConditionFormula>
        <errorDisplayField>ASI_eForm_No_of_Days__c</errorDisplayField>
        <errorMessage>Maximum of 180 days only</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_eForm_checkMaxDays365</fullName>
        <active>true</active>
        <errorConditionFormula>IF(ISPICKVAL( ASI_eForm_Leave_Type__c , &apos;육아휴직 Child-care unpaid leave of Absence&apos;)&amp;&amp; ASI_eForm_No_of_Days__c &gt; 365, true, false)</errorConditionFormula>
        <errorDisplayField>ASI_eForm_No_of_Days__c</errorDisplayField>
        <errorMessage>Maximum of 365 days only</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_eForm_checkMustHalfDay</fullName>
        <active>true</active>
        <errorConditionFormula>IF( (ISPICKVAL( ASI_eForm_Leave_Type__c , &apos;오전 연차 휴가 Annual Leave AM&apos; )||ISPICKVAL( ASI_eForm_Leave_Type__c , &apos;오후 연차 휴가 Annual Leave PM&apos; )) &amp;&amp;  (ASI_eForm_Date_From__c  &lt;&gt;  ASI_eForm_Date_To__c  ||  ASI_eForm_No_of_Days__c  &lt;&gt; 0.5), True, False)</errorConditionFormula>
        <errorMessage>Annual Leave AM / PM must be 0.5 day / Start and end date must be on the same day</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_eForm_checkNegative</fullName>
        <active>true</active>
        <errorConditionFormula>IF( ASI_eForm_No_of_Days__c  &lt; 0, TRUE, FALSE)</errorConditionFormula>
        <errorDisplayField>ASI_eForm_No_of_Days__c</errorDisplayField>
        <errorMessage>No. of days cannot input negative numbers</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_eForm_checkNotHalfDay</fullName>
        <active>true</active>
        <errorConditionFormula>IF((ISPICKVAL(ASI_eForm_Leave_Type__c,&apos;연차 휴가 Annual Leave Full&apos;)&amp;&amp;((FLOOR(ASI_eForm_No_of_Days__c)&lt;&gt;ASI_eForm_No_of_Days__c)||ASI_eForm_No_of_Days__c = 0))||(ISPICKVAL(ASI_eForm_Leave_Type__c,&apos;보건휴가 Menstruation Leave&apos;)&amp;&amp;((FLOOR( ASI_eForm_No_of_Days__c)&lt;&gt;ASI_eForm_No_of_Days__c)||ASI_eForm_No_of_Days__c = 0))||(ISPICKVAL(ASI_eForm_Leave_Type__c,&apos;특별휴가 Special Leave&apos;)&amp;&amp;((FLOOR( ASI_eForm_No_of_Days__c)&lt;&gt;ASI_eForm_No_of_Days__c)||ASI_eForm_No_of_Days__c = 0)), True, False)</errorConditionFormula>
        <errorDisplayField>ASI_eForm_No_of_Days__c</errorDisplayField>
        <errorMessage>0.5 day not allowed for the corresponding Leave Type</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_eForm_checkReasonMandatory</fullName>
        <active>true</active>
        <errorConditionFormula>IF((ISPICKVAL(ASI_eForm_Leave_Type__c,&apos;병가 휴가 Sick Leave&apos;)||ISPICKVAL(ASI_eForm_Leave_Type__c,&apos;보상 휴가 Compensation Leave&apos;)||ISPICKVAL(ASI_eForm_Leave_Type__c,&apos;기타 휴가 Other Leave&apos;)||ISPICKVAL(ASI_eForm_Leave_Type__c,&apos;기타휴직 Other leave of Absence&apos;)) &amp;&amp; ASI_eForm_Reason__c ==null , True, False)</errorConditionFormula>
        <errorDisplayField>ASI_eForm_Reason__c</errorDisplayField>
        <errorMessage>Please enter reason</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_eForm_checkReturnToWork</fullName>
        <active>true</active>
        <errorConditionFormula>IF( ISPICKVAL(ASI_eForm_Leave_Type__c, &apos;복직 Return to Work&apos;)&amp;&amp;ASI_eForm_No_of_Days__c&lt;&gt;0,True,False)</errorConditionFormula>
        <errorDisplayField>ASI_eForm_No_of_Days__c</errorDisplayField>
        <errorMessage>Please enter 0 in No. of days field in case of Return to Work</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_eForm_checkStartEnd</fullName>
        <active>true</active>
        <errorConditionFormula>ASI_eForm_Date_From__c  &gt; ASI_eForm_Date_To__c</errorConditionFormula>
        <errorDisplayField>ASI_eForm_Date_To__c</errorDisplayField>
        <errorMessage>End date cannot be earlier than Start date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_eForm_checkZero</fullName>
        <active>true</active>
        <errorConditionFormula>IF((!ISPICKVAL( ASI_eForm_Leave_Type__c , &apos;복직 Return to Work&apos;)) &amp;&amp; ASI_eForm_No_of_Days__c == 0 , TRUE, FALSE)</errorConditionFormula>
        <errorDisplayField>ASI_eForm_No_of_Days__c</errorDisplayField>
        <errorMessage>No. of days cannot input 0</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
