    <validationRules>
        <fullName>TXC_VR01_NameLengthValidation_AR</fullName>
        <active>true</active>
        <description>&apos;Certificate No.&apos; length cannot be greater than 8.</description>
        <errorConditionFormula>NOT(ISBLANK(Name)) &amp;&amp; LEN(Name) &gt; 8</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>You cannot enter a Certificate No. with more than 8 characters.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TXC_VR02_ValueTypeValidation_AR</fullName>
        <active>true</active>
        <errorConditionFormula>NOT( $Profile.Name=&apos;AR_System Administrator&apos;) &amp;&amp; 
NOT( $Profile.Name=&apos;System Administrator&apos;) &amp;&amp;
NOT( $Profile.Name=&apos;LAT_SystemAdministrator&apos;) &amp;&amp;
(
ISPICKVAL( ValueType_AR__c , &apos;IN&apos;) ||
ISPICKVAL( ValueType_AR__c , &apos;IS&apos;) || 
ISPICKVAL( ValueType_AR__c , &apos;IT&apos;) || 
ISPICKVAL( ValueType_AR__c , &apos;RT&apos;)
 )</errorConditionFormula>
        <errorMessage>PRA is exempt from withholding selected, the amount paid by the customer less arguing that plea must be claimed and remain as current account balance</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TXC_VR03_AmountValidation_AR</fullName>
        <active>true</active>
        <errorConditionFormula>Amount_AR__c &lt;=0</errorConditionFormula>
        <errorDisplayField>Amount_AR__c</errorDisplayField>
        <errorMessage>You can not enter an amount less than or equal to 0</errorMessage>
    </validationRules>
