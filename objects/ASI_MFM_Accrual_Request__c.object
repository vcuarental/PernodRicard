        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/connection.js&quot;)} 

var AccrualRequestStatus = &quot;{!ASI_MFM_Accrual_Request__c.ASI_MFM_Status__c}&quot;;

if(AccrualRequestStatus == &quot;New&quot;){
    var AccrualRequest = new sforce.SObject(&quot;ASI_MFM_Accrual_Request__c&quot;);
    AccrualRequest.Id = &apos;{!ASI_MFM_Accrual_Request__c.Id}&apos;; 

    AccrualRequest.ASI_MFM_Status__c = &quot;Submitted&quot;;

    var result = sforce.connection.update([AccrualRequest]);

    if (result[0].success==&apos;false&apos;) { 
        alert(result[0].errors.message); 
    } 
    else { 
        window.location.reload();
    }
}
else{
    alert(&quot;Only New Accrual Request can be submitted!&quot;);
}</url>
