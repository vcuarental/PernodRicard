<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>Settings for the Survey object.  Relates to Survey Answer and Survey Manager page</description>
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>gvp__Account_Call_Required_Before_Survey__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>If true, an account call must have been submitted by the user for an account before taking a survey</description>
        <externalId>false</externalId>
        <inlineHelpText>If true, an account call must have been submitted by the user for an account before taking a survey</inlineHelpText>
        <label>Account Call Required Before Survey</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>gvp__All_Questions_Required__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>When checked, all questions created with Survey Plan Manager will be required by default and the &quot;Mark Questions as Required&quot; checkbox on the Survey Plan Wizard will be hidden</description>
        <externalId>false</externalId>
        <label>All Questions Required</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
