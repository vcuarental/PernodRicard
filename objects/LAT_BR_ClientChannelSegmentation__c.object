    <validationRules>
        <fullName>LAT_BR_CCS_VR02_ValidatesActiveSegUpdate</fullName>
        <active>true</active>
        <description>Validates that a record can not be modified if the LAT_BR_ApprovalStatus__c = &apos;Active&apos; and the clientes Status__c is not &apos;Novo cadastro&apos; or &apos;Não aprovado&apos;.</description>
        <errorConditionFormula>($RecordType.DeveloperName = &apos;LAT_BR_CCS_OnTrade&apos; || 
$RecordType.DeveloperName = &apos;LAT_BR_CCS_OffTrade&apos;) 
&amp;&amp; 
NOT(ISNEW()) 
&amp;&amp; 
( 
ISPICKVAL(LAT_BR_ApprovalStatus__c,&apos;Active&apos;) &amp;&amp; 
NOT( 
ISPICKVAL(LAT_BR_Client__r.Status__c,&apos;Novo cadastro&apos;) || 
ISPICKVAL(LAT_BR_Client__r.Status__c,&apos;Não aprovado&apos;) 
) 
) 
&amp;&amp; 
NOT( 
$Profile.Name = &apos;LAT_SystemAdministrator&apos; || 
$Profile.Name = &apos;Administrador do sistema&apos; || 
$Profile.Name = &apos;Business administrator&apos; || 
$Profile.Name = &apos;LAT_BusinessAdministrator&apos; || 
$Profile.Name = &apos;PR BRAZIL System Administrator&apos; 
) 
&amp;&amp; 
