    <validationRules>
        <fullName>Prevent_Question_Type_Field_Edits</fullName>
        <active>false</active>
        <description>Prevent editing of the Question Type field once the question has been answered in Salesforce.</description>
        <errorConditionFormula>AND(
 NOT(ISNEW()),
 ISCHANGED( USA_Question_Type__c ),
 USA_Number_of_Answers__c &gt; 0
)</errorConditionFormula>
        <errorDisplayField>USA_Question_Type__c</errorDisplayField>
        <errorMessage>The Question Type for this question cannot be changed since it has already been answered in Briefcase.</errorMessage>
    </validationRules>
