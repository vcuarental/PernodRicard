{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/apex.js&quot;)}

var isHavingParent=false;

var isNNPLessThan0=false;
var TotalInv = 0;

var pagePAFSQL = sforce.connection.query(&quot;Select Id, ASI_HK_CRM_Total_Investment_Value__c, ASI_HK_CRM_Items_NNP_LessThan_0__c, ASI_HK_CRM_Parent_PAF__c from ASI_HK_CRM_Pre_Approval_Form__c where id =&apos;{!ASI_HK_CRM_Pre_Approval_Form__c.Id}&apos;&quot;);
var pagePAF = pagePAFSQL.getArray(&apos;records&apos;);

isNNPLessThan0=pagePAF[0].ASI_HK_CRM_Items_NNP_LessThan_0__c;

if ({!ISBLANK(ASI_HK_CRM_Pre_Approval_Form__c.ASI_HK_CRM_Parent_PAF__c)}){

    TotalInv = pagePAF[0].ASI_HK_CRM_Total_Investment_Value__c;
    var p = new sforce.SObject(&apos;ASI_HK_CRM_Pre_Approval_Form__c&apos;);
    p.id = &quot;{!ASI_HK_CRM_Pre_Approval_Form__c.Id}&quot;;
    p.ASI_HK_CRM_Total_Investment_Value_LookUp__c = TotalInv;
    result = sforce.connection.update([p]);
} else {

    var parentPafId = pagePAF[0].ASI_HK_CRM_Parent_PAF__c;
    var LookUpPAF = sforce.connection.query(&quot;SELECT Id, ASI_HK_CRM_Total_Investment_Value__c, ASI_HK_CRM_Total_Investment_Value_LookUp__c from ASI_HK_CRM_Pre_Approval_Form__c where ASI_HK_CRM_Parent_PAF__c=&apos;&quot; + parentPafId + &quot;&apos;OR id=&apos;&quot; + parentPafId + &quot;&apos;&quot;);
    var records = LookUpPAF.getArray(&quot;records&quot;);
    for(i=0;i&lt;records.length;i++){
        var TempInv = records[i].ASI_HK_CRM_Total_Investment_Value__c * 1;
        TotalInv += TempInv;
    }
    console.log(&quot;TotalInv(With LookUp) :&quot; + TotalInv )
    var p = new sforce.SObject(&apos;ASI_HK_CRM_Pre_Approval_Form__c&apos;);
    p.id = &quot;{!ASI_HK_CRM_Pre_Approval_Form__c.Id}&quot;;
    p.ASI_HK_CRM_Total_Investment_Value_LookUp__c = TotalInv;
    

var parentPAFSQL = sforce.connection.query(&quot;Select Id,ASI_HK_CRM_Parent_PAF__c, ASI_HK_CRM_Is_Final_Approved__c, ASI_HK_CRM_Total_Investment_Value__c,ASI_HK_CRM_PAF_Status__c from ASI_HK_CRM_Pre_Approval_Form__c where id =&apos;&quot; + parentPafId + &quot;&apos;&quot;);
var parentPAF = parentPAFSQL.getArray(&apos;records&apos;);
parentPAF.ASI_HK_CRM_Total_Investment_Value_LookUp__c = TotalInv;

result = sforce.connection.update([p,parentPAF]);
}


