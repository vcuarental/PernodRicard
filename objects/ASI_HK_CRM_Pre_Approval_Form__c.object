        <fullName>ASI_HK_CRM_Error_Message__c</fullName>
        <externalId>false</externalId>
        <formula>if(
AND(ISBLANK(ASI_HK_CRM_Adjusted_Start_Date__c), ISBLANK(ASI_HK_CRM_Adjusted_End_Date__c),
NOT(OR(
ISPICKVAL( ASI_HK_CRM_PAF_Status__c, &quot;SA Rejected&quot;), 
ISPICKVAL( ASI_HK_CRM_PAF_Status__c, &quot;Reject Final&quot;), 
ISPICKVAL( ASI_HK_CRM_PAF_Status__c, &quot;Supervisor Rejected&quot;), 
ISPICKVAL( ASI_HK_CRM_PAF_Status__c, &quot;Draft&quot;), 
ISPICKVAL( ASI_HK_CRM_PAF_Status__c, &quot;HS Rejected&quot;), 
ISPICKVAL( ASI_HK_CRM_PAF_Status__c, &quot;MD Rejected&quot;), 
ISPICKVAL( ASI_HK_CRM_PAF_Status__c, &quot;First Reject&quot;), 
ISPICKVAL( ASI_HK_CRM_PAF_Status__c, &quot;SA Rejected&quot;), 
ISPICKVAL( ASI_HK_CRM_PAF_Status__c, &quot;FD Rejected&quot;), 
ISPICKVAL( ASI_HK_CRM_PAF_Status__c, &quot;FM Rejected&quot;), 
ISPICKVAL( ASI_HK_CRM_PAF_Status__c, &quot;MKTD Rejected&quot;)
))
), &quot;You cannot submit the PAF in the current status&quot;,
IF(
AND(
NOT(ISBLANK(ASI_HK_CRM_Adjusted_Start_Date__c)), NOT(ISBLANK(ASI_HK_CRM_Adjusted_End_Date__c)),
OR(
ISPICKVAL(ASI_HK_CRM_PAF_Status__c, &quot;MD Final&quot;),
ISPICKVAL(ASI_HK_CRM_PAF_Status__c, &quot;MKTD Approved&quot;),
ISPICKVAL(ASI_HK_CRM_PAF_Status__c, &quot;FM Approved&quot;)
), ASI_HK_CRM_SYS_Start_Date_Difference__c &gt; 3
), &quot;The adjusted start date is more than 3 months from the original start date.&quot;,
IF(AND(
NOT(ISBLANK(ASI_HK_CRM_Adjusted_Start_Date__c)), NOT(ISBLANK(ASI_HK_CRM_Adjusted_End_Date__c)),
OR(
ISPICKVAL(ASI_HK_CRM_PAF_Status__c, &quot;MD Final&quot;),
ISPICKVAL(ASI_HK_CRM_PAF_Status__c, &quot;MKTD Approved&quot;),
ISPICKVAL(ASI_HK_CRM_PAF_Status__c, &quot;FM Approved&quot;)
), ASI_HK_CRM_SYS_Different_Duration__c), &quot;The adjusted contract duration is different from the original contract duration.&quot;, &quot;&quot;
)
)
)</formula>
