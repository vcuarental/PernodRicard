    <validationRules>
        <fullName>EUR_ATG_Cannot_be_changed</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISNEW()) &amp;&amp; ISCHANGED(EUR_CRM_ATG__c)</errorConditionFormula>
        <errorDisplayField>EUR_CRM_ATG__c</errorDisplayField>
        <errorMessage>You cannot change the Account Target Group, please create a new Automatic Sharing record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EUR_CRM_Restriction_Country_Code</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(BEGINS(EUR_CRM_ATG__r.RecordType.DeveloperName, &quot;EUR_&quot; &amp; TEXT(EUR_CRM_Country_Code__c)))</errorConditionFormula>
        <errorDisplayField>EUR_CRM_Country_Code__c</errorDisplayField>
        <errorMessage>The country code of your record must match with the country code of the Account Target Group.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EUR_Cannot_move_back_status_to_draft</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISNEW()) &amp;&amp; ISCHANGED(EUR_CRM_Status__c ) &amp;&amp; ISPICKVAL(EUR_CRM_Status__c , &quot;Draft&quot;)</errorConditionFormula>
        <errorDisplayField>EUR_CRM_Status__c</errorDisplayField>
        <errorMessage>You cannot move the status back to Draft</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EUR_Deletion</fullName>
        <active>true</active>
        <description>To avoid having automatic sharing without users.</description>
        <errorConditionFormula>EUR_CRM_Number_of_users__c &gt; 0 &amp;&amp; EUR_CRM_Number_of_users__c ==  EUR_CRM_Number_of_users_to_delete__c</errorConditionFormula>
        <errorMessage>If you remove this user, this automatic sharing will have no user. So instead please ask to remove the automatic sharing.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EUR_Draft_Mandatory_at_creation</fullName>
        <active>true</active>
        <errorConditionFormula>ISNEW() &amp;&amp; NOT(ISPICKVAL(EUR_CRM_Status__c, &apos;Draft&apos;))</errorConditionFormula>
        <errorDisplayField>EUR_CRM_Status__c</errorDisplayField>
        <errorMessage>Status has to be set to &quot;Draft&quot; at the creation</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EUR_Status_Restriction</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED( EUR_CRM_Status__c ) &amp;&amp; 
(ISPICKVAL( EUR_CRM_Status__c , &quot;Waiting for batch&quot;) || ISPICKVAL(EUR_CRM_Status__c , &quot;To be deleted&quot;)) &amp;&amp;  
EUR_CRM_Number_of_users__c = 0</errorConditionFormula>
        <errorDisplayField>EUR_CRM_Status__c</errorDisplayField>
        <errorMessage>You cannot change the status to Waiting for batch or To be deleted if you didn&apos;t define any users inside your automatic sharing.</errorMessage>
    </validationRules>
