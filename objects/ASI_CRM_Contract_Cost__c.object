    <validationRules>
        <fullName>ASI_CRM_MO_ContractChgChk</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
	$User.BypassVR__c != TRUE, 
	CONTAINS($RecordType.DeveloperName, &apos;ASI_CRM_MO&apos;),
	CONTAINS(ASI_CRM_CN_Contract__r.RecordType.DeveloperName, &apos;ASI_CRM_MO&apos;),
	NOT(ISNEW()),
	ISCHANGED(ASI_CRM_CN_Contract__c)
)</errorConditionFormula>
        <errorMessage>Cannot change the contract on an existing contract cost entry</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_MO_StatusChk_ContractCost</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
	$User.BypassVR__c != TRUE, 
	CONTAINS($RecordType.DeveloperName, &apos;ASI_CRM_MO&apos;),
	CONTAINS(ASI_CRM_CN_Contract__r.RecordType.DeveloperName, &apos;ASI_CRM_MO&apos;),
	TEXT(ASI_CRM_CN_Contract__r.ASI_TH_CRM_Contract_Status__c) != &apos;Draft&apos;,
	NOT(ASI_CRM_CN_Contract__r.ASI_CRM_Is_Processing__c = true)
)</errorConditionFormula>
        <errorMessage>You can only add/modify contract cost on a &apos;Draft&quot; contract</errorMessage>
    </validationRules>
