    <validationRules>
        <fullName>ASI_CRM_SG_Amount_9L_Or_Case</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
  RecordType.DeveloperName = &apos;ASI_CRM_SG_Wholesaler&apos;,
  OR (
    AND (
      ISBLANK( ASI_CRM_Blind_Bonus_Amount_Per_9L__c ) ,
      ISBLANK( ASI_CRM_Blind_Bonus_Amount_per_Case__c ) 
    ),
    AND (
      NOT(ISBLANK( ASI_CRM_Blind_Bonus_Amount_Per_9L__c )) ,
      NOT(ISBLANK( ASI_CRM_Blind_Bonus_Amount_per_Case__c )) 
    )
  )
)</errorConditionFormula>
        <errorMessage>Please choose either this is a amount per 9L or Case.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_SG_Wholesaler_Or_DefaultBB</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
  RecordType.DeveloperName = &apos;ASI_CRM_SG_Wholesaler&apos;,
  OR (
    AND (
      NOT( ASI_CRM_Default_Est_Blind_Bonus_Amount__c ),
      ISBLANK( ASI_CRM_Customer__c )
    ),
    AND (
      ASI_CRM_Default_Est_Blind_Bonus_Amount__c,
      NOT(ISBLANK( ASI_CRM_Customer__c ))
    )
  )
)</errorConditionFormula>
        <errorMessage>Please choose either this is a default or wholesaler&apos;s blind bonus rate.</errorMessage>
    </validationRules>
