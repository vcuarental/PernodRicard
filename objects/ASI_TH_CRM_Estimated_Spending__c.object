    <validationRules>
        <fullName>ASI_TH_CRM_Cannot_Modify_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND(  ISPICKVAL(ASI_TH_CRM_Contract__r.ASI_TH_CRM_Contract_Status__c, &apos;Freeze&apos;),
ISCHANGED( ASI_TH_CRM_Date__c ) = true, 
NOT(CONTAINS($UserRole.DeveloperName,&apos;Commercial_Analyst&apos;)), 
NOT( CONTAINS( $Profile.Name, &apos;System Admin&apos;)))</errorConditionFormula>
        <errorMessage>Cannot Change Estimated Spending Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_TH_CRM_EstimatedSpendingLocked</fullName>
        <active>true</active>
        <errorConditionFormula>AND( NOT(ISCHANGED(ASI_TH_CRM_Locked__c)) , 
ASI_TH_CRM_Locked__c = true, 
NOT(CONTAINS($UserRole.DeveloperName,&apos;Commercial_Analyst&apos;)), 
NOT( CONTAINS( $Profile.Name, &apos;System Admin&apos;)))</errorConditionFormula>
        <errorMessage>The Estimated Spending record is locked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_TH_CRM_EstimatedSpendingUpdate</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
	OR(TEXT(ASI_TH_CRM_Contract__r.ASI_TH_CRM_Contract_Status__c) = &apos;Final&apos;,
		AND(TEXT(ASI_TH_CRM_Contract__r.ASI_TH_CRM_Contract_Status__c) = &apos;Editing&apos;,
			NOT(CONTAINS($UserRole.DeveloperName,&apos;Commercial_Analyst&apos;)),
			NOT(CONTAINS($UserRole.DeveloperName,&apos;Sales_Co&apos;))),
		AND(TEXT(ASI_TH_CRM_Contract__r.ASI_TH_CRM_Contract_Status__c) = &apos;Freeze&apos;,
			NOT(CONTAINS($UserRole.DeveloperName,&apos;Commercial_Analyst&apos;)),
			NOT(CONTAINS($UserRole.DeveloperName,&apos;Sales_Co&apos;)),
			NOT(CONTAINS($UserRole.DeveloperName,&apos;CDP&apos;)),
			NOT(CONTAINS($UserRole.DeveloperName,&apos;Finance&apos;)),
                        NOT(CONTAINS( $Profile.Name ,&apos;System Admin&apos;)))
           ),
NOT(CONTAINS($UserRole.DeveloperName, &apos;System_Admin&apos;))
)</errorConditionFormula>
        <errorMessage>Estimated Spending can only be edited when Contract Status is Freeze.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_TH_CRM_FundAdvanceReadOnly</fullName>
        <active>true</active>
        <errorConditionFormula>AND(TEXT(ASI_TH_CRM_Contract__r.ASI_TH_CRM_Contract_Status__c) &lt;&gt; &apos;Editing&apos;,
TEXT(ASI_TH_CRM_Contract__r.ASI_TH_CRM_Contract_Status__c) &lt;&gt; &apos;Freeze&apos;,
ISCHANGED( ASI_TH_CRM_Fund_Advance__c),
NOT(CONTAINS( $UserRole.DeveloperName ,&apos;Sales_Co&apos;)),
NOT(CONTAINS($UserRole.DeveloperName,&apos;Commercial_Analyst&apos;)),
NOT(CONTAINS($Profile.Name, &apos;System Admin&apos;)))</errorConditionFormula>
        <errorMessage>Fund - Advance cannot be modified.</errorMessage>
    </validationRules>
