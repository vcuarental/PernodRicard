if (&apos;{!ASI_HK_CRM_Sales_Order__c.ASI_HK_CRM_SYS_Greater_Than_Min_Amount__c}&apos; &gt; 0){
if(dates.compare(criteria1date ,today ) == 0 ||dates.compare(criteriadateMonthFirst ,today ) == 0 ||dates.compare(criteriadateDayFirst ,today ) == 0 )
{
navigateToUrl(&apos;/apex/ASI_HK_SubmitApprovalHelperPage?id={!ASI_HK_CRM_Sales_Order__c.Id}&apos;);
