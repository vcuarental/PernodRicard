var criteria1date = new Date(criteria1);
criteria1date.setHours(0,0,0,0);


var split = criteria1.split(&apos;/&apos;);

var criteria1datetemp = [split[2], split[0], split[1] ].join(&apos;-&apos;);
var criteriadateMonthFirst = dates.convert(criteria1datetemp);
criteriadateMonthFirst.setHours(0,0,0,0);

criteria1datetemp = [split[2], split[1], split[0] ].join(&apos;-&apos;);
var criteriadateDayFirst = dates.convert(criteria1datetemp);
criteriadateDayFirst.setHours(0,0,0,0);

//Wilken 20190515 Prevent ???????? SK Number submitted
