    <validationRules>
        <fullName>ASI_CRM_HK_Account_PA</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 ISBLANK( ASI_HK_CRM_Account_PA__c ) ,
 ISPICKVAL( ASI_HK_CRM_Order_Status_Name__c , &quot;Interfaced to JDE&quot;) 
)</errorConditionFormula>
        <errorDisplayField>ASI_HK_CRM_Account_PA__c</errorDisplayField>
        <errorMessage>Account PA cannot be empty, please contact system administrator.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_HK_CRM_Check_Urgent_Order</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( ASI_HK_CRM_Urgent_Order_Confirmed__c, &quot;Yes&quot;) &amp;&amp;  NOT(ASI_HK_CRM_Urgent_Order__c)</errorConditionFormula>
        <errorDisplayField>ASI_HK_CRM_Urgent_Order__c</errorDisplayField>
        <errorMessage>Please check</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_HK_CRM_Confirm_SO</fullName>
        <active>false</active>
        <errorConditionFormula>ISPICKVAL( ASI_HK_CRM_Urgent_Order_Confirmed__c, &quot;&quot; ) &amp;&amp;   ISPICKVAL(ASI_HK_CRM_Order_Status_Name__c, &quot;Interfaced to JDE&quot; ) &amp;&amp;  ASI_HK_CRM_Urgent_Order__c</errorConditionFormula>
        <errorMessage>This is an urgent order, please confirm before approve</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_HK_CRM_Delivery_Add_Change</fullName>
        <active>true</active>
        <errorConditionFormula>if(not(and(ISBLANK( ASI_HK_CRM_Alternate_Delivery_Address_1__c),ISBLANK( ASI_HK_CRM_Alternate_Delivery_Address_2__c),ISBLANK( ASI_HK_CRM_Alternate_Delivery_Address_3__c),ISBLANK( ASI_HK_CRM_Alternate_Delivery_Address_4__c))),ISBLANK( ASI_HK_CRM_Reason_For_Change__c),not(ISBLANK( ASI_HK_CRM_Reason_For_Change__c)))</errorConditionFormula>
        <errorDisplayField>ASI_HK_CRM_Reason_For_Change__c</errorDisplayField>
        <errorMessage>You must fill in &quot;Reason for Change&quot; if you fill in Delivery Address!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_HK_CRM_Delivery_Date_Validation</fullName>
        <active>true</active>
        <description>Delivery Date no more than 30 days after Order Date</description>
        <errorConditionFormula>AND(NOT($Setup.ASI_HK_CRM_disableReqField__c.ASI_HK_CRM_disable_validation_rule__c),
(ASI_HK_CRM_Delivery_Date__c -  ASI_HK_CRM_Order_Date__c &gt;30))</errorConditionFormula>
        <errorDisplayField>ASI_HK_CRM_Delivery_Date__c</errorDisplayField>
        <errorMessage>Delivery Date can be no more than 30 days after Order Date!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_HK_CRM_Delivery_Date_is_Today</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
OR(ISNEW(),ISCHANGED(ASI_HK_CRM_Delivery_Date__c)),
ASI_HK_CRM_Delivery_Date__c = TODAY(),
NOT(ASI_HK_CRM_Urgent_Order__c)
)</errorConditionFormula>
        <errorDisplayField>ASI_HK_CRM_Delivery_Date__c</errorDisplayField>
        <errorMessage>Only urgent orders can be delivered on the same day!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_HK_CRM_Delivery_Greater_Order</fullName>
        <active>false</active>
        <errorConditionFormula>(ASI_HK_CRM_Order_Date__c - ASI_HK_CRM_Delivery_Date__c) &gt;= 0</errorConditionFormula>
        <errorDisplayField>ASI_HK_CRM_Delivery_Date__c</errorDisplayField>
        <errorMessage>The delivery date must greater than or equal to order date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_HK_CRM_Delivery_Remarks</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ASI_HK_CRM_Good_Acceptance__c == TRUE,
ISBLANK(ASI_HK_CRM_Delivery_Remarks__c)
)</errorConditionFormula>
        <errorDisplayField>ASI_HK_CRM_Delivery_Remarks__c</errorDisplayField>
        <errorMessage>You must fill in &quot;Delivery Remarks&quot; if you checked &quot;Change of Way of Good Acceptance&quot;!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_HK_CRM_Lock_Order_Num</fullName>
        <active>true</active>
        <errorConditionFormula>if(isblank(Name)&amp;&amp; !INCLUDES($User.BypassTriggers__c,&apos;ASI_HK_IT_admin&apos;),false,ISCHANGED(Name) )</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>Order No. Cannot be changed!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_HK_CRM_Order_date</fullName>
        <active>false</active>
        <errorConditionFormula>ASI_HK_CRM_Order_Date__c != TODAY()</errorConditionFormula>
        <errorDisplayField>ASI_HK_CRM_Order_Date__c</errorDisplayField>
        <errorMessage>Order Date must be today.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_HK_CRM_Poten_Acct_Cant_NA</fullName>
        <active>true</active>
        <errorConditionFormula>and(CONTAINS(  ASI_HK_CRM_Customer_DA__r.RecordType.DeveloperName  , &quot;Potential&quot;), TEXT( ASI_HK_CRM_Payment_Method__c ) =&quot;On Credit&quot;)</errorConditionFormula>
        <errorDisplayField>ASI_HK_CRM_Payment_Method__c</errorDisplayField>
        <errorMessage>This Sales Order is for a Potential Account, the Payment Method cannot be On Credit.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_HK_CRM_SO_Attention_Field</fullName>
        <active>true</active>
        <description>1. For Potential Account Sales Orders, the Attention field is mandatory.
2. If an alternate delivery address (for both Potential Accounts and/or JDE Credit Accounts) is specified, the Attention field is mandatory.</description>
        <errorConditionFormula>IF(ISBLANK(ASI_HK_CRM_Attention__c),
OR
(ASI_HK_CRM_Customer_DA__r.RecordType.DeveloperName = &quot;ASI_HK_CRM_Potential_Account&quot;,
ASI_HK_CRM_Customer_DA__r.RecordType.DeveloperName = &quot;ASI_HK_CRM_Potential_Account_Prestige&quot;,
NOT( ISBLANK(ASI_HK_CRM_Alternate_Delivery_Address_1__c)),
NOT( ISBLANK(ASI_HK_CRM_Alternate_Delivery_Address_2__c)),
NOT( ISBLANK(ASI_HK_CRM_Alternate_Delivery_Address_3__c)),
NOT( ISBLANK(ASI_HK_CRM_Alternate_Delivery_Address_4__c)) 
),
FALSE)</errorConditionFormula>
        <errorDisplayField>ASI_HK_CRM_Attention__c</errorDisplayField>
        <errorMessage>1. For Potential Account Sales Orders, the Attention field is mandatory.
2. If an alternate delivery address (for both Potential Accounts and/or JDE Credit Accounts) is specified, the Attention field is mandatory.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_HK_CRM_SO_OrderDateCannotBeInThePast</fullName>
        <active>true</active>
        <description>For new orders, the Order Date cannot be less than today.
For existing orders, the Order Date cannot be less than the Sales Order record Created Date.</description>
        <errorConditionFormula>IF( ISNEW() , 
 IF( ASI_HK_CRM_Order_Date__c &lt; TODAY(), TRUE, FALSE) , 
 IF(ASI_HK_CRM_Order_Date__c &lt;   DATEVALUE(CreatedDate ) , TRUE, FALSE) )</errorConditionFormula>
        <errorDisplayField>ASI_HK_CRM_Order_Date__c</errorDisplayField>
        <errorMessage>For new orders, the Order Date cannot be less than today.
For existing orders, the Order Date cannot be less than the Sales Order record Created Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_HK_CRM_Time_null_for_Time</fullName>
        <active>true</active>
        <errorConditionFormula>if( or( not(isblank(TEXT(ASI_HK_CRM_Delivery_Time_1__c))), not(isblank(TEXT(ASI_HK_CRM_Delivery_Time_2__c)))), isblank(TEXT( ASI_HK_CRM_Delivery_Window_Instruction__c )),false)</errorConditionFormula>
        <errorDisplayField>ASI_HK_CRM_Delivery_Window_Instruction__c</errorDisplayField>
        <errorMessage>Please fill in Delivery Window Instruction if Delivery time is not blank!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_HK_CRM_Time_null_for_Window</fullName>
        <active>true</active>
        <errorConditionFormula>if(not(isblank(TEXT(ASI_HK_CRM_Delivery_Window_Instruction__c))),isblank(TEXT( ASI_HK_CRM_Delivery_Time_1__c )),false)</errorConditionFormula>
        <errorDisplayField>ASI_HK_CRM_Delivery_Time_1__c</errorDisplayField>
        <errorMessage>Delivery Time cannot be empty if Delivery Window Instruction is not blank!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_HK_CRM_Uncheck_Urgent_Order</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( ASI_HK_CRM_Urgent_Order_Confirmed__c, &quot;No&quot;) &amp;&amp;  ASI_HK_CRM_Urgent_Order__c &amp;&amp; CONTAINS(RecordType.DeveloperName,&apos;HK&apos;)&amp;&amp; !INCLUDES($User.BypassTriggers__c,&apos;ASI_HK_IT_admin&apos;)</errorConditionFormula>
        <errorDisplayField>ASI_HK_CRM_Urgent_Order__c</errorDisplayField>
        <errorMessage>Please unchecked the box</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_HK_Mpay_Transaction_id</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( ASI_HK_CRM_Reason_PaymentMethodChange__c) &amp;&amp; TEXT(ASI_HK_CRM_Payment_Method__c) = &apos;PAID by credit card via Mpay&apos; &amp;&amp; CONTAINS(RecordType.DeveloperName,&apos;HK&apos;)&amp;&amp; !INCLUDES($User.BypassTriggers__c,&apos;ASI_HK_IT_admin&apos;)</errorConditionFormula>
        <errorDisplayField>ASI_HK_CRM_Payment_Method__c</errorDisplayField>
        <errorMessage>This is a cash before delivery order, please enter the Completed Transaction ID from Mpay onto below box:</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_HK_Wrong_Price_Item</fullName>
        <active>true</active>
        <errorConditionFormula>AND( CONTAINS(RecordType.DeveloperName,&apos;HK&apos;),ASI_CRM_Wrong_Free_Item__c &gt;0, !INCLUDES($User.BypassTriggers__c,&apos;ASI_HK_IT_admin&apos;))</errorConditionFormula>
        <errorMessage>This order includes Zero Pricing item(s)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_HK_payment_method_change_reason</fullName>
        <active>true</active>
        <errorConditionFormula>(NOW()-  CreatedDate)*24*60*60&gt;=20   &amp;&amp; ISBLANK( ASI_HK_CRM_Reason_PaymentMethodChange__c) &amp;&amp; (
(ASI_HK_CRM_Customer_DA__r.ASI_HK_CRM_Payment_Term_Code__c =&apos;COD&apos; &amp;&amp; TEXT(ASI_HK_CRM_Payment_Method__c) &lt;&gt; &apos;CHQ&apos;)
||
( BEGINS(ASI_HK_CRM_Customer_DA__r.ASI_HK_CRM_Payment_Term_Code__c, &apos;M&apos;) &amp;&amp; TEXT(ASI_HK_CRM_Payment_Method__c) &lt;&gt; &apos;On Credit&apos;
)
)</errorConditionFormula>
        <errorDisplayField>ASI_HK_CRM_Reason_PaymentMethodChange__c</errorDisplayField>
        <errorMessage>once user change &quot;Payment Method&quot;, shoud input &quot;Reason for Payment Method Change&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AS_HK_CRM_SO_Payment_Method</fullName>
        <active>true</active>
        <description>20161202 Wilken: Added Create Date checking to exclude historical SO causing error when transfer SO Owner</description>
        <errorConditionFormula>ASI_HK_CRM_Customer_DA__r.ASI_HK_CRM_Payment_Term_Code__c =&apos;COD&apos;  &amp;&amp; (
TEXT(ASI_HK_CRM_Payment_Method__c)  &lt;&gt; &apos;CASH&apos; &amp;&amp; 
TEXT(ASI_HK_CRM_Payment_Method__c)  &lt;&gt; &apos;CHQ&apos;  &amp;&amp; 
TEXT(ASI_HK_CRM_Payment_Method__c)  &lt;&gt; &apos;PAID by bank transfer&apos; &amp;&amp;
TEXT(ASI_HK_CRM_Payment_Method__c)  &lt;&gt; &apos;PAID by credit card via Mpay&apos;
)
&amp;&amp; 
 CreatedDate &gt;=  DATETIMEVALUE(&quot;2016-03-08 00:00:00&quot;)
&amp;&amp;  not( ISPICKVAL(ASI_HK_CRM_Order_Status_Name__c, &apos;Draft&apos;))</errorConditionFormula>
        <errorDisplayField>ASI_HK_CRM_Payment_Method__c</errorDisplayField>
        <errorMessage>For &quot;COD&quot; account, Payment Method must either be &quot;CASH&quot;, &quot;CHQ&quot; , &quot;PAID by bank transfer&quot; or &quot;PAID by credit card via Mpay&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AS_HK_VIP_Bank_Transfer</fullName>
        <active>true</active>
        <description>20161202 Wilken: Added Create Date checking to exclude historical SO causing error when transfer SO Owner</description>
        <errorConditionFormula>TEXT(ASI_HK_CRM_Customer_DA__r.ASI_HK_CRM_Customer_Type__c) !=&apos;3ZZ&apos;  &amp;&amp; 
TEXT(ASI_HK_CRM_Payment_Method__c)  = &apos;VIP Bank Transfer&apos;</errorConditionFormula>
        <errorDisplayField>ASI_HK_CRM_Payment_Method__c</errorDisplayField>
        <errorMessage>Payment method: VIP Bank Transfer only applicable to Luxury Team</errorMessage>
    </validationRules>
