    <validationRules>
        <fullName>PRH_CheckRecordTypeValue</fullName>
        <active>true</active>
        <description>Check value of checkbox PRH_Competitor_Brand__c regarding value of Record Type</description>
        <errorConditionFormula>OR( 
AND (PRH_Competitor_Brand__c = FALSE, $RecordType.Name = &quot;Luxury Account - Competitor&quot;), 
AND (PRH_Competitor_Brand__c = TRUE, $RecordType.Name = &quot;Luxury Account - Pernod&quot;),
AND (PRH_Competitor_Brand__c = TRUE, $RecordType.Name = &quot;Luxury Account - Ricard&quot;) 
)</errorConditionFormula>
        <errorDisplayField>PRH_Competitor_Brand__c</errorDisplayField>
        <errorMessage>If RecordType value is &quot;Luxury Account - Competitor&quot;, you must check field &quot;Competitor Brand&quot; otherwise field &quot;Competitor Brand&quot; must be unchecked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRH_CheckSequenceNumberValue</fullName>
        <active>true</active>
        <description>Sequence value must be :
- COMP{NNNN} if field Competitor Brand is checked
- LCEB{NNNN} otherwise</description>
        <errorConditionFormula>OR(
AND (PRH_Competitor_Brand__c= TRUE, NOT(REGEX(PRH_Sequence__c, &quot;COMP(\\d{4})?&quot;))),
AND (PRH_Competitor_Brand__c= FALSE, NOT(REGEX(PRH_Sequence__c, &quot;LCEB(\\d{4})?&quot;)))
)</errorConditionFormula>
        <errorDisplayField>PRH_Sequence__c</errorDisplayField>
        <errorMessage>Sequence value must be COMP{NNNN} if field Competitor Brand is checked, LCEB{NNNN} otherwise</errorMessage>
    </validationRules>
