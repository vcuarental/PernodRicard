        <externalId>false</externalId>
        <label>AES Key</label>
        <length>128</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>EncryptedPassword__c</fullName>
        <description>ESN - Password encrypted through AES encryption</description>
        <externalId>false</externalId>
        <label>Encrypted Password</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>ErrorNotificationEmail__c</fullName>
        <defaultValue>false</defaultValue>
        <description>ESN - When checked, the user defined in this Custom Settings will be notified by email in case of exception during the modification of the profile.</description>
        <externalId>false</externalId>
        <label>Error Notification by Email</label>
