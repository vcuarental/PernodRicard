        <externalId>false</externalId>
        <label>Production</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Username__c</fullName>
        <description>ESN - Username of the technical user responsible for calling the web service used to change the profile of the current user in the ESN Policy acceptance process</description>
        <externalId>false</externalId>
        <label>Username</label>
        <length>80</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
