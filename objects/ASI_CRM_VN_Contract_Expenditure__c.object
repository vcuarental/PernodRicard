    <validationRules>
        <fullName>ASI_CRM_VN_Invalid_AchievedTarget</fullName>
        <active>true</active>
        <errorConditionFormula>/*
ASI_CRM_VN_Achieved_Target_for_2nd_Pmt__c &gt;  ASI_CRM_VN_Achieved_Target_for_3rd_Pmt__c
||
ASI_CRM_VN_Achieved_Target_for_2nd_Pmt__c &gt;
ASI_CRM_VN_Achieved_Target_for_4th_Pmt__c 
||
ASI_CRM_VN_Achieved_Target_for_3rd_Pmt__c &gt;
ASI_CRM_VN_Achieved_Target_for_4th_Pmt__c
*/
AND(
 ISPICKVAL( ASI_CRM_VN_Item__c , &apos;Lump Sum&apos;) ,
!AND(
!ISBLANK(ASI_CRM_Achieved_Target_for_1st_Pmt__c), !ISBLANK(ASI_CRM_Achieved_Target_for_1st_Pmt__c),
IF(AND(ISBLANK(ASI_CRM_VN_Achieved_Target_for_2nd_Pmt__c)), true, ASI_CRM_Achieved_Target_for_1st_Pmt__c &lt; ASI_CRM_VN_Achieved_Target_for_2nd_Pmt__c) ,
IF(AND(ISBLANK(ASI_CRM_VN_Achieved_Target_for_3rd_Pmt__c)), true, ASI_CRM_VN_Achieved_Target_for_2nd_Pmt__c &lt; ASI_CRM_VN_Achieved_Target_for_3rd_Pmt__c) ,
IF(AND(ISBLANK(ASI_CRM_VN_Achieved_Target_for_4th_Pmt__c)), true, ASI_CRM_VN_Achieved_Target_for_3rd_Pmt__c &lt; ASI_CRM_VN_Achieved_Target_for_4th_Pmt__c) ,
IF(AND(ISBLANK(ASI_CRM_Achieved_Target_for_5th_Pmt__c)), true, ASI_CRM_VN_Achieved_Target_for_4th_Pmt__c &lt; ASI_CRM_Achieved_Target_for_5th_Pmt__c) ,
IF(AND(ISBLANK(ASI_CRM_Achieved_Target_for_6th_Pmt__c)), true, ASI_CRM_Achieved_Target_for_5th_Pmt__c &lt; ASI_CRM_Achieved_Target_for_6th_Pmt__c) ,
IF(AND(ISBLANK(ASI_CRM_Achieved_Target_for_7th_Pmt__c)), true, ASI_CRM_Achieved_Target_for_6th_Pmt__c &lt; ASI_CRM_Achieved_Target_for_7th_Pmt__c) ,
IF(AND(ISBLANK(ASI_CRM_Achieved_Target_for_8th_Pmt__c )), true, ASI_CRM_Achieved_Target_for_7th_Pmt__c &lt; ASI_CRM_Achieved_Target_for_8th_Pmt__c ) ,
IF(AND(ISBLANK(ASI_CRM_Achieved_Target_for_9th_Pmt__c )), true, ASI_CRM_Achieved_Target_for_8th_Pmt__c &lt; ASI_CRM_Achieved_Target_for_9th_Pmt__c ) ,
IF(AND(ISBLANK(ASI_CRM_Achieved_Target_for_10th_Pmt__c)), true, ASI_CRM_Achieved_Target_for_9th_Pmt__c &lt; ASI_CRM_Achieved_Target_for_10th_Pmt__c) 
)
)</errorConditionFormula>
        <errorMessage>Please enter at least 1 Payment schedule with Archieved Target % and Amount filled in. For each schedule, the subsequent Archieved Target % need to be higher than the former one</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_VN_Invalid_PaymentTime</fullName>
        <active>false</active>
        <errorConditionFormula>ISPICKVAL(ASI_CRM_VN_Payment_Times__c, &apos;4&apos;) &amp;&amp;  
(ISBLANK(ASI_CRM_VN_Achieved_Target_for_2nd_Pmt__c) ||
ISBLANK(ASI_CRM_VN_Achieved_Target_for_3rd_Pmt__c) || 
ISBLANK(ASI_CRM_VN_Achieved_Target_for_4th_Pmt__c))
||
ISPICKVAL(ASI_CRM_VN_Payment_Times__c, &apos;3&apos;) &amp;&amp;  
(ISBLANK(ASI_CRM_VN_Achieved_Target_for_2nd_Pmt__c) ||
ISBLANK(ASI_CRM_VN_Achieved_Target_for_3rd_Pmt__c))
||
ISPICKVAL(ASI_CRM_VN_Payment_Times__c, &apos;2&apos;) &amp;&amp;  ISBLANK(ASI_CRM_VN_Achieved_Target_for_2nd_Pmt__c)</errorConditionFormula>
        <errorMessage>Please input correct payment times and achieved target</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_VN_LumpSum_ScheduleCheck</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 TEXT(ASI_CRM_VN_Item__c) = &apos;Lump Sum&apos;,
 ASI_CRM_Total_Scheduled_Amount__c != ASI_CRM_Total_Amount__c
)</errorConditionFormula>
        <errorDisplayField>ASI_CRM_Achieved_Target_for_10th_Pmt__c</errorDisplayField>
        <errorMessage>Total Scheduled Amount must match the total Amount of Customer Sponsorship.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_VN_Mandatory_Amount</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 RecordType.DeveloperName = &apos;ASI_CRM_VN_Contract_Expenditure&apos;,
 TEXT(ASI_CRM_VN_Item__c) &lt;&gt; &apos;CS&apos;, 
 ISBLANK( ASI_CRM_VN_Amount__c )
)</errorConditionFormula>
        <errorDisplayField>ASI_CRM_VN_Amount__c</errorDisplayField>
        <errorMessage>Amount is mandatory</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_VN_Mandatory_PaymentTime</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 
ISPICKVAL(ASI_CRM_VN_Payment_Times__c, &apos;&apos;),
ISPICKVAL(ASI_CRM_VN_Item__c, &apos;CS&apos;)
)</errorConditionFormula>
        <errorDisplayField>ASI_CRM_VN_Payment_Times__c</errorDisplayField>
        <errorMessage>Payment Times is mandatory when Item is &apos;CS&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_VN_Missing_AchievedTarget</fullName>
        <active>true</active>
        <description>Achieved Target % must be inputted if corresponding payment is inputted.</description>
        <errorConditionFormula>AND(
ISPICKVAL(ASI_CRM_VN_Item__c, &apos;Lump Sum&apos;),
OR(
!ISBLANK(ASI_CRM_Amount_for_1st_Payment__c) &amp;&amp; ISBLANK(ASI_CRM_Achieved_Target_for_1st_Pmt__c),
!ISBLANK(ASI_CRM_Amount_for_2nd_Payment__c) &amp;&amp; ISBLANK(ASI_CRM_VN_Achieved_Target_for_2nd_Pmt__c),
