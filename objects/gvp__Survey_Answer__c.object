    <fields>
        <fullName>gvp__Answer_Formula__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF (ISPICKVAL(gvp__Question__r.gvp__Type__c,&quot;Text&quot;), gvp__Answer_Text__c,

IF (ISPICKVAL(gvp__Question__r.gvp__Type__c,&quot;Number&quot;), TEXT(gvp__Answer_Number__c),

IF (ISPICKVAL(gvp__Question__r.gvp__Type__c,&quot;Currency&quot;), &quot;$&quot; &amp; TEXT(gvp__Answer_Currency__c),

IF (ISPICKVAL(gvp__Question__r.gvp__Type__c,&quot;YesNo&quot;),
     IF(gvp__Answer_Text__c = &quot;true&quot;, $Label.gvp__yes ,
        IF(gvp__Answer_Text__c = &quot;false&quot;, $Label.gvp__no ,&quot;&quot;)),

IF (ISPICKVAL(gvp__Question__r.gvp__Type__c, &quot;Check&quot;),
     IF(gvp__Answer_Check__c, $Label.gvp__true , $Label.gvp__false ),

IF (ISPICKVAL(gvp__Question__r.gvp__Type__c,&quot;Radio&quot;), gvp__Answer_Text__c,

IF (ISPICKVAL(gvp__Question__r.gvp__Type__c,&quot;List&quot;), gvp__Answer_Text__c,

IF (ISPICKVAL(gvp__Question__r.gvp__Type__c,&quot;Percent&quot;), TEXT(gvp__Answer_Percent__c*100)&amp;&quot;%&quot;,

IF (ISPICKVAL(gvp__Question__r.gvp__Type__c,&quot;Set&quot;), gvp__Answer_Product_Set__r.Name,

IF (AND(ISPICKVAL(gvp__Question__r.gvp__Type__c, &quot;Lookup&quot;), NOT(ISBLANK(gvp__Answer_Item__c))), &quot;Item&quot;,

IF (AND(ISPICKVAL(gvp__Question__r.gvp__Type__c, &quot;Lookup&quot;), NOT(ISBLANK(gvp__Answer_Label__c))), &quot;Label&quot;,

IF (AND(ISPICKVAL(gvp__Question__r.gvp__Type__c, &quot;Lookup&quot;), NOT(ISBLANK(gvp__Answer_Brand__c))), gvp__Answer_Brand__r.Name,

IF (AND(ISPICKVAL(gvp__Question__r.gvp__Type__c, &quot;Lookup&quot;), NOT(ISBLANK(gvp__Answer_Competitor__c))), gvp__Answer_Competitor__r.Name,

IF (AND(ISPICKVAL(gvp__Question__r.gvp__Type__c, &quot;Lookup&quot;), NOT(ISBLANK(gvp__Answer_Program__c))), gvp__Answer_Program__r.Name,

IF (AND(ISPICKVAL(gvp__Question__r.gvp__Type__c, &quot;Lookup&quot;), NOT(ISBLANK(gvp__Answer_Product_Set__c))), gvp__Answer_Product_Set__r.Name,

&quot;&quot;)))))))))))))))</formula>
