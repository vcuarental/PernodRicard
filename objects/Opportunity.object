    <fields>
        <fullName>LAT_MX_CreditAndCollectionsStatus__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
	(RecordType.DeveloperName=&apos;LAT_MX_OPP_HeaderBlocked&apos; ||
	RecordType.DeveloperName=&apos;LAT_MX_OPP_NewOrder&apos; ||
	RecordType.DeveloperName=&apos;LAT_MX_OPP_OrderBlocked&apos;), 
	IF(
		(ISBLANK(NR_order_JDE__c) &amp;&amp; ISBLANK(LAT_MX_RetentionReason__c)),
		&apos;&apos;,
		CASE(LAT_MX_RetentionReason__c,
					&apos;C3&apos;,&apos;Bloqueado por C&amp;C&apos;,
					&apos;C1&apos;,&apos;Bloqueado por excedente limite de credito&apos;,
					&apos;CH&apos;,&apos;Bloqueado por cheque devuelto&apos;,
					&apos;C7&apos;,&apos;Bloqueado por saldos vencidos&apos;,
					&apos;??&apos;,&apos;Bloqueado por no confirmar el cobrable&apos;,
					&apos;&apos;,&apos;Pedido liberado&apos;,
					&apos;&apos;)
	), 
	&apos;&apos;
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Credit and Collections Status</label>
        <required>false</required>
