        window.alert(&quot;No se puede cancelar el pedido porque al menos una producto tiene estado mayor a 540&quot;);
    } else {

        var sMsg = sforce.apex.execute(&quot;WS03_SalesOrderHandlerInterface_AR&quot;,&quot;SalesOrderInterface&quot;,{idSalesforce:Id});

        if (sMsg != 0 ){
            window.alert(sMsg);
        }
