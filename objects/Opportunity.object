        <fullName>Cancelar_Pedido</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Cancelar Pedido</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/15.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/15.0/apex.js&quot;)} 

function cancelarPedido(){
  if(!confirm(&apos;Deseja realmente cancelar o Pedido?&apos;)){
    return;
  }
  var Id=&quot;{!Opportunity.Id }&quot;;
  var fase = &quot;{!Opportunity.StageName}&quot;;
  if(fase == &quot;Novo pedido&quot;){
    window.alert(&quot;Pedido com status Novo Pedido não pode ser Cancelado, pois ainda não foi enviado para o JDE.&quot;);
  }else if (fase == &quot;Pedido cancelado no JDE&quot;){
    window.alert(&quot;Pedido já cancelado no JDE.&quot;);
  }else{
    sforce.apex.execute(&quot;CancelarPedido&quot;,&quot;RealizarCancelamento&quot;,{idOportunidade:Id});
    var sMsg = sforce.apex.execute(&quot;SalesOrderHandlerInterface&quot;,&quot;SalesOrderInterface&quot;,{idSalesforce:Id}); 
    if(sMsg != 0){
      window.alert(sMsg); 
    }
    window.location.reload();
  }
}

cancelarPedido();</url>
