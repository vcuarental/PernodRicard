    <validationRules>
        <fullName>Bloqueia_alteracao_da_data_de_faturament</fullName>
        <active>true</active>
        <errorConditionFormula>(RecordType.DeveloperName=&apos;Nova_oportunidade&apos;
||RecordType.DeveloperName=&apos;Bloqueia_alteracao_do_cabecalho&apos;
||RecordType.DeveloperName=&apos;Bloqueia_alteracao&apos;) 
&amp;&amp; 
(
AND( NOT(Integracao__c), ISCHANGED(CloseDate),  NOT(ISPICKVAL(StageName, &quot;Novo pedido&quot;)))
)</errorConditionFormula>
        <errorDisplayField>CloseDate</errorDisplayField>
        <errorMessage>Data não pode ser alterada neste status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Bloqueia_alteracao_do_pedido</fullName>
        <active>true</active>
        <errorConditionFormula>(RecordType.DeveloperName=&apos;Nova_oportunidade&apos;
||RecordType.DeveloperName=&apos;Bloqueia_alteracao_do_cabecalho&apos;
||RecordType.DeveloperName=&apos;Bloqueia_alteracao&apos;) 
&amp;&amp; 
NOT( ISPICKVAL(StageName, &quot;Mobile Order&quot;))
&amp;&amp;
(
OR(
         AND(  NOT(Integracao__c) ,   
                     ISCHANGED(Name), 
                     ISCHANGED(StageName),
                     NOT(ISNEW()),
                     NOT( ISPICKVAL(StageName, &quot;Novo pedido&quot;)) 
                   ),
         AND(  
                     NOT(Integracao__c) ,
                     ISNEW(),
                     NOT( ISPICKVAL(StageName, &quot;Novo pedido&quot;)) 
                   )
       )
)</errorConditionFormula>
        <errorDisplayField>StageName</errorDisplayField>
        <errorMessage>O pedido não pode ser alterado devido ao seu status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Bloqueia_delecao_item_pedido</fullName>
        <active>false</active>
        <errorConditionFormula>(RecordType.DeveloperName=&apos;Nova_oportunidade&apos;
||RecordType.DeveloperName=&apos;Bloqueia_alteracao_do_cabecalho&apos;
||RecordType.DeveloperName=&apos;Bloqueia_alteracao&apos;) 
&amp;&amp; 
(
AND(PRIORVALUE(Quantidade_com_status__c) &gt;  Quantidade_com_status__c,
NOT(Integracao__c))
)</errorConditionFormula>
        <errorMessage>Este item não pode ser excluído</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Bloqueia_pedido_cancelado</fullName>
        <active>true</active>
        <errorConditionFormula>(
RecordType.DeveloperName=&apos;Nova_oportunidade&apos;
||
RecordType.DeveloperName=&apos;Bloqueia_alteracao_do_cabecalho&apos;
||
RecordType.DeveloperName=&apos;Bloqueia_alteracao&apos;
)
&amp;&amp;
NOT(Integracao__c)
&amp;&amp;
Cancelar_pedido__c
&amp;&amp;
(PRIORVALUE(TotalOpportunityQuantity) &lt;&gt;  TotalOpportunityQuantity)
&amp;&amp;
$Profile.Name &lt;&gt; &quot;Administrador do sistema&quot;
&amp;&amp;
$Profile.Name &lt;&gt; &quot;LAT_SystemAdministrator&quot;
&amp;&amp;
$Profile.Name &lt;&gt; &quot;PR BRAZIL System Administrator&quot;
&amp;&amp;
$Profile.Name &lt;&gt; &quot;System Administrator&quot;</errorConditionFormula>
        <errorMessage>Este pedido não pode ser alterado pois está cancelado.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Impede_criacao_pedido</fullName>
        <active>true</active>
        <errorConditionFormula>(RecordType.DeveloperName=&apos;Nova_oportunidade&apos; 
||RecordType.DeveloperName=&apos;Bloqueia_alteracao_do_cabecalho&apos; 
||RecordType.DeveloperName=&apos;Bloqueia_alteracao&apos;) &amp;&amp; 

ISBLANK( Account.Client_code_AN8__c )</errorConditionFormula>
        <errorMessage>Impossível gerar pedido, cliente não cadastrado no JDE ou sem grupo de preços.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LAT_MX_OPP_VR01_CloseDateValidation</fullName>
        <active>true</active>
        <description>Validates that when creating a new record the field CloseDate (Fecha de ingreso) can only be the current date.</description>
        <errorConditionFormula>(
RecordType.DeveloperName=&apos;LAT_MX_OPP_HeaderBlocked&apos; || 
RecordType.DeveloperName=&apos;LAT_MX_OPP_NewOrder&apos; || 
RecordType.DeveloperName=&apos;LAT_MX_OPP_OrderBlocked&apos;
)
&amp;&amp;
(
  (ISNEW() &amp;&amp; CloseDate != TODAY())
  ||
  (NOT(ISNEW()) &amp;&amp; PRIORVALUE(CloseDate) != CloseDate)
)
&amp;&amp;
NOT(Integracao__c)</errorConditionFormula>
        <errorDisplayField>CloseDate</errorDisplayField>
        <errorMessage>Must be equal to the date the Opportunity was created.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LAT_MX_OPP_VR02_OrderPaymentValidation</fullName>
        <active>true</active>
        <description>Validates that the field &apos;Order payment period reference&apos; can only be modified when Status is &apos;Novo pedido&apos;.</description>
        <errorConditionFormula>(
RecordType.DeveloperName=&apos;LAT_MX_OPP_HeaderBlocked&apos; || 
RecordType.DeveloperName=&apos;LAT_MX_OPP_NewOrder&apos; || 
RecordType.DeveloperName=&apos;LAT_MX_OPP_OrderBlocked&apos;
)
&amp;&amp;
ISCHANGED(LAT_MX_OrderPaymentPeriodReference__c)
&amp;&amp;
NOT(ISPICKVAL(StageName,&apos;Novo pedido&apos;))
&amp;&amp;
NOT(Integracao__c)</errorConditionFormula>
        <errorDisplayField>StageName</errorDisplayField>
        <errorMessage>Cannot be modified if &apos;Stage&apos; is not &apos;Novo pedido&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LAT_MX_OPP_VR03_PurchOrderSincValidation</fullName>
        <active>true</active>
        <description>Validates that the field &apos;Purchase Order (Since)&apos; is less or equal than &apos;Purchase Order (Until)&apos;.</description>
        <errorConditionFormula>(
