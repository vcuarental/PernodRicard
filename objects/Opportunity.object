if (window.confirm(&quot;Desea Cancelar el Pedido?&quot;))
  {

if (fase == &quot;Novo pedido&quot; || fase == &quot;Pedido Nuevo&quot;) {
    window.alert(&quot;El pedido aun no fue enviado a JDE. No es posible cancelarlo.&quot;);
} else if (fase == &quot;Pedido cancelado no JDE&quot; || fase == &quot;Pedido cancelado en JDE&quot;) {
