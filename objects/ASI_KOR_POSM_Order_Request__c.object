    <validationRules>
        <fullName>ASI_KOR_PreventSubmitIfNoDetail</fullName>
        <active>true</active>
        <description>Prevent users submitting the request if no POSM product is added</description>
        <errorConditionFormula>ISPICKVAL(ASI_KOR_Request_Status__c, &apos;Requested&apos;) &amp;&amp; ASI_KOR_Number_of_Items__c = 0</errorConditionFormula>
        <errorMessage>At least one product is required in order to submit the request</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_KOR_ValidateContractPeriodMonth</fullName>
        <active>true</active>
        <description>month number must be in 01 to 12</description>
        <errorConditionFormula>AND(
  CONTAINS(RecordType.DeveloperName , &apos;ASI_KOR&apos;),
  CONTAINS(RecordType.DeveloperName , &apos;Bar_Styling&apos;),
 (VALUE( RIGHT(ASI_KOR_Contract_Period__c, 2) ) &gt; 12) || (VALUE( RIGHT(ASI_KOR_Contract_Period__c, 2) ) &lt;= 0)
)</errorConditionFormula>
        <errorDisplayField>ASI_KOR_Contract_Period__c</errorDisplayField>
        <errorMessage>Invalid Month Number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_KOR_ValidateFiscalYear</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  CONTAINS(RecordType.DeveloperName , &apos;ASI_KOR&apos;),
  CONTAINS(RecordType.DeveloperName , &apos;Bar_Styling&apos;),
 ((LEFT(ASI_KOR_Fiscal_Year__c, 2)) &lt;&gt; &apos;FY&apos;) 
|| NOT(
	VALUE(RIGHT(TEXT((VALUE(LEFT(RIGHT(ASI_KOR_Fiscal_Year__c, 4), 2)) +1)), 2)) = VALUE((RIGHT(ASI_KOR_Fiscal_Year__c, 2)))
	)
)</errorConditionFormula>
        <errorDisplayField>ASI_KOR_Fiscal_Year__c</errorDisplayField>
        <errorMessage>Invalid Fiscal Year Format</errorMessage>
    </validationRules>
