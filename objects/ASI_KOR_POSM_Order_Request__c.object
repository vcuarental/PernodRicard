        <fullName>ASI_KOR_Bar_Styling_Finalize</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Finalize</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>&apos;{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/connection.js&quot;)}&apos; 
&apos;{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/apex.js&quot;)} &apos; 

sforce.connection.sessionId = &quot;{!$Api.Session_ID}&quot;;

try {

 var user = sforce.connection.getUserInfo();
 console.log(&apos;{!ASI_KOR_POSM_Order_Request__c.ASI_KOR_SYS_Approver_1Id__c}&apos;);
 console.log(user.userId.substring(0,15));

 // need to check attachment 
 if(&apos;{!ASI_KOR_POSM_Order_Request__c.ASI_KOR_SYS_Approver_1Id__c}&apos; != user.userId.substring(0,15)){ 
  alert(&apos;Only BMKT approver of this request can finalize.&apos;); 
 } else
 // need to check Installation Completion Date 
 if(&apos;{!ASI_KOR_POSM_Order_Request__c.ASI_KOR_Installation_Completion_Date__c}&apos;== &apos;&apos;){ 
  alert(&apos;IInstallation Completion Date is missing.&apos;); 
 } else
 // need to check attachment 
 if(&apos;{!ASI_KOR_POSM_Order_Request__c.ASI_KOR_Total_No_of_Installation_Report__c}&apos;== 0){ 
  alert(&apos;Installation Report is missing.&apos;); 
 } else{ 

  var ob = new sforce.SObject(&quot;ASI_KOR_POSM_Order_Request__c&quot;); 
  ob.Id = &apos;{!ASI_KOR_POSM_Order_Request__c.Id}&apos;; 
  ob.ASI_KOR_Freeze_Request__c = true; 
  result = sforce.connection.update([ob]); 

  if (!result[0].getBoolean(&apos;success&apos;)) { 
    alert(&apos;An error occurred. Details : &apos; + result[0].errors.message); 
  } else { 
    navigateToUrl(&apos;/{!ASI_KOR_POSM_Order_Request__c.Id}&apos;); 
  } 
 }
}catch(error) { 
 sforce.debug.log(error.faultcode); 
 sforce.debug.log(error.faultstring); 
 alert(&apos;An error occurred. Details : &apos; + error); 
}</url>
