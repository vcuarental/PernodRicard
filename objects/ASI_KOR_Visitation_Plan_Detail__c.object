    <validationRules>
        <fullName>ASI_CRM_KR_ADHocValidation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(CONTAINS( RecordType.DeveloperName, &apos;ASI_CRM_KR_Visitation_Plan_Detail_Ad_Hoc&apos;),ISPICKVAL(ASI_KOR_Status__c, &quot;&quot;), !CONTAINS($Profile.Name,&apos;Administrator&apos;))</errorConditionFormula>
        <errorDisplayField>ASI_KOR_Status__c</errorDisplayField>
        <errorMessage>Status cannot be null!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_KR_AccountOwnerValidation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ASI_KOR_Wholesaler__r.Owner.Id &lt;&gt; ASI_KOR_Visitation_Plan__r.OwnerId ,!CONTAINS($Profile.Name,&apos;Administrator&apos;), !$Permission.ASI_CRM_KR_Edit_Visitation_Plan_Detail )</errorConditionFormula>
        <errorDisplayField>ASI_KOR_Wholesaler__c</errorDisplayField>
        <errorMessage>Please select your own account(s)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_KR_AchievedPlanChecking</fullName>
        <active>true</active>
        <description>Users can change the status to &quot;Achieved&quot; even they don&apos;t check one of call objectives.</description>
        <errorConditionFormula>AND(!CONTAINS($Profile.Name,&apos;Administrator&apos;),ISPICKVAL(ASI_KOR_Status__c, &quot;Achieved&quot;) , !ASI_CRM_KR_PRK_Volume_Complete__c ,! ASI_CRM_KR_Market_Information_Complete__c ,! ASI_CRM_KR_Listing_Complete__c ,! ASI_CRM_KR_POS_Complete__c ,! ASI_CRM_KR_Visibility_Complete__c ,! ASI_CRM_KR_IncentivesnPromotionsComplete__c ,! ASI_CRM_KR_RelationshipBuilding_Complete__c ,! ASI_CRM_KR_Negotiation_Contract_Complete__c ,! ASI_KR_Negotiation_NonContract_Complete__c ,! ASI_CRM_KR_Venue_Check_Complete__c )</errorConditionFormula>
        <errorMessage>Please check one of call objectives.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_KR_Call_Objective_Checking</fullName>
        <active>true</active>
        <errorConditionFormula>AND( !ASI_KOR_Visitation_Plan__r.ASI_KOR_Confirmed__c , !CONTAINS($Profile.Name,&apos;Administrator&apos;), OR(ISCHANGED( ASI_CRM_KR_Market_Information_Complete__c ),ISCHANGED( ASI_CRM_KR_PRK_Volume_Complete__c ),ISCHANGED( ASI_CRM_KR_Listing_Complete__c ) ,ISCHANGED( ASI_CRM_KR_POS_Complete__c ) ,ISCHANGED( ASI_CRM_KR_Visibility_Complete__c ) ,ISCHANGED( ASI_CRM_KR_IncentivesnPromotionsComplete__c ) , ISCHANGED( ASI_CRM_KR_RelationshipBuilding_Complete__c ) ,ISCHANGED( ASI_KR_Negotiation_NonContract_Complete__c ) ,ISCHANGED( ASI_CRM_KR_Negotiation_Contract_Complete__c ) ,ISCHANGED( ASI_CRM_KR_Venue_Check_Complete__c ) ) )</errorConditionFormula>
        <errorMessage>The Visitation Plan have not confirmed, cannot edit Call Objective</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_KR_CancelledPlanValidation</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(ASI_KOR_Status__c, &quot;Cancelled&quot;) &amp;&amp; ISPICKVAL(PRIORVALUE( ASI_KOR_Status__c), &quot;&quot;)&amp;&amp; !CONTAINS($Profile.Name,&apos;Administrator&apos;)&amp;&amp;
ASI_KOR_Visitation_Plan__r.ASI_KOR_Confirmed__c</errorConditionFormula>
        <errorDisplayField>ASI_KOR_Status__c</errorDisplayField>
        <errorMessage>Cannot change Plan from new Event to Cancelled</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_KR_CancelledRequiredField</fullName>
        <active>true</active>
        <description>ASI_CRM_KR_CancelledRequiredField</description>
        <errorConditionFormula>ISPICKVAL(ASI_KOR_Status__c, &apos;Cancelled&apos;) &amp;&amp;  OR(  ISPICKVAL ( ASI_CRM_KR_Cancel_Type__c,&apos;&apos; ),  ISBLANK( ASI_KOR_Cancellation_Reason__c ) )&amp;&amp; !CONTAINS($Profile.Name,&apos;Administrator&apos;)</errorConditionFormula>
        <errorMessage>Please Input Cancel Type and Cancellation Reason.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_KR_ChangeAchievedPlan</fullName>
        <active>false</active>
        <errorConditionFormula>AND(ISPICKVAL(PRIORVALUE( ASI_KOR_Status__c), &quot;Achieved&quot;)
, ISCHANGED(ASI_KOR_Status__c) ,!CONTAINS($Profile.Name,&apos;Administrator&apos;))</errorConditionFormula>
        <errorDisplayField>ASI_KOR_Status__c</errorDisplayField>
        <errorMessage>Cannot change Status in Achieved Plan.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_KR_ChangeCancelledPlan</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(PRIORVALUE( ASI_KOR_Status__c), &quot;Cancelled&quot;), ISCHANGED(ASI_KOR_Status__c) ,!CONTAINS($Profile.Name,&apos;Administrator&apos;))</errorConditionFormula>
        <errorDisplayField>ASI_KOR_Status__c</errorDisplayField>
        <errorMessage>Cannot change Status in Cancelled Plan.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_KR_ConfirmedPlanChecking</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(ASI_KOR_Status__c, &quot;&quot;) &amp;&amp; ISPICKVAL(PRIORVALUE( ASI_KOR_Status__c), &quot;Cancelled&quot;)&amp;&amp; !CONTAINS($Profile.Name,&apos;Administrator&apos;)&amp;&amp; 
ASI_KOR_Visitation_Plan__r.ASI_KOR_Confirmed__c</errorConditionFormula>
        <errorDisplayField>ASI_KOR_Status__c</errorDisplayField>
        <errorMessage>Cannot change Plan from Cancelled to new Event, because Plan is confirmed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_KR_EditNewEventPlanValidation</fullName>
        <active>true</active>
        <description>ASI_CRM_KR_EditNewEventPlanValidation</description>
        <errorConditionFormula>ISPICKVAL( ASI_KOR_Status__c , &apos;&apos;) &amp;&amp; !ISNEW()  &amp;&amp; ASI_KOR_Visitation_Plan__r.ASI_KOR_Confirmed__c != TRUE &amp;&amp; !CONTAINS($Profile.Name,&apos;Administrator&apos;)</errorConditionFormula>
        <errorMessage>Cannot edit New Event Plan.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_KR_NewStatusToCancelled</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(ASI_KOR_Status__c, &quot;Cancelled&quot;) &amp;&amp; ISPICKVAL(PRIORVALUE( ASI_KOR_Status__c), &quot;&quot;)&amp;&amp; !CONTAINS($Profile.Name,&apos;Administrator&apos;)</errorConditionFormula>
        <errorDisplayField>ASI_KOR_Status__c</errorDisplayField>
        <errorMessage>Cannot change status from New Event to Cancelled !</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_KR_PlannedToNewEvent</fullName>
        <active>true</active>
        <description>ASI_CRM_KR_PlannedToNewEvent</description>
        <errorConditionFormula>ISPICKVAL(ASI_KOR_Status__c, &quot;&quot;) &amp;&amp; ISPICKVAL(PRIORVALUE( ASI_KOR_Status__c), &quot;Planned&quot;)&amp;&amp; !CONTAINS($Profile.Name,&apos;Administrator&apos;)&amp;&amp; 
ASI_KOR_Visitation_Plan__r.ASI_KOR_Confirmed__c</errorConditionFormula>
        <errorDisplayField>ASI_KOR_Status__c</errorDisplayField>
        <errorMessage>Cannot change status from Planned to New status!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_KR_PlannedValidation</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(ASI_KOR_Status__c, &quot;Planned&quot;) &amp;&amp; ASI_KOR_Visitation_Plan__r.ASI_KOR_Confirmed__c != TRUE &amp;&amp; !CONTAINS($Profile.Name,&apos;Administrator&apos;)</errorConditionFormula>
        <errorDisplayField>ASI_KOR_Status__c</errorDisplayField>
        <errorMessage>Cannot change to Planned, because Plan is not yet confirmed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_KOR_CannotEditConfirmed</fullName>
        <active>false</active>
        <description>Should not update confirmed visitations</description>
        <errorConditionFormula>ASI_KOR_Visitation_Plan__r.ASI_KOR_Confirmed__c == TRUE</errorConditionFormula>
        <errorMessage>The visitation plan is already confirmed and copied to visitation events.</errorMessage>
    </validationRules>
