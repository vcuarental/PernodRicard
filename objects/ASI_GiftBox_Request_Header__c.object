    <validationRules>
        <fullName>ASI_CRM_VN_CheckOrderDate</fullName>
        <active>true</active>
        <errorConditionFormula>AND
(
OR(
RecordType.DeveloperName == &apos;ASI_CRM_VN_FOC_Request&apos;,
RecordType.DeveloperName == &apos;ASI_CRM_POSM_VN_Request&apos;,
RecordType.DeveloperName == &apos;ASI_CRM_VN_Display_Listing_Fee_Request&apos;,
RecordType.DeveloperName == &apos;ASI_CRM_VN_Promotion_Request&apos;,
RecordType.DeveloperName == &apos;ASI_CRM_VN_CS_Request&apos;,
RecordType.DeveloperName == &apos;ASI_CRM_VN_Capsule_CS_Request&apos;
),
OR(
ASI_CRM_Order_Date__c &gt; ASI_CRM_VN_Contract__r.ASI_CRM_Valid_Claim_Date__c,
ASI_CRM_Order_Date__c &lt; ASI_CRM_VN_Contract_Start_Date__c)
)</errorConditionFormula>
        <errorDisplayField>ASI_CRM_Order_Date__c</errorDisplayField>
        <errorMessage>Order Date inputted must not be back-date, and must not exceed the MCF valid claim date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_VN_CheckPromotionPlanBalance</fullName>
        <active>true</active>
        <description>Promotion Plan Balance &gt; 0</description>
        <errorConditionFormula>IF( 
ISNULL(ASI_CRM_Trade_A_P_Plan__c ), 
false, 
AND(
ASI_CRM_Trade_A_P_Plan__r.ASI_CRM_Balance_Amount__c &lt;= 0, 
RecordType.DeveloperName == &apos;ASI_CRM_VN_Promotion_Request&apos;
)
)</errorConditionFormula>
        <errorMessage>Promotion Plan Balance must greater than 0.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_VN_CheckPromotionPlanDate</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ASI_CRM_Trade_A_P_Plan__r.ASI_CRM_End_Date__c &lt;  ASI_CRM_Order_Date__c, 
RecordType.DeveloperName == &apos;ASI_CRM_VN_Promotion_Request&apos;
)</errorConditionFormula>
        <errorDisplayField>ASI_CRM_Order_Date__c</errorDisplayField>
        <errorMessage>Order Date inputted must not exceed the Promotion Plan end date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_VN_CheckPromotionPlanItemBalance</fullName>
        <active>true</active>
        <description>At least 1 promotion plan item&apos;s balance &gt; 0.</description>
        <errorConditionFormula>AND( 
ASI_CRM_Trade_A_P_Plan__r.ASI_CRM_Item_Count__c &gt; 0,
ASI_CRM_Trade_A_P_Plan__r.ASI_CRM_Item_Balance_Sum__c &lt;= 0, 
RecordType.DeveloperName == &apos;ASI_CRM_VN_Promotion_Request&apos; 
)</errorConditionFormula>
        <errorMessage>Promotion plan items have no balance.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_CRM_VN_CheckReceiverInformation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
OR(
RecordType.DeveloperName == &apos;ASI_CRM_VN_CS_Request&apos;,
RecordType.DeveloperName == &apos;ASI_CRM_VN_Capsule_CS_Request&apos;
),
OR(
AND(
ASI_CRM_VN_Cash__c == false,
ASI_CRM_VN_Bank__c == false
),
AND(
ASI_CRM_VN_Cash__c == true,
ASI_CRM_VN_Bank__c == true
),
AND(
ASI_CRM_VN_Cash__c == true,
OR(
ISBLANK(ASI_CRM_VN_Cash_Payee_Name__c),
ISBLANK(ASI_CRM_VN_Cash_Payee_ID_Passport__c),
ISBLANK(ASI_CRM_VN_Cash_Payee_Contact__c) 
)
),
AND(
ASI_CRM_VN_Bank__c == true,
OR(
ISBLANK(ASI_CRM_VN_Bank_Payee_Name__c),
ISBLANK(ASI_CRM_VN_Bank_Payee_AC__c),
ISBLANK(ASI_CRM_VN_Bank_Payee_Bank_Name__c),
ISBLANK(ASI_CRM_VN_Bank_Payee_Branch__c)
)
)
)
)</errorConditionFormula>
        <errorMessage>Please input correct receiver information</errorMessage>
    </validationRules>
