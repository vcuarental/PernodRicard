    <webLinks>
        <fullName>ASI_CRM_VN_Submit_for_Approval</fullName>
        <availability>online</availability>
        <description>For record type &apos;ASI CRM VN FOC Request&apos;, &apos;ASI CRM VN Capsule CS Request&apos;, &apos;ASI CRM VN Display Listing Fee Request&apos;, or &apos;ASI POSM VN Request&apos; approval submission.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Submit for Approval</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!requireScript(&quot;/soap/ajax/29.0/connection.js&quot;)} 

block: { 
	var reason = prompt(&quot;Please enter approval reason&quot;, &quot;&quot;);	
	if(reason){		
		sforce.connection.sessionId = &quot;{!$Api.Session_ID}&quot;; 

		var requestItems = sforce.connection.query(&quot;Select Id, ASI_CRM_Over_Balance__c from ASI_GiftBox_Request_Item__c where ASI_GiftBox_Request_Id__c = &apos;{!ASI_GiftBox_Request_Header__c.Id}&apos; &quot;); 

		var records = requestItems.getArray(&quot;records&quot;); 

		if(records.length == 0){
			alert(&apos;You can not submit this request due to no line item&apos;); 
			break block; 
		}

		var rec = new sforce.SObject(&apos;ASI_GiftBox_Request_Header__c&apos;); 
		rec.Id = &quot;{!ASI_GiftBox_Request_Header__c.Id}&quot;; 
		rec.ASI_CRM_Sys_Pending_Approval__c = true; 

			for (var i = 0; i &lt; records.length; i++) {
			if(records[i].ASI_CRM_Over_Balance__c == &quot;YES&quot;)
			{
				rec.ASI_CRM_Over_Balance__c = true
			}
		}

			try{ 
			result = sforce.connection.update([rec]); 
					window.open(&quot;/apex/ASI_SubmitApprovalPage?id={!ASI_GiftBox_Request_Header__c.Id}&amp;reason=&quot; + reason,&quot;_self&quot;);
		}catch(error){ 
			sforce.debug.log(error.faultcode); 
			sforce.debug.log(error.faultstring); 
		} 
	}
	else{
		alert(&quot;Please enter approval reason&quot;);
	}
}</url>
