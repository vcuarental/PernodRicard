    <validationRules>
        <fullName>project_cloud__Access_Level_User_Only</fullName>
        <active>true</active>
        <description>When the attendee is a user an access level is required. If the attendee is a contact the access level must be blank.</description>
        <errorConditionFormula>IF(ISBLANK(project_cloud__User__c),
	NOT(ISBLANK(project_cloud__Access_Level__c)),
/*ELSE*/
	ISBLANK(project_cloud__Access_Level__c)
)</errorConditionFormula>
        <errorDisplayField>project_cloud__Access_Level__c</errorDisplayField>
        <errorMessage>User attendees require an access level. Contact attendees must not have an access level defined.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>project_cloud__Attendee_Required</fullName>
        <active>true</active>
        <description>A Meeting Attendee requires a person t o be selected, either a Contact or User</description>
        <errorConditionFormula>IF(ISBLANK(project_cloud__User__c), 0, 1) + IF(ISBLANK(project_cloud__Contact__c), 0, 1) != 1</errorConditionFormula>
        <errorMessage>A Meeting Attendee requires a single User or Contact to be specified.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
