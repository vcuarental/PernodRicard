    <fields>
        <fullName>Situation__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
	RecordType.DeveloperName == &quot;ESN_Milestone&quot;,
	IF(
		ESN_Completed__c == true,
		IMAGE(&apos;/resource/ProjectTab_Closed&apos;,&apos;Closed : Stream closed&apos;),
		IF(
			ESN_AtRisk__c == true,
			IMAGE(&apos;/resource/ProjectTab_Risk&apos;,&apos;Risk : the stream is identified as presenting a risk&apos;),
			IF(
				ESN_ActualDeadline__c &lt; today(),
				IMAGE(&apos;/resource/ProjectTab_Risk&apos;,&apos;Risk : the stream is identified as presenting a risk&apos;),
				IMAGE(&apos;/resource/1368629827000/ProjectTab_InProgress&apos;,&apos;Stream in progress&apos;)
			)
		)
	),
	IF(
		ISPICKVAL(Status__c, &quot;Closed&quot;),
		IMAGE(&apos;/resource/ProjectTab_Closed&apos;,&apos;Closed : Stream closed&apos;),
		IF(
			ESN_AtRisk__c == true,
			IMAGE(&apos;/resource/ProjectTab_Risk&apos;,&apos;Risk : the stream is identified as presenting a risk&apos;),
			IF(
				ESN_ActualStartDate__c &gt; today(),
				IF(
					ISPICKVAL(Status__c, &quot;Open&quot;),
					IMAGE(&apos;/resource/1368629827000/ProjectTab_InProgress&apos;,&apos;Stream in progress&apos;),
					IMAGE(&apos;/resource/ProjectTab_NotStarted&apos;,&apos;Not Started&apos;)
				),
				IF(
					today() &lt; ESN_ActualEndDate__c,
					IF(
						ISPICKVAL(Status__c, &quot;Open&quot;),
						IMAGE(&apos;/resource/1368629827000/ProjectTab_InProgress&apos;,&apos;Stream in progress&apos;),
						IMAGE(&apos;/resource/ProjectTab_Medium&apos;,&apos;Medum : Start date__c &lt; today and today &lt; End date__c and Status = &quot;Inactive&quot;&apos;)
					),
					IMAGE(&apos;/resource/ProjectTab_Risk&apos;,&apos;Risk : End date overtaken and stream not closed&apos;)
				)
			)
		)
	)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Situation</label>
        <required>false</required>
