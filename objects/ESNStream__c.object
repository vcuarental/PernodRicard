    <fields>
        <fullName>Train__c</fullName>
        <externalId>false</externalId>
        <formula>IF( 
	ISBLANK(ParentStream__c ) , 
	HYPERLINK(&apos;/&apos;+ Project__r.Id,  Project__r.Name, &apos;_parent&apos;) 
		&amp; &apos; &gt;&gt; &apos; &amp;  Name , 	
	IF(
		ISBLANK( ParentStream__r.ParentStream__c ), 
		HYPERLINK(&apos;/&apos;+ Project__r.Id,  Project__r.Name, &apos;_parent&apos;) 
			&amp; &apos; &gt;&gt; &apos; &amp; HYPERLINK(&apos;/&apos;+ ParentStream__r.Id,  ParentStream__r.Name, &apos;_parent&apos;) 
			&amp; &apos; &gt;&gt; &apos; &amp; Name, 
		IF(
			ISBLANK( ParentStream__r.ParentStream__r.ParentStream__c ), 
			HYPERLINK(&apos;/&apos;+ Project__r.Id,  Project__r.Name, &apos;_parent&apos;) 
				&amp; &apos; &gt;&gt; &apos; &amp; HYPERLINK(&apos;/&apos;+ ParentStream__r.ParentStream__r.Id,  ParentStream__r.ParentStream__r.Name, &apos;_parent&apos;) 
				&amp; &apos; &gt;&gt; &apos; &amp; HYPERLINK(&apos;/&apos;+ ParentStream__r.Id,  ParentStream__r.Name, &apos;_parent&apos;) 
				&amp; &apos; &gt;&gt; &apos; &amp; Name,
