    <validationRules>
        <fullName>STR_VR01_EndDateValue</fullName>
        <active>false</active>
        <description>The stream Startdate Value can&apos;t be less than the Project Start date value</description>
        <errorConditionFormula>AND(NOT( $User.BypassVR__c), OR(EndDate__c  &gt;  Project__r.EndDate__c,  EndDate__c  &lt;  StartDate__c))</errorConditionFormula>
        <errorDisplayField>EndDate__c</errorDisplayField>
        <errorMessage>The Stream end date can&apos;t be greater than the project end date or less than the Stream Start date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>STR_VR02_StartDateValue</fullName>
        <active>false</active>
        <description>The stream Startdate Value can&apos;t be less than the Project Start date value</description>
        <errorConditionFormula>AND(NOT( $User.BypassVR__c), OR(StartDate__c  &lt;  Project__r.StartDate__c,  StartDate__c  &gt;  EndDate__c))</errorConditionFormula>
        <errorDisplayField>StartDate__c</errorDisplayField>
        <errorMessage>The Stream Start date can&apos;t be less than the project Start date or greater than the Stream End date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>STR_VR03_Stream_ControlDate_WithProject</fullName>
        <active>true</active>
        <description>You cannot enter a Stream Start Date smaller than the Project Start Date or Stream End Date greater than the Project End Date. Please check and correct data.</description>
        <errorConditionFormula>AND(NOT( $User.BypassVR__c), NOT( RecordType.Name = &quot;Milestone&quot;), OR(ESN_ActualStartDate__c &lt; Project__r.ESN_ActualStartDate__c , ESN_ActualEndDate__c &gt; Project__r.ESN_ActualEndDate__c ))</errorConditionFormula>
        <errorMessage>You cannot enter a Stream Start Date smaller than the Project Start Date or Stream End Date greater than the Project End Date. Please check and correct data.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>STR_VR04_Stream_ControlDateConsistency</fullName>
        <active>true</active>
        <description>You cannot enter a Start Date greater that the End Date. Please check and correct data (either Planned Date or Actual Date).</description>
        <errorConditionFormula>AND(NOT( $User.BypassVR__c), NOT( RecordType.Name = &quot;Milestone&quot;), OR(StartDate__c &gt; EndDate__c, ESN_ActualStartDate__c &gt; ESN_ActualEndDate__c ))</errorConditionFormula>
        <errorMessage>You cannot enter a Start Date greater that the End Date. Please check and correct data (either Planned Date or Actual Date).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>STR_VR05_Milest_ControlDate_WithProject</fullName>
        <active>true</active>
        <description>You cannot enter a Milestone Deadline outside the Project Period. Please check and correct data.</description>
        <errorConditionFormula>AND(NOT( $User.BypassVR__c), NOT( RecordType.Name = &quot;Stream&quot;), OR(ESN_ActualDeadline__c &lt; Project__r.ESN_ActualStartDate__c ,ESN_ActualDeadline__c &gt; Project__r.ESN_ActualEndDate__c))</errorConditionFormula>
        <errorMessage>You cannot enter a Milestone Deadline outside the Project Period. Please check and correct data.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>STR_VR06_Stream_ControlDate_WithParent</fullName>
        <active>true</active>
        <description>You cannot enter a Sub Stream Start Date smaller than the Parent Stream Start Date or Sub Stream End Date greater than the Parent Stream End Date. Please check and correct data.</description>
        <errorConditionFormula>AND(NOT( $User.BypassVR__c), NOT( RecordType.Name = &quot;Milestone&quot;), ParentStream__c != NULL, OR(ESN_ActualStartDate__c &lt; ParentStream__r.ESN_ActualStartDate__c , ESN_ActualEndDate__c &gt; ParentStream__r.ESN_ActualEndDate__c ))</errorConditionFormula>
        <errorMessage>You cannot enter a Sub Stream Start Date smaller than the Parent Stream Start Date or Sub Stream End Date greater than the Parent Stream End Date. Please check and correct data.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>STR_VR07_PlannedDateNotChange</fullName>
        <active>true</active>
        <description>You cannot modified Planned Start Date and Planned End Date after creation.</description>
        <errorConditionFormula>AND( 
	NOT( $User.BypassVR__c ) , 
	IF(
		RecordType.DeveloperName == &quot;ESN_Stream&quot;,
		AND( OR( ISCHANGED( StartDate__c ) , ISCHANGED( EndDate__c ),ISCHANGED( ESN_PlannedDeadline__c ) ), NOT(ISPICKVAL( Status__c , &quot;Inactive&quot;)) ),
		OR( ISCHANGED( StartDate__c ) , ISCHANGED( EndDate__c ),ISCHANGED( ESN_PlannedDeadline__c ) )
	)
)</errorConditionFormula>
        <errorMessage>You cannot modified either Planned Start Date or Planned End Date for a Stream or Planned Deadline for a Milestone after Status is ongoing or closed. Please contact your Administrator for more information.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>STR_VR08_Milest_ControlDate_WithParent</fullName>
        <active>true</active>
        <description>You cannot enter a Milestone Deadline outside the Parent Stream Period. Please check and correct data.</description>
        <errorConditionFormula>AND(NOT( $User.BypassVR__c), NOT( RecordType.Name = &quot;Stream&quot;), ParentStream__c !=NULL, OR(ESN_ActualDeadline__c &lt; ParentStream__r.ESN_ActualStartDate__c ,ESN_ActualDeadline__c &gt; ParentStream__r.ESN_ActualEndDate__c ))</errorConditionFormula>
        <errorMessage>You cannot enter a Milestone Deadline outside the Parent Stream Period. Please check and correct data.</errorMessage>
    </validationRules>
