    <fields>
        <fullName>ESN_SituationText__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
	RecordType.DeveloperName == &quot;ESN_Milestone&quot;,
	IF(
		ESN_Completed__c == true,
		&quot;Closed&quot;,
		IF(
			ESN_AtRisk__c == true,
			&quot;At Risk&quot;,
			IF(
				ESN_ActualDeadline__c &lt; today(),
				&quot;Late&quot;,
				&quot;Planned&quot;
			)
		)
	),
	IF(
		ISPICKVAL(Status__c, &quot;Closed&quot;),
		&quot;Closed&quot;,
		IF(
			ESN_AtRisk__c == true,
			&quot;At Risk&quot;,
			IF(
				ESN_ActualStartDate__c &gt; today(),
				IF(
					ISPICKVAL(Status__c, &quot;Open&quot;),
					&quot;In Progress&quot;,
					&quot;Not Started&quot;
				),
				IF(
					today() &lt; ESN_ActualEndDate__c,
					IF(
						ISPICKVAL(Status__c, &quot;Open&quot;),
						&quot;In Progress&quot;,
						&quot;Late Start&quot;
					),
					&quot;Late&quot;
				)
			)
		)
	)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Situation Text</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
