    <fields>
        <fullName>BMCServiceDesk__TargetTypeValue__c</fullName>
        <deprecated>false</deprecated>
        <description>This field derives the value from custom label based on value in Target Type field.</description>
        <externalId>false</externalId>
        <formula>IF(LOWER(BMCServiceDesk__Target_Type__c)==&apos;resolutiontime&apos;, $Label.bmcservicedesk__resolutiontime,
IF(LOWER(BMCServiceDesk__Target_Type__c)==&apos;initiationtime&apos;, $Label.bmcservicedesk__initiationtime,
IF(LOWER(BMCServiceDesk__Target_Type__c)==&apos;responsetime&apos;, $Label.bmcservicedesk__responsetime,
IF(LOWER(BMCServiceDesk__Target_Type__c)==&apos;completiontime&apos;, $Label.bmcservicedesk__completiontime,&apos;&apos;))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Target Type~</label>
        <required>false</required>
