    <validationRules>
        <fullName>ASI_MFM_Fiscal_Year</fullName>
        <active>true</active>
        <errorConditionFormula>ASI_MFM_country_name__c  = &quot;TW&quot;  &amp;&amp; ISPICKVAL( ASI_MFM_Module__c , &quot;Plan&quot;) &amp;&amp;  ISBLANK( text( ASI_MFM_Fiscal_year__c ) )</errorConditionFormula>
        <errorDisplayField>ASI_MFM_Fiscal_year__c</errorDisplayField>
        <errorMessage>Please select fiscal year</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_MFM_GF_TRRecordTypeRegionCheck</fullName>
        <active>true</active>
        <description>For Gulf MFM Prefix, TR Record Type defines which region&apos;s customer is available to be selected in Plan and PO</description>
        <errorConditionFormula>AND( 
	CONTAINS( RecordType.DeveloperName , &quot;ASI_MFM_GF&quot;),
	OR( 
		ISBLANK( ASI_MFM_TR_Rec_Type__c ) ,
		NOT(OR(ASI_MFM_TR_Rec_Type__c = &quot;Duty Paid&quot;,ASI_MFM_TR_Rec_Type__c = &quot;Duty Free&quot;, ASI_MFM_TR_Rec_Type__c = &quot;SLM&quot;,
ASI_MFM_TR_Rec_Type__c = &quot;India Duty Free&quot;))
	) 
