    <validationRules>
        <fullName>ASI_NPP_9LC_conversion_calculation</fullName>
        <active>true</active>
        <description>check 9LC calculation</description>
        <errorConditionFormula>AND(IF (AND(!ISBLANK(TEXT(ASI_NPP_Target_Measurement__c)),!ISPICKVAL(ASI_NPP_Target_Measurement__c, &apos;9LC&apos;)), OR(ISBLANK(ASI_NPP_No_of_Bottles_Per_Case__c) ,  ISBLANK(ASI_NPP_Packing_Size_Per_Bottle__c )),false),  CONTAINS( RecordType.DeveloperName, &apos;ASI_NPP_SG&apos;) ,  !$User.BypassVR__c )</errorConditionFormula>
        <errorMessage>Please fill in the Packing Size Per Bottle (cl) and No. of Bottles Per Case to calculate the 9LC conversion</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_NPP_Check_Fiscal_Year</fullName>
        <active>true</active>
        <errorConditionFormula>AND(IF(AND(!ISBLANK(TEXT(ASI_NPP_FOB_Currency__c)),ISBLANK(TEXT( ASI_NPP_Fiscal_Year__c )),  !ISPICKVAL(ASI_NPP_FOB_Currency__c, &apos;SGD&apos;) ), true, false),  CONTAINS( RecordType.DeveloperName, &apos;ASI_NPP_SG&apos;),  !$User.BypassVR__c)</errorConditionFormula>
        <errorDisplayField>ASI_NPP_Fiscal_Year__c</errorDisplayField>
        <errorMessage>You have to select the fiscal year to continue the calculation</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_NPP_Check_Fiscal_Year_VAP</fullName>
        <active>true</active>
        <errorConditionFormula>AND(IF(AND(!ISBLANK(TEXT( ASI_NPP_VAP_Currency__c )),ISBLANK(TEXT( ASI_NPP_Fiscal_Year__c )), !ISPICKVAL(ASI_NPP_VAP_Currency__c , &apos;SGD&apos;) ), true, false),  CONTAINS( RecordType.DeveloperName, &apos;ASI_NPP_SG&apos;),  !$User.BypassVR__c)</errorConditionFormula>
        <errorDisplayField>ASI_NPP_VAP_Currency__c</errorDisplayField>
        <errorMessage>Please select the fiscal year to continue the calcluation</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_NPP_Check_Launch_Day</fullName>
        <active>false</active>
        <errorConditionFormula>AND(IF( AND ( !ISBLANK ( ASI_NPP_Launch_Date__c ), !ISBLANK ( ASI_NPP_Launch_Date__c ) ),IF( ASI_NPP_Start_Launch_Date__c &lt;&gt;  ASI_NPP_Launch_Date__c ,true,false),false),  CONTAINS( RecordType.DeveloperName, &apos;ASI_NPP_SG&apos;),  !$User.BypassVR__c)</errorConditionFormula>
        <errorDisplayField>ASI_NPP_Start_Launch_Date__c</errorDisplayField>
        <errorMessage>Leave the Start / Launch Date blank of set it equal to Launch Date at NPD Section</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_NPP_Day_Back_Error</fullName>
        <active>true</active>
        <errorConditionFormula>AND((ASI_NPP_Period_of_Sales_From__c &lt; TODAY() || ASI_NPP_Period_of_Sales_To__c &lt; TODAY()), CONTAINS( RecordType.DeveloperName, &apos;ASI_NPP_SG&apos;) ,OR(ISCHANGED(ASI_NPP_Period_of_Sales_From__c ),ISCHANGED(ASI_NPP_Period_of_Sales_To__c )),  !$User.BypassVR__c )</errorConditionFormula>
        <errorDisplayField>ASI_NPP_Period_of_Sales_From__c</errorDisplayField>
        <errorMessage>The Period of Sales must be later than today.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_NPP_ETA_Shipment_Checking</fullName>
        <active>true</active>
        <errorConditionFormula>AND(if( ASI_NPP_Delivery_ETA_Date_in_SGP__c &lt;  ASI_NPP_Shipment_BO__c, true,false ),  CONTAINS( RecordType.DeveloperName, &apos;ASI_NPP_SG&apos;),OR( ISCHANGED(ASI_NPP_Delivery_ETA_Date_in_SGP__c ),ISCHANGED(ASI_NPP_Shipment_BO__c) ),  !$User.BypassVR__c)</errorConditionFormula>
        <errorDisplayField>ASI_NPP_Delivery_ETA_Date_in_SGP__c</errorDisplayField>
        <errorMessage>Delivery ETA Date should be later than Shipment BO</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_NPP_Launch_Day_Order_checking</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR (  ASI_NPP_Launch_Date__c  &lt;  ASI_NPP_Sell_in_Date_PRS_to_WS__c  ,  ASI_NPP_Launch_Date__c  &lt;  ASI_NPP_Delivery_ETA_Date_in_SGP__c  ,  ASI_NPP_Launch_Date__c  &lt;   ASI_NPP_Shipment_BO__c ), CONTAINS( RecordType.DeveloperName, &apos;ASI_NPP_SG&apos;),  ISCHANGED(ASI_NPP_Launch_Date__c  ) ,  !$User.BypassVR__c)</errorConditionFormula>
        <errorDisplayField>ASI_NPP_Launch_Date__c</errorDisplayField>
        <errorMessage>Launch Date Should be later than Sell-in Date PRS to WS, Delivery ETA Date and Shipment Bo</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_NPP_Launch_Day_error</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ASI_NPP_Launch_Date__c &lt; TODAY(), ISCHANGED(ASI_NPP_Launch_Date__c),CONTAINS( RecordType.DeveloperName, &apos;ASI_NPP_SG&apos;) , !$User.BypassVR__c  )</errorConditionFormula>
        <errorDisplayField>ASI_NPP_Launch_Date__c</errorDisplayField>
        <errorMessage>Launch Day must be later than today</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_NPP_Period_of_Sales_From_checking</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR ( ASI_NPP_Period_of_Sales_From__c &lt;  ASI_NPP_Launch_Date__c, ASI_NPP_Period_of_Sales_From__c &lt; ASI_NPP_Sell_in_Date_PRS_to_WS__c , ASI_NPP_Period_of_Sales_From__c &lt; ASI_NPP_Delivery_ETA_Date_in_SGP__c , ASI_NPP_Period_of_Sales_From__c &lt; ASI_NPP_Shipment_BO__c ),  CONTAINS( RecordType.DeveloperName, &apos;ASI_NPP_SG&apos;), ISCHANGED(ASI_NPP_Period_of_Sales_From__c ) ,  !$User.BypassVR__c)</errorConditionFormula>
        <errorDisplayField>ASI_NPP_Period_of_Sales_From__c</errorDisplayField>
        <errorMessage>Period of Sales (From) should be later than Launch Date, Sell-in Date PRS to WS, Delivery ETA Date in SGP and Shipment BO</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_NPP_Period_of_Sales_To_Checking</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR ( ASI_NPP_Period_of_Sales_To__c &lt;  ASI_NPP_Period_of_Sales_From__c,
ASI_NPP_Period_of_Sales_To__c &lt; ASI_NPP_Launch_Date__c, ASI_NPP_Period_of_Sales_To__c &lt; ASI_NPP_Sell_in_Date_PRS_to_WS__c , ASI_NPP_Period_of_Sales_To__c &lt; ASI_NPP_Delivery_ETA_Date_in_SGP__c , ASI_NPP_Period_of_Sales_To__c &lt; ASI_NPP_Shipment_BO__c ),CONTAINS( RecordType.DeveloperName, &apos;ASI_NPP_SG&apos;), ISCHANGED(ASI_NPP_Period_of_Sales_To__c ) ,  !$User.BypassVR__c)</errorConditionFormula>
        <errorDisplayField>ASI_NPP_Period_of_Sales_To__c</errorDisplayField>
        <errorMessage>Preriod of Sales (To) should be later than Period of Sales (From), Launch Date, Sell-in Date, Delivery ETA date and Shipment BO</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_NPP_Sell_in_Date_Checking</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR(ASI_NPP_Sell_in_Date_PRS_to_WS__c  &lt;  ASI_NPP_Delivery_ETA_Date_in_SGP__c, ASI_NPP_Sell_in_Date_PRS_to_WS__c  &lt;  ASI_NPP_Shipment_BO__c ),CONTAINS( RecordType.DeveloperName, &apos;ASI_NPP_SG&apos;),  ISCHANGED(ASI_NPP_Sell_in_Date_PRS_to_WS__c  ),  !$User.BypassVR__c )</errorConditionFormula>
        <errorDisplayField>ASI_NPP_Sell_in_Date_PRS_to_WS__c</errorDisplayField>
        <errorMessage>Sell-in Date PRS to WS should be later than Delivery ETA Date in SGP and Shipment BO</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_NPP_Start_Launch_Date_Error_checking</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ASI_NPP_Start_Launch_Date__c &lt; TODAY(),  CONTAINS( RecordType.DeveloperName, &apos;ASI_NPP_SG&apos;), ISCHANGED(ASI_NPP_Start_Launch_Date__c ),  !$User.BypassVR__c )</errorConditionFormula>
        <errorDisplayField>ASI_NPP_Start_Launch_Date__c</errorDisplayField>
        <errorMessage>Start / Launch Date must be later than today</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_NPP_Sub_Brand_Selection_Error</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
	AND( 
		OR( ISPICKVAL (ASI_NPP_Sub_Brand_Type__c, &quot;Innovation&quot;),
			ISPICKVAL (ASI_NPP_Sub_Brand_Type__c, &quot;Sub-Brands&quot;)
		)  
		,!ISBLANK(ASI_NPP_Sub_brand__c)
	)
	,CONTAINS( RecordType.DeveloperName, &apos;ASI_NPP_SG&apos;)
	,  !$User.BypassVR__c
)</errorConditionFormula>
        <errorDisplayField>ASI_NPP_Sub_brand__c</errorDisplayField>
        <errorMessage>You can not select more than one sub-brand</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASI_NPP_Validate_PO_Owner</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
 CONTAINS( RecordType.DeveloperName , &quot;ASI_NPP_SG&quot;),
NOT(ISBLANK(ASI_NPP_PO_Number__c)),
OwnerId !=  ASI_NPP_PO_Number__r.OwnerId
)</errorConditionFormula>
        <errorDisplayField>ASI_NPP_PO_Number__c</errorDisplayField>
        <errorMessage>Please select PO that is owned by you</errorMessage>
    </validationRules>
