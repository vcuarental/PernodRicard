    <fields>
        <fullName>ASI_APP_FOB_Cost_conversion_to_SGD__c</fullName>
        <externalId>false</externalId>
        <formula>ASI_NPP_FOB_Cost_w_o_Duties__c *  ASI_NPP_FOB_Exchange_Rate__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>FOB Cost - conversion to SGD</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>ASI_NPP_9LC_conversion__c</fullName>
        <externalId>false</externalId>
        <formula>IF (ISPICKVAL ( ASI_NPP_Target_Measurement__c , &quot;Bottles&quot;) ,  (ASI_NPP_Forecast_AND_Target__c *  ASI_NPP_Packing_Size_Per_Bottle__c) / 900 ,  IF(ISPICKVAL ( ASI_NPP_Target_Measurement__c , &quot;Actual Case&quot;), (ASI_NPP_Forecast_AND_Target__c * (ASI_NPP_Packing_Size_Per_Bottle__c *  ASI_NPP_No_of_Bottles_Per_Case__c /900)) , ASI_NPP_Forecast_AND_Target__c))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>9LC conversion</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
