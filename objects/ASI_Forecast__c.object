        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/41.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/41.0/apex.js&quot;)} 


var forecastId = &quot;{!ASI_Forecast__c.Id}&quot;; 
console.log(forecastId);

var params = {forecastId:forecastId};
var callback = { onSuccess: function(result){
alert(&quot;Status Set to Draft Success&quot;); 
window.location.reload(true);
}, onFailure: function(error){} }; 

sforce.apex.execute(&quot;ASI_CRM_TW_Forecast_Create&quot;, &quot;forecastSetToDraft&quot;, params, callback);</url>
