<aura:component description="EUR_CRM_ObjAndPromoGeneratorService">

    <aura:method name="getPriceBookEntryGrouping">
        <aura:attribute name="pbEntryGrouping" type="Object[]"/>
        <aura:attribute name="productId" type="String"/>
    </aura:method>

    <aura:method name="showToast">
        <aura:attribute name="params" type="Object"/>
    </aura:method>

    <aura:method name="showModalDialog">
        <aura:attribute name="params" type="Object"/>
    </aura:method>

    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />

    <aura:attribute name="context" type="Object" />
    <!--CALLBACKS-->
    <aura:attribute name="onDeleteSuccess" type="Object" />
    <aura:attribute name="onDeleteIncomplete" type="Object" />
    <aura:attribute name="onDeleteError" type="Object" />
    <!--/CALLBACKS-->
    <!--TODO: move all minor and secondary logic from AnimationGenerator into this component-->

</aura:component>