<aura:event type="COMPONENT" description="Event fired when adding new category in Catalog Creation component">
    <aura:attribute name="isCreate" type="Boolean" access="public"/>
    <aura:attribute name="viewName" type="String" access="public"/>
    <aura:attribute name="level" type="String" access="public"/>
    <aura:attribute name="currentDepth" type="String" access="public"/>
    <aura:attribute name="branchIndex" type="String" access="public"/>
    <aura:attribute name="isDrop" type="Boolean" access="public"/>
    <aura:attribute name="sourceBranchIndex" type="String" access="public"/>
</aura:event>