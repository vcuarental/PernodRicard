<aura:interface description="EUR_CRM_Selectable: Marker interface describes components as selectable">

    <aura:attribute name="isSelected" access="public" type="Boolean" />
    <aura:attribute name="selectable" access="public" type="Boolean" default="true" />
    <aura:method name="select" access="public" action="{!c.handleSelectAction}" />
    <aura:registerEvent name="select" type="c:EUR_CRM_SelectableEvent" />

</aura:interface>