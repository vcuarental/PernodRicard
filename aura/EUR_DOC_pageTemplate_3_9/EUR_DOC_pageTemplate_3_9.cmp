<aura:component implements="lightning:appHomeTemplate" description="Two columns layout" access="global">
        
    <aura:attribute name="left" type="Aura.Component[]" access="global" />
    <aura:attribute name="center" type="Aura.Component[]" access="global" />
        
    <div aura:id="container">
        <lightning:layout>
            <lightning:layoutItem aura:id="leftColumn" size="3">
                {!v.left}
            </lightning:layoutItem>
            <lightning:layoutItem aura:id="centerColumn" size="9" class="center">
                {!v.center}
            </lightning:layoutItem>
        </lightning:layout>     
    </div>       
</aura:component>