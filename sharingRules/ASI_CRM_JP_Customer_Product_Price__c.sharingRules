<?xml version="1.0" encoding="UTF-8"?>
<SharingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <sharingCriteriaRules>
        <fullName>ASI_CRM_JP_DC_Wholesaler</fullName>
        <accessLevel>Edit</accessLevel>
        <description>This is to define the sharing rule for D&amp;C and Wholesaler Pricing for Accounting Group.</description>
        <label>ASI_CRM_JP_DC_Wholesaler</label>
        <sharedTo>
            <group>ASI_CRM_JP_Accounting</group>
        </sharedTo>
        <criteriaItems>
            <field>RecordTypeId</field>
            <operation>equals</operation>
            <value>JP CRM Wholesaler &amp; FOC on Invoice Price,JP CRM D&amp;C Product Price</value>
        </criteriaItems>
    </sharingCriteriaRules>
