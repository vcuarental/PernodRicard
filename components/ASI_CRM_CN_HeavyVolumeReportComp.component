<apex:component controller="ASI_CRM_CN_HeavyVolumeReportPDFCtrl" access="global" >
    
    <apex:attribute name="headerId" type="String" description=" " assignTo="{!currentHeaderId}"/>
    <apex:attribute name="StartDateString" description="Start Date String" type="String" assignto="{!StartDateStr}"/>
    <apex:attribute name="EndDateString" description="End Date String" type="String" assignto="{!EndDateStr}"/>
    
    <apex:variable var="dummyV" value="{!dummyVar}" id="dummyVar"/>
    
    <apex:stylesheet value="{!URLFOR($Resource.ASI_CRM_CN_PDF_Landscape_CSS)}"/>
    
    <head>        
        
        
    </head>
    
    <div class="header" >
        <table width="100%" border="0" cellspacing="0" cellpadding="2">
            <tr>
                <td colspan="8" align="center"><apex:image height="2cm" width="7cm" url="{!URLFOR($Resource.ASI_MFM_CN_Logo)}"/></td>
            </tr>
            <tr>
                <td colspan="8" align="center"><h1>Volume Report</h1></td>
            </tr>
            
            <tr>
                <td width="16%" ><Strong>Salesman:</Strong></td>
                <td width="16%" style="border-bottom: 1px solid black;"><apex:outputText value="{!ContractSelected.Owner.Name}" /></td>
                <td width="2%">&nbsp;</td>
                <td width="16%"><Strong>Region:</Strong></td>
                <td width="16%" style="border-bottom: 1px solid black;"><apex:outputText value="{!ContractSelected.ASI_CRM_CN_Region__c}" /></td>
                <td width="2%">&nbsp;</td>
                <td width="16%"><strong>Refresh Date:</strong></td>
                <td width="16%"  style="border-bottom: 1px solid black;">
                    <apex:outputText value="{!Day(TODAY())}/{!MONTH(TODAY())}/{!YEAR(TODAY())}" />
                </td>
                <td width="2%">&nbsp;</td>
            </tr>
            <tr>
                <td width="16%"><Strong>Outlet:</Strong></td>
                <td width="16%" style="border-bottom: 1px solid black;"><apex:outputText value="{!ContractSelected.ASI_CRM_CN_Outlet_WS__r.Name}" /></td>
                <td width="2%">&nbsp;</td>
                <td width="16%"><strong>{!IF(ContractSelected.ASI_CRM_New_FY_PO__c,'PO Province:','Area/City:')}</strong></td>
                <td width="16%" style="border-bottom: 1px solid black;"><apex:outputText value="{!IF(ContractSelected.ASI_CRM_New_FY_PO__c,ContractSelected.ASI_CRM_PO_Province__r.Name,IF(ContractSelected.ASI_CRM_PO_City__c != null,ContractSelected.ASI_CRM_PO_City__r.Name,ContractSelected.ASI_CRM_CN_Outlet_WS__r.ASI_CRM_CN_CCity__r.Name))}" /></td>
                <td width="2%">&nbsp;</td>
                <td width="16%"><Strong>PO No.:</Strong></td>
                <td width="16%" style="border-bottom: 1px solid black;"><apex:outputText value="{!ContractSelected.ASI_CRM_CN_PO_No__c}" rendered="{!ContractSelected.ASI_CRM_CN_PO_No__c!=null}" /></td>
                <td width="2%">&nbsp;</td>
            </tr>
            <tr>
                <td width="16%"><Strong>{!IF(ContractSelected.ASI_CRM_New_FY_PO__c,'PO Channel:','')}</Strong></td>
                <td width="16%" style="{!IF(ContractSelected.ASI_CRM_New_FY_PO__c,'border-bottom: 1px solid black;','')}"><apex:outputText value="{!IF(ContractSelected.ASI_CRM_New_FY_PO__c,ContractSelected.ASI_CRM_PO_Channel__r.Name,'')}" /></td>
                <td width="2%">&nbsp;</td>
                <td width="16%"><strong>{!IF(ContractSelected.ASI_CRM_New_FY_PO__c,'Area/City:','')}</strong></td>
                <td width="16%" style="{!IF(ContractSelected.ASI_CRM_New_FY_PO__c,'border-bottom: 1px solid black;','')}"><apex:outputText value="{!IF(ContractSelected.ASI_CRM_New_FY_PO__c,IF(ContractSelected.ASI_CRM_PO_City__c != null,ContractSelected.ASI_CRM_PO_City__r.Name,ContractSelected.ASI_CRM_CN_Outlet_WS__r.ASI_CRM_CN_CCity__r.Name),'')}" /></td>
                <td width="2%">&nbsp;</td>
                <td width="16%"><Strong>PO Period:</Strong></td>
                <td width="16%" style="border-bottom: 1px solid black;"><apex:outputText value="{!POPeriodStr}" /></td>
                <td width="2%">&nbsp;</td>
            </tr>
            
            
        </table>
    </div>
    
    
    
    <body>
        
        
        
        <apex:outputPanel rendered="{!noData}" id="noData">
            <table width="100%" cellspacing="0" cellpadding="2">
                <tr><td align="center">Sorry, no data to show.</td></tr>
            </table>
        </apex:outputPanel>
        
        <apex:outputPanel rendered="{!noData!=true}" id="table">
            
            
            
            
            <apex:variable value="{!0}" var="TotalTargetVol"/>
            <apex:variable value="{!0}" var="TotalIntakeVol"/>
            <apex:variable value="{!0}" var="TotalBtlCollection"/>
            <apex:variable value="{!0}" var="TotalOffTakeVol"/>
            <apex:variable value="{!0}" var="TotalCapwithIssues"/>
            
            <apex:variable value="{!0}" var="TotalTargetVol_ExclPremium"/>
            <apex:variable value="{!0}" var="TotalIntakeVol_ExclPremium"/>
            <apex:variable value="{!0}" var="TotalBtlCollection_ExclPremium"/>
            <apex:variable value="{!0}" var="TotalOffTakeVol_ExclPremium"/>
            <apex:variable value="{!0}" var="TotalCapwithIssues_ExclPremium"/>
            
            <table width="100%" cellspacing="0" cellpadding="2">
                <thead>
                    <tr>
                        <th colspan="11" bgcolor="#FFFF66" style="border-top: 1px solid black; border-left: 1px solid black; border-right: 1px solid black;" align="center" width="8%">
                            <apex:outputText value="Calendar Period: {!SelectedPeriodStr}" />
                        </th>
                    </tr>   
                    <tr>
                        <th style="border-top: 1px solid black; border-bottom: 1px solid black; border-left: 1px solid black; border-right: 1px solid black;" align="center" width="28%">
                            By Grade Unit:Bottle
                        </th>
                        <th style="border-top: 1px solid black; border-bottom: 1px solid black; border-right: 1px solid black;" align="center" width="7%">Contract Vol. Target</th>
                        <th style="border-top: 1px solid black; border-bottom: 1px solid black; border-right: 1px solid black;" align="center" width="7%">Intake Vol</th>
                        <th style="border-top: 1px solid black; border-bottom: 1px solid black; border-right: 1px solid black;" align="center" width="7%">Intake vs Contract % </th>
                        <th style="border-top: 1px solid black; border-bottom: 1px solid black; border-right: 1px solid black;" align="center" width="7%">Cap Collection (caps with issue deducted)</th>
                        <th style="border-top: 1px solid black; border-bottom: 1px solid black; border-right: 1px solid black;" align="center" width="7%">Collection vs Intake %</th>
                        <th style="border-top: 1px solid black; border-bottom: 1px solid black; border-right: 1px solid black;" align="center" width="7%"> Collection vs Contract % </th>
                        
                        <th style="border-top: 1px solid black; border-bottom: 1px solid black; border-right: 1px solid black;" align="center" width="7%">Offtake Vol</th>
                        <th style="border-top: 1px solid black; border-bottom: 1px solid black; border-right: 1px solid black;" align="center" width="7%">Offtake vs Intake % </th>
                        <th style="border-top: 1px solid black; border-bottom: 1px solid black; border-right: 1px solid black;" align="center" width="7%">Collection vs Offtake % </th>
                        <th style="border-top: 1px solid black; border-bottom: 1px solid black; border-right: 1px solid black;" align="center" width="7%">Deductible Caps with Issues</th>
                    </tr>
                </thead>
                
                <apex:repeat value="{!GradeVolumeMap}" var="SubBrandGrade">
                    
                    <apex:repeat value="{!GradeVolumeMap[SubBrandGrade].SubBrandLine}" var="SubBrand">
                        <tr>
                            <td style="border-right: 1px solid black; border-left: 1px solid black; border-bottom: 1px solid black;" align="left">
                                {!GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].sbname}
                            </td>
                            <td style="border-right: 1px solid black; border-bottom: 1px solid black;" align="right">
                                <apex:outputText value="{0,number,#,###,###}" rendered="{!GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].TargetVol!=null && GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].TargetVol!=0}">
                                    <apex:param value="{!GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].TargetVol}" />
                                </apex:outputText>
                                <apex:outputText value=" - " rendered="{!GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].TargetVol=null||GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].TargetVol=0}" />
                            </td>
                            <td style="border-right: 1px solid black; border-bottom: 1px solid black;" align="right">
                                <apex:outputText value="{0,number,#,###,###}" rendered="{!GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].IntakeVol!=null && GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].IntakeVol!=0}">
                                    <apex:param value="{!GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].IntakeVol}" />
                                </apex:outputText>
                                <apex:outputText value=" - " rendered="{!GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].IntakeVol=null||GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].IntakeVol=0}" />
                            </td>
                            <td style="border-right: 1px solid black; border-bottom: 1px solid black;" align="right">
                                <apex:outputText value="{0,number,#,###,###}%" rendered="{!GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].IntakeVSContract!=null && GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].IntakeVSContract!=0}">
                                    <apex:param value="{!GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].IntakeVSContract}" /></apex:outputText>
                                <apex:outputText value=" - " rendered="{!GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].IntakeVSContract==null || GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].IntakeVSContract==0 }" />
                            </td>
                            <td style="border-right: 1px solid black; border-bottom: 1px solid black;" align="right">
                                <apex:outputText value="{0,number,#,###,###}" rendered="{!GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].BtlCollection!=null && GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].BtlCollection!=0}">
                                    <apex:param value="{!GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].BtlCollection}" />
                                </apex:outputText>
                                <apex:outputText value=" - " rendered="{!GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].BtlCollection==null || GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].BtlCollection==0}" />
                            </td>
                            
                            <td style="border-right: 1px solid black; border-bottom: 1px solid black;" align="right">
                                <apex:outputText value="{0,number,#,###,###}%" rendered="{!GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].CollectionVSIntake!=null && GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].CollectionVSIntake!=0}">
                                    <apex:param value="{!GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].CollectionVSIntake}" /></apex:outputText>
                                <apex:outputText value=" - " rendered="{!GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].CollectionVSIntake==null || GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].CollectionVSIntake ==0}" />
                            </td>
                            <td style="border-right: 1px solid black; border-bottom: 1px solid black;" align="right">
                                <apex:outputText value="{0,number,#,###,###}%" rendered="{!GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].CollectionVSContract!=null && GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].CollectionVSContract!=0}">
                                    <apex:param value="{!GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].CollectionVSContract}" /></apex:outputText>
                                <apex:outputText value=" - " rendered="{!GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].CollectionVSContract=null || GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].CollectionVSContract ==0 }" />
                            </td>
                            <td style="border-right: 1px solid black; border-bottom: 1px solid black;" align="right">
                                <apex:outputText value="{0,number,#,###,###}" rendered="{!GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].OffTakeVol!=null && GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].OffTakeVol!=0}">
                                    <apex:param value="{!GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].OffTakeVol}" />
                                </apex:outputText>
                                <apex:outputText value=" - " rendered="{!GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].OffTakeVol==null||GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].OffTakeVol==0}" />
                            </td>
                            <td style="border-right: 1px solid black; border-bottom: 1px solid black;" align="right">
                                <apex:outputText value="{0,number,#,###,###}%" rendered="{!GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].OffTakeVSIntake!=null && GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].OffTakeVSIntake!=0}">
                                    <apex:param value="{!GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].OffTakeVSIntake}" /></apex:outputText>
                                <apex:outputText value=" - " rendered="{!GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].OffTakeVSIntake==null || GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].OffTakeVSIntake ==0}" />
                            </td>
                            <td style="border-right: 1px solid black; border-bottom: 1px solid black;" align="right">
                                <apex:outputText value="{0,number,#,###,###}%" rendered="{!GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].CollectionVSOfftake!=null && GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].CollectionVSOfftake!=0}">
                                    <apex:param value="{!GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].CollectionVSOfftake}" /></apex:outputText>
                                <apex:outputText value=" - " rendered="{!GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].CollectionVSOfftake==null || GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].CollectionVSOfftake==0}" />
                            </td>
                            <td style="border-right: 1px solid black; border-bottom: 1px solid black;" align="right">
                                <apex:outputText value="{0,number,#,###,###}" rendered="{!GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].DeductibleCapswithIssues!=null && GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].DeductibleCapswithIssues!=0}">
                                    <apex:param value="{!GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].DeductibleCapswithIssues}" />
                                </apex:outputText>
                                <apex:outputText value=" - " rendered="{!GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].DeductibleCapswithIssues==null || GradeVolumeMap[SubBrandGrade].SubBrandLine[SubBrand].DeductibleCapswithIssues==0}" />
                            </td>
                        </tr>
                    </apex:repeat>
                    
                    <apex:variable var="TotalTargetVol" value="{!TotalTargetVol+GradeVolumeMap[SubBrandGrade].VolumeData.TargetVol}"/>
                    <apex:variable var="TotalIntakeVol" value="{!TotalIntakeVol+GradeVolumeMap[SubBrandGrade].VolumeData.IntakeVol}"/>
                    <apex:variable var="TotalBtlCollection" value="{!TotalBtlCollection+GradeVolumeMap[SubBrandGrade].VolumeData.BtlCollection}"/>
                    <apex:variable var="TotalOffTakeVol" value="{!TotalOffTakeVol+GradeVolumeMap[SubBrandGrade].VolumeData.OffTakeVol}"/>
                    <apex:variable var="TotalCapwithIssues" value="{!TotalCapwithIssues+GradeVolumeMap[SubBrandGrade].VolumeData.DeductibleCapswithIssues}"/>
                    
                    
                    
                    <apex:variable var="TotalTargetVol_ExclPremium"  rendered="{!GradeVolumeMap[SubBrandGrade].SequenceNum<=4}" 
                                   value="{!TotalTargetVol_ExclPremium+GradeVolumeMap[SubBrandGrade].VolumeData.TargetVol}" />
                    <apex:variable var="TotalIntakeVol_ExclPremium" rendered="{!GradeVolumeMap[SubBrandGrade].SequenceNum<=4}" 
                                   value="{!TotalIntakeVol_ExclPremium+GradeVolumeMap[SubBrandGrade].VolumeData.IntakeVol}"/>
                    <apex:variable var="TotalBtlCollection_ExclPremium" rendered="{!GradeVolumeMap[SubBrandGrade].SequenceNum<=4}" 
                                   value="{!TotalBtlCollection_ExclPremium+GradeVolumeMap[SubBrandGrade].VolumeData.BtlCollection}"/>
                    <apex:variable var="TotalOffTakeVol_ExclPremium" rendered="{!GradeVolumeMap[SubBrandGrade].SequenceNum<=4}" 
                                   value="{!TotalOffTakeVol_ExclPremium+GradeVolumeMap[SubBrandGrade].VolumeData.OffTakeVol}"/>
                    <apex:variable var="TotalCapwithIssues_ExclPremium" rendered="{!GradeVolumeMap[SubBrandGrade].SequenceNum<=4}" 
                                   value="{!TotalCapwithIssues_ExclPremium+GradeVolumeMap[SubBrandGrade].VolumeData.DeductibleCapswithIssues}"/>
                    
                    
                    
                    <tr>
                        <td bgcolor="#E6E6E6" style="border-bottom: 1px solid black; border-left: 1px solid black; border-right: 1px solid black;" align="left">
                            {!GradeVolumeMap[SubBrandGrade].SubBrandGradeName}
                        </td>
                        <td bgcolor="#E6E6E6" style="border-bottom: 1px solid black; border-right: 1px solid black;" align="right">
                            <apex:outputText value="{0,number,#,###,###}" rendered="{!GradeVolumeMap[SubBrandGrade].VolumeData.TargetVol!=null&&GradeVolumeMap[SubBrandGrade].VolumeData.TargetVol!=0}">
                                <apex:param value="{!GradeVolumeMap[SubBrandGrade].VolumeData.TargetVol}" /></apex:outputText>
                            <apex:outputText value=" - " rendered="{!GradeVolumeMap[SubBrandGrade].VolumeData.TargetVol==null || GradeVolumeMap[SubBrandGrade].VolumeData.TargetVol==0}" />
                        </td>
                        <td bgcolor="#E6E6E6" style="border-bottom: 1px solid black; border-right: 1px solid black;" align="right">
                            <apex:outputText value="{0,number,#,###,###}" rendered="{!GradeVolumeMap[SubBrandGrade].VolumeData.IntakeVol!=null&& GradeVolumeMap[SubBrandGrade].VolumeData.IntakeVol!=0}">
                                <apex:param value="{!GradeVolumeMap[SubBrandGrade].VolumeData.IntakeVol}" /></apex:outputText>
                            <apex:outputText value=" - " rendered="{!GradeVolumeMap[SubBrandGrade].VolumeData.IntakeVol==null || GradeVolumeMap[SubBrandGrade].VolumeData.IntakeVol==0}" />
                        </td>
                        <td bgcolor="#E6E6E6" style="border-bottom: 1px solid black; border-right: 1px solid black;" align="right">
                            <apex:outputText value="{0,number,#,###,###}%" rendered="{!GradeVolumeMap[SubBrandGrade].VolumeData.IntakeVSContract!=null&&GradeVolumeMap[SubBrandGrade].VolumeData.IntakeVSContract!=0}">
                                <apex:param value="{!GradeVolumeMap[SubBrandGrade].VolumeData.IntakeVSContract}" /></apex:outputText>
                            <apex:outputText value=" - " rendered="{!GradeVolumeMap[SubBrandGrade].VolumeData.IntakeVSContract==null || GradeVolumeMap[SubBrandGrade].VolumeData.IntakeVSContract ==0}" />
                        </td>
                        <td bgcolor="#E6E6E6" style="border-bottom: 1px solid black; border-right: 1px solid black;" align="right">
                            <apex:outputText value="{0,number,#,###,###}" rendered="{!GradeVolumeMap[SubBrandGrade].VolumeData.BtlCollection!=null&&GradeVolumeMap[SubBrandGrade].VolumeData.BtlCollection!=0}">
                                <apex:param value="{!GradeVolumeMap[SubBrandGrade].VolumeData.BtlCollection}" />
                            </apex:outputText>
                            <apex:outputText value=" - " rendered="{!GradeVolumeMap[SubBrandGrade].VolumeData.BtlCollection==null || GradeVolumeMap[SubBrandGrade].VolumeData.BtlCollection==0}" />
                        </td>
                        <td bgcolor="#E6E6E6" style="border-bottom: 1px solid black; border-right: 1px solid black;" align="right">
                            <apex:outputText value="{0,number,#,###,###}%" rendered="{!GradeVolumeMap[SubBrandGrade].VolumeData.CollectionVSIntake!=null && GradeVolumeMap[SubBrandGrade].VolumeData.CollectionVSIntake!=0}">
                                <apex:param value="{!GradeVolumeMap[SubBrandGrade].VolumeData.CollectionVSIntake}" />
                            </apex:outputText>
                            <apex:outputText value=" - " rendered="{!GradeVolumeMap[SubBrandGrade].VolumeData.CollectionVSIntake==null || GradeVolumeMap[SubBrandGrade].VolumeData.CollectionVSIntake ==0}" />
                        </td>
                        <td bgcolor="#E6E6E6" style="border-bottom: 1px solid black; border-right: 1px solid black;" align="right">
                            <apex:outputText value="{0,number,#,###,###}%" rendered="{!GradeVolumeMap[SubBrandGrade].VolumeData.CollectionVSContract!=null && GradeVolumeMap[SubBrandGrade].VolumeData.CollectionVSContract!=0}">
                                <apex:param value="{!GradeVolumeMap[SubBrandGrade].VolumeData.CollectionVSContract}" /></apex:outputText>
                            <apex:outputText value=" - " rendered="{!GradeVolumeMap[SubBrandGrade].VolumeData.CollectionVSContract==null || GradeVolumeMap[SubBrandGrade].VolumeData.CollectionVSContract ==0 }" />
                        </td>
                        <td bgcolor="#E6E6E6" style="border-bottom: 1px solid black; border-right: 1px solid black;" align="right">
                            <apex:outputText value="{0,number,#,###,###}" rendered="{!GradeVolumeMap[SubBrandGrade].VolumeData.OffTakeVol!=null&&GradeVolumeMap[SubBrandGrade].VolumeData.OffTakeVol!=0}">
                                <apex:param value="{!GradeVolumeMap[SubBrandGrade].VolumeData.OffTakeVol}" />
                            </apex:outputText>
                            <apex:outputText value=" - " rendered="{!GradeVolumeMap[SubBrandGrade].VolumeData.OffTakeVol==null || GradeVolumeMap[SubBrandGrade].VolumeData.OffTakeVol==0}" />
                        </td>
                        <td bgcolor="#E6E6E6" style="border-bottom: 1px solid black; border-right: 1px solid black;" align="right">
                            <apex:outputText value="{0,number,#,###,###}%" rendered="{!GradeVolumeMap[SubBrandGrade].VolumeData.OffTakeVSIntake!=null && GradeVolumeMap[SubBrandGrade].VolumeData.OffTakeVSIntake!=0}">
                                <apex:param value="{!GradeVolumeMap[SubBrandGrade].VolumeData.OffTakeVSIntake}" /></apex:outputText>
                            <apex:outputText value=" - " rendered="{!GradeVolumeMap[SubBrandGrade].VolumeData.OffTakeVSIntake==null || GradeVolumeMap[SubBrandGrade].VolumeData.OffTakeVSIntake==0}" />
                        </td>
                        <td bgcolor="#E6E6E6" style="border-bottom: 1px solid black; border-right: 1px solid black;" align="right">
                            <apex:outputText value="{0,number,#,###,###}%" rendered="{!GradeVolumeMap[SubBrandGrade].VolumeData.CollectionVSOfftake!=null && GradeVolumeMap[SubBrandGrade].VolumeData.CollectionVSOfftake!=0}">
                                <apex:param value="{!GradeVolumeMap[SubBrandGrade].VolumeData.CollectionVSOfftake}" /></apex:outputText>
                            <apex:outputText value=" - " rendered="{!GradeVolumeMap[SubBrandGrade].VolumeData.CollectionVSOfftake==null || GradeVolumeMap[SubBrandGrade].VolumeData.CollectionVSOfftake==0}" />
                        </td>
                        <td bgcolor="#E6E6E6" style="border-bottom: 1px solid black; border-right: 1px solid black;" align="right">
                            <apex:outputText value="{0,number,#,###,###}" rendered="{!GradeVolumeMap[SubBrandGrade].VolumeData.DeductibleCapswithIssues!=null&&GradeVolumeMap[SubBrandGrade].VolumeData.DeductibleCapswithIssues!=0}">
                                <apex:param value="{!GradeVolumeMap[SubBrandGrade].VolumeData.DeductibleCapswithIssues}" />
                            </apex:outputText>
                            <apex:outputText value=" - " rendered="{!GradeVolumeMap[SubBrandGrade].VolumeData.DeductibleCapswithIssues==null || GradeVolumeMap[SubBrandGrade].VolumeData.DeductibleCapswithIssues==0}" />
                        </td>
                    </tr>
                </apex:repeat>
                
                
                <tr>
                    <td bgcolor="#CCFFFF" style="border-bottom: 1px solid black; border-left: 1px solid black; border-right: 1px solid black;" align="left">Total All Products</td>
                    <td bgcolor="#CCFFFF" style="border-bottom: 1px solid black; border-right: 1px solid black;" align="right">
                        <apex:outputText value="{0,number,#,###,###}" rendered="{!TotalTargetVol!=null&&TotalTargetVol!=0}">
                            <apex:param value="{!TotalTargetVol}" />
                        </apex:outputText>
                        <apex:outputText value=" - " rendered="{!TotalTargetVol==null||TotalTargetVol==0}" />
                    </td>
                    <td bgcolor="#CCFFFF" style="border-bottom: 1px solid black; border-right: 1px solid black;" align="right">
                        <apex:outputText value="{0,number,#,###,###}" rendered="{!TotalIntakeVol!=null && TotalIntakeVol!=0}">
                            <apex:param value="{!TotalIntakeVol}" />
                        </apex:outputText>
                        <apex:outputText value=" - " rendered="{!TotalIntakeVol==null||TotalIntakeVol==0}" />
                    </td>
                    
                    <td bgcolor="#CCFFFF" style=" border-bottom: 1px solid black; border-right: 1px solid black;" align="right">
                        <apex:outputText value="{0,number,#,###,###}%" rendered="{!TotalTargetVol!=null && TotalTargetVol!=0 && TotalIntakeVol!=0}">
                            <apex:param value="{!(TotalIntakeVol/TotalTargetVol)*100}" />
                        </apex:outputText>
                        <apex:outputText value=" - " rendered="{!TotalTargetVol==null || TotalTargetVol==0 || TotalIntakeVol==0}" />
                    </td>
                    <td bgcolor="#CCFFFF" style="border-bottom: 1px solid black; border-right: 1px solid black;" align="right">
                        <apex:outputText value="{0,number,#,###,###}" rendered="{!TotalBtlCollection!=null && TotalBtlCollection!=0}">
                            <apex:param value="{!TotalBtlCollection}" />
                        </apex:outputText>
                        <apex:outputText value=" - " rendered="{!TotalBtlCollection==null || TotalBtlCollection==0}" />
                    </td>
                    
                    <td bgcolor="#CCFFFF" style=" border-bottom: 1px solid black; border-right: 1px solid black;" align="right">
                        <apex:outputText value="{0,number,#,###,###}%" rendered="{!TotalIntakeVol!=null && TotalIntakeVol!=0 && TotalBtlCollection!=0 }">
                            <apex:param value="{!(TotalBtlCollection/TotalIntakeVol)*100}" />
