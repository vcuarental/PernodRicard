<apex:component controller="ASI_CRM_MY_ContractPrintOutCtrl"   access="global">
    
    <apex:attribute name="contract_id" description="Contract Record Id for Print-out" type="id" assignto="{!contractId}"/>
    
    <head>
        <style type="text/css">
            .htitle{
                font-weight:bold;
            }
            h3{
                font-weight:bold;
                font-size: 11px;
            }
            h1{
                font-size:35px;     
            }
            @Page:first {
                size: A4;
                margin:0cm;
                margin-top: 5.0cm;
                margin-bottom:3.0cm;
                @PageBreak{
                    page-break-inside:avoid;
                    page-break:always; 
                }       
                @top-center { 
                    content: element(header); 
                }
                @bottom-left
                {
                    content: normal;
                }
                @bottom-right
                {
                    content: normal; 
                }
            }
            @Page{
                size: A4;
                margin:0cm;
                margin-top: 5.0cm;
                margin-bottom:3.0cm;
                @PageBreak{
                    page-break-inside:avoid;
                    page-break:always; 
                }       
                @top-center { 
                    content: element(header); 
                }
                @bottom-left
                {
                    content: element(footer);
                    font-size: 10px; 
                }
                @bottom-right
                {
                    content: "\A\A\APage " counter(page) " of " counter(pages)"     ";
                    font-size: 10px;
                    white-space: pre;
                }
            }
            
            div.header {
                position: running(header);
            }
            div.footer {
                position: running(footer);
            }
            .nobreak{
                page-break-inside: avoid;       
            }  
            table {
                width: 100%;
                border-spacing: 0;
            }
            tr{
                page-break-inside:avoid;
            }
            body{
                font-family: Arial;
                font-size: 13px;
                font-weight:normal;
            }
            tbody {
                display:table-row-group;
            }
            .standardtd{
                white-space:pre-wrap;
                text-align: left;   
            }
            thead{
                display: table-header-group;
            } 
            p {
                text-align: Justify;
            }
            .proposal {
                margin:2.54cm;
                margin-top:0;
                
            }
            .cover {
                margin:2.54cm;
                margin-top:0;
            }
            .terms {
                margin-left:1cm;
                margin-right:1cm;
                position: relative;
                width:100%;
                font-size: 12px;
            }
            .address{
                margin-top:30px; 
                float:left;
                display:block;
                <!--width:220px;-->
            }
            div.wrap {
                position: relative;
                width:100%;
            }
            div.clear {
                clear:both;
            }
            div.left {
                position: absolute; left: 0; width: 50%; 
            }
            div.right {
                position: absolute; right: 0; width: 50%; 
            }
            div.footer1 {
                position: fixed;
                bottom: 0;
                width: 100%;
            }
            .pagebreak{
                page-break-before:always;
            }
            div.leftterms {
                position: absolute; left: 0; width: 45%; margin:0;
            }
            div.rightterms {
                position: absolute; right: 1.9cm; width: 45%; margin:0; 
            }
            p.termsline {
                margin:4px;
            }
            table.term-table tr td{
                vertical-align: top;
                margin:3px;
                text-align: Justify;
                padding-top:3px;
            }
            .termNO {
                font-size:9px;
                padding-top:4px;
                width:25px;
            }
            
            table.proposal-table {
                width:15.5cm;
                margin:0;
            border-spacing: 0px;
            border-collapse: collapse;
            }
            table.proposal-table tr td{
                vertical-align: top;
                text-align: left;   
            }
        </style>
    </head>
    <body >
        <!---  ************** First Page (Cover Page) Start: **************   -->
        <div class="cover" style="font-size:13px;"> 
            <div class="address"> 
                
                <table style="border-spacing:0;font-size:13px; width: 100%;">
                    <tr>
                        <td width="45%"><apex:outputText value="{!If((contractInfo.ASI_CRM_MY_Contact_Salutation__c!='' || contractInfo.ASI_CRM_MY_Contact_Name__c!=''), UPPER(contractInfo.ASI_CRM_MY_Contact_Salutation__c+' '+contractInfo.ASI_CRM_MY_Contact_Name__c),'[select: Mr./Mrs.] [insert name]')}" /> </td>
                        <td width="55%"></td>
                    </tr>
                    
                    <tr>
                        <td><apex:outputText value="{!If(contractInfo.ASI_CRM_MY_Contact_Title__c!='', UPPER(contractInfo.ASI_CRM_MY_Contact_Title__c),'[insert job title]')}" /></td>
                        <td></td>
                    </tr>
                    
                    <tr>
                        <td><apex:outputText value="{!If(contractInfo.ASI_CRM_CN_Outlet_WS__r.ASI_CRM_CN_Legal_Name__c!='',contractInfo.ASI_CRM_CN_Outlet_WS__r.ASI_CRM_CN_Legal_Name__c,'[Company Name]')}" />
                        </td>
                        <td></td>
                    </tr>
                    
                    <apex:variable var="v" value="" rendered="{!(contractInfo.ASI_CRM_CN_Outlet_WS__r.Name!=null)}">
                        <tr><td>{!('T/A '+contractInfo.ASI_CRM_CN_Outlet_WS__r.Name)}</td><td></td></tr>
                    </apex:variable>
                    
                    <apex:variable var="v" value="" rendered="{!(contractInfo.ASI_CRM_CN_Outlet_WS__r.ASI_CRM_CN_Address__c!=null)}">
                    	<tr><td>{!contractInfo.ASI_CRM_CN_Outlet_WS__r.ASI_CRM_CN_Address__c}</td><td></td></tr>
                    </apex:variable>
                    <!--
                    <tr><td>Malaysia</td><td></td></tr>
                    -->
                    <!-- [SH] 2018-11-26 -->
                    <tr>                        
                        <td>
                            <apex:variable var="v" value="" rendered="{!(contractInfo.ASI_CRM_CN_Outlet_WS__r.ASI_CRM_PostalCode__c!=null)}">
                                {!contractInfo.ASI_CRM_CN_Outlet_WS__r.ASI_CRM_PostalCode__c}&nbsp;
                            </apex:variable>
                            <apex:variable var="v" value="" rendered="{!(contractInfo.ASI_CRM_CN_Outlet_WS__r.ASI_CRM_MY_City__r.name!=null)}">
                        		{!contractInfo.ASI_CRM_CN_Outlet_WS__r.ASI_CRM_MY_City__r.name}
                            </apex:variable>
                        </td>                                                
                    </tr>
                    <apex:variable var="v" value="" rendered="{!(contractInfo.ASI_CRM_CN_Outlet_WS__r.ASI_CRM_Province__c!=null)}">
                    	<tr><td>{!contractInfo.ASI_CRM_CN_Outlet_WS__r.ASI_CRM_Province__c}</td><td></td></tr>
                    </apex:variable>
                    <!-- END - [SH] --> 
                    <tr>
                        <td><apex:outputText value="{0,date, dd MMM, yyyy}"  rendered="{!contractInfo.ASI_CRM_MY_Proposal_Date__c!=null}">
                                <apex:param value="{!contractInfo.ASI_CRM_MY_Proposal_Date__c}" />
                            </apex:outputText>
                            <apex:outputText value="[insert day] [insert month] [insert year]" rendered="{!contractInfo.ASI_CRM_MY_Proposal_Date__c==null}"/>
                        </td>
                        <td></td>
                    </tr>
                </table>
            </div>


            <div style="clear:both;"></div>
            <br/>
            <br/>
            <p>Dear&nbsp;<apex:outputText value="{!If(contractInfo.ASI_CRM_MY_Contact_Name__c!='', contractInfo.ASI_CRM_MY_Contact_Name__c,'[insert name]')}" />,</p>
            <p></p>
            <p><span style="font-weight:bold;"><i>Concern: Sales and Brand Promotion Agreement – Our reference: {!contractInfo.Name}</i></span></p>
            <p></p>
            <p>Further to our recent discussions regarding the captioned, we are pleased to present you the attached proposal for your consideration.</p>
            <p></p>
            <p>
                We are keen on building a strong working relationship with you and we trust that you will find this proposal attractive. If you find this proposal acceptable, please confirm your acceptance by initialing each page, and signing these documents, as well as returning the duplicate of it to us on or before <apex:outputText value="{0,date, dd MMMM, yyyy}"><apex:param value="{!contractInfo.ASI_CRM_MY_Proposal_Date__c+14}"/></apex:outputText>.
            </p>
            <p></p>
            <p> 
				Please note that by signing this letter, you are deemed to have accepted the proposal and agreed to abide by our Terms and Conditions of Sales and Brand Promotion. These terms and conditions, together with this letter and the proposal, shall constitute our entire agreement (“Agreement”). This Agreement shall be effective as a legally binding agreement as of the date on which this letter is signed by you or on your behalf.  
            </p>
            <p></p>
            <p>
				Should you have any queries, please do not hesitate to contact {!ownerInfo.Name} at {!ownerInfo.Email}
                <apex:variable var="v" value="" rendered="{!(ownerInfo.Phone!='')}"> or at {!ownerInfo.Phone}.</apex:variable></p>
            <p></p>
            <p>
				We look forward to a mutually rewarding arrangement and thank you for your continuous support.
            </p>
            <p></p>
            <p>Yours faithfully,</p>
            <br/>
            <div class="wrap">
                <table style="font-size:13px;">
                    <tr>
                        <td width="42%">Signed for and on behalf of</td>
                        <td width="16%"></td>
                        <td width="42%">Accepted and agreed by for and on behalf of</td>
                    </tr>
                    <tr>
                        <td><span style="font-weight:bold;">Pernod Ricard Malaysia Sdn Bhd</span></td>
                        <td></td>
                       	<td><span style="font-weight:bold;">{!contractInfo.ASI_CRM_CN_Outlet_WS__r.ASI_CRM_CN_Legal_Name__c}</span></td>
                    </tr>
                    <tr><td>&nbsp;</td><td></td><td>&nbsp;</td></tr>
                    <tr><td>&nbsp;</td><td></td><td>&nbsp;</td></tr>
                    <tr><td>&nbsp;</td><td></td><td>&nbsp;</td></tr>
                    <tr><td>&nbsp;</td><td></td><td>&nbsp;</td></tr>
                    <tr>
                        <td>_______________________________</td>
                        <td></td>
                        <td>_______________________________</td>
                    </tr>
                    <tr>
                        <td><apex:outputText value="{!If(contractInfo.ASI_CRM_MY_Contact_Approver_Name__c!='', contractInfo.ASI_CRM_MY_Contact_Approver_Name__c,'[Approver name]')}" /></td>
                        <td></td>
                        <td><apex:outputText value="{!If(contractInfo.ASI_CRM_MY_Contact_Name__c!='', contractInfo.ASI_CRM_MY_Contact_Name__c,'[Signature name]')}" /></td>
                    </tr>
                    <tr>
                        <td><apex:outputText value="{!If(contractInfo.ASI_CRM_MY_Contact_Approver_Title__c!='', contractInfo.ASI_CRM_MY_Contact_Approver_Title__c,'[Approver Title]')}" /></td>
                        <td></td>
                        <td><apex:outputText value="{!If(contractInfo.ASI_CRM_MY_Contact_Title__c!='', contractInfo.ASI_CRM_MY_Contact_Title__c,'[insert job title]')}" /></td>
                    </tr>
                    <tr>
                        <td></td>
                        <td></td>
                        <td>Date: </td>
                    </tr>
                </table>
            </div>
        </div>
        <!---  ************** First Page (Cover Page) End **************   -->

        
        <!---  ************** Contract Detail : **************   -->
        <br clear="all" style="page-break-before:always;" />
        
        <center style="font-size:13px; font-weight: bold;">CONTRACT</center>
        <div class="proposal" style="font-size:13px;"> 
            <p>
                <b>This contract is subject to our Terms and Conditions of Sales and Brand Promotion as attached here with.</b> 
            </p>
            <table class="proposal-table" style="font-size:13px; width:100%;">
                <tr style="width:100%;">
                    <td valign="top" style="border-top: 1px solid rgb(79,129,189);border-bottom: 1px solid rgb(79,129,189);border-collapse: collapse; width:40%; margin-left:5px;">
                        <b>Customer’s legal name and address:</b>
                    </td>
                    <td style="border-top: 1px solid rgb(79,129,189);border-bottom: 1px solid rgb(79,129,189);border-collapse: collapse; width:60%; margin-left:5px;">
                        <p>{!contractInfo.ASI_CRM_CN_Outlet_WS__r.ASI_CRM_CN_Legal_Name__c}</p>
                        <p>{!contractInfo.ASI_CRM_CN_Outlet_WS__r.ASI_CRM_CN_Address__c}</p>
                    </td>
                </tr>
                <tr>
                    <td valign="top" style="background-color:rgb(211,223,238); width:40%; margin-left:5px;"><b>Promotion Outlet:</b></td>
                    <td style="background-color:rgb(211,223,238); width:60%; margin-left:5px;">
                        <!--
                        <p><apex:outputText value="{!If(contractInfo.ASI_CRM_CN_Outlet_WS__r.ASI_CRM_CN_Legal_Name__c!='',contractInfo.ASI_CRM_CN_Outlet_WS__r.ASI_CRM_CN_Legal_Name__c,'[insert Name]')}" /></p>
                        <p>
                            <apex:repeat var="cOutlet" value="{!promotionOutlets}" >
                                <apex:outputText value="{!cOutlet.ASI_CRM_CN_Legal_Name__c}" /><br/>
                            </apex:repeat>
                        </p>
                        -->
                        <p>
                            <apex:repeat var="cOutlet" value="{!promotionOutlets}" >
                                <apex:outputText value="{!cOutlet.Name}" /><br/>
                            </apex:repeat>
                        </p>
                    </td>
                </tr>
                <tr>
                    <td valign="top" style="border-top: 1px solid rgb(79,129,189);border-bottom: 1px solid rgb(79,129,189);border-collapse: collapse; width:40%; margin-left:5px;">
                        <b>Term of the Agreement:</b>
                    </td>
                    <td style="border-top: 1px solid rgb(79,129,189);border-bottom: 1px solid rgb(79,129,189);border-collapse: collapse; width:60%; margin-left:5px;">
                        <apex:outputText value="{0,date, dd MMM yyyy}"><apex:param value="{!contractInfo.ASI_TH_CRM_Start_Date__c}"/></apex:outputText>
                        to 
                        <apex:outputText value="{0,date, dd MMM yyyy}"><apex:param value="{!contractInfo.ASI_TH_CRM_End_Date__c}"/></apex:outputText>
                        &nbsp;({!totalContractMonths} Month(s))
                    </td>
                </tr>
            </table>
            <br />


            <!---  ************** Section 1. Trading Terms : **************   -->
            <p>
                <b>1. SALES TARGET</b> 
            </p>

            <!---  ************** 1.1. List and Price of Product : **************   -->
            <p>
                <b>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;1.1 List of Products and Target</b> 
            </p>
            <table class="proposal-table" style="font-size:13px; width:100%;">
                <tr>
                    <th style="width:50%;text-align:center;border: 1px solid rgb(79,129,189);border-bottom: 2px solid rgb(79,129,189);color:rgb(31,73,125);border-collapse: collapse;">
                        Product
                    </th>
                    <th style="width:25%;text-align:center;border: 1px solid rgb(79,129,189);border-bottom: 2px solid rgb(79,129,189);color:rgb(31,73,125);border-collapse: collapse;">
                        Packing / Size
                    </th>
                    <th style="width:25%;text-align:center;border: 1px solid rgb(79,129,189);border-bottom: 2px solid rgb(79,129,189);color:rgb(31,73,125);border-collapse: collapse;">
                        Sales Target (dozen case) 
                    </th>
                </tr>

                <apex:repeat value="{!mapOfContractTarget}" var="MeItems" >
                    <tr style="background-color: rgb(211,223,238);">
                        <td style="border: 1px solid rgb(79,129,189); margin-left:5px;">
                           	<b><i>{!MeItems}</i></b>
                        </td>
                        <td style="border: 1px solid rgb(79,129,189);text-align:right;">
                        </td>
                        <td style="border: 1px solid rgb(79,129,189);text-align:right;">
                        </td>
                    </tr>
                    <apex:repeat value="{!mapOfContractTarget[MeItems]}" var="targetSKU" >
                        <tr>
                            <td style="border: 1px solid rgb(79,129,189); margin-left:5px;">
                                {!targetSKU.sku_description}
                            </td>
                            <td style="border: 1px solid rgb(79,129,189);text-align:left; margin-left:5px;">
                               	{!targetSKU.packFactor}
                            </td>
                            <td style="border: 1px solid rgb(79,129,189);text-align:right; margin-right:5px;">
                                <apex:outputText value="{0, number, ###,###,###,##0.0}">  
                                    <apex:param value="{!targetSKU.salesTarget_dozenCase}"/>  
                                </apex:outputText> 
                            </td>
                        </tr>
                    </apex:repeat>
                </apex:repeat>
                <!--
                <tr style="background-color: rgb(211,223,238);">                    
                    <td style="border: 1px solid rgb(79,129,189); margin-left:5px; font-weight: bold;">
                        Total:
                    </td>
                    <td style="border: 1px solid rgb(79,129,189);text-align:right;">
                    	
                    </td>
                    <td style="border: 1px solid rgb(79,129,189); margin-left:5px; font-weight: bold; text-align:right; margin-right:5px;">
                        {!salesTarget_totalCase}
                    </td>
                </tr>
				-->
            </table>
            <p>
                Sales target computed based on packing of 12 bottles.
            </p>
            <br />

            <!---  ************** 1.2. Sales Incentive (Trade Offer) : **************   -->
            <p>
                <b>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;1.2 Sales Incentive (Trade Offer)</b> 
            </p>
            <table class="proposal-table" style="font-size:13px; width:100%;">
                <tr>
                    <th style="width:30%;text-align:center;border: 1px solid rgb(79,129,189);border-bottom: 2px solid rgb(79,129,189);color:rgb(31,73,125);border-collapse: collapse;">
                        Product
                    </th>
                    <th style="width:15%;text-align:center;border: 1px solid rgb(79,129,189);border-bottom: 2px solid rgb(79,129,189);color:rgb(31,73,125);border-collapse: collapse;">
                        No. of bottles
                    </th>
                    <th style="width:15%;text-align:center;border: 1px solid rgb(79,129,189);border-bottom: 2px solid rgb(79,129,189);color:rgb(31,73,125);border-collapse: collapse;">
                        FOC (WS) (bottles) 
                    </th>
                    <th style="width:15%;text-align:center;border: 1px solid rgb(79,129,189);border-bottom: 2px solid rgb(79,129,189);color:rgb(31,73,125);border-collapse: collapse;">
                        FOC (PRM) (bottles)
                    </th>
                    <!--
                    <th style="width:15%;text-align:center;border: 1px solid rgb(79,129,189);border-bottom: 2px solid rgb(79,129,189);color:rgb(31,73,125);border-collapse: collapse;">
                        Specific payment term (if any)
                    </th>
                    -->
                    <th style="width:25%;text-align:center;border: 1px solid rgb(79,129,189);border-bottom: 2px solid rgb(79,129,189);color:rgb(31,73,125);border-collapse: collapse;">
                        FOC Item
                    </th>
                </tr>
                <apex:repeat value="{!mapOfSalesIncentive}" var="salesIncentiveKey" >
                    <tr style="background-color: rgb(211,223,238);">
                        
                        <td style="border: 1px solid rgb(79,129,189); margin-left:5px;">
                            <b><i>{!salesIncentiveKey}</i></b>
                        </td>
                        <td style="border: 1px solid rgb(79,129,189);">
                           
                        </td>
                        <td style="border: 1px solid rgb(79,129,189);">
							
                        </td>
                        <td style="border: 1px solid rgb(79,129,189);">
							
                        </td>
                        <td style="border: 1px solid rgb(79,129,189);">
							
                        </td>
                    </tr> 
                    <apex:repeat value="{!mapOfSalesIncentive[salesIncentiveKey]}" var="salesIncSKU" >
                        <tr>
                            <td style="border: 1px solid rgb(79,129,189); margin-left:5px;">
                                {!salesIncSKU.sku_description}
                            </td>
                            <td style="border: 1px solid rgb(79,129,189);text-align:right; margin-right:5px; ">
                                12.0
                            </td>
                            <td style="border: 1px solid rgb(79,129,189);text-align:right; margin-right:5px;">
                                <apex:outputText value="{0, number, ###,###,###,##0.0}">  
                                    <apex:param value="{!salesIncSKU.FOCWS_dozen}"/>  
                                </apex:outputText>
                            </td>
                            <td style="border: 1px solid rgb(79,129,189);text-align:right; margin-right:5px;">
                                <apex:outputText value="{0, number, ###,###,###,##0.0}">  
                                    <apex:param value="{!salesIncSKU.FOCPRM_dozen}"/>  
                                </apex:outputText>
                            </td>
                            <!--
                            <td style="border: 1px solid rgb(79,129,189);text-align:center;">
                                Monthly
                            </td>
                        -->
                            <td style="border: 1px solid rgb(79,129,189);text-align:left;">
                                {!salesIncSKU.crossbrand_sku_description}
                            </td>
                        </tr>
                    </apex:repeat>
                </apex:repeat>
                <!--
                <tr style="background-color: rgb(211,223,238);">                    
                    <td style="border: 1px solid rgb(79,129,189); margin-left:5px; font-weight: bold;">
                        Total:
                    </td>
                    <td style="border: 1px solid rgb(79,129,189);text-align:right;">
                    	
                    </td>
                    <td style="border: 1px solid rgb(79,129,189);text-align:center;font-weight: bold;">
                       	{!SaleInc_totalFOCWSBtl}
                    </td>
                    <td style="border: 1px solid rgb(79,129,189);text-align:center;font-weight: bold;">
                        {!SaleInc_totalFOCPRMBtl}
                    </td>
                    <td style="border: 1px solid rgb(79,129,189);text-align:right;">
                        
                    </td>
                </tr>
				-->
            </table>
           	<br />

            <apex:variable value="" var="v" rendered="{!contractInfo.ASI_CRM_MY_Trade_Offer_List__c!=''}" >
            <p>
                <apex:outputField value="{!contractInfo.ASI_CRM_MY_Trade_Offer_List__c}" />
            </p>
            </apex:variable>

            <!--
            <table class="proposal-table" style="font-size:13px;">
                <tr>
                    <th style="width:50%;text-align:center;border: 1px solid rgb(79,129,189);border-bottom: 2px solid rgb(79,129,189);color:rgb(31,73,125);border-collapse: collapse;">
                        Product
                    </th>
                    <th style="width:50%;text-align:center;border: 1px solid rgb(79,129,189);border-bottom: 2px solid rgb(79,129,189);color:rgb(31,73,125);border-collapse: collapse;">
                        Terms
                    </th>
                </tr>
                <apex:repeat value="{!mapOfSalesIncentive}" var="salesIncentiveKey" >
                    <apex:variable var="SalesIncentiveCount" value="{!0}"/>
                    <apex:repeat value="{!mapOfSalesIncentive[salesIncentiveKey]}" var="salesIncSKU" >
                        <tr style="{!IF(MOD(SalesIncentiveCount,2) == 0, 'background-color: rgb(211,223,238);','')}">
                            <td style="border: 1px solid rgb(79,129,189);">
                                {!salesIncSKU.sku_description}
                            </td>
                            <td style="border: 1px solid rgb(79,129,189);text-align:right;">
                                <apex:variable var="v" value="">
                                Buy {!salesIncSKU.AchievedBtl} get {!salesIncSKU.totalFOC} free
                                </apex:variable>
                            </td>
                        </tr>
                        <apex:variable var="SalesIncentiveCount" value="{!SalesIncentiveCount+1}"/> 
                    </apex:repeat>
                </apex:repeat>
           	</table>
           -->



            <!---  ************** 1.3 Volume Incentive : **************   -->
            <!--<br clear="all" style="page-break-before:always;" />-->
            <br />
            <p>
                <b>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;1.3 Volume Incentive </b> 
            </p>
