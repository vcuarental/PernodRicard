<apex:component controller="ASI_CRM_HK_ContractPDFController" access="global">
    <apex:variable var="dummyV" value="{!dummyVar}" id="dummyVar"/>
    <apex:attribute name="paf_id" description="ID of PAF Contract PDF printout" type="id" assignto="{!PAFID}"/>
    <apex:attribute name="cus_id" description="ID of PAF Customer PDF printout" type="id" assignto="{!CusID}"/>
    <head>
        <style>
            .htitle{
            font-weight:bold;
            }
            h3{
            font-weight:bold;
            font-size: 11px;
            }
            h1{
            font-size:35px;     
            }
            
            @Page:first {
            size: A4;
            margin:0cm;
            margin-top: 3.0cm;
            margin-bottom:0.5cm;
            @PageBreak{
            page-break-inside:avoid;
            page-break:always; 
            }       
            @top-center { 
            content: element(header); 
            }
            @bottom-left
            {
            content: normal
            }
            @bottom-center
            {
            content: normal; 
            }
            }
            @Page{
            size: A4;
            margin:0cm;
            margin-top: 3.0cm;
            margin-bottom:0.5cm;
            @PageBreak{
            page-break-inside:avoid;
            page-break:always; 
            }       
            @top-center { 
            content: element(header); 
            }
            @bottom-left
            {
            content: element(footer);
            font-size: 10px; 
            }
            @bottom-center
            {
            content: "Page " counter(page) " of " counter(pages);
            font-size: 10px; 
            }
            }
            
            div.header {
            position: running(header);
            }
            div.footer {
            position: running(footer);
            }
            .nobreak{
            page-break-inside: avoid;       
            }  
            table {
            width: 100%;
            border-spacing: 0;
            }
            tr{
            page-break-inside:avoid;
            }
            body{
            font-family: Arial;
            font-size: 13px;
            font-weight:normal;
            }
            tbody {
            display:table-row-group;
            }
            .standardtd{
            white-space:pre-wrap;
            text-align: left;   
            }
            thead{
            display: table-header-group;
            } 
            p {
            text-align: Justify;
            }
            .proposal {
            margin:2.54cm;
            margin-top:0;
            text-align: Justify;
            }
            .cover {
            margin:2.54cm;
            margin-top:0;
            }
            .terms {
            margin-left:1cm;
            margin-right:1cm;
            position: relative;
            width:100%;
            font-size: 12px;
            }
            .address{
            margin-top:30px; 
            float:right;
            display:block;
            }
            div.wrap {
            position: relative;
            width:100%;
            }
            div.clear {
            clear:both;
            }
            div.left {
            position: absolute; left: 0; width: 50%; 
            }
            div.right {
            position: absolute; right: 0; width: 50%; 
            }
            div.footer1 {
            position: fixed;
            bottom: 0;
            width: 100%;
            }
            .pagebreak{
            page-break-before:always;
            }
            div.leftterms {
            position: absolute; left: 0; width: 45%; margin:0;
            }
            div.rightterms {
            position: absolute; right: 1.9cm; width: 45%; margin:0; 
            }
            p.termsline {
            margin:4px;
            }
            table.term-table tr td{
            vertical-align: top;
            margin:3px;
            text-align: Justify;
            padding-top:3px;
            }
            .termNO {
            font-size:9px;
            padding-top:4px;
            width:25px;
            }
            
            table.proposal-table {
            width:15.5cm;
            margin:0;
            border-spacing: 0px;
            border-collapse: collapse;
            }
            table.proposal-table tr td{
            vertical-align: top;
            text-align: left;	
            }
        </style>
    </head>
    
    <div class="header">
        <center><apex:image width="10.76cm" height="2.08inch" url="{!URLFOR($Resource.ASI_CRM_HK_Logo)}"/></center>
    </div>
    
    <div class="footer">
        <center><apex:outputText value="a division of Pernod Ricard Asia Duty Free Limited"
                                 style="font-family:century gothic,verdana,arial,helvetica,sans-serif;font-size: 12px;font-weight: bold;color: #20315d;"></apex:outputText></center>
        <center><apex:outputText value="25/F, Sun Life Tower, Harbour City, 15 Canton Road, Tsim Sha Tsui, Hong Kong • Tel:(852)2398 3636 • Fax:(852)2381 8922"
                                 style="font-family:century gothic,verdana,arial,helvetica,sans-serif;font-size: 10px;color: #26385E;"></apex:outputText></center>
        <!--<center><apex:image width="15.85cm" height="0.635cm" url="{!URLFOR($Resource.ASI_CRM_HK_Contract_Footer_Logo)}"/></center>-->
        &nbsp;&nbsp;&nbsp;&nbsp;<apex:outputText value="{!CurrentDate}" style="font-size: 10px;text-align:right;"></apex:outputText>
    </div>
    
    <body>
        <div class="cover"> 
            <div class="address"> 
                <table>
                    <tr><td>{!CustomerInfo.Salutation} {!CustomerInfo.Name}</td></tr>
                    <tr><td>{!CustomerInfo.Title}</td></tr>
                    <tr><td>{!OutletInfoPN}</td></tr><!--20160226,edited by Leo-->
                    <tr><td>{!IF(OutletInfo.Parent.Name==null,OutletInfoN,'O/B '+OutletInfoN)}</td></tr><!--20160226,edited by Leo-->
                    <!--<tr><td>O/B {!OutletInfo.Name} </td></tr>-->
                    <tr><td>{!OutletInfo.ASI_HK_CRM_Address__c} </td></tr>
                    <tr><td>Hong Kong</td></tr>
                    <tr><td>&nbsp;</td></tr>
                    <tr><td>{!CurrentDate}</td></tr>
                </table>
            </div>
            <div class="clear"></div>
            <br />
            <br />
            <p>
                Dear {!CustomerInfo.Salutation} {!CustomerInfo.Name},
            </p><br />
            
            <p>
                <span style="font-weight:bold;"><i>Concern: Sales and Brand Promotion Agreement – Our reference: {!PAF.Name}</i></span>
            </p>
            <br />
            <p>
                Further to our recent discussions regarding the captioned, we are pleased to present you the attached proposal for your consideration. 
            </p>
            <p>  
                We are keen to build a strong working relationship with you and we trust that you will find this proposal attractive. If you find this proposal acceptable, please confirm your acceptance by initialing each page, and signing these documents, as well as returning the duplicate of it to us on or before <apex:outputText value="{0,date, dd MMMM,  yyyy}"><apex:param value="{!printDate}"/></apex:outputText>.
            </p>
            <p> 
                Please note that by signing this letter, you are deemed to have accepted the proposal and agreed to abide by our Terms and Conditions of Sales and Brand Promotion. These terms and conditions, together with this letter and the proposal shall constitute our entire agreement (“Agreement”). This Agreement shall be effective as a legally binding agreement as of the date on which this letter is signed by you or on your behalf.  
            </p>      
            <p>
                Should you have any queries, please do not hesitate to contact {!PAF.Owner.Name} at {!PAF.Owner.Email} or at {!PAF.Owner.Phone}.
            </p>
            <p>
                We look forward to a mutually rewarding arrangement and thank you for your continuous support.
            </p><br />
            <p>
                Yours faithfully, 
            </p><br />
            <div class="wrap">
                <div class="left">
                    <table>
                        <tr><td>Signed for and on behalf of</td></tr>
                        <tr><td><span style="font-weight:bold;">Pernod Ricard Asia Duty Free Limited</span></td></tr>
                        <tr><td>&nbsp;</td></tr>
                        <tr><td>&nbsp;</td></tr>
                        <tr><td>&nbsp;</td></tr>
                        <tr><td>&nbsp;</td></tr>
                        <tr><td>_______________________________</td></tr>
                        <tr><td>{!SignatureName}</td></tr>
                        <!--<tr><td>{!SignaturePost}</td></tr>-->
                        <tr><td>Managing Director</td></tr>
                        <tr><td>Hong Kong &amp; Macau</td></tr>
                    </table>
                </div>
                <div class="right">
                    <table>
                        <tr><td>Accepted and agreed by for and on behalf of</td></tr>
                        <!--<tr><td><span style="font-weight:bold;">{!PAF.ASI_HK_CRM_PAF_Customer__r.Parent.Name}</span></td></tr>-->
                        <tr><td><span style="font-weight:bold;">{!IF(PAF.ASI_HK_CRM_PAF_Customer__r.Parent.Name==null,OutletInfoN,OutletInfoPN)}</span></td></tr><!--20160226,edited by Leo-->
                        <tr><td>&nbsp;</td></tr>
                        <tr><td>&nbsp;</td></tr>
                        <tr><td>&nbsp;</td></tr>
                        <tr><td>&nbsp;</td></tr>
                        <tr><td>_______________________________</td></tr>
                        <tr><td>Name:</td></tr>
                        <tr><td>Title:</td></tr>
                        <tr><td>Date:</td></tr>
                    </table>
                </div>
            </div>
            
            <!--<div class="footer1">
<center>
<apex:outputText value="a division of Pernod Ricard Asia Duty Free Limited" style="font-size: 10px;text-align:right;"></apex:outputText>
<br/>
<apex:outputText value="12/F,Sun Life Tower, 15 Canton Road, Harbour City, Tsimshatsui, Kowloon, Hong Kong • Tel:(852)2398 3636 • Fax:(852)2381| 8922" style="font-size: 10px;text-align:right;"></apex:outputText>
<br/>
<apex:image width="15.85cm" height="0.635cm" url="{!URLFOR($Resource.ASI_CRM_HK_Contract_Footer_Logo)}"/>
</center>
</div>-->
        </div>
        <div class="pagebreak"></div>
        <center><b>PROPOSAL</b></center>
        <div class="proposal"> 
            <p>
                <b>This proposal is subject to our Terms and Conditions of Sales and Brand Promotion as attached herewith.</b> 
            </p>
            <table class="proposal-table">
                <tr>
                    <td style="border-top: 1px solid rgb(79,129,189);border-bottom: 1px solid rgb(79,129,189);border-collapse: collapse;">
                        <b>Customer’s name and address:</b>
                    </td>
                    <td style="border-top: 1px solid rgb(79,129,189);border-bottom: 1px solid rgb(79,129,189);border-collapse: collapse;">
                        <b>{!OutletInfo.Name}<br />{!OutletInfo.ASI_HK_CRM_Address__c}</b>
                    </td>
                </tr>
                <tr>
                    <td style="background-color:rgb(211,223,238)"><b>Promotion Outlet:</b></td>
                    <td style="background-color:rgb(211,223,238)"></td>
                </tr>
                <tr>
                    <td><b>Term of the Agreement:</b></td>
                    <td>
                        <apex:outputText value="{0,date, dd MMMM yyyy}"><apex:param value="{!PAF.ASI_HK_CRM_PAF_Start_Date__c}"/></apex:outputText>
                        to 
                        <apex:outputText value="{0,date, dd MMMM yyyy}"><apex:param value="{!PAF.ASI_HK_CRM_PAF_End_Date__c}"/></apex:outputText>
                    </td>
                </tr>
                <tr>	
                    <td style="border-collapse: collapse;background-color:rgb(211,223,238)"><b>Credit Limit and payment term:</b></td>
                    <td style="border-collapse: collapse;background-color:rgb(211,223,238)">
                        <apex:outputText value="{0, number, ###,##0.00}">
                            <apex:param value="{!OutletInfo.ASI_HK_CRM_Credit_Limit__c}"/>
                        </apex:outputText>
                        <br />
                        {!OutletInfo.ASI_HK_CRM_Payment_Term_Description__c}
                    </td>
                </tr>
                
                
            </table>
            <p>
                <b>1. TRADING TERMS</b> 
            </p>
            <p>
                <b>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;1.1 List and Price of Products</b> 
            </p>
            <p>
                &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;a)	House pouring Product List
            </p>
            <table class="proposal-table">
                <tr>
                    <th style="width:50%;text-align:center;border: 1px solid rgb(79,129,189);border-bottom: 2px solid rgb(79,129,189);color:rgb(31,73,125)">
                        House pouring Product
                    </th>
                    <!--<th style="width:25%;text-align:center;border: 1px solid rgb(79,129,189);border-bottom: 2px solid rgb(79,129,189);color:rgb(31,73,125)">
                        Packing / Size
                    </th>--><!--20160226,edited by Leo-->
                    <th style="width:15%;text-align:center;border: 1px solid rgb(79,129,189);border-bottom: 2px solid rgb(79,129,189);color:rgb(31,73,125)">
                        List Price<br />(HK$ per bottle)
                    </th>
                    <th style="width:15%;text-align:center;border: 1px solid rgb(79,129,189);border-bottom: 2px solid rgb(79,129,189);color:rgb(31,73,125)">
                        On-Invoice Discount<br />(HK$ per bottle)<!--20160301,edited by Leo-->
                    </th>
                    <!--<th style="width:15%;text-align:center;border: 1px solid rgb(79,129,189);border-bottom: 2px solid rgb(79,129,189);color:rgb(31,73,125)">
                        On-Invoice Price<br />(HK$ per bottle)
                    </th>--><!--20160408,edited by Leo--><!--20160301,edited by Leo-->
                </tr>
                <apex:variable var="count" value="{!0}"/>
                <apex:repeat value="{!ItemsMap}" var="MeItems" ><!--20160301,edited by Leo-->
                    <apex:variable var="v" value="" rendered="{!ItemsMap[MeItems].ASI_HK_CRM_Pouring__c == TRUE && ItemsMap[MeItems].ASI_CRM_HK_All_Product__c == FALSE}"><!--20160408,edited by Leo--><!--20160301,edited by Leo-->
                        <tr style="{!IF(MOD(count,2) == 0, 'background-color: rgb(211,223,238);','')}">
                            
                            <td style="border: 1px solid rgb(79,129,189);">
                                {!ItemsMap[MeItems].ASI_HK_CRM_SKU_Lookup__r.ASI_MFM_Item_Group__r.ASI_CRM_ITEM_GROUP_Contract_Desc__c}<!--20160301,edited by Leo-->
                            </td>
                            <!--<td style="border: 1px solid rgb(79,129,189);">
                                {!MeItems.ASI_HK_CRM_SKU_Lookup__r.ASI_FOC_Packing_Description__c}
                            </td>--><!--20160226,edited by Leo-->
                            <td style="border: 1px solid rgb(79,129,189);text-align:right;">
                                <apex:outputText value="{0, number, ###,##0.00}">
                                    <apex:param value="{!ItemsMap[MeItems].ASI_HK_CRM_Price__c}"/><!--20160301,edited by Leo-->
                                </apex:outputText>
                            </td>
                            <td style="border: 1px solid rgb(79,129,189);text-align:right;">
                                <apex:outputText value="{!IF(ItemsMap[MeItems].ASI_HK_CRM_Mechanic__r.ASI_CRM_HK_PAF_Report_Section__c != 'On-Invoice Discount', '-', IF(ItemsMap[MeItems].ASI_HK_CRM_Rebate_Disc_Amt_Per_BT__c != null,ItemsMap[MeItems].ASI_HK_CRM_Rebate_Disc_Amt_Per_BT__c,ItemsMap[MeItems].ASI_HK_CRM_Price__c*ItemsMap[MeItems].ASI_HK_CRM_Rebate_Disc_Percent__c/100))}"/>
                            </td><!--20160301,edited by Leo-->
                            <!--<td style="border: 1px solid rgb(79,129,189);text-align:right;">
                                <apex:outputText value="{!IF(ItemsMap[MeItems].ASI_HK_CRM_Mechanic__r.ASI_CRM_HK_PAF_Report_Section__c != 'On-Invoice Discount', '-', IF(ItemsMap[MeItems].ASI_HK_CRM_Rebate_Disc_Amt_Per_BT__c != null,ItemsMap[MeItems].ASI_HK_CRM_Price__c - ItemsMap[MeItems].ASI_HK_CRM_Rebate_Disc_Amt_Per_BT__c,ItemsMap[MeItems].ASI_HK_CRM_Price__c-(ItemsMap[MeItems].ASI_HK_CRM_Price__c*ItemsMap[MeItems].ASI_HK_CRM_Rebate_Disc_Percent__c/100)))}"/>
                            </td>--><!--20160408,edited by Leo--><!--20160301,edited by Leo-->
                        </tr>
                    </apex:variable>
                    <apex:variable var="count" value="{!count+1}"/> 
                </apex:repeat>
            </table>
            <p>
                &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;b)	Other Product List
            </p>
            <table class="proposal-table">
                <tr>
                    <th style="width:50%;text-align:center;border: 1px solid rgb(79,129,189);border-bottom: 2px solid rgb(79,129,189);color:rgb(31,73,125)">
                        Product
                    </th>
                    <!--<th style="width:25%;text-align:center;border: 1px solid rgb(79,129,189);border-bottom: 2px solid rgb(79,129,189);color:rgb(31,73,125)">
                        Packing / Size
                    </th>--><!--20160226,edited by Leo-->
                    <th style="width:15%;text-align:center;border: 1px solid rgb(79,129,189);border-bottom: 2px solid rgb(79,129,189);color:rgb(31,73,125)">
                        List Price<br />(HK$ per bottle)<!--20160226,edited by Leo-->
                    </th>
                    <th style="width:15%;text-align:center;border: 1px solid rgb(79,129,189);border-bottom: 2px solid rgb(79,129,189);color:rgb(31,73,125)">
                        On-Invoice Discount<br />(HK$ per bottle)<!--20160301,edited by Leo-->
                    </th>
                    <!--<th style="width:15%;text-align:center;border: 1px solid rgb(79,129,189);border-bottom: 2px solid rgb(79,129,189);color:rgb(31,73,125)">
                        On-Invoice Price<br />(HK$ per bottle)
                    </th>--><!--20160408,edited by Leo--><!--20160301,edited by Leo-->
                </tr>
                <apex:variable var="count" value="{!0}"/>
                <apex:repeat value="{!ItemsMap}" var="MeItems" ><!--20160301,edited by Leo-->
                    <apex:variable var="v" value="" rendered="{!ItemsMap[MeItems].ASI_HK_CRM_Pouring__c == FALSE && ItemsMap[MeItems].ASI_CRM_HK_All_Product__c == FALSE}"><!--20160408,edited by Leo--><!--20160301,edited by Leo-->
                        <tr style="{!IF(MOD(count,2) == 0, 'background-color: rgb(211,223,238);','')}">
                            <td style="border: 1px solid rgb(79,129,189);">
                                {!ItemsMap[MeItems].ASI_HK_CRM_SKU_Lookup__r.ASI_MFM_Item_Group__r.ASI_CRM_ITEM_GROUP_Contract_Desc__c}<!--20160301,edited by Leo-->
                            	[{!ItemsMap[MeItems].ASI_HK_CRM_SKU_Lookup__r.ASI_MFM_Sub_brand__r.Name}]
                            </td>
                            <!--<td style="border: 1px solid rgb(79,129,189);">
                                {!MeItems.ASI_HK_CRM_SKU_Lookup__r.ASI_FOC_Packing_Description__c}
                            </td>--><!--20160226,edited by Leo-->
                            <td style="border: 1px solid rgb(79,129,189);text-align:right;">
                                <apex:outputText value="{0, number, ###,##0.00}">
                                    <apex:param value="{!ItemsMap[MeItems].ASI_HK_CRM_Price__c}"/><!--20160301,edited by Leo-->
                                </apex:outputText>
                            </td>
                            <td style="border: 1px solid rgb(79,129,189);text-align:right;">
                                <apex:outputText value="{!IF(ItemsMap[MeItems].ASI_HK_CRM_Mechanic__r.ASI_CRM_HK_PAF_Report_Section__c != 'On-Invoice Discount', '-', IF(ItemsMap[MeItems].ASI_HK_CRM_Rebate_Disc_Amt_Per_BT__c != null,ItemsMap[MeItems].ASI_HK_CRM_Rebate_Disc_Amt_Per_BT__c,ItemsMap[MeItems].ASI_HK_CRM_Price__c*ItemsMap[MeItems].ASI_HK_CRM_Rebate_Disc_Percent__c/100))}"/>
                            </td><!--20160301,edited by Leo-->
                            <!--<td style="border: 1px solid rgb(79,129,189);text-align:right;">
                                <apex:outputText value="{!IF(ItemsMap[MeItems].ASI_HK_CRM_Mechanic__r.ASI_CRM_HK_PAF_Report_Section__c != 'On-Invoice Discount', '-', IF(ItemsMap[MeItems].ASI_HK_CRM_Rebate_Disc_Amt_Per_BT__c != null,ItemsMap[MeItems].ASI_HK_CRM_Price__c - ItemsMap[MeItems].ASI_HK_CRM_Rebate_Disc_Amt_Per_BT__c,ItemsMap[MeItems].ASI_HK_CRM_Price__c-(ItemsMap[MeItems].ASI_HK_CRM_Price__c*ItemsMap[MeItems].ASI_HK_CRM_Rebate_Disc_Percent__c/100)))}"/>
                            </td>--><!--20160408,edited by Leo--><!--20160301,edited by Leo-->
                        </tr>
                        <apex:variable var="count" value="{!count+1}"/>
                    </apex:variable>
                </apex:repeat>
            </table>
            <p>
                <b>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;1.2 Sales Target, Volume Discount and Volume Rebate</b>
            </p>
            <table class="proposal-table">
                <tr>
                    <th style="width:37.5%;text-align:center;border: 1px solid rgb(79,129,189);border-bottom: 2px solid rgb(79,129,189);color:rgb(31,73,125)">
                        Product
                    </th>
                    <th style="width:12.5%;text-align:center;border: 1px solid rgb(79,129,189);border-bottom: 2px solid rgb(79,129,189);color:rgb(31,73,125)">
                        Sales target (bottles / or HK$ / Term)
                    </th>
                    <th style="width:12.5%;text-align:center;border: 1px solid rgb(79,129,189);border-bottom: 2px solid rgb(79,129,189);color:rgb(31,73,125)">
                        Volume discount (HK$ / Term)
                    </th>
                    <th style="width:12.5%;text-align:center;border: 1px solid rgb(79,129,189);border-bottom: 2px solid rgb(79,129,189);color:rgb(31,73,125)">
                        Volume rebate (HK$ / bottle)
                    </th>
                    <th style="width:25%;text-align:center;border: 1px solid rgb(79,129,189);border-bottom: 2px solid rgb(79,129,189);color:rgb(31,73,125)">
                        Specific payment term<br />(if any)
                    </th>
                </tr>
                <apex:variable value="{!0}" var="total_t_qty"/>
                <apex:variable value="{!0}" var="total_vol_dis"/>
                <apex:variable value="{!0}" var="total_vol_reb"/>
                <apex:variable var="count" value="{!0}"/>
                <apex:repeat value="{!MechanicItems}" var="MeItems" >
                    <apex:variable var="v" value="" rendered="{!MeItems.ASI_HK_CRM_Mechanic__r.ASI_CRM_HK_PAF_Report_Section__c == 'Sales target, volume discount and volume rebate'}">
                        <tr style="{!IF(MOD(count,2) == 0, 'background-color: rgb(211,223,238);','')}">
                            <td style="border: 1px solid rgb(79,129,189);">
                                {!MeItems.ASI_HK_CRM_SKU_Lookup__r.ASI_MFM_Item_Group__r.ASI_CRM_ITEM_GROUP_Contract_Desc__c}
                            </td>
                            <td style="border: 1px solid rgb(79,129,189);text-align:right;">
                                {!MeItems.ASI_HK_CRM_Target_Volume_Qty__c}
                                <apex:variable var="total_t_qty" value="{!total_t_qty + MeItems.ASI_HK_CRM_Target_Volume_Qty__c}" />
                            </td>
                            <td style="border: 1px solid rgb(79,129,189);text-align:right;">
                                <apex:outputText value="{0, number, ###,##0.00}" rendered="{!MeItems.ASI_HK_CRM_Rebate_Disc_Amt_Per_BT__c==null}">
                                    <apex:param value="{!MeItems.ASI_HK_CRM_Investment_Value__c}"/>
                                    <apex:variable var="total_vol_dis" value="{!total_vol_dis + MeItems.ASI_HK_CRM_Investment_Value__c}" />
                                </apex:outputText>
                            </td>
                            <td style="border: 1px solid rgb(79,129,189);text-align:right;">
                                <apex:outputText value="{0, number, ###,##0.00}">
                                    <apex:param value="{!MeItems.ASI_HK_CRM_Rebate_Disc_Amt_Per_BT__c}"/>
                                    <apex:variable var="total_vol_reb" value="{!total_vol_reb + IF(MeItems.ASI_HK_CRM_Rebate_Disc_Amt_Per_BT__c==null,0,MeItems.ASI_HK_CRM_Rebate_Disc_Amt_Per_BT__c)}" />	
                                </apex:outputText>
                            </td>
                            <td style="border: 1px solid rgb(79,129,189);">
                                {!MeItems.ASI_HK_CRM_Conditions_for_Payment__c}
                            </td>
